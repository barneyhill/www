10784656, 10784805], [10785052, 10785157], [10791907, 10792080], [10796230, 10796542], [10802124, 10802292], [10803952, 10804124], [10809042, 10809175], [10813876, 10813956], [10818082, 10818204], [10819118, 10819173], [10830780, 10831110], [10838735, 10838772]], "strand": -1, "transcript": "NR_134935", "txend": 10838772, "txstart": 10762955}, {"cdsend": 10838772, "cdsstart": 10838772, "chr": "6", "position": [[10762955, 10764839], [10770343, 10770463], [10775560, 10775692], [10779558, 10779730], [10784656, 10784805], [10791907, 10792080], [10796230, 10796542], [10802124, 10802292], [10803952, 10804124], [10809042, 10809175], [10813876, 10813956], [10818082, 10818204], [10819118, 10819173], [10830780, 10831110], [10838574, 10838772]], "strand": -1, "transcript": "NR_134936", "txend": 10838772, "txstart": 10762955}], "generif": [{"pubmed": 12084720, "text": "identification and transcriptional activation by androgen in prostate cancer cells"}, {"pubmed": 16951154, "text": "MAK plays a general role in androgen receptor(AR) function in prostate cancer cells and is likely to be a general coactivator of AR in prostate tissues."}, {"pubmed": 16954377, "text": "MRK phosphorylates Scythe at T1080 in vitro as determined by site-directed mutagenesis and mass spectrometry, supporting the consensus and suggesting Scythe as a physiological substrate for MRK."}, {"pubmed": 21825139, "text": "Exome sequencing to identify a homozygous Alu insertion in exon 9 of male germ cell-associated kinase (MAK) as the cause of disease in an isolated individual with retinitis pigmentosa."}, {"pubmed": 21835304, "text": "Exome sequencing combined with this approach identified a homozygous nonsense mutation in male germ cell-associated kinase (MAK) in the single affected member of a consanguineous Turkish family with retinitis pigmentosa."}, {"pubmed": 21986944, "text": "these data implicate MAK in both adrogen receptor activation and chromosomal instability, acting in both early and late prostate cancer development."}, {"pubmed": 22110072, "text": "The patterns of disease expression in the MAK form of arRP showed some resemblance to patterns described in autosomal dominant RP, especially the form caused by RP1 mutations."}, {"pubmed": 23743448, "text": "In glioblastoma cells with deregulated high levels of CCRK, its depletion restores cilia through ICK and an ICK-related kinase MAK, thereby inhibiting glioblastoma cell proliferation."}, {"pubmed": 24244486, "text": "the expressions of ICK/MAK/MOK proteins in the intestinal tract can be differentially and dynamically regulated, implicating a significant functional diversity within this group of protein kinases."}, {"pubmed": 25255364, "text": "We identified the MAK and DHDDS mutations homozygously in only 2.1% and 0.8%, respectively, of patients of mixed ethnicity, but in 25.7% and 8.6%, respectively, of cases reporting Jewish ancestry"}, {"pubmed": 25385675, "text": "Nonsense and missense mutations in MAK give rise to a non-syndromic recessive RP phenotype without apparent extra-ocular features."}, {"pubmed": 26558903, "text": "One patient was homozygous for the insertion, one compound heterozygous with a missense change on the other allele (c. 46G>A; p.Gly16Arg), and three were heterozygous carriers."}, {"pubmed": 26894652, "text": "We report the first case of leaking intraretinal cystoid spaces due to mutation in MAK. Mak regulates microtubule stability via phosphorylating RP1. Abnormal Mak may impact retinal photoreceptor ciliary length and subcompartmentalization."}, {"pubmed": 29276052, "text": "The clinical data show that in general, patients with biallelic MAK mutations had a later age of onset and a milder retinal phenotype compared with patients with biallelic DHDDS mutations."}, {"pubmed": 29781741, "text": "The natural history of this individual's retinitis pigmentosa (RP) is consistent with previously described MAK mutations, being significantly milder than that associated with other photoreceptor ciliopathies. We suggest inclusion of MAK as part of wider genetic testing in all individuals presenting with RP."}], "genomic_pos": {"chr": "6", "end": 10838553, "ensemblgene": "ENSG00000111837", "start": 10762723, "strand": -1}, "genomic_pos_hg19": {"chr": "6", "end": 10838764, "start": 10762956, "strand": -1}, "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0006468", "pubmed": 21986944, "qualifier": "involved_in", "term": "protein phosphorylation"}, {"evidence": "NAS", "gocategory": "BP", "id": "GO:0007283", "pubmed": 2183027, "qualifier": "involved_in", "term": "spermatogenesis"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0030154", "qualifier": "involved_in", "term": "cell differentiation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0035556", "qualifier": "involved_in", "term": "intracellular signal transduction"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0042073", "qualifier": "involved_in", "term": "intraciliary transport"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0045494", "qualifier": "involved_in", "term": "photoreceptor cell maintenance"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0045893", "qualifier": "involved_in", "term": "positive regulation of DNA-templated transcription"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0046777", "pubmed": 21986944, "qualifier": "involved_in", "term": "protein autophosphorylation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0060271", "qualifier": "involved_in", "term": "cilium assembly"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:1902856", "qualifier": "involved_in", "term": "negative regulation of non-motile cilium assembly"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:1905515", "qualifier": "involved_in", "term": "non-motile cilium assembly"}], "CC": [{"evidence": "ISS", "gocategory": "CC", "id": "GO:0001750", "qualifier": "located_in", "term": "photoreceptor outer segment"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:000