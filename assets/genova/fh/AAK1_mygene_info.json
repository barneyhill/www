09, 69870406], [69870706, 69870870]], "strand": -1, "transcript": "NM_001371575", "txend": 69870870, "txstart": 69685128}, {"cdsend": 69870172, "cdsstart": 69688798, "chr": "2", "position": [[69685128, 69688837], [69736382, 69736592], [69741602, 69741881], [69746085, 69746372], [69747965, 69748120], [69752164, 69752244], [69754347, 69754451], [69757139, 69757272], [69757756, 69757838], [69759172, 69759294], [69769654, 69769797], [69771567, 69771676], [69783991, 69784110], [69870009, 69870406], [69870706, 69870871]], "strand": -1, "transcript": "NM_001371577", "txend": 69870871, "txstart": 69685128}, {"cdsend": 69870172, "cdsstart": 69703000, "chr": "2", "position": [[69685128, 69703095], [69704011, 69704122], [69706082, 69706193], [69707991, 69708093], [69709842, 69709944], [69723116, 69723212], [69732700, 69732805], [69734552, 69734710], [69736362, 69736592], [69741602, 69741881], [69746085, 69746372], [69747965, 69748120], [69752164, 69752244], [69754347, 69754451], [69757139, 69757272], [69757756, 69757838], [69759172, 69759294], [69769654, 69769797], [69771567, 69771676], [69783991, 69784110], [69870009, 69870406], [69870706, 69870871]], "strand": -1, "transcript": "NM_014911", "txend": 69870871, "txstart": 69685128}], "generif": [{"pubmed": 17494869, "text": "These observations suggest that AAK1 functions at multiple steps of the endosomal pathway by regulating transferrin internalization and its rapid recycling back to the plasma membrane from early/sorting endosome."}, {"pubmed": 19772629, "text": "Observational study, meta-analysis, and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 21464124, "text": "AAK1 increases the localization of activated Notch to Rab5-positive endocytic vesicles, while AAK1 depletion or overexpression of Numb, an inhibitor of the pathway, interferes with this localization."}, {"pubmed": 25653444, "text": "AAK1 and GAK are critical regulators of HCV entry that function in part by activating EGFR, AP2M1, and NUMB, and as the molecular targets underlying the antiviral effect of sunitinib and erlotinib, respectively."}, {"pubmed": 25852190, "text": "Two of these kinases that were classified as resistors were PX domain-containing serine/threonine kinase (PXK) and AP2-associated kinase 1 (AAK1), which promote receptor endocytosis and may enable cells to resist TRAIL-induced apoptosis"}, {"pubmed": 26853940, "text": "Results present the first structures of AAK1 and BIKE which reveal that all members of the Numb-associated kinase family share unusual activation segment architecture."}, {"pubmed": 30605688, "text": "Author show that AAK1 promotes clearance of LRP6 from the plasma membrane to suppress the WNT pathway."}, {"pubmed": 31905947, "text": "The Serine/Threonine Kinase AP2-Associated Kinase 1 Plays an Important Role in Rabies Virus Entry."}, {"pubmed": 36733250, "text": "Extracellular vesicle-transmitted miR-671-5p alleviates lung inflammation and injury by regulating the AAK1/NF-kappaB axis."}], "genomic_pos": {"chr": "2", "end": 69674349, "ensemblgene": "ENSG00000115977", "start": 69457997, "strand": -1}, "genomic_pos_hg19": {"chr": "2", "end": 69901481, "start": 69688532, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0006468", "qualifier": "involved_in", "term": "protein phosphorylation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0006468", "pubmed": [17494869, 18657069], "qualifier": "involved_in", "term": "protein phosphorylation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0032880", "pubmed": 18657069, "qualifier": "involved_in", "term": "regulation of protein localization"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0045747", "pubmed": 21464124, "qualifier": "involved_in", "term": "positive regulation of Notch signaling pathway"}, {"evidence": "IDA", "gocategory": "