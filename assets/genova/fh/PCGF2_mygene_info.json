2775001], [2775609, 2775761], [2783514, 2783655]], "strand": -1, "transcript": "NM_001369614", "txend": 2783655, "txstart": 2769216}, {"cdsend": 38740402, "cdsstart": 38735222, "chr": "17", "position": [[38733897, 38735600], [38736089, 38736170], [38738352, 38738448], [38738540, 38738595], [38738752, 38738861], [38739067, 38739118], [38739197, 38739253], [38739585, 38739682], [38740290, 38740442], [38746467, 38746628]], "strand": -1, "transcript": "NM_001369615", "txend": 38746628, "txstart": 38733897}, {"cdsend": 2775721, "cdsstart": 2770541, "chr": "17_KI270857v1_alt", "position": [[2769216, 2770919], [2771408, 2771489], [2773671, 2773767], [2773859, 2773914], [2774071, 2774180], [2774386, 2774437], [2774516, 2774572], [2774904, 2775001], [2775609, 2775761], [2781786, 2781947]], "strand": -1, "transcript": "NM_001369615", "txend": 2781947, "txstart": 2769216}, {"cdsend": 2775721, "cdsstart": 2770541, "chr": "17_KI270857v1_alt", "position": [[2769216, 2770919], [2771408, 2771489], [2773671, 2773767], [2773859, 2773914], [2774071, 2774180], [2774386, 2774437], [2774516, 2774572], [2774904, 2775001], [2775609, 2775761], [2783197, 2783290], [2783514, 2783655]], "strand": -1, "transcript": "NM_007144", "txend": 2783655, "txstart": 2769216}, {"cdsend": 38740402, "cdsstart": 38735222, "chr": "17", "position": [[38733897, 38735600], [38736089, 38736170], [38738352, 38738448], [38738540, 38738595], [38738752, 38738861], [38739067, 38739118], [38739197, 38739253], [38739585, 38739682], [38740290, 38740442], [38747878, 38747971], [38748195, 38748336]], "strand": -1, "transcript": "NM_007144", "txend": 38748336, "txstart": 38733897}], "exons_hg19": [{"cdsend": 36896655, "cdsstart": 36891475, "chr": "17", "position": [[36890150, 36891853], [36892342, 36892423], [36894605, 36894701], [36894793, 36894848], [36895005, 36895114], [36895320, 36895371], [36895450, 36895506], [36895838, 36895935], [36896543, 36896695], [36904448, 36904589]], "strand": -1, "transcript": "NM_001369614", "txend": 36904589, "txstart": 36890150}, {"cdsend": 36896655, "cdsstart": 36891475, "chr": "17", "position": [[36890150, 36891853], [36892342, 36892423], [36894605, 36894701], [36894793, 36894848], [36895005, 36895114], [36895320, 36895371], [36895450, 36895506], [36895838, 36895935], [36896543, 36896695], [36902720, 36902881]], "strand": -1, "transcript": "NM_001369615", "txend": 36902881, "txstart": 36890150}, {"cdsend": 36896655, "cdsstart": 36891475, "chr": "17", "position": [[36890150, 36891853], [36892342, 36892423], [36894605, 36894701], [36894793, 36894848], [36895005, 36895114], [36895320, 36895371], [36895450, 36895506], [36895838, 36895935], [36896543, 36896695], [36904131, 36904224], [36904448, 36904589]], "strand": -1, "transcript": "NM_007144", "txend": 36904589, "txstart": 36890150}], "generif": [{"pubmed": 11520462, "text": "The mouse counterpart of this gene regulates the expression of Th2 cytokines, and plays a critical role in Th2 cell differentiation and Th-2 dependent immune responses."}, {"pubmed": 11750047, "text": "The mouse counterpart of this gene regulates the expression of various chemokines and chemokine receptors, which may be important for T cell migration and differentiation."}, {"pubmed": 17145810, "text": "The oncogenic role of MEL-18 in human primary breast carcinomas is determined by its capacity to inhibit INK4a/ARF proteins(p16INK4a, p14ARF, or h-TERT) or to induce telomerase activity."}, {"pubmed": 17151361, "text": "Our data suggest that Mel-18 regulates Bmi-1 expression during senescence via down-regulation of c-Myc."}, {"pubmed": 17452456, "text": "These results suggest that Bmi-1 and Mel-18 may have overlapping functions in cancer cell growth."}, {"pubmed": 17545584, "text": "Mel-18 and Bmi-1 may regulate the Akt pathway in breast cancer cells, and that Mel-18 functions as a tumor suppressor by repressing the expression of Bmi-1 and consequently down-regulating Akt activity."}, {"pubmed": 17672918, "text": "validated occurrence of an unusual TG 3' splice site in intron 1"}, {"pubmed": 17936708, "text": "Results show that a phosphorylated form of Mel-18 targets the Ring1B histone H2A ubiquitin ligase to chromatin."}, {"pubmed": 18706886, "text": "The results of this study strengthen the conclusion that mel-18 functions as an anti-SUMO E3 factor, and extend its targets to include regulation of the sumoylation of the important cellular protein RanGAP1."}, {"pubmed": 19395284, "text": "Loss of Mel-18 is associated with prostate cancer."}, {"pubmed": 19585577, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19585577, "text": "Single Nucleotide Polymorphism and down regulation of Mel-18 is associated with prostate cancer."}, {"pubmed": 19727227, "text": "An association of Mel18 with emerin was observed in Hutchinson-Gilford progeria syndrome, but not in WT cells."}, {"pubmed": 20170541, "text": "BMI1 acts as an oncogene and Mel-18 functions as a tumor suppressor via downregulation of BMI1."}, {"pubmed": 20444850, "text": "Mel-18 may serve as a useful marker in prognostic evaluation for patients with breast cancer."}, {"pubmed": 20717685, "text": "Our analysis showed correlation between BMI1 and PCGF2 gene's expression and survival in children with medulloblastoma."}, {"pubmed": 20801102, "text": "Mel-18 plays a significant role in the angiogenic function of endothelial cells by regulating endothelial gene expression."}, {"pubmed": 21059209, "text": "Decreased Mel-18 and increased Bmi-1 mRNA expression was associated with the carcinogenesis and progression of gastric cancer"}, {"pubmed": 21162745, "text": "Bmi-1/Mel-18 ratio can be potentially used as a tool for stratifying women at risk of developing breast malignancy."}, {"pubmed": 21602890, "text": "these findings provide that Mel-18 is a novel regulator of tumor angiogenesis through regulating HIF-1alpha and its target VEGF expressions mediated by the PTEN/PI3K/Akt pathway, suggesting a new tumor-suppressive role of Mel-18 in human breast cancer."}, {"pubmed": 22085718, "text": "PCGF2, a PRC1 gene, played a negative role in the granulocytic differentiation of human APL cells."}, {"pubmed": 22954590, "text": "Findings suggest that Mel-18 is a novel negative regulator of breast cancer stem cell (CSC) that inhibits the stem cell population and in vitro and in vivo self-renewal through the inactivation of Wnt-mediated Notch signaling."}, {"pubmed": 23474752, "text": "Mel-18 functions as a tumor suppressor by its novel negative control of the epithelial-mesenchymal transition in breast cancer."}, {"pubmed": 24964959, "text": "It was therefore concluded that the lower Mel-18 expression might contribute to colorectal cancer development/progression."}, {"pubmed": 25822021, "text": "Mel-18 underexpression in luminal breast cancer cells caused ER-alpha downregulation.Its overexpression restored it in triple-negative breast cancer cells. MEL-18 suppressed SUMOylation of the ESR1 transactivators p53 and SP1."}, {"pubmed": 27030546, "text": "Suggest a novel role of PCGF2 in arsenic trioxide-mediated degradation of PML-RARA that PCGF2 might act as a negative regulator of UBE2I via direct interaction."}, {"pubmed": 27542229, "text": "Low expression of Mel-18 is correlated with gastric cancer."}, {"pubmed": 33664452, "text": "AKT-mediated regulation of chromatin ubiquitylation and tumorigenesis through Mel18 phosphorylation."}, {"pubmed": 35427425, "text": "SLCO4A1-AS1 triggers the malignant behaviours of melanoma cells via sponging miR-1306-5p to enhance PCGF2."}], "genomic_pos": [{"chr": "17", "end": 38749817, "ensemblgene": "ENSG00000277258", "start": 38733898, "strand": -1}, {"chr": "HSCHR17_7_CTG4", "end": 2785136, "ensemblgene": "ENSG00000278644", "start": 2769216, "strand": -1}], "genomic_pos_hg19": {"chr": "17", "end": 36906070, "start": 36890150, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0000122", "qualifier": "involved_in", "