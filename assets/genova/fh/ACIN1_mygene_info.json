f": [{"pubmed": 16177823, "text": "Akt phosphorylation of acinus on serine 422 and 573 results in its resistance to caspase cleavage in the nucleus and the inhibition of acinus-dependent chromatin condensation."}, {"pubmed": 16537548, "text": "Acinus is not involved in DNA condensation but rather has a role in internucleosomal DNA cleavage during programmed cell death"}, {"pubmed": 17409846, "text": "We found hypermethylation of the ACIN1 gene in early stage lung adenocarcinoma. The role of methylation status in the development and malignant transformation of lung adenocarcinoma requires clarification."}, {"pubmed": 18250153, "text": "identify Acinus-S' as a novel RAR-interacting protein[Acinus-S']"}, {"pubmed": 19387494, "text": "The authors report here that AAC-11, a survival protein whose expression prevents apoptosis that occurs on deprivation of growth factors, physiologically binds to Acinus and prevents Acinus-mediated DNA fragmentation."}, {"pubmed": 19615784, "text": "A critical role for AKT activation in protecting cells from ionizing radiation-induced apoptosis and the regulation of acinus gene expression is reported."}, {"pubmed": 19668232, "text": "Study shows that transcription corepressor CtBP2 directly binds acinus, which is regulated by nerve growth factor (NGF), inhibiting its stimulatory effect on cyclin A1, but not cyclin A2, expression in leukemia."}, {"pubmed": 20800603, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 23205493, "text": "Chronic low-dose radiation increases the levels of AKT and acinus proteins via NF-kappaB activation"}, {"pubmed": 24444330, "text": "An electronegative region on acinusS restricts SRPK2 phosphorylation to a single specific site."}, {"pubmed": 25079509, "text": "Differences in the nuclear localization of Acinus-S' and Acinus-L may suggest that these two isoforms have different functional roles."}, {"pubmed": 25205379, "text": "the ligand-dependent splicing activity of Acinus is related to the retinoic acid-activated retinoic acid receptor"}, {"pubmed": 25907765, "text": "acinus L was identified as a potential novel mTORC1 target."}, {"pubmed": 27365209, "text": "Genome-wide identification of RNA targets of Acinus revealed its role in splicing regulation as well as its involvement in other cellular pathways, including cell cycle progression."}, {"pubmed": 29366779, "text": "We conclude that RNPS1 is a key factor for the quality control of mRNAs that is essential for the phenotypes including cell division."}, {"pubmed": 30201066, "text": "ACIN1 mRNA was highly expressed in platelets of lung cancer patients"}, {"pubmed": 33142236, "text": "The SRSF3-MBNL1-Acin1 circuit constitutes an emerging axis to lessen DNA fragmentation in colorectal cancer via an alternative splicing mechanism."}], "genomic_pos": {"chr": "14", "end": 23095614, "ensemblgene": "ENSG00000100813", "start": 23058564, "strand": -1}, "genomic_pos_hg19": {"chr": "14", "end": 23564823, "start": 23527773, "strand": -1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0006397", "qualifier": "involved_in", "term": "mRNA processing"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0008380", "qualifier": "involved_in", "term": "RNA splicing"}, {"evidence": "IEP", "gocategory": "BP", "id": "GO:0030218", "pubmed": 11208865, "qualifier": "involved_in", "term": "erythrocyte differentiation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0030263", "pubmed": 10490026, "qualifier": "involved_in", "term": "apoptotic chromosome condensation"}, {"evidence": "IEP", "gocategory": "BP", "id": "GO:0045657", "pubmed": 12393560, "qualifier": "involved_in", "term": "positive regulation of monocyte differentiation"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0005634", "pubmed": 10490026, "qualifier": "located_in", "term": "nucleus"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": 