40407, 45040441], [45043256, 45043421], [45061066, 45061226], [45065270, 45065389], [45068180, 45068246], [45068876, 45069052], [45072316, 45072438], [45073364, 45077185]], "strand": 1, "transcript": "NM_003104", "txend": 45077185, "txstart": 45023194}, {"cdsend": 45077185, "cdsstart": 45077185, "chr": "15", "position": [[45023194, 45023349], [45040407, 45040441], [45043256, 45043421], [45060981, 45061226], [45065270, 45065389], [45068180, 45068246], [45068876, 45069052], [45072316, 45072438], [45073364, 45077185]], "strand": 1, "transcript": "NR_034039", "txend": 45077185, "txstart": 45023194}], "exons_hg19": [{"cdsend": 45365728, "cdsstart": 45315481, "chr": "15", "position": [[45315392, 45315547], [45332605, 45332639], [45335454, 45335619], [45353264, 45353424], [45357468, 45357587], [45360378, 45360444], [45361074, 45361250], [45364514, 45364636], [45365562, 45369383]], "strand": 1, "transcript": "NM_003104", "txend": 45369383, "txstart": 45315392}, {"cdsend": 45369383, "cdsstart": 45369383, "chr": "15", "position": [[45315392, 45315547], [45332605, 45332639], [45335454, 45335619], [45353179, 45353424], [45357468, 45357587], [45360378, 45360444], [45361074, 45361250], [45364514, 45364636], [45365562, 45369383]], "strand": 1, "transcript": "NR_034039", "txend": 45369383, "txstart": 45315392}], "generif": [{"pubmed": 12595725, "text": "crystals of sorbitol dehydrogenase belong to the monoclinic C2 space group, with unit-cell parameters a = 145.9, b = 52.3, c = 169.0 A, beta = 101.8 degrees"}, {"pubmed": 14965227, "text": "Sorbitol dehydrogenase (SDH), a member of the medium-chain dehydrogenase/reductase protein family and the second enzyme of the polyol pathway of glucose metabolism, converts sorbitol to fructose strictly using NAD(+) as coenzyme."}, {"pubmed": 17343568, "text": "Results compare the catalytic mechanism of liver sorbitol dehydrogenase with wild-type and Glu154-->Cys forms of yeast xylitol dehydrogenase."}, {"pubmed": 19423711, "text": "The zinc-finger protein ZAC1 is up-regulated under hypertonic stress and negatively regulates expression of sorbitol dehydrogenase, allowing for accumulation of sorbitol as a compatible organic osmolyte."}, {"pubmed": 20372835, "text": "The expression of SORD is regulated by androgens in human prostate. In prostate cancer, increased immunostaining was associated with high Gleason patterns and high serum PSA concentrations."}, {"pubmed": 20628624, "text": "Meta-analysis of gene-disease association. (HuGE Navigator)"}, {"pubmed": 23452182, "text": "The SDH level was significantly decreased in patients with proliferative compared with non-proliferative retinopathy in both insulin and oral diabetic groups."}, {"pubmed": 23850972, "text": "Thus, our findings suggest that the -888G > C polymorphism in the SORD gene is not involved in the pathogenesis of diabtic retinopathy in type 2 diabetes."}, {"pubmed": 24567419, "text": "One of the most striking changes involved sorbitol dehydrogenase, a key enzyme in the polyol pathway. Validation studies revealed dramatically increased sorbitol dehydrogenase concentrations and activity in adenomas and cancer cell lines, along with important changes in the expression of other enzymes in the same (AKR1B1) and related (KHK) pathways."}, {"pubmed": 27628063, "text": "SDH gene expression was increased by hypoxia and oxidative stress, but not extracellular hyperosmolarity. Hyperosmolarity and hypoxia did not alter the SDH protein level."}, {"pubmed": 31539366, "text": "ALR2 rs759853 and SDH rs2055858 polymorphisms were respectively associated with a higher and lower risk of diabetic retinopathy."}, {"pubmed": 33314640, "text": "Evaluation of SORD mutations as a novel cause of Charcot-Marie-Tooth disease."}, {"pubmed": 33875678, "text": "Biallelic variants in the SORD gene are one of the most common causes of hereditary neuropathy among Czech patients."}, {"pubmed": 35436891, "text": "Association of SORD mutation with autosomal recessive asymmetric distal hereditary motor neuropathy."}, {"pubmed": 37418112, "text": "Hereditary polyneuropathy with conduction block associated with SORD mutation in three siblings."}, {"pubmed": 37584201, "text": "Expanding the genetic and clinical spectrum of SORD-related peripheral neuropathy by reporting a novel variant c.210T>G and evidence of subclinical muscle involvement."}], "genomic_pos": {"chr": "15", "end": 45077185, "ensemblgene": "ENSG00000140263", "start": 45023147, "strand": 1}, "genomic_pos_hg19": {"chr": "15", "end": 45369383, "start": 45315302, "strand": 1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0006006", "pubmed": 14965227, "qualifier": "involved_in", "term": "glucose metabolic process"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006062", "qualifier": "involved_in", "term": "sorbitol catabolic process"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0006062", "pubmed": [3365415, 6870831, 8487505, 12962626], "qualifier": "involved_in", "term": "sorbitol catabolic process"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0006062", "pubmed": 32367058, "qualifier": "involved_in", "term": "sorbitol catabolic process"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0019640", "qualifier": "involved_in", "term": "glucuronate catabolic process to xylulose 5-phosphate"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0030317", "qualifier": "involved_in", "term": "flagellated sperm motility"}, {"evidence": "NAS", "gocategory": "BP", "id": "GO:0030317", "pubmed": 16278369, "qualifier": "involved_in", "term": "flagellated sperm motility"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0046370", "pubmed": [3365415, 6870831, 8487505], "qualifier": "involved_in", "term": "fructose biosynthetic process"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0051160", "pubmed": 8487505, "qualifier": "involved_in", "term": "L-xylitol catabolic process"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0051164", "pubmed": 3365415, "