02], [58285486, 58285623], [58311223, 58311273], [58312015, 58312085], [58313473, 58313561], [58315475, 58315581], [58316765, 58316858], [58350235, 58350368], [58358942, 58359063], [58362275, 58362334], [58366800, 58366967], [58373450, 58373609], [58386483, 58387055]], "strand": 1, "transcript": "NM_001288836", "txend": 58387055, "txstart": 58273776}, {"cdsend": 58386828, "cdsstart": 58275966, "chr": "2", "position": [[58134785, 58134974], [58252799, 58252905], [58253878, 58253996], [58260361, 58260688], [58275961, 58276102], [58311223, 58311273], [58312015, 58312085], [58313473, 58313561], [58315475, 58315581], [58316765, 58316858], [58350235, 58350368], [58358942, 58359063], [58362275, 58362334], [58366800, 58366967], [58373450, 58373609], [58386483, 58387055]], "strand": 1, "transcript": "NM_001288837", "txend": 58387055, "txstart": 58134785}, {"cdsend": 58381897, "cdsstart": 58275966, "chr": "2", "position": [[58134785, 58134974], [58252799, 58252905], [58260361, 58260688], [58275961, 58276102], [58311223, 58311273], [58312015, 58312085], [58313473, 58313561], [58315475, 58315581], [58316765, 58316858], [58350235, 58350368], [58358942, 58359063], [58362275, 58362334], [58366800, 58366967], [58373450, 58373609], [58381888, 58381937], [58386483, 58387055]], "strand": 1, "transcript": "NM_001288838", "txend": 58387055, "txstart": 58134785}, {"cdsend": 58386828, "cdsstart": 58315485, "chr": "2", "position": [[58273776, 58274003], [58275961, 58276102], [58311223, 58311273], [58313473, 58313561], [58315475, 58315581], [58316765, 58316858], [58350235, 58350368], [58358942, 58359063], [58362275, 58362334], [58366800, 58366967], [58373450, 58373609], [58386483, 58387055]], "strand": 1, "transcript": "NM_001288839", "txend": 58387055, "txstart": 58273776}, {"cdsend": 58386828, "cdsstart": 58275966, "chr": "2", "position": [[58273940, 58274003], [58275961, 58276102], [58311223, 58311273], [58312015, 58312085], [58313473, 58313561], [58315475, 58315581], [58316765, 58316858], [58350235, 58350368], [58358942, 58359063], [58362275, 58362334], [58366800, 58366967], [58373450, 58373609], [58386483, 58387005]], "strand": 1, "transcript": "NM_006296", "txend": 58387005, "txstart": 58273940}], "generif": [{"pubmed": 16704422, "text": "VRK2 induces p53 stabilization by post-translational modification, largely due to threonine 18 phosphorylation in tumor cell lines."}, {"pubmed": 16963744, "text": "human VRK2 interacts specifically with EBV BHRF1 and that the interaction is involved in protecting cells from apoptosis"}, {"pubmed": 17709393, "text": "Data show that TAK1 forms a stable complex with JIP1 and regulates the activation of JNK, which in turn determines the cellular stress response to hypoxia; this activation of TAK1-JIP1-JNK is suppressed by vaccinia-related kinase 2."}, {"pubmed": 18286207, "text": "the activity of JIP1-JNK complexes is downregulated by VRK2 in response to interleukin-1beta"}, {"pubmed": 18612383, "text": "the downregulation of VRK2 protein levels, as a consequence of p53 accumulation, is thus dependent on the levels of the p300/CBP protein available for transcriptional complexes"}, {"pubmed": 18617507, "text": "Ran is a novel negative regulator of nuclear VRK1 and VRK2 kinase activity, which may vary in different subcellular localizations generating an asymmetric intracellular distribution of kinase activity depending on local protein interactions."}, {"pubmed": 20679487, "text": "Data suggest a role for VRK2A in ErbB2-MAPK signaling."}, {"pubmed": 21791550, "text": "Common variants at VRK2 show genome-wide significant association with schizophrenia."}, {"pubmed": 22752157, "text": "VRK2A can form a high molecular size complex with both MEK1 and KSR1; the KSR1 complex assembled and retained by VRK2A in the endoplasmic reticulum can have a modulatory effect on the signal mediated by MAPK,locally affecting the magnitude of its responses"}, {"pubmed": 22949513, "text": "Generalized epilepsies implicates susceptibility genes: CHRM3 at 1q43, VRK2 at 2p16.1, ZEB2 at 2q22.3, SCN1A at 2q24.3 and PNPO at 17q21.32."}, {"pubmed": 23082977, "text": "Data from molecular dynamic simulations suggest that bivalent cations play key structural roles in stabilization of VRK1/VRK2; bivalent cations have no obvious effects on VRK3. These kinases are targets in search for antineoplastic agents. [REVIEW]"}, {"pubmed": 23102693, "text": "This study analyzed 5 genome-wide supported variants in a Han Chinese sample, and the variant rs2312147 at VRK2 showed significant association, which was confirmed in the meta-analysis combining multiple Asian and European."}, {"pubmed": 23105117, "text": "Human VRK2 is an active kinase playing a role in regulation of cancer cell invasion through the NFAT pathway and COX-2 expression."}, {"pubmed": 23449449, "text": "Low levels of VRK2A causes an increase in mitochondrial Bax protein level, leading to an increase in the release of cytochrome C and caspase activation, detected by PARP processing."}, {"pubmed": 24298020, "text": "VRK2 is crucial to regulate the ubiquitination-proteosomal degradation of eukaryotic chaperonin TCP-1 ring complex"}, {"pubmed": 25079070, "text": "data may provide evidence for the effect of VRK2 on WM connectivity in patients with schizophrenia"}, {"pubmed": 25755282, "text": "Findings suggest that ubiquitin-specific protease 25 (USP25) as a VRK2 substrate that acts on TRiC deubiquitination."}, {"pubmed": 26345874, "text": "Our data provide preliminary evidence that the VRK2 gene might play a major role in the development of SCZ in the Northwest Chinese Han population."}, {"pubmed": 26941264, "text": "Reduced VRK2 mRNA levels are involved in the underlying mechanisms in schizophrenia spectrum disorders."}, {"pubmed": 27377031, "text": "GSK3beta may inhibit VRK2 catalytic activity by disrupting its flexibility. The inhibition of VRK2 catalytic activity by GSK3beta may also inhibit VRK2-induced degradation of TRiC, which could suppress polyQ-expanded Htt aggregation."}, {"pubmed": 27382989, "text": "The data provides further evidence for the genetic contributions of VRK2 rs2312147 to schizophrenia susceptibility especially in Europeans, while further replication analyses in Asian populations are still needed. [meta-analysis]"}, {"pubmed": 27456229, "text": "VRK1 AND VRK2 expression are predictive of tumor response to neoadjuvant chemoradiation therapy in rectal adenocarcinoma."}, {"pubmed": 27494321, "text": "we replicated one known signal in PAX8 (2.6 minutes per allele, 95% CI [1.9, 3.2], P = 5.7x10-16) and identified and replicated two novel associations at VRK2 (2.0 minutes per allele, 95% CI [1.3, 2.7], P = 1.2x10-9; and 1.6 minutes per allele, 95% CI [1.1, 2.2], P = 7.6x10-9)"}, {"pubmed": 27922604, "text": "The combined analysis confirmed evidence of genome-wide significant associations in the Han Chinese population for three loci, at 2p16.1 (rs1051061, in an exon of VRK2, P=1.14 x 10-12, odds ratio (OR)=1.17), 6p22.1 (rs115070292 in an intron of GABBR1, P=4.96 x 10-10, OR=0.77) and 10q24.32 (rs10883795 in an intron of AS3MT, P=7.94 x 10-10, OR=0.87; rs10883765 at an intron of ARL3, P=3.06 x 10-9, OR=0.87)."}, {"pubmed": 28515294, "text": "These data support a model in which vaccinia virus B1 and human VRK2 share additional substrates important for the replication of cytoplasmic poxviruses."}, {"pubmed": 28790404, "text": "Structural characterization of human vaccinia-related kinases, VRK1 and VRK2, bound to small-molecule inhibitors, has been reported."}, {"pubmed": 29100046, "text": "VRK2 is suggested as a candidate gene for neurological disorders through its role in signaling pathway, the neuronal loss and stress response."}, {"pubmed": 29872222, "text": "a novel insight into the oncogenic activity of VRK2-Akt complexes in the lysosomes via modulation of autophagy"}, {"pubmed": 30062698, "text": "our work reveals the regulation of dysbindin by VRK2, providing the association of these two proteins, which are commonly implicated in schizophrenia"}, {"pubmed": 33522046, "text": "Evaluation of the relationship between VRK2, rs4380187 polymorphisms, and genetic susceptibility to schizophrenia in the Chinese Han population."}, {"pubmed": 33875785, "text": "Vaccinia-related kinase 2 blunts sorafenib's efficacy against hepatocellular carcinoma by disturbing the apoptosis-autophagy balance."}, {"pubmed": 35173553, "text": "VRK2 activates TNFalpha/NF-kappaB signaling by phosphorylating IKKbeta in pancreatic cancer."}, {"pubmed": 36040810, "text": "VRK1 as a synthetic lethal target in VRK2 promoter-methylated cancers of the nervous system."}, {"pubmed": 37452335, "text": "Reduced Vrk2 expression is associated with higher risk of depression in humans and mediates depressive-like behaviors in mice."}, {"pubmed": 37943248, "text": "Elucidating the interaction of C-terminal domain of Vaccinia-Related Kinase 2A (VRK2A) with B-cell lymphoma-extra Large (Bcl-xL) to decipher its anti-apoptotic role in cancer."}], "genomic_pos": {"chr": "2", "end": 58159920, "ensemblgene": "ENSG00000028116", "start": 57907629, "strand": 1}, "genomic_pos_hg19": {"chr": "2", "end": 58387055, "start": 58134786, "strand": 1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0006468", "pubmed": 9344656, "qualifier": "involved_in", "term": "protein phosphorylation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007165", "qualifier": "involved_in", "term": "signal transduction"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0018105", "qualifier": "involved_in", "term": "peptidyl-serine phosphorylation"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0034599", "pubmed": 17709393, "qualifier": "involved_in", "term": "cellular response to oxidative stress"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0043408", "pubmed": 20679487, "qualifier": "involved_in", "term": "regulation of MAPK cascade"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0046777", "pubmed":