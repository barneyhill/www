 1111991, "cdsstart": 1052359, "chr": "5", "position": [[1050383, 1052451], [1053348, 1053482], [1057470, 1057649], [1060343, 1060451], [1063843, 1063975], [1064082, 1064252], [1065282, 1065478], [1073632, 1073801], [1074566, 1074671], [1075370, 1075490], [1076137, 1076236], [1076693, 1076812], [1077832, 1078007], [1078700, 1078758], [1079397, 1079496], [1081576, 1081744], [1083744, 1083956], [1085231, 1085473], [1086902, 1087033], [1088305, 1088360], [1088981, 1089128], [1093532, 1093655], [1094153, 1094248], [1111867, 1112063]], "strand": -1, "transcript": "NM_006598", "txend": 1112063, "txstart": 1050383}], "exons_hg19": [{"cdsend": 1112106, "cdsstart": 1052474, "chr": "5", "position": [[1050498, 1052566], [1053463, 1053597], [1057585, 1057764], [1060458, 1060566], [1063958, 1064090], [1064197, 1064367], [1065397, 1065593], [1073747, 1073916], [1074681, 1074786], [1075485, 1075605], [1076252, 1076351], [1076808, 1076927], [1077947, 1078122], [1078815, 1078873], [1079512, 1079611], [1081691, 1081859], [1083859, 1084071], [1085346, 1085588], [1087017, 1087148], [1088420, 1088475], [1089096, 1089243], [1093647, 1093770], [1094268, 1094363], [1111982, 1112178]], "strand": -1, "transcript": "NM_006598", "txend": 1112178, "txstart": 1050498}], "generif": [{"pubmed": 11976689, "text": "Mice lacking SLC12A7 function suffer from deafness and renal metabolic acidosis. The phenotype suggests a function in inner ear potassium recycling. A function in the proton secreting alpha-intercalating cells of the kidney is shown."}, {"pubmed": 12637262, "text": "human osteoblasts express functional K-Cl cotransporters in their cell membrane that seem to be able to induce the indirect activation of volume-sensitive Cl- channels by KCl through an increase in the intracellular ions, water influx and cell swelling."}, {"pubmed": 15262997, "text": "KCC activation by IGF-1 plays an important role in IGF-1 signaling to promote growth and spread of gynecological cancers."}, {"pubmed": 17133354, "text": "Among patients with early-stage node-negative breast cancer, disease-free survival (DFS) and overall survival (OS) curves were significantly different based on IGF-1 and KCC expression."}, {"pubmed": 18984587, "text": "KCC4 and H+,K+-ATPase are the main machineries for basal HCl secretion in the apical canalicular membrane of the resting parietal cell. They also may contribute in part to massive acid secretion in the stimulated state."}, {"pubmed": 19887603, "text": "In the metastatic cancer tissues, KCC4 colocalizes with IGF-I or EGF."}, {"pubmed": 20516068, "text": "analysis of differences in large extracellular loop between the K(+)-Cl(-) cotransporters KCC2 and KCC4"}, {"pubmed": 20734064, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 21613606, "text": "The Wnk3 protein isoforms have a similar effect on SLC12 cotransporters. NKCC1/2 and NCC were inhibited, even in hypertonicity, while KCCs were activated, even in isotonic conditions."}, {"pubmed": 21733850, "text": "KCC3 is the dominant isoform in erythrocytes, with variable expression of KCC1 and KCC4 that could result in modulation of KCC activity"}, {"pubmed": 21760919, "text": "Negative stain transmission electron microscopy and single particle analysis of KCC4 and the aquaporin-1 AQP1 water channel, revealed the expected quaternary structures within homogeneous preparations, and thus correct protein folding and assembly."}, {"pubmed": 26454676, "text": "SLC12A7 gene amplification and overexpression occurs frequently in adrenocortical carcinoma and may play a role in adrenocortical carcinoma tumorigenesis."}, {"pubmed": 29884238, "text": "SLC12A7 alters adrenocortical carcinoma cell adhesion properties to promote an aggressive invasive behavior"}, {"pubmed": 31034883, "text": "The IGF1 gene was amplified in 9 of 19 ACC samples, similar to findings in The Cancer Genome Atlas database. The IGF1 overexpression was observed in 5 samples and was associated with SLC12A7 overexpression and non-functional, early-stage tumors (p < 0.05). endogenous overexpression and silencing of SLC12A7 significantly altered IGF1 and IGF1R expression without impacting other IGFs."}, {"pubmed": 31393094, "text": "SLC12A ion transporter mutations in sporadic and familial human congenital hydrocephalus."}], "genomic_pos": [{"chr": "5", "end": 1112063, "ensemblgene": "ENSG00000113504", "start": 1050384, "strand": -1}, {"chr": "HSCHR5_4_CTG1", "end": 129717, "ensemblgene": "ENSG00000276482", "start": 68885, "strand": -1}], "genomic_pos_hg19": {"chr": "5", "end": 1112150, "start": 1050499, "strand": -1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0006811", "qualifier": "involved_in", "term": "monoatomic ion transport"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006884", "qualifier": "involved_in", "term": "cell volume homeostasis"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007268", "qualifier": "involved_in", "term": "chemical synaptic transmission"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0055064", "qualifier": "involved_in", "term": "chloride ion homeostasis"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0055075", "qualifier": "involved_in", "term": "potassium ion homeostasis"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0071333", "qualifier": "involved_in", "term": "cellular response to glucose stimulus"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0071805", "qualifier": "involved_in", "term": "potassium ion transmembrane transport"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0140157", "qualifier": "involved_in", "term": "ammonium import across plasma membrane"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:1902476", "qualifier": "involved_in", "term": "chloride transmembrane transport"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:1990573", "qualifier": "involved_in", "term": "potassium ion import across plasma membrane"}], "CC": [{"evidence": "IBA", "g