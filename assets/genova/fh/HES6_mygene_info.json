: [[238238266, 238239251], [238239486, 238239568], [238239660, 238239747], [238239824, 238240038]], "strand": -1, "transcript": "NM_018645", "txend": 238240038, "txstart": 238238266}], "exons_hg19": [{"cdsend": 239148546, "cdsstart": 239147467, "chr": "2", "position": [[239146907, 239147892], [239148127, 239148203], [239148301, 239148388], [239148465, 239148679]], "strand": -1, "transcript": "NM_001142853", "txend": 239148679, "txstart": 239146907}, {"cdsend": 239148546, "cdsstart": 239147763, "chr": "2", "position": [[239146907, 239147892], [239148167, 239148209], [239148301, 239148388], [239148465, 239148679]], "strand": -1, "transcript": "NM_001282434", "txend": 239148679, "txstart": 239146907}, {"cdsend": 239148546, "cdsstart": 239147467, "chr": "2", "position": [[239146907, 239147892], [239148127, 239148209], [239148301, 239148388], [239148465, 239148679]], "strand": -1, "transcript": "NM_018645", "txend": 239148679, "txstart": 239146907}], "generif": [{"pubmed": 12957362, "text": "Differentially expressed in metastatic colon carcinoma"}, {"pubmed": 12972610, "text": "Hes6 promotes cortical neurogenesis and inhibits Hes1 transcription repression activity."}, {"pubmed": 15896295, "text": "Here, we show that WRPW motif deletion mutant Hes6 protein is substantially stabilized in comparison to the full length protein and that the enhanced stability is due to its resistance to proteasomal degradation."}, {"pubmed": 17300753, "text": "Hes6 overexpression using a retroviral vector led to a decrease in Hes1 levels, an increase in beta-cell transcription factors and partial restoration of insulin expression"}, {"pubmed": 17301032, "text": "Increased expression of Notch3, Jagged1, Hes1, and Hes6 gene transcripts were observed during differentiation of cultured human skeletal muscle cells."}, {"pubmed": 17826772, "text": "ATOH1 activates HES6 transcription through binding to three clustered E boxes of its promoter"}, {"pubmed": 18160400, "text": "the Hes6-CBP complex in PML-NB may influence the proliferation of cells via p53-dependent and -independent pathways."}, {"pubmed": 19058789, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19481584, "text": "Observational study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 19481584, "text": "These four variants were tested for association with mood disorder diagnosis or antidepressant response in a family study of depression, but no significant associations were observed."}, {"pubmed": 19891787, "text": "Data suggest that Hes-6 is a potential oncogene overexpressed in breast cancer, with a tumor-promoting and proliferative function. Hes-6 is a novel estrogen-regulated gene in breast cancer cells."}, {"pubmed": 21785461, "text": "HES6 gene is selectively overexpressed in glioma and represents an important transcriptional regulator of glioma proliferation."}, {"pubmed": 22982728, "text": "HES6 contributes to the pathogenesis of alveolar rhabdomyosarcoma by enhancing both proliferation and cell motility."}, {"pubmed": 24737870, "text": "HES6 is up-regulated in aggressive human prostate cancer and drives tumour growth by enhancing the transcriptional activity of the AR, which is preferentially directed to a regulatory network enriched for transcription factors such as E2F1."}, {"pubmed": 25006183, "text": "HES6 mutations are associated with castration resistant and androgen independent tumors in prostate cancer."}, {"pubmed": 25864518, "text": "prostate cancer-specific up-regulation of NOTCH3 and HES6, is reported."}, {"pubmed": 30015909, "text": "High HES6 expression is associated with metastasis in colorectal cancer."}, {"pubmed": 33462406, "text": "Single-cell RNA sequencing reveals intratumoral heterogeneity in primary uveal melanomas and identifies HES6 as a driver of the metastatic disease."}, {"pubmed": 35934844, "text": "ERCC5, HES6 and RORA are potential diagnostic markers of coronary artery disease."}, {"pubmed": 36929421, "text": "The novel GATA1-interacting protein HES6 is an essential transcriptional cofactor for human erythropoiesis."}], "genomic_pos": {"chr": "2", "end": 238240662, "ensemblgene": "ENSG00000144485", "start": 238238267, "strand": -1}, "genomic_pos_hg19": {"chr": "2", "end": 239149303, "start": 239146908, "strand": -1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0000122", "qualifier": "involved_in", "term": "negative regulation of transcription by RNA polymerase II"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0006355", "pubmed": 10851137, "qualifier": "involved_in", "term": "regulation of DNA-templated transcription"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006357", "qualifier": "involved_in", "term": "regulation of transcription by RNA polymerase II"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0007399", "qualifier": "involved_in", "term": "nervous system development"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0009952", "qualifier": "involved_in", "term": "anterior/posterior pattern specification"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0030154", "qualifier": "involved_in", "term": "cell differentiation"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0043433", "qualifier": "involved_in", "term": "negative regulation of DNA-binding transcription factor activity"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0050767", "qualifier": "involved_in", "term": "regulation of neurogenesis"}], "CC": [{"evidence": "ISA", "gocategory": "CC", "id": "GO:0000785", "qualifier": "part_of", "term": "chromatin"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "is_active_in", "term": "nucleus"}, {"evidence": "ISS", "gocategory": "CC", "id": "GO:0005667", "pubmed": 10851137, "qualifier": "part_of", "term": "transcription regulator complex"}], "MF": [{"category": "MF", "evidence": "IBA", "id": "GO:0000978", "qualifier": "ena