nerif": [{"pubmed": 12804766, "text": "GCA interacts with SRI in vivo and in vitro."}, {"pubmed": 18071670, "text": "The reported type 1 diabetes association is from a linkage disequilibrium region including three candidate genes, FAP, IFIH1 and GCA."}, {"pubmed": 18285833, "text": "IFIH1 interferon induced helicase, GCA grancalcin or the potassium channel KCNH7 - are potential candidates implicated in the pathogenesis of multiple sclerosis."}, {"pubmed": 18285833, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19156166, "text": "IFIH1-GCA-KCNH7 locus is not associated with genetic susceptibility to multiple sclerosis in French patients"}, {"pubmed": 19156166, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20237496, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20800603, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 26648480, "text": "Results indicate that heterodimerization of GCA and TLR9 is important for TLR9-mediated downstream signaling and might serve to fine tune processes against viral infection."}, {"pubmed": 30929559, "text": "GCA activates TRAF6 ubiquitin ligase activity to induce Lys63 ubiquitination of ULK1, a crucial regulator of autophagy, resulting in its stabilization and activation. It was also highlighted the role of GCA-TRAF6-ULK1 autophagy regulatory axis in imatinib resistance."}], "genomic_pos": {"chr": "2", "end": 162371595, "ensemblgene": "ENSG00000115271", "start": 162318840, "strand": 1}, "genomic_pos_hg19": {"chr": "2", "end": 163228105, "start": 163175350, "strand": 1}, "go": {"BP": {"evidence": "TAS", "gocategory": "BP", "id": "GO:0061025", "pubmed": 1530588, "qualifier": "involved_in", "term": "membrane fusion"}, "CC": [{"evidence": "TAS", "gocategory": "CC", "id": "GO:0005576", "qualifier": "located_in", "term": "extracellular region"}, {"evidence": "TAS", "gocategory