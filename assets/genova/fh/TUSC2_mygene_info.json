, {"pubmed": 19671678, "text": "These results suggest that the three miRNAs are negative regulators of Fus1 expression in lung cancers."}, {"pubmed": 19852844, "text": "The goal of this study was to analyze possible involvement of TUSC2 in malignant pleural mesothelioma."}, {"pubmed": 21273575, "text": "Absence of tumor suppressor FUS1 protein expression is associated with bone and soft tissue sarcomas."}, {"pubmed": 21645495, "text": "RNA sequence elements in FUS1 UTRs that regulate FUS1 protein express were identified."}, {"pubmed": 24146957, "text": "The tumor suppressor gene TUSC2 (FUS1) sensitizes NSCLC to the AKT inhibitor MK2206 in LKB1-dependent manner."}, {"pubmed": 24394498, "text": "TUSC2P and TUSC2 3'-UTR expression inhibits cell proliferation, survival, migration, invasion and colony formation, and increases tumor cell death.  TUSC2P and TUSC2 3'-UTR bind to miRNAs and arrest their functions, leading to increased TUSC2 translation."}, {"pubmed": 26053020, "text": "The therapeutic activity of TUSC2 could extend the use of erlotinib to lung cancer patients with wildtype EGFR."}, {"pubmed": 26367773, "text": "FOXP1, TP53INP1, TNFAIP3, and TUSC2 were identified as miR-19a targets."}, {"pubmed": 27661106, "text": "Data show that TUSC2 is a direct target of miR-584, which is transcriptionally regulated by TWIST1."}, {"pubmed": 27845352, "text": "these findings show that the combination of TUSC2-erlotinib induces additional novel vulnerabilities that can be targeted with Auranofin."}, {"pubmed": 30219035, "text": "TUSC2P can suppresses the tumor function of esophageal squamous cell carcinoma by regulating TUSC2 expression and may also serve as a prognostic factor for esophageal squamous cell carcinoma patients."}, {"pubmed": 30896880, "text": "Low TUSC2 expression is associated with bladder cancer."}, {"pubmed": 31481748, "text": "MicroRNA-138 is a Prognostic Biomarker for Triple-Negative Breast Cancer and Promotes Tumorigenesis via TUSC2 repression."}, {"pubmed": 31973107, "text": "The TUSC2 Tumour Suppressor Inhibits the Malignant Phenotype of Human Thyroid Cancer Cells via SMAC/DIABLO Protein."}, {"pubmed": 35167936, "text": "NEDD4 degrades TUSC2 to promote glioblastoma progression."}], "genomic_pos": {"chr": "3", "end": 50328251, "ensemblgene": "ENSG00000114383", "start": 50320027, "strand": -1}, "genomic_pos_hg19": [{"chr": "3", "end": 50365682, "start": 50357458, "strand": -1}, {"chr": "HG325_PATCH", "end": 50365683, "start": 50357459, "strand": -1}], "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0001779", "qualifier": "involved_in", "term": "natural killer cell differentiation"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0006909", "qualifier": "involved_in", "term": "phagocytosis"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006954", "qualifier": "involved_in", "term": "inflammatory response"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0032700", "qualifier": "involved_in", "term": "negative regulation of interleukin-17 production"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0032733", "qualifier": "involved_in", "term": "positive regulation of interleukin-10 production"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0048469", "qualifier": "involved_in", "term": "cell maturation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0051881", "qualifier": "involved_in", "term": "regulation of mitochondrial membrane potential"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0070945", "qualifier": "involved_in", "term": "neutrophil-mediated killing of gram-negative bacterium"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:2000377", "qualifier": "involved_in", "term": "regulation of reactive oxygen species metabolic process"}], "CC": {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005739", "qualifier": "is_active_in", "term": "mitochondrion"}, "MF": {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [25416956, 29892012, 32296183], "qualif