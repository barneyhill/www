n": [[2319347, 2323890], [2324024, 2324198], [2341402, 2341492], [2344783, 2344853], [2367501, 2367644], [2371054, 2371170], [2376817, 2376958], [2380979, 2381179], [2405497, 2405625], [2415052, 2415182]], "strand": -1, "transcript": "NM_024086", "txend": 2415182, "txstart": 2319347}], "generif": [{"pubmed": 20602751, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 27872311, "text": "RNA immunoprecipitation after in vivo UV cross-linking and an in situ proximity ligation assay for RNA-protein interactions confirmed an association between METTL16 and MALAT1 in cells. METTL16 is an abundant ( approximately 5 x 10(5) molecules per cell) nuclear protein in HeLa cells."}, {"pubmed": 28525753, "text": "METTL16 that controls MAT2A intron retention in response to intracellular SAM levels; splicing of the MAT2A retained intron is rapidly induced upon Met depletion, and this effect requires a conserved hairpin which is a METTL16 m6A substrate."}, {"pubmed": 29051200, "text": "Human METTL16 is a N(6)-methyladenosine methyltransferase that targets pre-mRNAs and various non-coding RNAs."}, {"pubmed": 29262316, "text": "METTL16 is involved in MAT2A mRNA stability control."}, {"pubmed": 29593291, "text": "A native gel-shift assay shows that METTL16 binds to the MALAT1 RNA triple helix, but monomeric METTL16_291 does not. Results provide insights into the molecular structure of METTL16, which is distinct from METTL3/METTL14."}, {"pubmed": 31665743, "text": "We speculate that m4C839 is involved in the stabilization of 12S rRNA folding, therefore facilitating the assembly of the mitochondrial small ribosomal subunits. Taken together our data show that METTL15 is a novel protein necessary for efficient translation in human mitochondria"}, {"pubmed": 31940410, "text": "While METTL16 has been reported to be a nuclear protein, findings suggest that METTL16 is also a cytoplasmic methyltransferase that may alter its RNA binding preferences depending on its cellular localization."}, {"pubmed": 32266935, "text": "Mechanistic insights into m6A modification of U6 snRNA by human METTL16."}, {"pubmed": 33428944, "text": "Enzymatic characterization of three human RNA adenosine methyltransferases reveals diverse substrate affinities and reaction optima."}, {"pubmed": 33671635, "text": "METTL16, Methyltransferase-Like Protein 16: Current Insights into Structure and Function."}, {"pubmed": 34075693, "text": "METTL16 promotes cell proliferation by up-regulating cyclin D1 expression in gastric cancer."}, {"pubmed": 34102905, "text": "Systematic analysis of molecular characterization and clinical relevance of m6A regulators in digestive system pan-cancers."}, {"pubmed": 34110327, "text": "The pan-cancer analysis of the two types of uterine cancer uncovered clinical and prognostic associations with m6A RNA methylation regulators."}, {"pubmed": 34227247, "text": "RNA methyltransferase METTL16: Targets and function."}, {"pubmed": 34634806, "text": "The comprehensive interactomes of human adenosine RNA methyltransferases and demethylases reveal distinct functional and regulatory features."}, {"pubmed": 35145225, "text": "METTL16 exerts an m(6)A-independent function to facilitate translation and tumorigenesis."}, {"pubmed": 35596159, "text": "METTL16 promotes hepatocellular carcinoma progression through downregulating RAB11B-AS1 in an m(6)A-dependent manner."}, {"pubmed": 36138131, "text": "METTL16 antagonizes MRE11-mediated DNA end resection and confers synthetic lethality to PARP inhibition in pancreatic ductal adenocarcinoma."}, {"pubmed": 36434904, "text": "METTL16 epigenetically enhances GPX4 expression via m6A modification to promote breast cancer progression by inhibiting ferroptosis."}, {"pubmed": 36474378, "text": "Clinical Significance of Methyltransferase-like 16 Expression in Epithelial Ovarian Cancer."}, {"pubmed": 36584291, "text": "Elucidating the Kinetic Mechanism of Human METTL16."}, {"pubmed": 36608679, "text": "METTL16 drives leukemogenesis and leukemia stem cell self-renewal by reprogramming BCAA metabolism."}, {"pubmed": 36840945, "text": "METTL16 promotes translation and lung tumorigenesis by sequestering cytoplasmic eIF4E2."}, {"pubmed": 37340443, "text": "METTL16 promotes glycolytic metabolism reprogramming and colorectal cancer progression."}, {"pubmed": 37357526, "text": "METTL16 promotes osteosarcoma progression by downregulating VPS33B in an m[6] A-dependent manner."}, {"pubmed": 37423176, "text": "Genetic Variations Within METTL16 and Susceptibility to Sudden Cardiac Death in Chinese Populations With Coronary Artery Disease."}, {"pubmed": 37567786, "text": "Functional analysis of 3'-UTR hairpins supports a two-tiered model for posttranscriptional regulation of MAT2A by METTL16."}, {"pubmed": 37673880, "text": "METTL16 controls Kaposi's sarcoma-associated herpesvirus replication by regulating S-adenosylmethionine cycle."}, {"pubmed": 37817227, "text": "The RNA methyltransferase METTL16 enhances cholangiocarcinoma growth through PRDM15-mediated FGFR4 expression."}, {"pubmed": 37927399, "text": "METTL16 Inhibits the Malignant Progression of Epithelial Ovarian Cancer through the lncRNA MALAT1/beta-Catenin Axis."}, {"pubmed": 38084791, "text": "METTL16-mediated N6-methyladenosine modification of Soga1 enables proper chromosome segregation and chromosomal stability in colorectal cancer."}, {"pubmed": 38131226, "text": "Targeting key RNA methylation enzymes to improve the outcome of colorectal cancer chemotherapy (Review)."}, {"pubmed": 38302992, "text": "METTL16 promotes liver cancer stem cell self-renewal via controlling ribosome biogenesis and mRNA translation."}, {"pubmed": 38334797, "text": "METTL16 inhibits papillary thyroid cancer tumorigenicity through m[6]A/YTHDC2/SCD1-regulated lipid metabolism."}, {"pubmed": 38385084, "text": "METTL16 suppressed the proliferation and cisplatin-chemoresistance of bladder cancer by degrading PMEPA1 mRNA in a m6A manner through autophagy pathway."}, {"pubmed": 38605226, "text": "A Mettl16/m[6]A/mybl2b/Igf2bp1 axis ensures cell cycle progression of embryonic hematopoietic stem and progenitor cells."}], "genomic_pos": {"chr": "17", "end": 2511891, "ensemblgene": "ENSG00000127804", "start": 2405562, "strand": -1}, "genomic_pos_hg19": {"chr": "17", "end": 2415185, "start": 2308856, "strand": -1}, "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0006397", "pubmed": [30197297, 30197299, 33428944], "qualifier": "involved_in", "term": "mRNA processing"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0006402", "qualifier": "involved_in", "term": "mRNA catabolic process"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0006556", "pubmed": [28525753, 30197297], "qualifier": "acts_upstream_of", "term": "S-adenosylmethionine biosynthetic process"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0010608", "pubmed": 28525753, "qualifier": "involved_in", "term": "post-transcriptional regulation of gene expression"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0048024", "pubmed": 28525753, "qualifier": "involved_in", "term": "regulation of mRNA splicing, via spliceosome"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0061157", "qualifier": "involved_in", "term": "mRNA destabilization"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0070475", "qualifier": "involved_in", "term": "rRNA base methylation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0120049", "pubmed": [28525753, 29051200, 32266935], "qualifier": "involved_in", "term": "snRNA (adenine-N6)-methylation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:1905869", "pubmed": 28525753, "qualifier": "involved_in", "term": "negative regulation of 3'-UTR-mediated mRNA stabilization"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "is_active_in", "term": "nucleus"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005634", "pubmed": [27872311, 31940410], "qualifier":