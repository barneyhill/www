g19": [{"cdsend": 52535712, "cdsstart": 52472203, "chr": "14", "position": [[52471526, 52472214], [52472454, 52472567], [52473254, 52473378], [52474527, 52474685], [52477593, 52477765], [52478271, 52478401], [52481004, 52481174], [52481771, 52481993], [52485778, 52486012], [52486776, 52486896], [52493918, 52494062], [52495439, 52495568], [52496264, 52496408], [52505464, 52505695], [52507368, 52507569], [52508822, 52509068], [52509499, 52509649], [52520296, 52520656], [52520737, 52521039], [52526841, 52527074], [52534575, 52534881], [52535484, 52535777]], "strand": -1, "transcript": "NM_007361", "txend": 52535777, "txstart": 52471526}], "generif": [{"pubmed": 11733994, "text": "short arm region of laminin-5 gamma2 chain: structure, mechanism of processing and binding to heparin, nidogen-1, fibulin-1 and fibulin-2"}, {"pubmed": 12005023, "text": "found to be ubiquitous component of basement membrane zones underneath developing epithelia of most of the major organ systems; may participate in cell biological functions during human development"}, {"pubmed": 12475645, "text": "Nidogen-2 compensation hypothesis to explain limited phenotype observed following elimination of nidogen-1 gene."}, {"pubmed": 17008882, "text": "nidogen-2 is equivalent to nidogen-1 in skin-organotypic coculture, and both can promote the development of a functional basement membrane zone"}, {"pubmed": 17328794, "text": "CpG islands of both NID1 and NID2 genes are aberrantly methylated in human cancer samples and cancer cell lines."}, {"pubmed": 19883638, "text": "Nidogen-2 is a new biomarker for ovarian cancer which correlates closely with CA125."}, {"pubmed": 21558411, "text": "NID2 promoter hypermethylation is associated with oral cavity squamous cell carcinoma."}, {"pubmed": 21815147, "text": "Decreased nidogen-2 expression is associated with hepatocellular carcinoma"}, {"pubmed": 23681936, "text": "The cleavage of nidogen-1 and -2 is partially inhibited in human tumor samples."}, {"pubmed": 23682613, "text": "combined methylation analysis of Twist homolog 1 and nidogen-2 genes may be a simple, noninvasive, sensitive, and specific method for detecting cancer cells in urine."}, {"pubmed": 26683663, "text": "Knockdown of nidogen-2 reduced SOX9 expression, whereas it up-regulated RUNX2 expression"}, {"pubmed": 27793011, "text": "Promoter methylation of NID2 was significantly higher in nasopharyngeal carcinoma and esophageal squamous cell carcinoma cells. NID2 suppresses the EGFR/Akt and integrin/FAK/PLCgamma metastasis-related pathways."}, {"pubmed": 28106542, "text": "Addition of a TWIST1/NID2-based DNA methylation assay adds diagnostic value to urine cytology and the model is sensitive to the classification of equivocal cytology."}, {"pubmed": 29278876, "text": "the serum NID2 levels show significant elevation in the ESCC patients and those with relatively high serum NID2 levels ( 32.6 mu mu g/L) are significantly associated with poor survival outcome compared with those with very low serum NID2 levels (< < 20.0 mu mu g/L)."}, {"pubmed": 29970687, "text": "High NID2 methylation is associated with non-small cell lung cancer."}, {"pubmed": 30826972, "text": "Silencing NID2 by DNA Hypermethylation Promotes Lung Cancer."}, {"pubmed": 30975489, "text": "Our results indicated that NID2, COL4A1 and COL4A2 could be the potential novel biomarkers for gastric cancer diagnosis prognosis and the promising therapeutic targets"}, {"pubmed": 31362888, "text": "Our study demonstrates that up-regulated NID2 plays an important role in promoting the invasion and migration of gastric cancer cells and has a potential of being a novel biomarker for diagnosis, treatment and prognosis of gastric cancer in the future."}, {"pubmed": 32171289, "text": "Evaluation of NID2 promoter methylation for screening of Oral squamous cell carcinoma."}, {"pubmed": 32472113, "text": "A pilot proteomic study with a prospective cohort suspected to develop preeclampsia."}, {"pubmed": 33758160, "text": "Genome-Scale Analysis Identified NID2, SPARC, and MFAP2 as Prognosis Markers of Overall Survival in Gastric Cancer."}, {"pubmed": 34315224, "text": "Nidogen-2 Maintains the Contractile Phenotype of Vascular Smooth Muscle Cells and Prevents Neointima Formation via Bridging Jagged1-Notch3 Signaling."}, {"pubmed": 34593672, "text": "Nidogen-2: A new biomarker in colon cancer patients."}], "genomic_pos": {"chr": "14", "end": 52069059, "ensemblgene": "ENSG00000087303", "start": 52004809, "strand": -1}, "genomic_pos_hg19": {"chr": "14", "end": 52535712, "start": 52471521, "strand": -1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0007155", "pubmed": 9733643, "qualifier": "involved_in", "term": "cell adhesion"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007160", "qualifier": "involved_in", "term": "cell-matrix adhesion"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0071711", "pubmed": 22952693, "qualifier": "involved_in", "term": "basement membrane organization"}], "CC": [{"evidence": "TAS", "gocategory": "CC", "id": "GO:0005576", "qualifier": "located_in", "term": "extracellular region"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005604", "qualifier": "is_active_in", "term": "basement membrane"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005604", "pubmed": 22952693, "qualifier": "located_in", "term": "basement membrane"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005615", "qualifier": "is_active_in", "term": "extracellular space"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005886", "qualifier": "located_in", "term": "plasma membrane"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0062023", "pubmed": 20551380, "qualifier": "colocalizes_with", "term": "collagen-containing extracellular matrix"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0062023", "pubmed": [23658023, 27068509, 27559042, 28327460, 28675934], "qualifier": "located_in", "term": "collagen-containing extracellular matrix"}, {"