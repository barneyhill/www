": [{"pubmed": 17157313, "text": "We have solved the structures of human calpain 1 and calpain 9 protease cores ; both structures have clear implications for the function of non-catalytic domains of full-length calpains in the calcium-mediated activation of the enzyme."}, {"pubmed": 19074885, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20453000, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 25539577, "text": "calpain-9 may play a role in breast cancer and that low expression is associated with poorer patient clinical outcome following endocrine therapy."}, {"pubmed": 27404891, "text": "Study shows that calpain-9 is a tumor suppressor that can be regarded as a potential prognosis indicator for clinical outcomes in gastric cancer."}, {"pubmed": 31316008, "text": "these data provide a strong signature for tolerance of therapeutic strategies for fibrosis aimed at CAPN9 antagonism."}], "genomic_pos": {"chr": "1", "end": 230802003, "ensemblgene": "ENSG00000135773", "start": 230747384, "strand": 1}, "genomic_pos_hg19": {"chr": "1", "end": 230937749, "start": 230883130, "strand": 1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0006508", "qualifier": "involved_in", "term": "proteolysis"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0007586", "pubmed": 9524069, "qualifier": "involved_in", "term": "digestion"}], "CC": [{"evidence": "ND", "gocategory": "CC", "id": "GO:0005575", "qualifier": "is_active_in", "term": "cellular_component"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005737", "qualifier": "is_active_in", "term": "cytoplasm"}], "MF": [{"category": "MF", "evidence": "IBA", "id": "GO:0004198", "qualifier": "enables", "term": "calcium-dependent cysteine-type endopeptidase activity"}, {"category": "MF", "evidence": "IEA", "id"