osition": [[206136616, 206137078], [206137292, 206137486], [206141445, 206141523], [206145453, 206145578], [206153956, 206155151]], "strand": 1, "transcript": "NM_001385245", "txend": 206155151, "txstart": 206136616}, {"cdsend": 206154051, "cdsstart": 206139391, "chr": "1", "position": [[206138490, 206138814], [206139324, 206139456], [206141445, 206141523], [206145453, 206145578], [206153956, 206155151]], "strand": 1, "transcript": "NM_001385248", "txend": 206155151, "txstart": 206138490}, {"cdsend": 206154051, "cdsstart": 206139391, "chr": "1", "position": [[206138490, 206138720], [206139324, 206139456], [206141445, 206141523], [206145453, 206145578], [206153956, 206155151]], "strand": 1, "transcript": "NM_001385249", "txend": 206155151, "txstart": 206138490}, {"cdsend": 206154051, "cdsstart": 206139304, "chr": "1", "position": [[206138490, 206139336], [206141445, 206141523], [206145453, 206145578], [206153956, 206155151]], "strand": 1, "transcript": "NM_001385251", "txend": 206155151, "txstart": 206138490}, {"cdsend": 206155151, "cdsstart": 206155151, "chr": "1", "position": [[206138490, 206138720], [206139440, 206139456], [206141445, 206141523], [206145453, 206145578], [206153956, 206155151]], "strand": 1, "transcript": "NR_134239", "txend": 206155151, "txstart": 206138490}], "generif": [{"pubmed": 18676834, "text": "Using deletion constructs, the authors find that Ugene binds to the first 25 amino acids of the UNG2 NH(2) terminus. They suggest that Ugene induction in cancer may contribute to the cancer phenotype by interacting with the BER pathway."}, {"pubmed": 19755123, "text": "this study provides an additional possible mechanism of neurotoxicity in Alzheimer's disease, the induction of p17(FAM72B), through which Abeta acts to induce apoptosis and exhibit other Alzheimer's disease characteristics."}, {"pubmed": 21317926, "text": "It was found that Ugene, designated herein as LMP1-induced protein (LMPIP), was induced, in a time-dependent manner, in EBV-infected peripheral blood mononuclear cells and LMP1-transfected 293 cells."}, {"pubmed": 23900679, "text": "Data indicate that p17 (p17 amyloid-beta peptide-induced protein; known as Ugene, LMPIP, or FAM72A/B) drives the cell cycle into the G0/G1 phase and enhances survival of proliferating cells."}, {"pubmed": 26206078, "text": "An epistemological characterization of the human tumorigenic neuronal paralogous FAM72 gene loci (FAM72A, FAM72B, FAM72C, FAM72D)."}, {"pubmed": 34819670, "text": "FAM72A antagonizes UNG2 to promote mutagenic repair during antibody maturation."}, {"pubmed": 36613817, "text": "Prognostic and Immunological Implications of FAM72A in Pan-Cancer and Functional Validations."}, {"pubmed": 36757015, "text": "The pan-cancer analysis of the oncogenic role of FAM72A as a BRCA prognostic biomarker and immunotherapeutic target."}, {"pubmed": 36868233, "text": "Fam72a functions as a cell-cycle-controlled gene during proliferation and antagonizes apoptosis through reprogramming PP2A substrates."}], "genomic_pos": {"chr": "1", "end": 206204414, "ensemblgene": "ENSG00000196550", "start": 206186179, "strand": -1}, "genomic_pos_hg19": [{"chr": "1", "end": 206155151, "start": 206136916, "strand": 1}, {"chr": "HG1293_PATCH", "end": 206265981, "start": 206249817, "strand": -1}], "go": {"CC": [{"evidence": "IEA", "gocategory": "CC", "id": "GO:0005739", "qualifier": "located_in", "term": "mitochondrion"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005829", "qualifier": "is_active_in", "term": "cytosol"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0043231", "qualifier": "located_in", "term": "intracellular membrane-bounded organelle"}], "MF": {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [25416956, 32296183], "qualifier": "enables", "term": "protein binding"}}, "homologene": {"genes": [[7955, 541418], [8364, 549108], [9031, 41984