661504, 15661701], [15668431, 15668552], [15669696, 15669890], [15673134, 15673246], [15677747, 15677899], [15679075, 15679196], [15699502, 15699642], [15702027, 15702160], [15704484, 15704605], [15710388, 15710457], [15713126, 15713210], [15718526, 15718562], [15722350, 15722432], [15731786, 15731877], [15732972, 15733049], [15733630, 15733765], [15734612, 15734735], [15739830, 15739985], [15742388, 15742524], [15747870, 15747991], [15749023, 15751265]], "strand": 1, "transcript": "NM_030667", "txend": 15751265, "txstart": 15475441}, {"cdsend": 15747975, "cdsstart": 15702156, "chr": "12", "position": [[15699544, 15699642], [15702027, 15702160], [15704484, 15704605], [15710388, 15710457], [15718526, 15718562], [15722350, 15722432], [15731786, 15731877], [15732972, 15733049], [15733630, 15733765], [15734612, 15734735], [15739830, 15739985], [15742388, 15742524], [15747870, 15747991], [15749023, 15751265]], "strand": 1, "transcript": "NM_030668", "txend": 15751265, "txstart": 15699544}, {"cdsend": 15747975, "cdsstart": 15702156, "chr": "12", "position": [[15699544, 15699642], [15702027, 15702160], [15704484, 15704605], [15710388, 15710457], [15713126, 15713210], [15718526, 15718562], [15722350, 15722432], [15731786, 15731877], [15732972, 15733049], [15733630, 15733765], [15734612, 15734735], [15739830, 15739985], [15742388, 15742524], [15747870, 15747991], [15749023, 15751265]], "strand": 1, "transcript": "NM_030669", "txend": 15751265, "txstart": 15699544}, {"cdsend": 15747975, "cdsstart": 15702156, "chr": "12", "position": [[15699544, 15699642], [15702027, 15702160], [15704484, 15704605], [15710388, 15710457], [15718526, 15718562], [15722350, 15722432], [15731786, 15731877], [15732972, 15733049], [15733630, 15733765], [15734612, 15734735], [15739830, 15739985], [15742388, 15742524], [15747870, 15751265]], "strand": 1, "transcript": "NM_030670", "txend": 15751265, "txstart": 15699544}, {"cdsend": 15747975, "cdsstart": 15702156, "chr": "12", "position": [[15699544, 15699642], [15702027, 15702160], [15704484, 15704605], [15710388, 15710457], [15713126, 15713210], [15718526, 15718562], [15722350, 15722432], [15731786, 15731877], [15732972, 15733049], [15733630, 15733765], [15734612, 15734735], [15739830, 15739985], [15742388, 15742524], [15747870, 15751265]], "strand": 1, "transcript": "NM_030671", "txend": 15751265, "txstart": 15699544}], "generif": [{"pubmed": 12949066, "text": "conclude that an intronic promoter within the protein tyrosine phosphatase, receptor type(GLEPP1) gene drives the expression of the protein tyrosine phosphatase-osteoclast(PTP) in a cell type-specific manner"}, {"pubmed": 16888096, "text": "overexpression of PTPROt inhibited BCR-triggered SYK tyrosyl phosphorylation, activation of the associated adaptor proteins SHC and BLNK, and downstream signaling events, and inhibition lymphoma cell proliferation and inducion of lymphoma cell apoptosis"}, {"pubmed": 17545520, "text": "PTPRO methylation and silencing has a role in chronic lymphocytic leukemia tumorigenesis"}, {"pubmed": 17593862, "text": "GLEPP1 expression may be a useful marker of podocyte injury in immunoglobulin A nephropathy, and may be predictive of clinical and pathological severity"}, {"pubmed": 18644370, "text": "PTPRO gene is co-regulated by both E2F1 and miR-17-92."}, {"pubmed": 19095770, "text": "These data suggest that estrogen-mediated suppression of PTPRO is probably one of the early events in estrogen-induced tumorigenesis and that expression of PTPRO could facilitate endocrine therapy of breast cancer."}, {"pubmed": 19573017, "text": "Dimerization of neuronal type III receptor-protein tyrosine phosphatase PTPRO in living cells is regulated by disulfide linkages in the PTPRO intracellular domain."}, {"pubmed": 19834535, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19855081, "text": "BCL6 repressed PTPROt transcription via a direct interaction with functional BCL6 binding sites in the PTPROt promoter"}, {"pubmed": 19913121, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20628086, "text": "Observational study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 21722858, "text": "The present study introduces mutations in PTPRO as another cause of autosomal-recessive nephrotic syndrome."}, {"pubmed": 22001392, "text": "These findings further substantiate the role of TCL1 in PTPROt suppression and its importance in the pathogenesis of chronic lymphocytic leukemia."}, {"pubmed": 22099875, "text": "hypermethylated PTPRO occurs frequently in esophageal squamous cell carcinoma"}, {"pubmed": 22126837, "text": "Three intronic SNPs in PTPRO were associated with learning and memory."}, {"pubmed": 22851698, "text": "ErbB2 is a direct substrate of PTPRO and decreased expression of PTPRO predicts poor prognosis for ErbB2-positive breast cancer patients."}, {"pubmed": 23533167, "text": "methylation and downregulation of PTPRO in a subset of primary human HCC and establish VCP as a novel functionally important substrate of this tyrosine phosphatase that could be a potential molecular target for HCC therapy."}, {"pubmed": 24090193, "text": "PTPRO methylation is associated with poor survival only in HER2-positive patients."}, {"pubmed": 24128416, "text": "PTPRO level was decreased in the early phase but reversed in the late phase of hepatic ischemia reperfusion injury."}, {"pubmed": 25034527, "text": "Our study suggests an interesting PTPRO/TLR4/NF-kappaB signaling feedback loop in Hhepatocellular carcinomcarcinogenesis and progression"}, {"pubmed": 25301722, "text": "Results highlight the PTPRO contribution in negative regulation of SRC/EGFR signaling in colon cancer."}, {"pubmed": 25339662, "text": "PTPRO was drastically decreased in fulminant hepatitis, and this was associated with enhanced beta-catenin accumulation but reduced IFN-gamma secretion."}, {"pubmed": 25462809, "text": "The optimal pH value of PTP-oc is approximately 7.0."}, {"pubmed": 25482129, "text": "Loss of PTPRO expression is associated with chronic lymphocytic leukemia."}, {"pubmed": 25633279, "text": "These observations confirm that PTPRO plays a critical role in liver fibrogenesis by affecting PDGF signaling in HSC activation and might be developed into a feasible therapeutic approach for the treatment of chronic fibrotic liver diseases."}, {"pubmed": 26117839, "text": "PTPRO truncated serves as an important tumor suppressor in hepatocellular carcinoma microenvironment."}, {"pubmed": 27272414, "text": "Single nucleotide polymorphism in PTPRO gene is associated with Acute Renal Graft Rejection."}, {"pubmed": 27345410, "text": "Results provide evidence that PTPRO inhibits ERBB2-driven breast cancer through dephosphorylation leading to dual effects of ERBB2 signaling suppression and endosomal internalization of ERBB2."}, {"pubmed": 28199135, "text": "PTPRO is a novel candidate gene in emphysema with severe airflow obstruction."}, {"pubmed": 28586036, "text": "the present study demonstrated that PTPRO inhibits tumor growth in vitro and in vivo, indicating the tumor suppressive function of PTPRO in LSCC. This study highlights PTPRO as an epigenetically silenced gene, and a candidate tumor-suppressor of LSCC."}, {"pubmed": 30443949, "text": "PTPRO was significantly upregulated and negatively associated with miR-548c-5p in placental mononuclear cells in patients with preeclampsia."}, {"pubmed": 31448453, "text": "LncRNA HULC promotes lung squamous cell carcinoma by regulating PTPRO via NF-kappaB."}, {"pubmed": 31452237, "text": "PTPRO exaggerates inflammation in ulcerative colitis through TLR4/NF-kappaB pathway."}, {"pubmed": 32581055, "text": "IL-6 promotes PD-L1 expression in monocytes and macrophages by decreasing protein tyro