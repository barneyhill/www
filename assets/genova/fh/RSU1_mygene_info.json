, "position": [[16632610, 16635495], [16737021, 16737154], [16794537, 16794652], [16794916, 16794999], [16796869, 16796988], [16806388, 16806509], [16824032, 16824083], [16858971, 16859083], [16859313, 16859423]], "strand": -1, "transcript": "NM_012425", "txend": 16859423, "txstart": 16632610}, {"cdsend": 16824033, "cdsstart": 16635392, "chr": "10", "position": [[16632616, 16635495], [16737021, 16737154], [16794537, 16794652], [16794916, 16794999], [16796869, 16796988], [16806388, 16806509], [16824032, 16824083], [16859313, 16859453]], "strand": -1, "transcript": "NM_152724", "txend": 16859453, "txstart": 16632616}], "generif": [{"pubmed": 12510772, "text": "alternative splicing of exon 8 of the RSU-1 RNA to produce the exon-deleted form constitutes a mechanism for reduction or loss of functional Rsu-1. The exon-deleted form was not found in normal tissue."}, {"pubmed": 16385451, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 18436335, "text": "Rsu-1 links the Ras pathway with the IPP (integrin-linked kinase {ILK}, PINCH-1/LIMS1, parvin) complex and the perturbations of cell attachment-dependent signaling that occur in the malignant process"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 25482629, "text": "Rsu1 is required for EGF-induced activation of the p38 map kinase signaling"}, {"pubmed": 25647720, "text": "Upregulation of RSU1 is associated with metastatic breast cancer."}, {"pubmed": 25750304, "text": "results showed that RSU-1 expression is elevated in HepG2 cells both at the mRNA and protein level"}, {"pubmed": 26170296, "text": "integrin/Rsu1/Rac1/actin signaling has a conserved role in modulating reward-related phenotypes, including ethanol consumption, across phyla"}, {"pubmed": 28423706, "text": "our results indicate the important role of RSU-1 in BC metastasis and set the foundations for its validation as potential BC metastasis marker."}, {"pubmed": 28459205, "text": "Ras suppressor protein 1 was confirmed to be directly targeted by miR-409-5p in breast cancer cells. Small interfering RNA-mediated Ras suppressor protein 1 inhibition reversely promoted cancer proliferation and migration in miR-409-5p-downregualted breast cancer cells. MiR-409-5p is downregulated in breast cancer and its inhibition has anti-cancer effect on breast cancer development both in vitro and in vivo."}, {"pubmed": 30621163, "text": "This gene encodes a protein that is involved in the Ras signal transduction pathway, growth inhibition, and nerve-growth factor induced differentiation processes."}, {"pubmed": 31123330, "text": "Ras suppressor-1 (RSU-1) promotes cell invasion in aggressive glioma cells and inhibits it in non-aggressive cells through STAT6 phospho-regulation."}, {"pubmed": 31296919, "text": "Depletion of Ras Suppressor-1 (RSU-1) promotes cell invasion of breast cancer cells through a compensatory upregulation of a truncated isoform."}, {"pubmed": 32592097, "text": "Integrin-linked kinase (ILK) regulates KRAS, IPP complex and Ras suppressor-1 (RSU1) promoting lung adenocarcinoma progression and poor survival."}, {"pubmed": 32751711, "text": "MicroRNA-Dependent Targeting of RSU1 and the IPP Adhesion Complex Regulates the PTEN/PI3K/AKT Signaling Pathway in Breast Cancer Cell Lines."}, {"pubmed": 33587032, "text": "Complex structures of Rsu1 and PINCH1 reveal a regulatory mechanism of the ILK/PINCH/Parvin complex for F-actin dynamics."}, {"pubmed": 33853759, "text": "RSU-1 interaction with prohibitin-2 links cell-extracellular matrix detachment to downregulation of ERK signaling."}, {"pubmed": 33891945, "text": "Molecular basis for Ras suppressor-1 binding to PINCH-1 in focal adhesion assembly."}, {"pubmed": 35642986, "text": "Focal adhesion proteins in hepatocellular carcinoma: RSU1 a novel tumour suppressor with prognostic significance."}, {"pubmed": 35729367, "text": "Ras suppressor-1 (RSU1) exerts a tumor suppressive role with prognostic significance in lung adenocarcinoma."}, {"pubmed": 36375714, "text": "Ras suppressor 1 long form (RSU1L) silencing promotes apoptosis in invasive breast cancer cells."}, {"pubmed": 38312030, "text": "CircRSU1 alleviates LPS-induced human pulmonary microvascular endothelial cell injury by targeting miR-1224-5p/ITGA5 axis."}], "genomic_pos": {"chr": "10", "end": 16817463, "ensemblgene": "ENSG00000148484", "start": 16590611, "strand": -1}, "genomic_pos_hg19": {"chr": "10", "end": 16859527, "start": 16632610, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0007165", "qualifier": "involved_in", "term": "signal transduction"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0010810", "pubmed": 20926685, "qualifier": "NOT involved_in", "term": "regulation of cell-substrate adhesion"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0010811", "pubmed": 20926685, "qualifier": "involved_in", "term": "positive regulation of cell-substrate adhesion"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0043547", "pubmed": 20926685, "qualifier": "involved_in", "term": "positive regulation of GTPase activity"}], "CC": [{"evidence": "TAS", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005925", "pubmed": 20926685, "qualifier": "located_in", "term": "focal adhesion"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0070062", "pubmed": 20458337, "qualifier": "located_in", "term": "extracellular exosome"}], "MF": {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [20926685, 23414517, 25416956, 28514442, 29892012], "qualifier": "enables", "term": "protein binding"}}, "homologene": {"genes": [[6239, 175451], [7165, 1275645], [7227, 34774], [7955, 553276], [8364, 548412], [9031, 420524], [9598, 738898], [9606, 6251], [9615, 477993], [9913, 617534], [10090, 20163], [10116, 680419]], "id": 7521}, "inter