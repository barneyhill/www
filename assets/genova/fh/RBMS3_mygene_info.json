chr": "3", "position": [[29322802, 29323247], [29476233, 29476406], [29529931, 29529990], [29628604, 29628696], [29781210, 29781368], [29804400, 29804480], [29910348, 29910455], [29922269, 29922308], [29925652, 29925699], [29938869, 29938966], [29941195, 29941246], [29977576, 29977687], [30029633, 30029714], [30032572, 30032809]], "strand": 1, "transcript": "NM_001177712", "txend": 30032809, "txstart": 29322802}, {"cdsend": 30045353, "cdsstart": 29323172, "chr": "3", "position": [[29322802, 29323247], [29476236, 29476406], [29529931, 29529990], [29628604, 29628696], [29781210, 29781368], [29804400, 29804480], [29910348, 29910455], [29925652, 29925699], [29938869, 29938966], [29941195, 29941246], [29977576, 29977687], [29985697, 29985745], [30029633, 30029714], [30032572, 30032700], [30045346, 30051886]], "strand": 1, "transcript": "NM_001330696", "txend": 30051886, "txstart": 29322802}, {"cdsend": 30032704, "cdsstart": 29323172, "chr": "3", "position": [[29322802, 29323247], [29476233, 29476406], [29529931, 29529990], [29628604, 29628696], [29781210, 29781368], [29804400, 29804480], [29910348, 29910455], [29925652, 29925699], [29938869, 29938966], [29941195, 29941246], [29977576, 29977687], [30029633, 30029714], [30032572, 30032809]], "strand": 1, "transcript": "NM_014483", "txend": 30032809, "txstart": 29322802}], "generif": [{"pubmed": 17586524, "text": "These results suggest that RBMS3, by binding Prx1 mRNA in a sequence-specific manner, controls Prx1 expression and indirectly collagen synthesis."}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 21844183, "text": "a tumor suppression function for the human RBMS3 gene in esophageal squamous cell carcinoma, acting through c-Myc downregulation, with genetic loss of this gene contributing to poor outcomes in disease"}, {"pubmed": 22267851, "text": "genetic susceptibility plays a role in the pathophysiology of Bisphosphonate-related osteonecrosis of the jaw (BRONJ), with RBMS3 having a significant effect in the risk."}, {"pubmed": 22957092, "text": "RBMS3 at 3p24 inhibits nasopharyngeal carcinoma development via inhibiting cell proliferation, angiogenesis, and inducing apoptosis"}, {"pubmed": 23045156, "text": "one interacting gene pair (RBMS3 versus ZNF516) which, even after Bonferroni correction for multiple testing, showed consistently significant effects on hip bone mineral density."}, {"pubmed": 25588924, "text": "RBMS3 is a novel tumor suppressor gene in lung squamous cell carcinoma"}, {"pubmed": 27503288, "text": "We identified rs117026326 on GTF2I with GWAS significance (P = 1.10 x 10(-15)) and rs13079920 on RBMS3 with suggestive significance (P = 2.90 x 10(-5)) associating with Primary Sjogren's syndrome in women."}, {"pubmed": 27902480, "text": "These findings implicated that RBMS3 and nuclear HIF1A could act as prognostic biomarkers and therapeutic targets for GC."}, {"pubmed": 28409548, "text": "RBMS3 inhibited the proliferation and tumorigenesis of breast cancer cells, at least in part, through inactivation of the Wnt/beta-catenin signaling pathway"}, {"pubmed": 30279231, "text": "Results indicate that genetic ablation of RNA binding motif single stranded interacting protein 3 (RBMS3) contributes to chemoresistance RBMS3-deleted epithelial ovarian cancer (EOC)."}, {"pubmed": 30819235, "text": "our study revealed a novel mechanism of the RBMS3/Twsit1/MMP2 axis in the regulation of invasion and metastasis of breast cancer"}, {"pubmed": 32016951, "text": "RBMS3 delays disc degeneration by inhibiting Wnt/beta-catenin signaling pathway."}, {"pubmed": 32627033, "text": "Increased expression of RBMS3 predicts a favorable prognosis in human gallbladder carcinoma."}, {"pubmed": 33845141, "text": "LncRNA MEG3 regulates breast cancer proliferation and apoptosis through miR-141-3p/RBMS3 axis."}, {"pubmed": 33910421, "text": "Tumor Suppressor Effect of RBMS3 in Breast Cancer."}, {"pubmed": 34608266, "text": "RNA binding protein RBMS3 is a common EMT effector that modulates triple-negative breast cancer progression via stabilizing PRRX1 mRNA."}, {"pubmed": 36075788, "text": "Comprehensive bioinformatics analysis confirms RBMS3 as the central candidate biological target for ovarian cancer."}, {"pubmed": 36414028, "text": "Disruption of RBMS3 suppresses PD-L1 and enhances antitumor immune activities and therapeutic effects of auranofin against triple-negative breast cancer."}, {"pubmed": 36769184, "text": "Expression of RBMS3 in Breast Cancer Progression."}, {"pubmed": 38618967, "text": "The RNA-binding protein RBMS3 inhibits the progression of colon cancer by regulating the stability of LIMS1 mRNA."}], "genomic_pos": {"chr": "3", "end": 30010391, "ensemblgene": "ENSG00000144642", "start": 28574791, "strand": 1}, "genomic_pos_hg19": {"chr": "3", "end": 30051886, "start": 29322473, "strand": 1}, "go": {"BP": [{"evidence": "IMP", "gocategory": "BP", "id": "GO:0002357", "pubmed": 28409548, "qualifier": "involved_in", "term": "defense response to tumor cell"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0010628", "pubmed": 28409548, "qualifier": "involved_in", "term": "positive regulation of gene expression"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0010629", "pubmed": 28409548, "qualifier": "involved_in", "term": "negative regulation of gene expression"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0090090", "pubmed": 28409548, "qualifier": "involved_in", "term": "negative regulation of canonical Wnt signaling pathway"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "is_active_in", "term": "nucleus"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005737", "pubmed": 10675610, "qualifier": "located_in", "term": "cytoplasm"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005829", "qualifier": "is_active_in", "term": "cytosol"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:1990904", "quali