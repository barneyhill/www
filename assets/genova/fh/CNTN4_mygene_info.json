erif": [{"pubmed": 15106122, "text": "Our results demonstrate the association of CNTN4 disruption with the 3p deletion syndrome phenotype and strongly suggest a causal relationship"}, {"pubmed": 17030759, "text": "The contactin 4 gene (CNTN4) is associated with cerebellar degeneration in spinocerebellar ataxia type 16. Additional studies are necessary to prove 4,256C-->T to be a causative mutation."}, {"pubmed": 17915252, "text": "Observational study of genotype prevalence. (HuGE Navigator)"}, {"pubmed": 17915252, "text": "pathological examinations and molecular biological examinations are needed to confirm that this mutation is a true cause of SCA16"}, {"pubmed": 18349135, "text": "This report suggests that mutations affecting CNTN4 function may be relevant to Autism spectrum disorder pathogenesis."}, {"pubmed": 19509545, "text": "results do not support the candidacy of CHL1, CNTN6, and CNTN4 as tumor suppressor genes in the 3p26-pter region in ovarian cancer"}, {"pubmed": 19736351, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19913121, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20201926, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20628086, "text": "Observational study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 21079607, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 21308999, "text": "Using array comparative genome hybridization (CGH), we identified a maternally inherited approximately 535 kb deletion at 3p26.3 encompassing the 5' end of the contactin 4 gene (CNTN4) in a patient with autism."}, {"pubmed": 22750301, "text": "these results suggest that rare copy number variations in CNTN4 may also influence autism susceptibility in Asian populations."}, {"pubmed": 24764060, "text": "We identified CNTN4 as a novel candidate gene for POAG."}, {"pubmed": 25959733, "text": "data reveal critical and novel roles for CNTN4/amyloid precursor protein in promoting target-specific axon arborization"}, {"pubmed": 26146898, "text": "By combining methylation and SNP data, CNTN4 was identified as a risk factor for regular alcohol use."}, {"pubmed": 27039371, "text": "SNPs in ITPR1 and CNTN4 are involved in the regulation of serum uric acid concentrations in Mexican Americans"}, {"pubmed": 27995817, "text": "Association between contactin 4 (CNTN4) and antisaccade and P300 in schizophrenia."}, {"pubmed": 28595731, "text": "SNPs within the CNTN4 gene are associated with increased risk of oral cancer."}, {"pubmed": 28938490, "text": "High CNTN4 expression is associated with Pheochromocytomas and Paragangliomas."}, {"pubmed": 29258006, "text": "PPARGC1B and CNTN4 genotypes are associated with elevated thromboxane A2 formation and with an excess of cardiovascular events. Aspirin appears to blunt these associations."}, {"pubmed": 38745463, "text": "CNTN4 modulates neural elongation through interplay with APP."}], "genomic_pos": {"chr": "3", "end": 3057959, "ensemblgene": "ENSG00000144619", "start": 2098813, "strand": 1}, "genomic_pos_hg19": {"chr": "3", "end": 3099645, "start": 2140497, "strand": 1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0007156", "qualifier": "involved_in", "term": "homophilic cell adhesion via plasma membrane adhesion molecules"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0007158", "pubmed": 14571131, "qualifier": "involved_in", "term": "neuron cell-cell adhesion"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0007399", "pubmed": 15106122, "qualifier": "involved_in", "term": "nervous system development"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0007409", "pubmed": 14571131, "qualifier": "involved_in", "term": "axonogenesis"}, {"evidence": "IBA", "gocategory"