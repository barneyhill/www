_hg19": [{"cdsend": 133813994, "cdsstart": 133779450, "chr": "9", "position": [[133777824, 133779710], [133780620, 133780800], [133787178, 133787275], [133799130, 133799267], [133799623, 133799783], [133804953, 133805433], [133813922, 133814035], [133814389, 133814455]], "strand": -1, "transcript": "NM_001145106", "txend": 133814455, "txstart": 133777824}, {"cdsend": 133813994, "cdsstart": 133779450, "chr": "9", "position": [[133777826, 133779710], [133780620, 133780800], [133787178, 133787275], [133799130, 133799267], [133799623, 133799783], [133804953, 133805433], [133813922, 133814634]], "strand": -1, "transcript": "NM_032843", "txend": 133814634, "txstart": 133777826}], "generif": [{"pubmed": 19710473, "text": "FIBCD1 is a high-affinity receptor for chitin and chitin fragments, and may play an important role in controlling the exposure of intestine to chitin and chitin fragments"}, {"pubmed": 19892701, "text": "The recognition unit of FIBCD1 organizes into a noncovalently linked tetrameric structure and uses a hydrophobic funnel (S1) for acetyl group recognition."}, {"pubmed": 20237496, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 24293368, "text": "The high affinity ligand N-acetylmannosamine (ManNAc) binds FIBCD1 in the S1 site, predominantly via the acetyl group with the oxygen and acetamide nitrogen hydrogen-bonded to the protein and the methyl group inserted into a hydrophobic pocket."}, {"pubmed": 29220632, "text": "FIBCD1 is expressed in epithelial cells derived from all three germ layers. Endodermal-derived epithelial cells throughout the gastrointestinal tract and the respiratory system showed high expression of FIBCD1 and also mesodermal-derived cells in the genitourinary system and ectodermal-derived epidermis and sebaceous glands cells expressed FIBCD1."}, {"pubmed": 29659669, "text": "FIBCD1 may be a novel biomarker to evaluate the prognosis of gastric cancer."}, {"pubmed": 30279687, "text": "thses findings support FIBCD1 as a lung epithelial pattern recognition receptor that recognizes the complex Aspergillus fumigatus cell wall polysaccharides and modulates the lung epithelial inflammatory response by suppressing inflammatory mediators and mucins"}, {"pubmed": 35501350, "text": "The myokine Fibcd1 is an endogenous determinant of myofiber size and mitigates cancer-induced myofiber atrophy."}, {"pubmed": 35916241, "text": "FIBCD1 is an endocytic GAG receptor associated with a novel neurodevelopmental disorder."}, {"pubmed": 36888604, "text": "The role of FIBCD1 in response to Aspergillus fumigatus in lung epithelial cells."}, {"pubmed": 38072065, "text": "Crystal structures of human immune protein FIBCD1 suggest an extended binding site compatible with recognition of pathogen-associated carbohydrate motifs."}], "genomic_pos": {"chr": "9", "end": 130939286, "ensemblgene": "ENSG00000130720", "start": 130902438, "strand": -1}, "genomic_pos_hg19": {"chr": "9", "end": 133814673, "start": 133777825, "strand": -1}, "go": {"CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005615", "qualifier": "is_active_in", "term": "extracellular space"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0016020", "pubmed": 19710473, "qualifier": "located_in", "term": "membrane"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0062023", "qualifier": "is_active_in", "term": "collagen-containing extracellular matrix"}], "MF": [{"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [25416956, 32296183], "qualifier": "enables", "term": "protein binding"}, {"category": "MF", "evidence": "IDA", "id": "GO:0008061", "pubmed": 19710473, "qualifier": "enables", "term": "chitin binding"}, {"category": "MF", "evidence": "IEA", "id": "GO:0046872", "qualifier": "enables", "term": "metal ion binding"}]}, "homologene": {"genes": [[7227, 32773], [7227, 33888], [7227, 35845], [7227, 37522], [7227, 246525], [7955, 567525], [7955, 100151577], [8364, 779974], [9031, 427762], [9544, 7224