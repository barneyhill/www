": [{"cdsend": 25824881, "cdsstart": 25760950, "chr": "3", "position": [[25760434, 25761126], [25761504, 25761682], [25770623, 25770809], [25773809, 25773974], [25775362, 25775473], [25777799, 25777891], [25778824, 25778946], [25781067, 25781290], [25792588, 25792754], [25805556, 25805802], [25820064, 25820179], [25824750, 25824989]], "strand": -1, "transcript": "NM_001145293", "txend": 25824989, "txstart": 25760434}, {"cdsend": 25831356, "cdsstart": 25760950, "chr": "3", "position": [[25760434, 25761126], [25761504, 25761682], [25770623, 25770809], [25773809, 25773974], [25775362, 25775473], [25777494, 25777640], [25778824, 25778946], [25781067, 25781290], [25792588, 25792754], [25805556, 25805802], [25820064, 25820179], [25831351, 25831530]], "strand": -1, "transcript": "NM_001145294", "txend": 25831530, "txstart": 25760434}, {"cdsend": 25824881, "cdsstart": 25761060, "chr": "3", "position": [[25760434, 25761126], [25770623, 25770809], [25773809, 25773974], [25775362, 25775473], [25777494, 25777640], [25778824, 25778946], [25781067, 25781290], [25792588, 25792754], [25805556, 25805802], [25820064, 25820179], [25824750, 25824989]], "strand": -1, "transcript": "NM_001145295", "txend": 25824989, "txstart": 25760434}, {"cdsend": 25824881, "cdsstart": 25760950, "chr": "3", "position": [[25760434, 25761126], [25761504, 25761682], [25770623, 25770809], [25773809, 25773974], [25775362, 25775473], [25777494, 25777640], [25778824, 25778946], [25781067, 25781290], [25792588, 25792754], [25805556, 25805802], [25820064, 25820179], [25824750, 25824934]], "strand": -1, "transcript": "NM_018297", "txend": 25824934, "txstart": 25760434}], "generif": [{"pubmed": 10831608, "text": "Describes the function of Yeast PNG1 and identifies similar proteins in mouse, human, D. melanogaster, C. elegans, and S. pombe."}, {"pubmed": 16807242, "text": "the PUB domain functions as a p97 binding module in human peptide N-glycanase"}, {"pubmed": 17015730, "text": "generation of an HLA-A*0201-associated epitope from tyrosinase with deamidation of Asn to Asp is dependent on glycosylation in the endoplasmic reticulum (ER), and subsequent deglycosylation by peptide-N-glycanase in the cytosol"}, {"pubmed": 20668520, "text": "As the generation of the bulk of fOS is unaffected by co-down regulation of Ngly1p and Engase1p, alternative quantitatively important mechanisms must underlie the liberation of these fOS from either LLO or glycoproteins during protein N-glycosylation."}, {"pubmed": 22575648, "text": "PNGase-PUB serves not only as p97-binding module but also as a possible activator of HR23 in endoplasmic reticulum-associated degradation mechanisms."}, {"pubmed": 24651605, "text": "Data indicate that N-glycanase 1 (NGLY1) deficiency is a novel autosomal recessive disorder of the endoplasmic reticulum-associated degradation pathway associated with neurological dysfunction, abnormal tear production, and liver disease."}, {"pubmed": 25220016, "text": "NGLY1 mutation causes neuromotor impairment, intellectual disability, and neuropathy"}, {"pubmed": 25398991, "text": "This review summarizes the research history of cytoplasmic PNGase."}, {"pubmed": 25900930, "text": "The patients with NGLY1 deficiency show developmental delay, seizures, peripheral neuropathy, abnormal liver function and alacrima."}, {"pubmed": 27388694, "text": "Our prospective phenotyping expands the clinical spectrum of NGLY1-CDDG, offers prognostic information, and provides baseline data for evaluating therapeutic interventions"}, {"pubmed": 28750948, "text": "Across these distinct evolutionary models of cytosolic NGLY1 deficiency, a consistent disruption of mitochondrial physiology was present involving modestly reduced mitochondrial content with more pronounced impairment of mitochondrial membrane potential, increased mitochondrial matrix oxidant burden, and reduced cellular respiratory capacity."}, {"pubmed": 31875565, "text": "N-Glycanase 1 Transcriptionally Regulates Aquaporins Independent of Its Enzymatic Activity."}, {"pubmed": 31957011, "text": "Variants in NGLY1 lead to intellectual disability, myoclonus epilepsy, sensorimotor axonal polyneuropathy and mitochondrial dysfunction."}, {"pubmed": 31965062, "text": "Novel NGLY1 gene variants in Chinese children with global developmental delay, microcephaly, hypotonia, hypertransaminasemia, alacrimia, and feeding difficulty."}, {"pubmed": 32265286, "text": "Loss of N-Glycanase 1 Alters Transcriptional and Translational Regulation in K562 Cell Lines."}, {"pubmed": 33320481, "text": "Congenital disorder of deglycosylation associated with N-glycanse 1 deficiency\", trans \"Wrodzone zaburzenie deglikozylacji zwiazane z deficytem N-glikanazy 1."}, {"pubmed": 33497766, "text": "NGLY1 deficiency: Novel variants and literature review."}, {"pubmed": 34969094, "text": "Ever-expanding NGLY1 biology."}, {"pubmed": 35271393, "text": "Ferroptosis regulation by the NGLY1/NFE2L1 pathway."}, {"pubmed": 35322011, "text": "Deficiency of N-glycanase 1 perturbs neurogenesis and cerebral development modeled by human organoids."}, {"pubmed": 35565658, "text": "Comprehensive Analysis of the Structure and Function of Peptide:N-Glycanase 1 and Relationship with Congenital Disorder of Deglycosylation."}, {"pubmed": 36102038, "text": "N-glycoproteomics reveals distinct glycosylation alterations in NGLY1-deficient patient-derived dermal fibroblasts."}, {"pubmed": 36209585, "text": "Comparative proteomics reveals elevated CCN2 in NGLY1-deficient cells."}, {"pubmed": 36528660, "text": "NGLY1 deficiency: estimated incidence, clinical features, and genotypic spectrum from the NGLY1 Registry."}, {"pubmed": 38039131, "text": "NGLY1 mutations cause protein aggregation in human neurons."}, {"pubmed": 38697387, "text": "Ocular features of NGLY1 deficiency from a prospective longitudinal cohort."}], "genomic_pos": {"chr": "3", "end": 25790039, "ensemblgene": "ENSG00000151092", "start": 25718944, "strand": -1}, "genomic_pos_hg19": {"chr": "3",