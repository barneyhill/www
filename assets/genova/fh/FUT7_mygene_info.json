nontcga": {"exp_lof": 10.7833659057, "exp_mis": 153.096977861, "exp_syn": 61.6452793832, "lof_z": 3.23935468735682, "mis_z": 0.728672818552398, "mu_lof": 1.08228165726e-06, "mu_mis": 1.64313952761e-05, "mu_syn": 6.57085849794e-06, "n_lof": 0.0, "n_mis": 135.0, "n_syn": 60.0, "p_li": 0.973317411939734, "p_null": 3.25588868212267e-05, "p_rec": 0.0266500291734451, "syn_z": 0.132682066228419}, "transcript": "ENST00000314412.6"}, "exons": [{"cdsend": 137031991, "cdsstart": 137030709, "chr": "9", "position": [[137030173, 137031725], [137031978, 137032088]], "strand": -1, "transcript": "NM_004479", "txend": 137032088, "txstart": 137030173}], "exons_hg19": [{"cdsend": 139926443, "cdsstart": 139925161, "chr": "9", "position": [[139924625, 139926177], [139926430, 139926540]], "strand": -1, "transcript": "NM_004479", "txend": 139926540, "txstart": 139924625}], "generif": [{"pubmed": 12506041, "text": "The Fuc-T VII promoter is transactivated by Tax in concert with CBP through a CRE-like sequence in a manner similar to that of viral CRE in HTLV-1 LTR."}, {"pubmed": 15262995, "text": "H-Ras mediates FucT-VII induction in Jurkat T cells via the activation of the Raf, PI3K, and a distinct, H-Ras-specific effector signaling pathway."}, {"pubmed": 15464988, "text": "These results suggest that the FucT-VII may be a major regulator of the biosynthesis of the sLe(x)-epitopes on T lymphoblasts."}, {"pubmed": 15579466, "text": "human FucT-IV and -VII both contribute and cooperate in regulating L-selectin-, P-selectin-, and E-selectin-dependent rolling on PSGL-1, with FucT-VII playing a predominant role in conferring selectin binding activity to PSGL-1"}, {"pubmed": 15926890, "text": "A differential functional impact of N-glycosylation on C2GnT-1 and FucT-VII and disclose that a strongly reduced FucT-VII activity retains the ability to fucosylate PSGL-1 on the core2-based binding site(s) for the three selectins."}, {"pubmed": 17229154, "text": "Alpha 1,3-fucosyltransferase-VII regulates the signaling molecules of the insulin receptor pathway"}, {"pubmed": 17436081, "text": "The up regulation of alpha1,3FucT-VII mRNA and cell surface SLe(x) (alpha1,3FucT-VII product) by UV and down regulation of them by ATRA was speculated to be one of the mechanisms that alpha1,3FucT-VII decreased and increased [susceptibility to] apoptosis."}, {"pubmed": 17760744, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 18402946, "text": "Overexpression of fucosyltransferase VII (FUT7) promotes embryo adhesion and implantation."}, {"pubmed": 18452157, "text": "This is the first time to report that alpha1,3FucT-VII can regulate the mRNA expression of integrin."}, {"pubmed": 19851296, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20198315, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20428816, "text": "Overexpression of alpha1,3 FucT-VII promoted the expression of CD24 and SLe X and is associated with colorectal carcinoma metastases."}, {"pubmed": 21197561, "text": "After transfection of JAR cells with fucosyltransferase VII, the expression of FUT7 and sLeX synthesis were increased, and the percent adhesion of trophoblast cells to RL95-2 cell monolayer was significantly increased."}, {"pubmed": 24176542, "text": "This study presents the first example of a distinct regulatory mechanism involving transcriptional down-regulation of Fut7 by MAPCs that could modulate the trafficking behavior of T cells in vivo."}, {"pubmed": 27591321, "text": "The DNA demethylation within the fut7 gene controls selectin ligand expression in humans, including the inducible topographic commitment of T cells for skin and inflamed sites."}, {"pubmed": 28585149, "text": "FUT7 silencing causes inhibition of adhesion and migration of HepG2 hepatocellular carcinoma cells in vitro."}, {"pubmed": 29138114, "text": "FUT7 overexpression significantly promoted monocyte-endothelial adhesion, while FUT7 knockdown obviously inhibited IL-1beta-induced monocyte-endothelial adhesion"}, {"pubmed": 30226570, "text": "Knockdown of FUT7 inhibits human hepatocarcinoma cell proliferation."}, {"pubmed": 30455694, "text": "the findings in this study suggest that hypomethylated CpG sites, present in four regions of the FOXP3 locus, CAMTA1 and FUT7 gene regions, can potentially be used to distinguish subsets of CD4+ T lymphocytes in both sexes."}, {"pubmed": 31104223, "text": "High FUT7 expression and CD15 are associated with lung and brain metastasis."}, {"pubmed": 34134578, "text": "Integrated analysis reveals the participation of IL4I1, ITGB7, and FUT7 in reshaping the TNBC immune microenvironment by targeting glycolysis."}, {"pubmed": 34973535, "text": "Activation of alpha7 nicotinic acetylcholine receptors attenuates monocyte-endothelial adhesion through FUT7 inhibition."}, {"pubmed": 36463240, "text": "Novel blood-based FUT7 DNA methylation is associated with lung cancer: especially for lung squamous cell carcinoma."}, {"pubmed": 36898609, "text": "Identification of FUT7 hypomethylation as the blood biomarker in the prediction of early-stage lung cancer."}], "genomic_pos": {"chr": "9", "end": 137032088, "ensemblgene": "ENSG00000180549", "start": 137030174, "strand": -1}, "genomic_pos_hg19": {"chr": "9", "end": 139927462, "start": 139924626, "strand": -1}, "go": {"BP": [{"evidence": "ISS", "gocategory": "BP", "id": "GO:0001807", "qualifier": "involved_in", "term": "regulation of type IV hypersensitivity"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0002361", "qualifier": "involved_in", "term": "CD4-positive, CD25-positive, alpha-beta regulatory T cell differentiation"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0002522", "qualifier": "involved_in", "term": "leukocyte migration involved in immune response"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0002523", "qualifier": "involved_in", "term": "leukocyte migration involved in inflamm