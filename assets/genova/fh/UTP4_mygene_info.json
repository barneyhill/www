erif": [{"pubmed": 12417987, "text": "A missense mutation (R565W) in cirhin (FLJ14728) in North American Indian childhood cirrhosis"}, {"pubmed": 16225863, "text": "Cirhin is a nucleolar protein and the R565W mutation, for which all North American Indian childhood cirrhosis patients are homozygous, has no effect on subcellular localization."}, {"pubmed": 19732766, "text": "Cirhin is a transcriptional regulatory factor of this NF-kappaB sequence and could be a participant in the regulation of other genes with NF-kappaB responsive elements."}, {"pubmed": 28350096, "text": "Results show that CIRH1A was highly expressed in carcinoma samples and colorectal cancer cells and suggest that CIRH1A plays a critical role in the proliferation, cell cycle distribution, and apoptosis of human malignant colorectal cells."}], "genomic_pos": [{"chr": "HSCHR16_2_CTG3_1", "end": 87707, "ensemblgene": "ENSG00000262788", "start": 60798, "strand": -1}, {"chr": "16", "end": 69231130, "ensemblgene": "ENSG00000141076", "start": 69131291, "strand": 1}], "genomic_pos_hg19": [{"chr": "16", "end": 69265033, "start": 69165194, "strand": 1}, {"chr": "HSCHR16_2_CTG3_1", "end": 69202911, "start": 69166418, "strand": 1}], "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0000462", "qualifier": "involved_in", "term": "maturation of SSU-rRNA from tricistronic rRNA transcript (SSU-rRNA, 5.8S rRNA, LSU-rRNA)"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0006355", "pubmed": 19732766, "qualifier": "involved_in", "term": "regulation of DNA-templated transcription"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0030490", "pubmed": 22916032, "qualifier": "involved_in", "term": "maturation of SSU-rRNA"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0042274", "pubmed": 34516797, "qualifier": "involved_in", "term": "ribosomal small subunit biogenesis"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0001650", "pubmed": 24219289, "qualifier": "located_in", "term": "fibril