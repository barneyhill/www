position": [[43084394, 43085505], [43087357, 43087489], [43092935, 43093190], [43093608, 43093887], [43097692, 43098052], [43098916, 43099076]], "strand": -1, "transcript": "NM_001816", "txend": 43099076, "txstart": 43084394}], "generif": [{"pubmed": 17909799, "text": "All the leukemic samples showed overexpression of CEACAM6 and 8 when compared with normal granulocytes."}, {"pubmed": 18056392, "text": "CD66b molecules are involved in regulating adhesion and activation of eosinophils, possibly through their localization in lipid rafts and interaction with other cell surface molecules, such as CD11b."}, {"pubmed": 19077207, "text": "Data show that desensitization of neutrophils to any two CEACAMs, 1, 3, 6, or 8, results in selective desensitization to those two CEACAMs, while the cells remain responsive to the other two neutrophil CEACAMs."}, {"pubmed": 21470677, "text": "The highly elevated gene expression of CEACAM6 and CEACAM8 in primary myelofibrosis can serve as molecular markers of myelofibrotic transformation."}, {"pubmed": 23686079, "text": "Granulocytes from type 2 diabetes patients display higher granulocyte cell surface expression of CD66b compared with values of healthy controls."}, {"pubmed": 24743304, "text": "CEACAM8 interacts with CEACAM1 inhibiting TLR2-triggered immune responses"}, {"pubmed": 26176669, "text": "Inhibition of C5aR1 leads to a significant reduction of CD66b and CD11b expression indicating a lower neutrophil activation status and block of granule exocytosis."}, {"pubmed": 26483485, "text": "Data suggest ways in which carcinoembryonic antigen-related cell adhesion molecules CEACAM6 and CEACAM8 regulate the biological functions of one another."}, {"pubmed": 27579896, "text": "a 7-gene signature was identified which correctly predicted the primary prefibrotic myelofibrosis group with a sensitivity of 100% and a specificity of 89%. The 7 genes included MPO, CEACAM8, CRISP3, MS4A3, CEACAM6, HEMGN, and MMP8"}, {"pubmed": 27863478, "text": "Increased CD66b positive tumor-infiltrating neutrophils (CD66b + TINs) was significantly associated with presence of metastasis, S stage, and nonseminomatous germ cell tumor diagnosis."}, {"pubmed": 31222613, "text": "High CEACAM8 expression is associated with metastases in breast carcinoma."}, {"pubmed": 32632664, "text": "CD66b(+) monocytes represent a proinflammatory myeloid subpopulation in cancer."}, {"pubmed": 37162631, "text": "Clinical Significance of CD66b Expression in Non-Small Cell Lung Cancer."}], "genomic_pos": {"chr": "19", "end": 42595055, "ensemblgene": "ENSG00000124469", "start": 42580243, "strand": -1}, "genomic_pos_hg19": {"chr": "19", "end": 43099207, "start": 43084393, "strand": -1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0006955", "pubmed": 2208113, "qualifier": "involved_in", "term": "immune response"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007157", "qualifier": "involved_in", "term": "heterophilic cell-cell adhesion via plasma membrane cell adhesion molecules"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0007157", "pubmed": [8776764, 11590190], "qualifier": "involved_in", "term": "heterophilic cell-cell adhesion via plasma membrane cell adhesion molecules"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0005615", "pubmed": 25645918, "qualifier": "located_in", "term": "extracellular space"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005886", "qualifier": "located_in", "term": "plasma membrane"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0009986", "qualifier": "is_active_in", "term": "cell surface"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0009986", "pubmed": 8776764, "qualifier": "located_in", "term": "cell surface"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0035577", "qualifier": "located_in", "term": "azurophil granule membrane"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0035579", "qualifier": "located_in", "term": "specific granule membrane