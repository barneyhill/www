, {"cdsend": 22833562, "cdsstart": 22696415, "chr": "11", "position": [[22696319, 22696560], [22707213, 22707335], [22747837, 22747979], [22759250, 22759314], [22770665, 22770807], [22777391, 22777499], [22833343, 22834601]], "strand": 1, "transcript": "NM_005256", "txend": 22834601, "txstart": 22696319}, {"cdsend": 22833562, "cdsstart": 22696415, "chr": "11", "position": [[22689653, 22689881], [22696395, 22696560], [22707213, 22707335], [22747837, 22747979], [22759250, 22759314], [22770665, 22770807], [22777391, 22777499], [22833343, 22834601]], "strand": 1, "transcript": "NM_177553", "txend": 22834601, "txstart": 22689653}, {"cdsend": 22834601, "cdsstart": 22834601, "chr": "11", "position": [[22688156, 22688740], [22696395, 22696560], [22707213, 22707335], [22747837, 22747979], [22759250, 22759314], [22770665, 22770807], [22777391, 22777499], [22783312, 22783360], [22783942, 22783974], [22787986, 22788014], [22833343, 22834601]], "strand": 1, "transcript": "NR_147085", "txend": 22834601, "txstart": 22688156}], "generif": [{"pubmed": 15817486, "text": "Gas2DN can increase the activity of calpain and induce degradation of stabilized/mutated beta-catenin"}, {"pubmed": 20677014, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20679491, "text": "Studies have identified a Gas2/calpain-dependent mechanism by which ICSBP influences beta-catenin activity in myeloid leukemia."}, {"pubmed": 24465953, "text": "GAS2 is up-regulated in chronic myeloid leukemia cells."}, {"pubmed": 25925944, "text": "Truncated HBx deregulates cell growth via direct silencing of GAS2 and thereby provides a survival advantage for pre-neoplastic hepatocytes to facilitate cancer development via TP53 signaling."}, {"pubmed": 26358320, "text": "Taken together, our results demonstrated the deregulation of GAS2 in both AML and ALL and the requirement of GAS2-Calpain2 axis for the growth of leukemic cells."}, {"pubmed": 27085973, "text": "This study found a highly significant correlation between colorectal cancers relapse and growth arrest-specific 2 (GAS2) gene expression. Based on cell models, the overexpressed GAS2 was associated with cellular growth rate, cell cycle regulation, and with chemotherapeutic sensitivity."}, {"pubmed": 31528096, "text": "GAS2 protein expression was lower in hepatocellular carcinoma (HCC) than in normal tissues. Overexpression of GAS2 inhibited the proliferation of HCC cells with wide type p53, while knockdown of GAS2 promoted the proliferation of hepatocytes. Results suggest that GAS2 plays a vital role in HCC cell proliferation and apoptosis, possibly by regulating the cell cycle and p53-dependent apoptosis pathway."}, {"pubmed": 32572885, "text": "UCHL1 enhances the malignant development of glioma via targeting GAS2."}, {"pubmed": 33103301, "text": "Growth arrest-specific 2 protein family: Structure and function."}, {"pubmed": 33964205, "text": "Cochlear supporting cells require GAS2 for cytoskeletal architecture and hearing."}, {"pubmed": 36054080, "text": "Growth arrest-specific protein 2 (GAS2) interacts with CXCR4 to promote T-cell leukemogenesis partially via c-MYC."}], "genomic_pos": {"chr": "11", "end": 22813001, "ensemblgene": "ENSG00000148935", "start": 22625509, "strand": 1}, "genomic_pos_hg19": {"chr": "11", "end": 22834601, "start": 22647188, "strand": 1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0001544", "qualifier": "involved_in", "term": "initiation of primordial ovarian follicle growth"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0001547", "qualifier": "involved_in", "term": "antral ovarian follicle growth"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0006915", "qualifier": "involved_in", "term": "apoptotic process"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0008360", "qualifier": "involved_in", "term": "regulation of cell shape"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0008593", "qualifier": "involved_in", "term": "r