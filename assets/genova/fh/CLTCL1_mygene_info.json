, [19212975, 19213156], [19213741, 19213906], [19217360, 19217498], [19219998, 19220121], [19220688, 19220841], [19220944, 19221145], [19222031, 19222229], [19223218, 19223392], [19226797, 19226911], [19230297, 19230459], [19241481, 19241750], [19263145, 19263353], [19279122, 19279242]], "strand": -1, "transcript": "NM_001835", "txend": 19279242, "txstart": 19166985}, {"cdsend": 19279164, "cdsstart": 19167731, "chr": "22", "position": [[19166985, 19167482], [19167711, 19167751], [19168243, 19168319], [19170902, 19171124], [19175069, 19175240], [19175492, 19175603], [19178815, 19178947], [19183776, 19183926], [19183999, 19184167], [19187244, 19187352], [19188839, 19189003], [19195662, 19195821], [19196431, 19196624], [19197835, 19198019], [19203620, 19203766], [19207393, 19207516], [19208899, 19209134], [19209473, 19209616], [19210206, 19210332], [19211413, 19211577], [19212975, 19213156], [19213741, 19213906], [19217360, 19217498], [19219998, 19220121], [19220688, 19220841], [19220944, 19221145], [19222031, 19222229], [19223218, 19223392], [19226797, 19226911], [19230297, 19230459], [19241481, 19241750], [19263145, 19263353], [19279122, 19279242]], "strand": -1, "transcript": "NM_007098", "txend": 19279242, "txstart": 19166985}], "generif": [{"pubmed": 15133132, "text": "Clathrin isoform CHC22 binds to sorting nexin 5 through a coiled-coil domain."}, {"pubmed": 19478182, "text": "role for CHC22 in formation of insulin-responsive GLUT4 compartments in muscle & adipocytes; CHC22 associated with expanded GLUT4 compartments in muscle in type 2 diabetes"}, {"pubmed": 20065094, "text": "CHC22 was required for retrograde trafficking of certain cargo molecules from endosomes to the trans-Golgi network."}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 22360420, "text": "A protein encoded by this locus was found to be differentially expressed in postmortem brains from patients with atypical frontotemporal lobar degeneration."}, {"pubmed": 22891263, "text": "Depletion of clathrin heavy chain (CHC)17, but not the CHC22 clathrin isoform, by ribonucleic acid interference (RNAi) induces centrosome amplification and multipolar spindles."}, {"pubmed": 26068709, "text": "CLTCL1 is significantly upregulated in the developing human brain"}, {"pubmed": 29097553, "text": "Provide biochemical evidence for separate regulation and distinct functional niches for CHC17 and CHC22 in HeLa cells. Furthermore, the greater stability of the CHC22 coat relative to the CHC17 coat may be relevant to its excessive accumulation with GLUT4 during insulin resistance."}, {"pubmed": 29402896, "text": "Clathrin heavy chain 22 contributes to the control of neuropeptide degradation and secretion during neuronal development. Study suggests This suggests that a reduction in CHC22 expression in sensory neural precursors may license a step in neuron precursor neurodevelopment; and that this step is mediated through control of a novel neuropeptide processing pathway."}, {"pubmed": 31159924, "text": "These analyses suggest that ancestral human dietary change influenced selection of allotypes that affect CHC22's role in metabolism and have potential to differentially influence the human insulin response."}, {"pubmed": 32620516, "text": "Building GLUT4 Vesicles: CHC22 Clathrin's Human Touch."}], "genomic_pos": {"chr": "22", "end": 19291719, "ensemblgene": "ENSG00000070371", "start": 19179473, "strand": -1}, "genomic_pos_hg19": [{"chr": "22", "end": 19279239, "start": 19166986, "strand": -1}, {"chr": "HG329_PATCH", "end": 19279229, "start": 19166989, "strand": -1}], "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0000278", "pubmed": 20065094, "qualifier": "NOT involved_in", "term": "mitotic cell cycle"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0000278", "pubmed": 19509056, "qualifier": "involved_in", "term": "mitotic cell cycle"}, {"evidence": "IEA", "gocategory":