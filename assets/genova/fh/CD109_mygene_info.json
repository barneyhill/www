519706, 74519897], [74520714, 74520819], [74521927, 74521993], [74524703, 74524846], [74528110, 74528258], [74530184, 74530287], [74533181, 74538041]], "strand": 1, "transcript": "NM_001159587", "txend": 74538041, "txstart": 74405807}, {"cdsend": 74533357, "cdsstart": 74405938, "chr": "6", "position": [[74405807, 74406012], [74407122, 74407295], [74432973, 74433002], [74446105, 74446231], [74466365, 74466405], [74468666, 74468751], [74472106, 74472203], [74472463, 74472605], [74473298, 74473408], [74475652, 74475877], [74476481, 74476583], [74476670, 74476733], [74477777, 74477954], [74481151, 74481304], [74490146, 74490221], [74490981, 74491042], [74492336, 74492478], [74493429, 74493547], [74495086, 74495200], [74496956, 74497175], [74498190, 74498335], [74502348, 74502525], [74512942, 74513024], [74516566, 74516795], [74517805, 74517971], [74519706, 74519897], [74520714, 74520870], [74521927, 74521993], [74524703, 74524846], [74528110, 74528258], [74530184, 74530287], [74533181, 74538041]], "strand": 1, "transcript": "NM_001159588", "txend": 74538041, "txstart": 74405807}, {"cdsend": 74533357, "cdsstart": 74405938, "chr": "6", "position": [[74405925, 74406012], [74407122, 74407295], [74432973, 74433002], [74440066, 74440297], [74446105, 74446231], [74466365, 74466405], [74468666, 74468751], [74472106, 74472203], [74472463, 74472605], [74473298, 74473408], [74475652, 74475877], [74476481, 74476583], [74476670, 74476733], [74477777, 74477954], [74481151, 74481304], [74490146, 74490221], [74490981, 74491042], [74492336, 74492478], [74493429, 74493547], [74495086, 74495200], [74496956, 74497175], [74498190, 74498335], [74502348, 74502525], [74512942, 74513024], [74516566, 74516795], [74517805, 74517971], [74519706, 74519897], [74520714, 74520870], [74521927, 74521993], [74524703, 74524846], [74528110, 74528258], [74530184, 74530287], [74533181, 74538037]], "strand": 1, "transcript": "NM_133493", "txend": 74538037, "txstart": 74405925}], "generif": [{"pubmed": 11861284, "text": "Cell surface antigen CD109 is a novel member of the alpha(2) macroglobulin/C3, C4, C5 family of thioester-containing proteins. cDNA sequence determination and protein structure analysis given."}, {"pubmed": 11861285, "text": "A tyrosine703serine polymorphism of CD109 defines the Gov platelet alloantigens."}, {"pubmed": 15569238, "text": "Observational study of genotype prevalence. (HuGE Navigator)"}, {"pubmed": 15826242, "text": "Findings suggest that CD109 may become a molecular target for the development of new therapeutics for squamous cell carcinoma of various tissue origins."}, {"pubmed": 16754747, "text": "CD109 plays a unique role in the regulation of isoform-specific TGF-beta signaling in keratinocytes."}, {"pubmed": 16959974, "text": "Found with a mutation in 7% of colon cancers studied."}, {"pubmed": 17493171, "text": "Findings indicate that CD109 is a useful marker for the diagnosis of invasive breast and prostate carcinomas."}, {"pubmed": 17611685, "text": "the novel gene HEPT3 may function through its noncoding RNA and its overexpression may play a role in hepatocarcinogenesis [HEPT3]"}, {"pubmed": 17922683, "text": "CD109 expression may play a role in the development of lung squamous cell carcinoma."}, {"pubmed": 18519826, "text": "Clinical trial and genome-wide association study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 19016750, "text": "Up-regulation of CD109 expression is associated with carcinogensis of the squamous epithelium of the oral cavity."}, {"pubmed": 19570064, "text": "Observational study of genetic testing. (HuGE Navigator)"}, {"pubmed": 19898481, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19948007, "text": "Observational study of genotype prevalence. (HuGE Navigator)"}, {"pubmed": 20035377, "text": "Report PRSS3/mesotrypsin upregulation in breast cancer cells and identify CD109 as the functional proteolytic target of mesotrypsin."}, {"pubmed": 20101215, "text": "Processing of CD109 into 180 kDa and 25 kDa proteins by furin, followed by complex formation with the type I TGF-beta receptor is required for the regulation of TGF-beta signaling in cancer cells and keratinocytes."}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20667040, "text": "Observational study of genotype prevalence. (HuGE Navigator)"}, {"pubmed": 20946523, "text": "These findings suggest that CD109 is involved in bladder tumorigenesis and is a potential target for cancer immunotherapy."}, {"pubmed": 21295082, "text": "CD109 regulates TGF-beta receptor endocytosis and degradation to inhibit TGF-beta signaling."}, {"pubmed": 21539622, "text": "CD109 release from the cell surface in human keratinocytes regulates TGF-beta receptor expression, TGF-beta signalling and STAT3 activation."}, {"pubmed": 21898545, "text": "CD109 is an important regulator of SMAD7/Smurf2-mediated degradation of TGFBR1."}, {"pubmed": 22694813, "text": "The upregulation of CD109 protein in SSc may represent an adaptation or consequence of aberrant TGF-beta signaling in systemic sclerosis"}, {"pubmed": 23509816, "text": "expression of CD109 (human platelet antigen 15) mRNA is different in various human cell types"}, {"pubmed": 24376795, "text": "High expression of CD109 antigen regulates the phenotype of cancer stem-like cells/cancer-initiating cells in a novel epithelioid sarcoma cell line ESX and is related to poor prognosis of soft tissue sarcoma."}, {"pubmed": 24396288, "text": "Hematopoietic cell marker CD109 is expressed in hepatic progenitor cells."}, {"pubmed": 24667143, "text": "CD109 plays a critical role in non-small-cell lung cancer (NSCLC) progression, and is overexpressed in advanced NSCLC"}, {"pubmed": 25149155, "text": "CD109 is highly expressed in TNBC and is a potential biomarker for the initiation, progression, and differentiation of breast cancer tumors."}, {"pubmed": 25271095, "text": "expression increased in cutaneous squamous cell carcinoma"}, {"pubmed": 25388101, "text": "CD109 is specifically expressed in endothelial cells of cutaneous cavernous haemangioma."}, {"pubmed": 25506915, "text": "The results suggest that circulating endothelial cells express CD109 and represent a rare population of circulating tumor endothelial cells, that play a potentially useful prognostic role in patients with glioblastoma."}, {"pubmed": 25724945, "text": "Three glioblastoma cell lines, SK-MG-1, U251MG and MG178, were tested and CD109 overexpression attenuated TGF-beta1 signaling and enhanced EGF signaling in SK-MG-1, but not in U251MG or MG178."}, {"pubmed": 26031650, "text": "CD109 overexpression was significantly associated with surgical stage, distant metastasis, and poor prognosis in myxofibrosarcoma."}, {"pubmed": 26122747, "text": "findings show elevated CD109 protein expression in esophageal squamous cell carcinoma (ESCC); CD109 expression is restricted in squamous cells of ESCCs"}, {"pubmed": 26249215, "text": "CD109 may be a potential pathology marker for gallbladder squamous cell/adenosquamous carcinomas."}, {"pubmed": 26621871, "text": "sCD109 can bind TGF-beta, inhibit TGF-beta binding to its receptors and decrease TGF-beta signalling and TGF-beta-induced cellular responses."}, {"pubmed": 26707640, "text": "These findings indicate that CD109 is an exosomal protein and that the C-terminal region of CD109 is required for its presence in the exosome."}, {"pubmed": 26838754, "text": "Expression levels of CD109 was reduced significantly in psoriasis.  Lower expression of CD109 and TGF-beta RI was highly correlated with higher expression of Smad7 and Ki67, suggesting that CD109 may induce the pathogenesis of psoriasis through Smad7-mediated degradation of TGF-beta RI."}, {"pubmed": 27019315, "text": "The most common HPA genotypes among Saudis were HPA-1 a + b- (75%), HPA-2 a + b- (62%), HPA-3 a + b- (51.5%), HPA-4 a + b- (99%), HPA-5 a + b- (76.5%), HPA-6 a + b- (100%) and HPA-15 a + b + (50%). The prevalent allele among the HPA systems was (a), except in the HPA-15 system where the (b) allele was found in 52% of the subjects."}, {"pubmed": 27121053, "text": "CD109 knockdown upregulated IL-8 expression through activation of TGF-beta/Akt/NF-kappaB pathway in human umbilical vein endothelial cells"}, {"pubmed": 28032275, "text": "CD109 is a putative biomarker for identifying a high-risk group among DLBCL patients."}, {"pubmed": 28888050, "text": "High expression of CD109 in brain tumor stem cells is involved in glioma progression."}, {"pubmed": 29654145, "text": "GRP78 binds to and acts in concert with a glycosylphosphatidylinositol-anchored protein, CD109, in blocking TGF-beta signaling by promoting the routing of the TGF-beta receptor to the caveolae, thereby disrupting its binding to and activation of Smad2."}, {"pubmed": 31695056, "text": "CD109 acts as a gatekeeper of the epithelial trait by suppressing epithelial to mesenchymal transition in squamous cell carcinoma cells in vitro."}, {"pubmed": 32007357, "text": "CD109 promotes the tumorigenic ability and metastatic motility of pancreatic ductal adenocarcinoma cells."}, {"pubmed": 32133706, "text": "Expression of CD109 is correlated with the invasive and metastatic capacities of lung adenocarcinoma cells. CD109 is upregulated in tumorous tissues, and CD109 overexpression is associated with tumor progression, distant metastasis, and poor prognosis in patients. Inhibition of CD109 decreases EGFR phosphorylation, diminishes EGF-elicited activation of AKT/mTOR, and sensitizes tumor cells to an EGFR inhibitor."}, {"pubmed": 32507856, "text": "CD109 mediates tumorigenicity and cancer aggressiveness via regulation of EGFR and STAT3 signalling in cervical squamous cell carcinoma."}, {"pubmed": 32938908, "text": "Glioma-initiating cells at tumor edge gain signals from tumor core cells to promote their malignancy."}, {"pubmed": 33375719, "text": "Upregulation of CD109 Promotes the Epithelial-to-Mesenchymal Transition and Stemness Properties of Lung Adenocarcinomas via Activation of the Hippo-YAP Signaling."}, {"pubmed": 33565282, "text": "Serum CD109 levels reflect the node metastasis status in head and neck squamous cell carcinoma."}, {"pubmed": 33986188, "text": "CD109-GP130 interaction drives glioblastoma stem cell plasticity and chemoresistance through STAT3 activity."}, {"pubmed": 34389612, "text": "Single-cell analysis pinpoints distinct populations of cytotoxic CD4(+) T cells and an IL-10(+)CD109(+) TH2 cell population in nasal polyps."}, {"pubmed": 34743190, "text": "The GPI-anchored protein CD109 protects hematopoietic progenitor cells from undergoing erythroid differentiation induced by TGF-beta."}, {"pubmed": 34762199, "text": "CD109 expression in tumor cells and stroma correlates with progression and prognosis in pancreatic cancer."}, {"pubmed": 35347029, "text": "The Significance of CD109 Expression in Oropharyngeal Squamous Cell Carcinoma."}, {"pubmed": 35385222, "text": "Placental and plasma early predictive biomarkers for gestational diabetes mellitus."}, {"pubmed": 36929132, "text": "CD109-regulated mechanical properties of endothelial cells."}, {"pubmed": 37030166, "text": "Significance of expression of CD109 in osteosarcoma and its involvement in tumor progression via BMP signaling."}, {"pubmed": 37373457, "text": "CD109 Promotes Drug Resistance in A2780 Ovarian Cancer Cells by Regulating the STAT3-NOTCH1 Signaling Axis."}, {"pubmed": 37661780, "text": "H3K27 acetylation activated-CD109 evokes 5-fluorouracil resistance in gastric cancer via the JNK/MAPK signaling pathway."}], "genomic_pos": {"chr": "6", "end": 73828316, "ensemblgene": "ENSG00000156535", "start": 73695785, "strand": 1}, "genomic_pos_hg19": {"chr": "6", "end": 74538040, "start": 74405508, "strand": 1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0001933