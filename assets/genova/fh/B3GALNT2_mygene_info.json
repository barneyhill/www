strand": -1, "transcript": "NM_001277155", "txend": 148044, "txstart": 107049}, {"cdsend": 235504252, "cdsstart": 235450205, "chr": "1", "position": [[235447189, 235450340], [235453089, 235453146], [235454155, 235454315], [235455558, 235455684], [235458602, 235458786], [235465635, 235465714], [235470849, 235470960], [235480053, 235480149], [235484321, 235484515], [235489167, 235489268], [235494680, 235494828], [235504140, 235504452]], "strand": -1, "transcript": "NM_152490", "txend": 235504452, "txstart": 235447189}, {"cdsend": 147844, "cdsstart": 93797, "chr": "1_KQ458384v1_alt", "position": [[90781, 93932], [96681, 96738], [97747, 97907], [99150, 99276], [102194, 102378], [109227, 109306], [114441, 114552], [123645, 123741], [127913, 128107], [132759, 132860], [138272, 138420], [147732, 148044]], "strand": -1, "transcript": "NM_152490", "txend": 148044, "txstart": 90781}], "exons_hg19": [{"cdsend": 235667552, "cdsstart": 235628935, "chr": "1", "position": [[235626771, 235629031], [235634163, 235634274], [235643369, 235643465], [235647637, 235647831], [235652472, 235652573], [235657990, 235658138], [235659495, 235659618], [235667440, 235667752]], "strand": -1, "transcript": "NM_001277155", "txend": 235667752, "txstart": 235626771}, {"cdsend": 235667552, "cdsstart": 235613520, "chr": "1", "position": [[235610504, 235613655], [235616401, 235616458], [235617467, 235617627], [235618870, 235618996], [235621910, 235622094], [235628952, 235629031], [235634163, 235634274], [235643369, 235643465], [235647637, 235647831], [235652472, 235652573], [235657990, 235658138], [235667440, 235667752]], "strand": -1, "transcript": "NM_152490", "txend": 235667752, "txstart": 235610504}], "generif": [{"pubmed": 14724282, "text": "Although the GalNAcbeta1-3GlcNAcbeta1-R structure has not been reported in humans or other mammals, we have discovered a novel human glycosyltransferase producing this structure on N- and O-glycans."}, {"pubmed": 23453667, "text": "Results demonstrate a role for B3GALNT2 in the glycosylation of alpha-DG and show that B3GALNT2 mutations can cause dystroglycanopathy with muscle and brain involvement."}, {"pubmed": 24084573, "text": "B3GALNT2 is a gene associated with congenital muscular dystrophy with brain malformations."}, {"pubmed": 24285400, "text": "B3GALNT2 overexpression is associated with breast cancer."}, {"pubmed": 29273094, "text": "Mutations in B3GALNT2 give rise to a novel muscular dystrophy-dystroglycanopathies syndrome presentation, characterized by intellectual disability and seizure, but without any apparent muscular involvement."}, {"pubmed": 29618368, "text": "B3GALNT2 reduced expression of some metabolic enzymes and thus downregulated levels of secreted acetoacetate. This relieved the activity of MIF and enhanced macrophage recruitment to promote tumor growth."}, {"pubmed": 30898876, "text": "B3GALNT2 primarily transferred LDN to intracellular glycoproteins, thereby clearly delineating proteins that carry type-I or type-II LacdiNAcs"}, {"pubmed": 33290285, "text": "Novel mutations in B3GALNT2 gene causing alpha-dystroglycanopathy in Chinese patients."}, {"pubmed": 35338537, "text": "Prenatal diagnosis of Walker-Warburg syndrome due to compound mutations in the B3GALNT2 gene."}, {"pubmed": 37368380, "text": "[Genetic analysis of a Chinese family affected with alpha-dystroglycanopathy due to variant of B3GALNT2 gene]."}], "genomic_pos": [{"chr": "1", "end": 235504452, "ensemblgene": "ENSG00000162885", "start": 235447190, "strand": -1}, {"chr": "HSCHR1_5_CTG32_1", "end": 148044, "ensemblgene": "ENSG00000282880", "start": 90782, "strand": -1}], "genomic_pos_hg19": {"chr": "1", "end": 235667781, "start": 235613238, "strand": -1}, "go": {"BP": [{"evidence": "IMP", "gocategory": "BP", "id": "GO:0006486", "pubmed": 23453667, "qualifier": "involved_in", "term": "protein glycosylation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006493", "qualifier": "involved_in", "term": "protein O-linked glycosylation"