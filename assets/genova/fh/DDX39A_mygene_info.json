ons": [{"cdsend": 14413220, "cdsstart": 14408935, "chr": "19", "position": [[14408797, 14408952], [14409036, 14409184], [14409302, 14409447], [14409535, 14409645], [14409741, 14409873], [14410215, 14410334], [14410988, 14411172], [14411505, 14411598], [14412550, 14412678], [14413012, 14413224], [14419269, 14419341]], "strand": -1, "transcript": "NM_005804", "txend": 14419341, "txstart": 14408797}, {"cdsend": 14419383, "cdsstart": 14419383, "chr": "19", "position": [[14408797, 14408952], [14409036, 14409184], [14409302, 14409447], [14409741, 14409873], [14410215, 14410334], [14410988, 14411172], [14411505, 14411598], [14412550, 14412678], [14413012, 14413224], [14419269, 14419383]], "strand": -1, "transcript": "NR_046366", "txend": 14419383, "txstart": 14408797}], "exons_hg19": [{"cdsend": 14524032, "cdsstart": 14519747, "chr": "19", "position": [[14519609, 14519764], [14519848, 14519996], [14520114, 14520259], [14520347, 14520457], [14520553, 14520685], [14521027, 14521146], [14521800, 14521984], [14522317, 14522410], [14523362, 14523490], [14523824, 14524036], [14530081, 14530153]], "strand": -1, "transcript": "NM_005804", "txend": 14530153, "txstart": 14519609}, {"cdsend": 14530195, "cdsstart": 14530195, "chr": "19", "position": [[14519609, 14519764], [14519848, 14519996], [14520114, 14520259], [14520553, 14520685], [14521027, 14521146], [14521800, 14521984], [14522317, 14522410], [14523362, 14523490], [14523824, 14524036], [14530081, 14530195]], "strand": -1, "transcript": "NR_046366", "txend": 14530195, "txstart": 14519609}], "generif": [{"pubmed": 15047853, "text": "Regulation of URH49 mRNA expression and comparison with the related UAP56 RNA helicase."}, {"pubmed": 16949217, "text": "Reducing the expression of UAP56 and URH49 resulted in a reduction in reporter gene expression as well as cell death within 72 h suggest that both helicases have essential but largely overlapping functions in the processing and export of mammalian mRNAs."}, {"pubmed": 17196963, "text": "CIP29 physically and functionally associates with DDX39, suggesting their cooperation in the RNA metabolism."}, {"pubmed": 17548965, "text": "DDX39 is a novel RNA helicase capable of promoting cancer cell growth"}, {"pubmed": 20573985, "text": "Data show that the two closely related RNA helicases UAP56 and URH49 have evolved to form distinct mRNA export machineries, which regulate mitosis at different steps."}, {"pubmed": 20610707, "text": "Using a UL69 viral mutant that is unable to bind UAP56 and URH49, the authors demonstrated that UL69's interaction with UAP56 or URH49 does not contribute to the growth phenotype associated with the UL69 deletion mutant."}, {"pubmed": 21388492, "text": "Requirement of DDX39 DEAD box RNA helicase for genome integrity and telomere protection"}, {"pubmed": 21799930, "text": "UAP56 and URH49 exhibit an intrinsic CRM1-independent nucleocytoplasmic shuttling"}, {"pubmed": 21859714, "text": "Mx proteins exert their antiviral activity against IAV by interfering with the function of the RNA helicases UAP56 and URH49"}, {"pubmed": 22119546, "text": "-dependent RNA helicase DDX39 has a role in poor prognosis of patients with gastrointestinal stromal tumor"}, {"pubmed": 22494014, "text": "Results suggest that DDX39 is a suppressor of invasion and loss of its function predicts disease progression in bladder cancers."}, {"pubmed": 23749908, "text": "The protein levels of DDX39 were found to be higher in malignant pleural mesothelioma cell lines compared to normal pleural mesothelial cells."}, {"pubmed": 26469522, "text": "DDX39A is a potential biomarker for unfavorable neuroblastoma"}, {"pubmed": 28025139, "text": "DDX39B and its paralog DDX39A regulate androgen receptor splice variant AR-V7 generation"}, {"pubmed": 29867138, "text": "DDX39 regulates hepatocellular carcinoma progression by activating Wnt/beta-catenin pathway."}, {"pubmed": 31917363, "text": "UAP56- and URH49-dependent mRNA export pathways are integrated into NXF1 through the remodeling of each complex formation by ATP loading."}, {"pubmed": 32393512, "text": "SUMOylation of DDX39A Alters Binding and Export of Antiviral Transcripts to Control Innate Immunity."}, {"pubmed": 33941617, "text": "The Mammalian Ecdysoneless Protein Interacts with RNA Helicase DDX39A To Regulate Nuclear mRNA Export."}, {"pubmed": 34421357, "text": "DDX39 as a predictor of clinical prognosis and immune checkpoint therapy efficacy in patients with clear cell renal cell carcinoma."}, {"pubmed": 38225262, "text": "Structural differences between the closely related RNA helicases, UAP56 and URH49, fashion distinct functional apo-complexes."}, {"pubmed": 38377942, "text": "Terminal regions of UAP56 and URH49 are required for their distinct complex formation functioning to an essential role in mRNA processing and export."}], "genomic_pos": {"chr": "19", "end": 14419383, "ensemblgene": "ENSG00000123136", "start": 14408798, "strand": -1}, "genomic_pos_hg19": {"chr": "19", "end": 14530192, "start": 14519631, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0000398", "qualifier": "involved_in", "term": "mRNA splicing, via spliceosome"}, {"evidence": "IGI", "gocategory": "BP", "id": "GO:0000398", "pubmed": 15047853, "qualifier": "involved_in", "term": "mRNA splicing, via spliceosome"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0006405", "qualifier": "involved_in", "term": "RNA export from nucleus"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006406", "qualifier": "involved_in", "term": "mRNA export from nucleus"}, {"evidence": "IGI", "gocategory": "BP", "id": "GO:0006406", "pubmed": 15047853, "qualifier": "involved_in", "term": "mRNA export from nucleus"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0005634", "pubmed": 21859714, "qualifier": "located_in", "term": "nucleus"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "IDA", "goca