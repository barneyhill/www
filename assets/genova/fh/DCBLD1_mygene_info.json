on."}, {"pubmed": 30902898, "text": "The DCBLD receptor family: emerging signaling roles in development, homeostasis and disease."}, {"pubmed": 32231272, "text": "SNP rs17079281 decreases lung cancer risk through creating an YY1-binding site to suppress DCBLD1 expression."}, {"pubmed": 32606017, "text": "FYN and ABL Regulate the Interaction Networks of the DCBLD Receptor Family."}, {"pubmed": 34140574, "text": "DCBLD1 is associated with the integrin signaling pathway and has prognostic value in non-small cell lung and invasive breast carcinoma."}, {"pubmed": 36308382, "text": "SNP rs9387478 at ROS1-DCBLD1 Locus is Significantly Associated with Lung Cancer Risk and Poor Survival in Indian Population."}, {"pubmed": 38291438, "text": "Lactylation stabilizes DCBLD1 activating the pentose phosphate pathway to promote cervical cancer progression."}], "genomic_pos": {"chr": "6", "end": 117569858, "ensemblgene": "ENSG00000164465", "start": 117453817, "strand": 1}, "genomic_pos_hg19": {"chr": "6", "end": 117891021, "start": 117774980, "strand": 1}, "go": {"CC": {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005886", "qualifier": "is_active_in", "term": "plasma membrane"}, "MF": {"category": "MF", "evidence": "IBA", "id": "GO:0038023", "qualifier": "enables", "term": "signaling receptor activity"}}, "homologene": {"genes": [[8364, 448154], [9031, 421734], [9544, 715234], [9598, 472109], [9606, 285761], [9913, 100298760], [10090, 66686], [10116, 309773]], "id": 12010}, "interpro": [{"desc": "Coagulation factor 5/8 C-terminal domain", "id": "IPR000421", "short_desc": "FA58C"}, {"desc": "CUB domain", "id": "IPR000859", "short_desc": "CUB_dom"}, {"desc": "LCCL domain", "id": "IPR004043", "short_desc": "LCCL"}, {"desc": "Galactose-binding-like domain superfamily", "id": "IPR008979", "short_desc": "Galactose-bd-like_sf"}, {"desc": "Spermadhesin, CUB domain superfamily", "id": "IPR035914", "short_desc": "Sperma_CUB_dom_sf"}, {"desc": "LCCL domain superfamily", "id": "IPR036609",