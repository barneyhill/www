ition": [[132767006, 132781989], [132785131, 132785214], [132789541, 132789614], [132791089, 132791186], [132791712, 132791765], [132792601, 132792739], [132793420, 132793514], [132796731, 132796801], [132824584, 132824727], [132834145, 132834255]], "strand": -1, "transcript": "NM_003569", "txend": 132834255, "txstart": 132767006}, {"cdsend": 132834255, "cdsstart": 132834255, "chr": "6", "position": [[132767006, 132781989], [132785131, 132785214], [132791089, 132791186], [132791712, 132791765], [132792601, 132792739], [132793420, 132793514], [132796731, 132796801], [132824584, 132824727], [132834145, 132834255]], "strand": -1, "transcript": "NR_137169", "txend": 132834255, "txstart": 132767006}], "generif": [{"pubmed": 12730232, "text": "syntaxin 7 has a role in the intracellular vacuolation induced by VacA"}, {"pubmed": 18198266, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 18570918, "text": "Results report that syntaxin 7, syntaxin 8, vti1b and VAMP8 physically and functionally interact with CFTR."}, {"pubmed": 18980942, "text": "Palmitoylation-defective syntaxin 7 is selectively retained on the plasma membrane, suggesting that palmitoylation is important for intercompartmental transport of syntaxin 7."}, {"pubmed": 19714869, "text": "The expression level of STX7 protein was inversely correlated to tumor stage, suggesting that decreased expression of STX7 is associated with more aggressive tumors."}, {"pubmed": 20170677, "text": "data reveal the presence of syntaxin 7 in the membranes of exocytosis-prone granules and phagocytosis-related granules in human neutrophils, and therefore it might play a role in both exocytosis and phagocytosis in human neutrophils"}, {"pubmed": 21438968, "text": "Data suggest that accumulation of recycling T cell receptors at the immunological synapse is SNARE-dependent and that Stx7-mediated processing of recycling TCRs through endosomes is a prerequisite for the cytolytic function of cytotoxic T lymphocytes."}, {"pubmed": 35762511, "text": "Syntaxin 7 contributes to breast cancer cell invasion by promoting invadopodia formation."}, {"pubmed": 37031804, "text": "Syntaxin 7 modulates seizure activity in epilepsy."}], "genomic_pos": {"chr": "6", "end": 132513198, "ensemblgene": "ENSG00000079950", "start": 132445867, "strand": -1}, "genomic_pos_hg19": {"chr": "6", "end": 132834337, "start": 132767006, "strand": -1}, "go": {"BP": [{"evidence": "IMP", "gocategory": "BP", "id": "GO:0001916", "pubmed": 21438968, "qualifier": "involved_in", "term": "positive regulation of T cell mediated cytotoxicity"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006886", "qualifier": "involved_in", "term": "intracellular protein transport"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006906", "qualifier": "involved_in", "term": "vesicle fusion"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0048278", "qualifier": "involved_in", "term": "vesicle docking"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0051640", "pubmed": 21438968, "qualifier": "involved_in", "term": "organelle localization"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0070925", "pubmed": 21438968, "qualifier": "involved_in", "term": "organelle assembly"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:1902685", "pubmed": 21438968, "qualifier": "involved_in", "term": "positive regulation of receptor localization to synapse"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:1903076", "pubmed": 18570918, "qualifier": "involved_in", "term": "regulation of protein localization to plasma membrane"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0001772", "pubmed": 21438968, "qualifier": "located_in", "term": "immunological synapse"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005764", "pubmed": 18980942, "qualifier": "located_in", "term": "lysosome"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0005765", "pubmed": 17897319, "qualifier": "located_in", "term