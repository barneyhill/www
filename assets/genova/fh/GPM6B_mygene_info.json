f": [{"pubmed": 15214007, "text": "it is unlikely that mutations in M6B have a role in Pelizaeus-Merzbacher-like syndrome, as examined in eight patients"}, {"pubmed": 16416265, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 17918249, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 18581270, "text": "Data suggest that M6B regulates serotonin uptake by affecting cellular trafficking of the serotonin transporter."}, {"pubmed": 21638316, "text": "Microarray expression analysis of GPM6B-depleted osteogenic human mesenchymal stem cells revealed significant changes in genes involved in cytoskeleton organization and biogenesis."}, {"pubmed": 23950870, "text": "Data show the Differences in high resolution melting analysis in promoters of tumor markers neuronal membrane glycoprotein M6-B, melanoma antigen family A12 and immunoglobulin superfamily Fc receptor indicated invasiveness of hepatocellular carcinoma."}, {"pubmed": 24696529, "text": "Circulating levels of DR6 and Gpm6B correlate with breast cancer tumor grade."}, {"pubmed": 24916915, "text": "Data indicate that neuronal membrane glycoproteins GPM6A and GPM6B may act as novel oncogenes in the development of lymphoid leukemia."}, {"pubmed": 25113253, "text": "Increase in the expression levels of mRNA and protein for the Gpm6B is associated with various types of gynaecological malignancy."}, {"pubmed": 30372567, "text": "Findings demonstrate that GPM6B plays a crucial role in SMC differentiation and regulates SMC differentiation through the activation of TGF-beta-Smad2/3 signaling via direct interactions with TbetaRI."}, {"pubmed": 33294057, "text": "GPM6B Inhibit PCa Proliferation by Blocking Prostate Cancer Cell Serotonin Absorptive Capacity."}], "genomic_pos": {"chr": "X", "end": 13938638, "ensemblgene": "ENSG00000046653", "start": 13770939, "strand": -1}, "genomic_pos_hg19": {"chr": "X", "end": 13956757, "start": 13789150, "strand": -1