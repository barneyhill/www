{"AllianceGenome": "379", "HGNC": "379", "MIM": "604001", "_id": "10142", "_version": 1, "accession": {"genomic": ["AC000066.1", "AC000120.1", "AC003086.1", "AC004013.1", "AJ010770.1", "CH236949.1", "CH471091.2", "CP068271.2", "KF458509.1", "NC_000007.14", "NC_060931.1", "NG_011623.1"], "protein": ["AAB86384.1", "AAB96867.2", "AAC60380.1", "AAD22767.1", "AAD39719.1", "AAH15533.1", "AAH40932.1", "AAK51115.3", "AAS07419.1", "AAT44400.1", "BAA34523.1", "BAA78718.1", "BAG52021.1", "CAA09361.1", "CAB40713.1", "CAB55912.2", "EAL24155.1", "EAL24156.1", "EAL24157.1", "EAL24158.1", "EAW76860.1", "EAW76861.1", "EAW76862.1", "EAW76863.1", "EAW76864.1", "EAW76865.1", "EAW76866.1", "EAW76867.1", "EAW76868.1", "NP_001366206.1", "NP_005742.4", "NP_671714.1", "Q99996.4"], "rna": ["AB018346.1", "AB019691.1", "AF026245.1", "AF083037.1", "AF091711.1", "AF247727.4", "AJ131693.1", "AK000270.1", "AK025160.1", "AK026444.1", "AK057546.1", "AK074869.1", "AL117418.1", "AY528715.1", "BC015533.2", "BC027455.1", "BC040932.1", "BC062448.1", "CR997756.1", "NM_001379277.1", "NM_005751.5", "NM_147166.1", "NM_147171.1", "NM_147185.3"], "translation": [{"protein": "NP_001366206.1", "rna": "NM_001379277.1"}, {"protein": "AAH40932.1", "rna": "BC040932.1"}, {"protein": "AAD39719.1", "rna": "AF091711.1"}, {"protein": "AAD22767.1", "rna": "AF083037.1"}, {"protein": "CAB40713.1", "rna": "AJ131693.1"}, {"protein": "BAG52021.1", "rna": "AK074869.1"}, {"protein": "BAA78718.1", "rna": "AB019691.1"}, {"protein": "AAB86384.1", "rna": "AF026245.1"}, {"protein": "NP_005742.4", "rna": "NM_005751.5"}, {"protein": "AAK51115.3", "rna": "AF247727.4"}, {"protein": "AAT44400.1", "rna": "AY528715.1"}, {"protein": "AAH15533.1", "rna": "BC015533.2"}, {"protein": "NP_671714.1", "rna": "NM_147185.3"}, {"protein": "BAA34523.1", "rna": "AB018346.1"}, {"protein": "CAB55912.2", "rna": "AL117418.1"}]}, "alias": ["AKAP-9", "AKAP350", "AKAP450", "CG-NAP", "HYPERION", "LQT11", "MU-RMS-40.16A", "PPP1R45", "PRKA9", "YOTIAO"], "clingen"