hr": "19", "position": [[46347086, 46347217], [46353747, 46353989], [46375603, 46375751], [46376452, 46376574], [46383410, 46383476], [46383779, 46383878], [46384803, 46384909], [46387092, 46387235], [46387365, 46387453], [46388407, 46388448], [46388552, 46388731], [46390050, 46390132], [46390283, 46390975]], "strand": 1, "transcript": "NM_006247", "txend": 46390975, "txstart": 46347086}], "exons_hg19": [{"cdsend": 46893603, "cdsstart": 46850353, "chr": "19", "position": [[46850343, 46850474], [46857004, 46857246], [46878860, 46879008], [46879709, 46879831], [46887036, 46887135], [46888060, 46888166], [46890349, 46890492], [46890622, 46890710], [46891664, 46891705], [46891809, 46891988], [46893307, 46893389], [46893540, 46894232]], "strand": 1, "transcript": "NM_001204284", "txend": 46894232, "txstart": 46850343}, {"cdsend": 46893603, "cdsstart": 46850353, "chr": "19", "position": [[46850343, 46850474], [46857004, 46857246], [46878860, 46879008], [46879709, 46879831], [46886667, 46886733], [46887036, 46887135], [46888060, 46888166], [46890349, 46890492], [46890622, 46890710], [46891664, 46891705], [46891809, 46891988], [46893307, 46893389], [46893540, 46894232]], "strand": 1, "transcript": "NM_006247", "txend": 46894232, "txstart": 46850343}], "generif": [{"pubmed": 12519780, "text": "PP5 suppresses a pathway regulating the expression of p21(waf1)"}, {"pubmed": 14734805, "text": "protein phosphatase 5 interacts with DNA-PKcs and dephosphorylates with surprising specificity at least two functional sites."}, {"pubmed": 15155720, "text": "structure of Serine/threonine protein phosphatase-5(PP5c) provides a structural basis for explaining the exceptional catalytic proficiency of protein phosphatases."}, {"pubmed": 15218033, "text": "PP5 is regulated by mTOR and activates apoptosis signal-regulating kinase 1 signaling"}, {"pubmed": 15328343, "text": "PP5 plays an important role in the survival of cells in a low oxygen environment by suppressing a hypoxia-induced ASK-1/MKK4/JNK signaling cascade that promotes an apoptotic response"}, {"pubmed": 15577939, "text": "Results describe the structure of the autoinhibited state of protein phosphatase 5, revealing mechanisms of tetratricopeptide repeat-mediated phosphatase inhibition and Hsp90- and arachidonic acid-induced stimulation of phosphatase activity."}, {"pubmed": 16260606, "text": "PP5 plays a critical role in ATR-mediated checkpoint activation"}, {"pubmed": 16531226, "text": "We report the solution structure of a complex of the TPR domain of Ppp5 with the C-terminal pentapeptide of Hsp90.This may be important for the relief of autoinhibition in Ppp5 and for the mechanisms of TPR-mediated recognition of Hsp90 by other proteins."}, {"pubmed": 16790549, "text": "PP5, casein kinase Iepsilon, and cryptochrome dynamically regulate the mammalian circadian clock"}, {"pubmed": 16892053, "text": "PP5 is a physiological regulator of Raf-1 signalling pathways."}, {"pubmed": 17084641, "text": "Protein phosphatase 5 as a key dephosphorylation regulator of Raf-1 activation."}, {"pubmed": 18280813, "text": "these studies indicate that elevated levels of PP5 protein occur in human breast cancer and suggest that PP5 over-expression may aid tumor progression."}, {"pubmed": 18547097, "text": "domain amphiphilicity is of higher importance than amino acid sequence specificity in the determination of protein adsorption and interfacial activity."}, {"pubmed": 19176521, "text": "PP5 plays an important role in the regulation of 53BP1 phosphorylation and activity in vivo"}, {"pubmed": 19586900, "text": "Estrogen inhibits glucocorticoid action via protein phosphatase 5 (PP5)-mediated glucocorticoid receptor dephosphorylation."}, {"pubmed": 19948726, "text": "Specific binding of PP5 to activated Rac1 provides a direct mechanism by which PP5 can be stimulated and recruited to participate in Rac1-mediated signaling pathways."}, {"pubmed": 20875921, "text": "Findings are indicative of a regulatory role of PP5 in cardiac function."}, {"pubmed": 21144835, "text": "PP5 plays a crucial role in ATR-mediated repair of UV-induced DNA damage."}, {"pubmed": 21921034, "text": "genetic disruption of PP5 is associated with enhanced and prolonged phosphorylation of a single serine (Ser-345) on Chk1"}, {"pubmed": 22399290, "text": "S100A1 and permanently active S100P inhibited the apoptosis signal-regulating kinase 1 (ASK1) and PP5 interaction, resulting the inhibition of dephosphorylation of phospho-ASK1 by PP5"}, {"pubmed": 22592921, "text": "proasthmatic cytokine-induced corticosteroid insensitivity in airway smooth muscle cells is due, in part, to PP5-mediated impairment of glucocorticoid receptor-Ser211 phosphorylation."}, {"pubmed": 23102700, "text": "Suggest that Slim/KLHDC10 is an activator of ASK1, contributing to oxidative stress-induced cell death through the suppression of PP5."}, {"pubmed": 23184943, "text": "Chp-1 and melusin can interact with cochaperones PP5 and Sgt1 and with each other in an ATP-dependent manner"}, {"pubmed": 23352616, "text": "these results suggest that caveolin-1 is a novel physiological activator of PP5."}, {"pubmed": 24327656, "text": "These results demonstrate that Hsp70 recruits PP5 and activates its phosphatase activity which suggests dual roles for PP5 that might link chaperone systems with signaling pathways in cancer and development."}, {"pubmed": 25269953, "text": "Data indicate that Cu-oxidized S100 calcium binding protein A4 (S100A4) interacted with S100 calcium binding protein A1 (S100A1) and prevented protein phosphatase 5 (PP5) activation."}, {"pubmed": 25326185, "text": "We demonstrate for the first time that PPP5C is essential for growth of HCC cells, which suggests that inhibition of PPP5C by RNAi may be a potential therapeutic strategy for the treatment of hepatocellular carcinoma."}, {"pubmed": 25796168, "text": "PPP5C could be essential for glioma cell growth."}, {"pubmed": 26157124, "text": "Measles Virus Infection Inactivates Cellular Protein Phosphatase 5 with Consequent Suppression of Sp1 and c-Myc Activities."}, {"pubmed": 27501780, "text": "PP5-dependent impairment of GRalpha function represents a novel mechanism driving GC insensitivity in ASM in severe asthma."}, {"pubmed": 27742828, "text": "Our results showed 1) a critical role for PP5 in cytokine-induced resistance to GC-mediated eosinophil death, 2) supported the dependence of GCR phosphorylation on PP5 activity, and 3) revealed that PP5 is a target of the lipoxin A4-induced pathway countering cytokine-induced resistance to GCs in eosinophils."}, {"pubmed": 28534961, "text": "High PPP5C expression is associated with urinary bladder cancer."}, {"pubmed": 29343782, "text": "PP5 binds and dephosphorylates the elastic N2B-unique sequence (N2Bus) of titin in cardiomyocytes. PP5 is pathologically elevated and likely contributes to hypo-phosphorylation of N2Bus in failing human hearts. Furthermore, Hsp90-activated PP5 interacts with components of a sarcomeric, N2Bus-associated, mechanosensor complex, and blocks mitogen-activated protein-kinase signaling in this complex."}, {"pubmed": 30280322, "text": "This work offers insights of how cantharidin-like inhibitors interact with human PP5, potentially allowing the design of more specific and even less cytotoxic drugs for cancer treatments."}, {"pubmed": 30679389, "text": "These data demonstrate that LB-100 is a catalytic inhibitor of both PP2AC and PPP5C and suggest that the observed antitumor activity might be due to an additive effect achieved by suppressing both PP2A and PPP5C."}, {"pubmed": 30826056, "text": "The role of the catalytic Mn(2+) ions in regulating the binding of PP5 with its substrate was investigated through MD simulations. Results reveal that the different states of Mn(2+) ions can alter the structure of the active site and the conformations of the residues coordinating the Mn(2+) ions or the substrate."}, {"pubmed": 32703456, "text": "Over-expression of human PP5 gene in mice induces corneal hyperplasia and leads to ocular surface squamous neoplasia."}], "genomic_pos": {"chr": "19", "end": 46392981, "ensemblgene": "ENSG00000011485", "start": 46347087, "strand": 1}, "genomic_pos_hg19": {"chr": "19", "end": 46896238, "start": 46850251, "strand": 1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0000165", "qualifier": "involved_in", "term": "MAPK cascade"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0000278", "pubmed": 7925273, "qualifier": "involved_in", "term": "mitotic cell cycle"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0006302", "qualifier": "involved_in", "term": "double-strand break repair"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0006351", "pubmed": 7925273, "qualifier": "involved_in", "term": "DNA-templated transcription"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0006470", "pubmed": 7925273, "qualifier": "involved_in", "term": "protein dephosphorylation"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0010288", "qualifier": "involved_in", "term": "response to lead ion"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0035970", "pubmed": 16262633, "qualifier": "involved_in", "term": "peptidyl-threonine dephosphorylation"}, {"evidence": "HMP", "gocategory": "BP", "id": "GO:0043123", "pubmed": 12761501, "qualifier": "involved_in", "term": "positive regulation of canonical NF-kappaB signal transduction"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0043278", "qualifier": "involved_in", "term": "response to morphine"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0070262", "pubmed": 30699359, "qualifier": "involved_in", "term": "peptidyl-serine dephosphorylation"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0070262", "pubmed": 16262633, "qualifier": "involved_in", "term": "peptidyl-serine dephosphorylation"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:1904550", "pubmed": 15546861, "qualifier": "involved_in", "term": "response to arachidonic ac