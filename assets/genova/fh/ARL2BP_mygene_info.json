c": {"_license": "http://bit.ly/2H9c4hg", "all": {"exp_lof": 9.27090230579, "exp_mis": 48.6027820554, "exp_syn": 24.135667985, "lof_z": 1.06413363975545, "mis_z": 0.95436301553299, "mu_lof": 6.17066565314e-07, "mu_mis": 4.20724097863e-06, "mu_syn": 2.00742516403e-06, "n_lof": 6.0, "n_mis": 35.0, "n_syn": 22.0, "p_li": 0.000774155836962192, "p_null": 0.228709994637729, "p_rec": 0.770515849525309, "syn_z": 0.269497000917644}, "bp": 492, "cds_end": 57286179, "cds_start": 57279279, "n_exons": 6, "nonpsych": {"exp_lof": 8.27327495669, "exp_mis": 43.8724227961, "exp_syn": 21.7273219501, "lof_z": 0.776184916244266, "mis_z": 1.13252423217027, "mu_lof": 6.17066565314e-07, "mu_mis": 4.20724097863e-06, "mu_syn": 2.00742516403e-06, "n_lof": 6.0, "n_mis": 29.0, "n_syn": 19.0, "p_li": 0.000524825285450927, "p_null": 0.337137932851789, "p_rec": 0.66233724186276, "syn_z": 0.368380392701716}, "nontcga": {"exp_lof": 8.51774931481, "exp_mis": 44.7030815629, "exp_syn": 22.193333903, "lof_z": 0.85100341947254, "mis_z": 1.17010803718395, "mu_lof": 6.17066565314e-07, "mu_mis": 4.20724097863e-06, "mu_syn": 2.00742516403e-06, "n_lof": 6.0, "n_mis": 29.0, "n_syn": 18.0, "p_li": 0.000556281661873113, "p_null": 0.29494664249049, "p_rec": 0.704497075847637, "syn_z": 0.563598870191759}, "transcript": "ENST00000219204.3"}, "exons": [{"cdsend": 57252267, "cdsstart": 57245367, "chr": "16", "position": [[57245258, 57245405], [57246079, 57246141], [57248536, 57248643], [57249766, 57249852], [57250410, 57250507], [57252165, 57253635]], "strand": 1, "transcript": "NM_012106", "txend": 57253635, "txstart": 57245258}], "exons_hg19": [{"cdsend": 57286179, "cdsstart": 57279279, "chr": "16", "position": [[57279170, 57279317], [57279991, 57280053], [57282448, 57282555], [57283678, 57283764], [57284322, 57284419], [57286077, 57287547]], "strand": 1, "transcript": "NM_012106", "txend": 57287547, "txstart": 57279170}], "generif": [{"pubmed": 19368893, "text": "Crystal structure of the ARL2-GTP-BART complex reveals a novel recognition and binding mode of small GTPase with effector."}, {"pubmed": 21665939, "text": "overexpression of the amino (N)-terminal region of G3BP, including the binding region for BART mRNA, dominant-negatively inhibits formation of the complex between endogenous G3BP and BART mRNA, and increases the expression of BART."}, {"pubmed": 21833473, "text": "Our results imply that BART increases active RhoA by inhibiting ARL2 function, which in turn inhibits invasiveness of cancer cells."}, {"pubmed": 21901094, "text": "We identify a subset of BART miRNAs that are restricted to Latency III in normal infection but are up regulated in tumors that express Latency I and II."}, {"pubmed": 22532868, "text": "These results imply that BART contributes to regulating PKCalpha activity through binding to ANX7, thereby affecting the invasiveness of pancreatic cancer cells."}, {"pubmed": 22745590, "text": "Our results imply that BART regulates actin-cytoskeleton rearrangements at membrane ruffles through modulation of the activity of Rac1, which, in turn, inhibits pancreatic cancer cell invasion."}, {"pubmed": 23685147, "text": "EBV-miR-BART1 could influence the expression of metabolism-associated genes and might be involved in cancer metabolism in nasopharyngeal carcinoma"}, {"pubmed": 23849777, "text": "Mutations in ARL2BP cause autosomal-recessive retinitis pigmentosa."}, {"pubmed": 24899173, "text": "EBV also downregulates two immediate early genes by miR-BART20-5p."}, {"pubmed": 26135619, "text": "Alteration of EBV encoded miR-BART1 expression results in an increase in migration and invasion of nasopharyngeal carcinoma in vitro and causes metastasis in vivo. EBV-miR-BART1 directly targets the cellular tumour suppressor PTEN."}, {"pubmed": 27790702, "text": "Subsequent analysis of 844 index cases did not reveal further pathogenic chances in ARL2BP indicating that mutations in ARL2B are a rare cause of arRCD (about 0.1%) in a large cohort of French patients."}, {"pubmed": 30210231, "text": "This study identified two homozygous variants in ARL2BP as a rare cause of autosomal recessive retinitis pigmentosa. Further studies are required to define the underlying disease mechanism causing retinal degeneration as a result of mutations in ARL2BP and any phenotype-genotype correlation associated with residual levels of the wild-type transcript."}, {"pubmed": 31425546, "text": "Study identified multiple ciliopathy phenotypes associated with mutations in ARL2BP in human patients and in a mouse knockout model. Spermiogenesis is impaired, resulting in abnormally shaped heads, shortened and mis-assembled sperm tails, and loss of axonemal doublets. ARL2BP is required for the structural maintenance of cilia as well as of the sperm flagellum, and that its deficiency leads to syndromic ciliopathy."}, {"pubmed": 33438581, "text": "ARL3 activation requires the co-GEF BART and effector-mediated turnover."}, {"pubmed": 34681596, "text": "Epstein-Barr Virus miR-BART1-3p Regulates the miR-17-92 Cluster by Targeting E2F3."}], "genomic_pos": {"chr": "16", "end": 57253635, "ensemblgene": "ENSG00000102931", "start": 57245259, "strand": 1}, "genomic_pos_hg19": {"chr": "16", "end": 57287516, "start": 57279010, "strand": 1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0007165", "pubmed": 10488091, "qualifier": "involved_in", "term": "signal transduction"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0042531", "qualifier": "involved_in", "term": "positive regulation of tyrosine phosphorylation of STAT protein"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0042531", "pubmed": 18234692, "qualifier": "involved_in", "term": "positive regulation of tyrosine phosphorylation of STAT protein"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0045893", "qualifier": "involved_in", "term": "positive regulation of DNA-templated transcription"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0051457", "qualifier": "involved_in", "term