n": [[69865763, 69865940], [69866296, 69866558], [69902696, 69902872], [69905231, 69905283], [69908109, 69908224], [69909796, 69909868], [69918242, 69918384], [69921485, 69921509], [69925458, 69925575], [69926050, 69926423], [69933822, 69934413], [69935079, 69935218], [69948661, 69948883], [69954119, 69954269], [69955206, 69955289], [69957108, 69957235], [69959124, 69959332], [69961585, 69961751], [69966526, 69966660], [69970042, 69971773]], "strand": 1, "transcript": "NR_045662", "txend": 69971773, "txstart": 69865763}, {"cdsend": 69971773, "cdsstart": 69971773, "chr": "10", "position": [[69869244, 69869480], [69881194, 69882097], [69902696, 69902872], [69905231, 69905283], [69908109, 69908224], [69909796, 69909868], [69918242, 69918384], [69925458, 69925575], [69926050, 69926423], [69933822, 69934413], [69948661, 69948883], [69954119, 69954269], [69955206, 69955289], [69957108, 69957235], [69959124, 69959332], [69961585, 69961751], [69966526, 69966660], [69970042, 69971773]], "strand": 1, "transcript": "NR_045663", "txend": 69971773, "txstart": 69869244}], "generif": [{"pubmed": 12482578, "text": "myopalladin plays a signaling role in targeting and orienting nebulin during sarcomere assembly"}, {"pubmed": 16385451, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 18006477, "text": "myopalladin gene is a new gene associated with dilated cardiomyopathy and observed mutations in 3-4% of cases in a population. of European descent."}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20801532, "text": "mutations in PDLIM3 and MYPN are infrequent in hypertrophic cardiomyopathies"}, {"pubmed": 22286171, "text": "Two nonsense and 13 missense MYPN variants were identified in subjects with hypertrophic, dilated and/or restrictive cardiomyopathy."}, {"pubmed": 22892539, "text": "the clinical significance of myopalladin for the functional integrity of the sarcomeric apparatus and the protection against dilated cardiomyopathy"}, {"pubmed": 25541130, "text": "Heterozygote Mypn(WT/Q526X) knock-in mice develop RCM due to persistence of mutant Mypn(Q526X) protein in the nucleus."}, {"pubmed": 28017374, "text": "results suggest that MYPN screening should be considered in individuals with mild nemaline myopathy, especially when cardiac problems or intranuclear rods are present"}, {"pubmed": 28220527, "text": "Homozygous truncating mutations in MYPN in 2 unrelated families with a slowly progressive congenital cap myopathy."}, {"pubmed": 28427417, "text": "Targeted sequencing revealed trigenic mutations: c.700G>A/p.E234K in DES, c.2966G>A/p.R989H in MYPN, and c.5918G>C/p.R1973P in CACNA1C in a family of hypertrophic cardiomyopathy with early repolarization and short QT syndrome."}, {"pubmed": 31133047, "text": "A consanguineous family with congenital to adult-onset muscle weakness and hanging big toe was reported. Muscle biopsy showed minimal changes with internal nuclei, type 1 fiber predominance, and ultrastructural defects of Z line. Cardiac involvement was demonstrated by magnetic resonance imaging and late gadolinium enhancement."}], "genomic_pos": {"chr": "10", "end": 68212017, "ensemblgene": "ENSG00000138347", "start": 68087897, "strand": 1}, "genomic_pos_hg19": {"chr": "10", "end": 69971774, "start": 69865912, "strand": 1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0007156", "qualifier": "involved_in", "term": "homophilic cell adhesion via plasma membrane adhesion molecules"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007411", "qualifier": "involved_in", "term": "axon guidance"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0045214", "pubmed": 11309420, "qualifier": "involved_in", "term": "sarcomere organization"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0070593", "qualifier": "involved_in", "term": "dendrite self-avoidance"}], "CC": [{"evidence": "ISS", "gocategory"