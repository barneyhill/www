osition": [[109052575, 109052761], [109056681, 109056791], [109057925, 109058108], [109067523, 109067627], [109071611, 109071710], [109072304, 109072350], [109073437, 109073532], [109081333, 109081393], [109081932, 109082019], [109082662, 109082743], [109082842, 109083062], [109084952, 109085073], [109085666, 109088018]], "strand": 1, "transcript": "NM_032663", "txend": 109088018, "txstart": 109052575}], "exons_hg19": [{"cdsend": 109523736, "cdsstart": 109494496, "chr": "12", "position": [[109460893, 109460972], [109462607, 109462877], [109465269, 109465361], [109485394, 109485519], [109494486, 109494596], [109495730, 109495913], [109505328, 109505432], [109509416, 109509515], [109510109, 109510155], [109511242, 109511337], [109519138, 109519198], [109519737, 109519824], [109520467, 109520548], [109520647, 109520867], [109522757, 109522878], [109523471, 109525831]], "strand": 1, "transcript": "NM_001301175", "txend": 109525831, "txstart": 109460893}, {"cdsend": 109523736, "cdsstart": 109490483, "chr": "12", "position": [[109490380, 109490566], [109494486, 109494596], [109495730, 109495913], [109505328, 109505432], [109509416, 109509515], [109510109, 109510155], [109511242, 109511337], [109519138, 109519198], [109519737, 109519824], [109520467, 109520548], [109520647, 109520867], [109522757, 109522878], [109523471, 109525823]], "strand": 1, "transcript": "NM_032663", "txend": 109525823, "txstart": 109490380}], "generif": [{"pubmed": 18287522, "text": "USP30 participates in the maintenance of mitochondrial morphology, a finding that provides new insight into the cellular function of deubiquitination."}, {"pubmed": 24896179, "text": "reducing USP30 activity enhances mitochondrial degradation in neurons"}, {"pubmed": 25621951, "text": "The mechanism by which parkin, an E3 ubiquitin ligase, and USP30, a mitochondrion-localized deubiquitylase, regulate mitophagy, was investigated."}, {"pubmed": 25739811, "text": "These results provide the first evidence for a fundamental role of USP30 in determining the threshold for mitochondrial cell death and suggest USP30 as a potential target for combinatorial anti-cancer therapy."}, {"pubmed": 25915564, "text": "show that USP30 delays mitophagy by delaying PARK2 recruitment to the mitochondria during mitophagy"}, {"pubmed": 28945249, "text": "Here the authors report crystal structures of human USP30 bound to monoubiquitin and Lys6-linked diubiquitin, which explain how USP30 achieves Lys6-linkage preference through unique ubiquitin binding interfaces."}, {"pubmed": 29178074, "text": "the role of USP30 in the dynamic networks of mitochondrial quality control and its physiological implications."}, {"pubmed": 29895712, "text": "we use two distinct mitophagy reporter systems to reveal tonic suppression by USP30, of a PINK1-dependent component of basal mitophagy in cells lacking detectable Parkin."}, {"pubmed": 30143522, "text": "GNPAT and USP30-mediated stabilization of DRP1 play a critical role in the development of hepatocellular carcinoma"}, {"pubmed": 30700497, "text": "USP30 can rescue the peroxisome loss observed in some disease-causing peroxisome mutations, pointing to a potential therapeutic target."}, {"pubmed": 32142685, "text": "Define the primary specificity of endogenous Parkin action on mitochondria, the abundance of ubiquitin proteoforms, the role of p97 in remodeling the mitochondrial proteome downstream of Parkin, and the target specificity of USP30 during mitophagic signaling."}, {"pubmed": 32221968, "text": "The IKKbeta-USP30-ACLY Axis Controls Lipogenesis and Tumorigenesis."}, {"pubmed": 32484330, "text": "Probing the Active Site of Deubiquitinase USP30 with Noncanonical Tryptophan Analogues."}, {"pubmed": 32636217, "text": "USP30 sets a trigger threshold for PINK1-PARKIN amplification of mitochondrial ubiquitylation."}, {"pubmed": 34694569, "text": "LncRNA USP30-AS1 promotes the survival of acute myeloid leukemia cells by cis-regulating USP30 and ANKRD13A."}, {"pubmed": 38040789, "text": "USP30 inhibition protects dopaminergic neurons."}], "genomic_pos": {"chr": "12", "end": 109088023, "ensemblgene": "ENSG00000135093", "start": 109023089, "strand": 1}, "genomic_pos_hg19": {"chr": "12", "end": 109525831, "start": 109460894, "strand": 1}, "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0000422", "pubmed": 25621951, "qualifier": "involved_in", "term": "autophagy of mitochondrion"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0000422", "pubmed": 24896179, "qualifier": "involved_in", "term": "autophagy of mitochondrion"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0000425", "qualifier": "involved_in", "term": "pexophagy"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0006508", "qualifier": "involved_in", "term": "proteolysis"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0008053", "qualifier": "involved_in", "term": "mitochondrial fusion"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0016579", "qualifier": "involved_in", "term": "protein deubiquitination"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0016579", "pubmed": 24896179, "qualifier": "involved_in", "term": "protein deubiquitination"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0035871", "pubmed": 25621951, "qualifier": "involved_in", "term": "protein K11-linked deubiquitination"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0044313", "pubmed": 25621951, "qualifier": "involved_in", "term": "protein K6-linked deubiquitination"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:1901525", "qualifier": "involved_in", "term": "negative regulation of mitophagy"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "is_active_in", "term": "nucleus"}, {"evidence": "ISS", "gocategory": "CC", "id": "GO:0005739", "qualifier": "located_in", "term": "mitochondrion"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005741", "pubmed": 24896179, "qualifier": "located_in", "term": "mitochondrial outer memb