 "exons_hg19": [{"cdsend": 35343810, "cdsstart": 35222713, "chr": "14", "position": [[35221936, 35222910], [35224032, 35224120], [35227909, 35228052], [35230962, 35231429], [35233912, 35234139], [35234226, 35234474], [35240716, 35240865], [35242777, 35242932], [35243532, 35243696], [35245124, 35245721], [35249391, 35249516], [35252324, 35252438], [35252967, 35253133], [35254982, 35255207], [35255331, 35255427], [35261980, 35262127], [35263954, 35264093], [35264875, 35264971], [35269429, 35269596], [35270299, 35270399], [35272059, 35272194], [35276656, 35276744], [35280140, 35280242], [35295218, 35295362], [35331249, 35331528], [35343697, 35343868], [35344343, 35344566]], "strand": -1, "transcript": "NM_013448", "txend": 35344566, "txstart": 35221936}, {"cdsend": 35343810, "cdsstart": 35222713, "chr": "14", "position": [[35221936, 35222910], [35224032, 35224120], [35227909, 35228052], [35230962, 35231429], [35233912, 35234139], [35234226, 35234474], [35240716, 35240865], [35242777, 35242932], [35243532, 35243696], [35245124, 35245721], [35249391, 35249516], [35252324, 35252438], [35252967, 35253133], [35254982, 35255207], [35261980, 35262127], [35263954, 35264093], [35264875, 35264971], [35269429, 35269596], [35270299, 35270399], [35272059, 35272194], [35276656, 35276744], [35280140, 35280242], [35295218, 35295362], [35331249, 35331528], [35343697, 35343868], [35344343, 35344566]], "strand": -1, "transcript": "NM_182648", "txend": 35344566, "txstart": 35221936}], "generif": [{"pubmed": 16877760, "text": "ACF1 alters the remodeling strategy of SNF2h"}, {"pubmed": 17519354, "text": "Has a critical role in the transcriptional repression of specific nuclear hormone receptor-regulated genes."}, {"pubmed": 19450608, "text": "The data support a model in which the human ACF anf yeast RSC remodeling enzymes move nucleosomes to new locations by a general sequence-independent mechanism."}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 21172662, "text": "The ACF1 and its complexes play important roles in DNA double-strand breaks (DSBs) repair."}, {"pubmed": 21738833, "text": "ISWI proteins Snf2H, Snf2L as well as Acf1 accumulate at UV-induced DNA damage sites within tens of seconds and reach a plateau after a few minutes."}, {"pubmed": 21745822, "text": "hACF1-containing factors are more generally involved in the DNA damage response."}, {"pubmed": 25533843, "text": "CHD3.1 and ACF1-SNF2H display counteractive activities but similar histone affinities."}, {"pubmed": 26288249, "text": "Results from the study on gene variability in early stage embryos identifies BAZ1A as a putative variability marker of the 8-cell embryo stage."}, {"pubmed": 27328812, "text": "Study reports de novo mutation in BAZ1A, in a patient with a syndromic form of intellectual disability (ID). This mutation affects the expression of genes involved in several biological pathways such as vitamin D regulation, Wnt signaling, and postsynaptic signaling. Data point to an important role for BAZ1A in neurodevelopment, and highlight a possible link for BAZ1A to ID."}, {"pubmed": 28412501, "text": "Results from the study suggest that acute cocaine exposure increased Baz1a expression in nucleus accumbens while repeated cocaine exposure produced the opposite effect. Findings implicate BAZ1A in molecular and behavioral plasticity to cocaine and offer new insight into the pathophysiology of cocaine addiction."}, {"pubmed": 29021563, "text": "promotes recovery after DNA damage, in part by recruiting SMARCA5 to damaged chromatin"}, {"pubmed": 31085244, "text": "results revealed chromatin remodeling modulator BAZ1A acting as a novel regulator of cellular senescence in both normal and cancer cells, indicating a new target for potential cancer treatment"}, {"pubmed": 37082578, "text": "African American Prostate Cancer Displays Quantitatively Distinct Vitamin D Receptor Cistrome-transcriptome Relationships Regulated by BAZ1A."}], "genomic_pos": {"chr": "14", "end": 34875647, "ensemblgene": "ENSG00000198604", "start": 34752731, "strand": -1}, "genomic_pos_hg19": {"chr": "14", "end": 35344853, "start": 35221937, "strand": -1}, "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0006261", "pubmed": 12434153, "qualifier": "involved_in", "term": "DNA-templated DNA replication"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0006275", "pubmed": 12434153, "qualifier": "involved_in", "term": "regulation of DNA replication"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0006334", "pubmed": 14759371, "qualifier": "involved_in", "term": "nucleosome assembly"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0006338", "pubmed": 10880450, "qualifier": "involved_in", "term": "chromatin remodeling"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0006338", "pubmed": [10662543, 27328812], "qualifier": "involved_in", "term": "chromatin remodeling"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006355", "qualifier": "involved_in", "term": "regulation of DNA-templated transcription"}, {"evidence": "NAS", "gocategory": "BP", "id": "GO:0006357", "pubmed": 10662543, "qualifier": "involved_in", "term": "regulation of transcription by RNA polymerase II"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0031445", "qualifier": "involved_in", "term": "regulation of heterochromatin formation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0045740", "qualifier": "involved_in", "term": "positive regulation of DNA replication"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0045740", "pubmed": 12434153, "qualifier": "involved_in", "term": "positive regulation of DNA replication"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0000228", "qualifier": "is_active_in", "term": "nuclear chromosome"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005634", "pubmed": [10655480, 25593309], "qualifier": "located_in", "term": "nucleus"}, {"e