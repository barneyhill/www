xons": [{"cdsend": 16146751, "cdsstart": 16129322, "chr": "6", "position": [[16129085, 16129409], [16130556, 16130747], [16141624, 16141810], [16143019, 16143217], [16143698, 16143863], [16144896, 16145317], [16146661, 16148247]], "strand": 1, "transcript": "NM_013262", "txend": 16148247, "txstart": 16129085}], "exons_hg19": [{"cdsend": 16146982, "cdsstart": 16129553, "chr": "6", "position": [[16129316, 16129640], [16130787, 16130978], [16141855, 16142041], [16143250, 16143448], [16143929, 16144094], [16145127, 16145548], [16146892, 16148478]], "strand": 1, "transcript": "NM_013262", "txend": 16148478, "txstart": 16129316}], "generif": [{"pubmed": 12826659, "text": "MSAP interacts with this protein that enhances neurite outgrowth and increases myosin regulatory light chain in fetal and adult brain."}, {"pubmed": 16785530, "text": "c-MIR is the first example of an E3 ubiquitin ligase that is capable of inhibiting major histocompatibility (MHC) class II expression in antigen-presenting cells; c-MIR might potently regulate immune responses in vivo."}, {"pubmed": 19520913, "text": "study shows the LXR-Idol(Mylip)-LDLR axis defines a complementary pathway to sterol response element-binding proteins for sterol regulation of cholesterol uptake"}, {"pubmed": 19688294, "text": "Novel insights into the physiology of this receptor come from studies on the ubiquitination and degradation of LDL receptor by the ubiquitin ligase Mylip/Idol that is induced in cells by the nuclear receptor, LXR."}, {"pubmed": 19936222, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20347265, "text": "Observational study of gene-disease association and gene-gene interaction. (HuGE Navigator)"}, {"pubmed": 20864672, "text": "Observational study, meta-analysis, and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 21069265, "text": "Data report that statins exert opposite effects on PCSK9 and Idol gene expression in human hepatoma-derived cell lines and primary hepatocytes isolated from hamsters and rats."}, {"pubmed": 21685362, "text": "identify the IDOL-UBE2D complex as an important determinant of LDLR activity, and provide insight into molecular mechanisms underlying the regulation of cholesterol uptake"}, {"pubmed": 21734303, "text": "both the FERM and RING domains are required for promoting lysosomal degradation of the LDLR by IDOL."}, {"pubmed": 21765216, "text": "N342S MYLIP polymorphism is associated with high total cholesterol and increased LDL receptor degradation in humans"}, {"pubmed": 22261164, "text": "expression levels rise with increasing age in hearts of men"}, {"pubmed": 22378787, "text": "FGF21 also enhanced expression of Canopy2 (Cnpy2)/MIR-interacting Saposin-like protein (Msap) that is known to interact with Mylip/Idol."}, {"pubmed": 22741812, "text": "No association of the MYLIP rs9370867 genotypes with lipid profile, hemodynamic data, and coronary angiographic data was in a Brazilian population."}, {"pubmed": 23107276, "text": "MYLIP rs3757354 SNP is associated with serum TC, HDL-C and ApoAI levels in the Bai Ku Yao and Han populations. But the association is different between the two ethnic groups."}, {"pubmed": 23109900, "text": "Several lipid-related gene polymorphisms interact with overweight/obesity to modulate blood pressure levels."}, {"pubmed": 23324548, "text": "Results show that IDOL contributes to variation in circulating levels of LDL-C."}, {"pubmed": 23382078, "text": "IDOL is recruited to plasma membrane by low-density lipoprotein receptor (LDLR), promotes LDLR internalization in the absence of clathrin or caveolae, and facilitates LDLR degradation by shuttling it into the multivesicular body protein-sorting pathway"}, {"pubmed": 23733886, "text": "evidence for the existence of an LXR-IDOL-mediated internalization pathway for the LDLR that is distinct from that used for lipoprotein uptake"}, {"pubmed": 24935961, "text": "Liver-specific expression of dominant-active IDOL is associated with hypercholesterolemia and a marked elevation in atherosclerotic lesions in transgenic mice."}, {"pubmed": 25171759, "text": "study indicates that MYLIP p.N342S might be a pharmacogenetic marker for lipid-lowering therapy in patients with FH."}, {"pubmed": 25927920, "text": "IDOL N342S Variant, Atherosclerosis Progression and Cardiovascular Disorders in the Italian General Population."}, {"pubmed": 26527619, "text": "The study identified MARCH6 as a negative regulator of SREBP2-mediated transcription and described an unexpected E3 circuit functionally linking MARCH6 and IDOL to limit uptake of low-density lipoprotein via the LDLR pathway."}, {"pubmed": 26601593, "text": "Specifically, loss of IDOL increases LDLR distribution in the hepatic cell, and subsequently reduces serum LDL-C levels in dyslipidemic patients"}, {"pubmed": 26666640, "text": "Identify USP2 as a novel regulator of lipoprotein clearance owing to its ability to control ubiquitylation-dependent degradation of the LDLR by IDOL."}, {"pubmed": 26719329, "text": "Data suggest inducible expression of IDOL is subject to robust, rapid regulation by process that is sensitive to deubiquitinase inhibition in human/mouse cell lines and primary human cells; transcriptional induction of IDOL leads to degradation of LDLR."}, {"pubmed": 26786161, "text": "the effects caused by human inducible degrader of the low-density lipoprotein expression are LDLR- dependent given the unchanged plasma lipids in LAhB mice lacking low-density lipoprotein receptor"}, {"pubmed": 27071970, "text": "RP1-13D10.2 is a long noncoding RNA that regulates LDLR and may contribute to low-density lipoprotein cholesterol response to statin treatment."}, {"pubmed": 27071970, "text": "The long noncoding RNA RP1-13D10.2 may contribute to LDL cholesterol levels in response to statins."}, {"pubmed": 31597442, "text": "IDOL(G51S) is a gain-of-function variant responsible for high LDL-C in both humans and mice. These results suggest that IDOL is a key player regulating cholesterol level in humans."}, {"pubmed": 32727844, "text": "Structural analysis of the LDL receptor-interacting FERM domain in the E3 ubiquitin ligase IDOL reveals an obscured substrate-binding site."}, {"pubmed": 32868861, "text": "IDOL gene variant is associated with hyperlipidemia in Han population in Xinjiang, China."}, {"pubmed": 33154164, "text": "SUMOylation of the ubiquitin ligase IDOL decreases LDL receptor levels and is reversed by SENP1."}, {"pubmed": 33185588, "text": "Identification of MYLIP gene and miRNA-802 involved in the growth and metastasis of cervical cancer cells."}, {"pubmed": 33450693, "text": "Novel associations of SNPs MYLIP rs3757354 and ABCA1 2230806 gene with early-onset-preeclampsia: A case-control candidate genetic study."}, {"pubmed": 33845890, "text": "Genetic polymorphism of IDOL gene was associated with the susceptibility of coronary artery disease in Han population in Xinjiang, China."}, {"pubmed": 37047406, "text": "Mouse Spinal Cord Vascular Transcriptome Analysis Identifies CD9 and MYLIP as Injury-Induced Players."}, {"pubmed": 37094639, "text": "Effect of type 2 diabetes on the inducible degrader of LDL receptor."}], "genomic_pos": {"chr": "6", "end": 16148248, "ensemblgene": "ENSG00000007944", "start": 16129086, "strand": 1}, "genomic_pos_hg19": {"chr": "6", "end": 16148479, "start": 16129356, "strand": 1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0006511", "qualifier": "involved_in", "term": "ubiquitin-dependent protein catabolic process"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0007399", "pubmed": 10593918, "qualifier": "involved_in", "term": "nervous system development"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0010977", "pubmed": 10593918, "qualifier": "involved_in", "term": "negative regulation of neuron projection development"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0