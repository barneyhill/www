rt": 51707010, "n_exons": 8, "nonpsych": {"exp_lof": 11.2574768839, "exp_mis": 86.2225029377, "exp_syn": 39.9237782994, "lof_z": 2.12430623456279, "mis_z": -0.42246563786595, "mu_lof": 8.66873631839e-07, "mu_mis": 8.60183049609e-06, "mu_syn": 3.83671382741e-06, "n_lof": 4.0, "n_mis": 94.0, "n_syn": 46.0, "p_li": 0.0602037342160933, "p_null": 0.0202078392473402, "p_rec": 0.919588426536567, "syn_z": -0.605454012389364}, "nontcga": {"exp_lof": 11.5871726623, "exp_mis": 87.7865914156, "exp_syn": 40.7255004558, "lof_z": 1.90893685324396, "mis_z": -0.277215505735705, "mu_lof": 8.66873631839e-07, "mu_mis": 8.60183049609e-06, "mu_syn": 3.83671382741e-06, "n_lof": 5.0, "n_mis": 93.0, "n_syn": 45.0, "p_li": 0.0123274543198117, "p_null": 0.0355073688688458, "p_rec": 0.952165176811342, "syn_z": -0.424105976072582}, "transcript": "ENST00000457354.2"}, "exons": [{"cdsend": 51254519, "cdsstart": 51240292, "chr": "14", "position": [[51240246, 51240444], [51243855, 51243971], [51245312, 51245358], [51247091, 51247220], [51249325, 51249371], [51249467, 51249569], [51249692, 51249765], [51254340, 51257654]], "strand": 1, "transcript": "NM_030755", "txend": 51257654, "txstart": 51240246}], "exons_hg19": [{"cdsend": 51721237, "cdsstart": 51707010, "chr": "14", "position": [[51706964, 51707162], [51710573, 51710689], [51712030, 51712076], [51713809, 51713938], [51716043, 51716089], [51716185, 51716287], [51716410, 51716483], [51721058, 51724372]], "strand": 1, "transcript": "NM_030755", "txend": 51724372, "txstart": 51706964}], "generif": [{"pubmed": 19741092, "text": "These results suggest a specific role for transmembrane thioredoxin-related protein TMX and its mechanism of action in redox-based ER quality control."}, {"pubmed": 22045338, "text": "TMX1 is enriched on the mitochondria-associated membrane. Targeting TMX to the MAM requires palmitoylation of two membrane-proximal cytosolic cysteines."}, {"pubmed": 22228764, "text": "the silencing of TMX in the prostatic cell line DU145 reduced the sensitivity of the cells to ricin intoxication further confirming a role for this enzyme in intracellular ricin activation."}, {"pubmed": 22924822, "text": "We conclude that TMX plays a major role in host defense under the type of inflammatory conditions associated with oxidative stress."}, {"pubmed": 26246604, "text": "Here we show that TMX1, one of the few transmembrane members of the family, forms functional complexes with the ER lectin calnexin and preferentially intervenes during maturation of cysteine-containing, membrane-associated proteins"}, {"pubmed": 27502484, "text": "TMX1 increases mitochondrial ATP production and apoptosis progression."}, {"pubmed": 29932915, "text": "TMX1 selectively intervenes in clearance of membrane-tethered Endoplasmic Reticulum-Associated Degradation substrates. TMX1 preferentially acts on membrane-tethered folding-defective polypeptides essentially ignoring the same misfolded ectodomains, when not associated to the Endoplasmic Reticulum membrane."}, {"pubmed": 31304984, "text": "Data show that TMX1 expression is upregulated in melanoma cell lines and patient samples. TMX knockdown altered mitochondrial organization, enhanced bioenergetics, and elevated mitochondrial- and NOX4-derived ROS. The TMX-knockdown-induced oxidative stress suppressed melanoma proliferation, migration, and xenograft tumor growth by inhibiting NFAT1. TMX1 is associated with poor disease outcome."}, {"pubmed": 33940944, "text": "Thiol Isomerases Orchestrate Thrombosis and Hemostasis."}, {"pubmed": 38147024, "text": "TMX family genes and their association with prognosis, immune infiltration, and chemotherapy in human pan-cancer."}], "genomic_pos": {"chr": "14", "end": 51257655, "ensemblgene": "ENSG00000139921", "start": 51240162, "strand": 1}, "genomic_pos_hg19": {"chr": "14", "end": 51722759, "start": 51706880, "strand": 1}, "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0010868", "pubmed": 37648867, "qualifier": "involved_in", "t