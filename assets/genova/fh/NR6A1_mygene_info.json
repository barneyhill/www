enerif": [{"pubmed": 11969338, "text": "RTR mRNA was expressed in the human placental choriocarcinoma cell lines BeWo, JAR, and JEG-3."}, {"pubmed": 12080054, "text": "characterization of a novel nuclear protein, referred to as RAP80, that interacts with the retinoid-related testis-associated receptor"}, {"pubmed": 15456763, "text": "down-regulation of CREMtau-mediated gene expression by GCNF"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20583170, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 21056546, "text": "These experiments confirmed that NR6A1 works as a regulator for hypocretin transcription."}, {"pubmed": 23495137, "text": "Findings show a role for Gcnf as a regulator of iPS cell pluripotency gene expression. It also demonstrates that reactivation of the Gcnf gene may serve as a marker to distinguish completely reprogrammed iPS cells from incompletely pluripotent cells."}, {"pubmed": 23532770, "text": "Our results show that cellular levels of NR6A1 are correlated with disease progression in prostate cancer."}, {"pubmed": 26769970, "text": "the genes, and up-regulated 64% in undifferentiated hES cells. In addition, GCNF also showed a regulatory gene pattern that is different from RA treatment during hES cell differentiation"}, {"pubmed": 29057499, "text": "GCNF acts as a transcriptional repressor in the regulation of OCT4 gene expression through cooperative interaction with three NR binding elements in pluripotent NCCIT cells."}, {"pubmed": 31315616, "text": "the role of NR6A1 in the lipogenesis in HepG2 cells"}, {"pubmed": 33006200, "text": "hsa_circ_001653 up-regulates NR6A1 expression and elicits gastric cancer progression by binding to microRNA-377."}, {"pubmed": 34689384, "text": "Novel LAMC2 fusion protein has tumor-promoting properties in ovarian carcinoma."}, {"pubmed": 37903971, "text": "Integrated analysis of histone lysine lactylation (Kla)-specific genes suggests that NR6A1, OSBP2 and UNC119B are novel therapeutic targets for hepatocellular carcinoma."}], "genomic_pos": {"chr": "9", "end": 124771311, "ensemblgene": "ENSG00000148200", "start": 124517275, "strand": -1}, "genomic_pos_hg19": {"chr": "9", "end": 127533589, "start": 127279554, "strand": -1}, "go": {"BP": [{"evidence": "ISS", "gocategory": "BP", "id": "GO:0000122", "qualifier": "involved_in", "term": "negative regulation of transcription by RNA polymerase II"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006357", "qualifier": "involved_in", "term": "regulation of transcription by RNA polymerase II"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0007276", "qualifier": "involved_in", "term": "gamete generation"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0030522", "qualifier": "involved_in", "term": "intracellular receptor signaling pathway"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0045944", "qualifier": "involved_in", "term": "positive regulation of transcription by RNA polymerase II"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0000785", "qualifier": "part_of", "term": "chromatin"}, {"evidence": "ISA", "gocategory": "CC", "id": "GO:0000785", "qualifier": "part_of", "term": "chromatin"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "is_active_in", "term": "nucleus"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "ISS", "gocategory": "CC", "id": "GO:0005667", "qualifier": "part_of", "term": "transcription regulator complex"}], "MF": [{"category": "MF", "evidence": "ISA", "id": "GO:0000981", "qualifier": "enables", "term": "DNA-binding transcription factor activity, RNA polymerase II-specific"}, {"category": "MF", "evidence": "IEA", "id": "GO:0001228", "qualifier": "enables", "term": "DNA-binding transcription activator activity, RNA polymerase II-specific"}, {"categ