position": [[54396867, 54397077], [54399893, 54399976], [54401523, 54401579], [54408744, 54408789], [54411483, 54411706], [54415898, 54415930], [54417847, 54417951], [54418213, 54418306], [54419991, 54420216]], "strand": 1, "transcript": "NM_001330173", "txend": 54420216, "txstart": 54396867}, {"cdsend": 54420078, "cdsstart": 54397068, "chr": "14", "position": [[54397006, 54397077], [54399893, 54399976], [54401523, 54401579], [54408744, 54408789], [54411483, 54411706], [54415898, 54415930], [54417847, 54417951], [54419991, 54420216]], "strand": 1, "transcript": "NM_005192", "txend": 54420216, "txstart": 54397006}], "exons_hg19": [{"cdsend": 54886796, "cdsstart": 54863786, "chr": "14", "position": [[54863672, 54863795], [54866611, 54866630], [54875462, 54875507], [54878201, 54878424], [54882616, 54882648], [54884565, 54884669], [54886709, 54886934]], "strand": 1, "transcript": "NM_001130851", "txend": 54886934, "txstart": 54863672}, {"cdsend": 54884991, "cdsstart": 54863786, "chr": "14", "position": [[54863585, 54863795], [54866611, 54866694], [54868241, 54868297], [54875462, 54875507], [54878201, 54878424], [54882616, 54882648], [54884565, 54884669], [54884931, 54885024], [54886709, 54886934]], "strand": 1, "transcript": "NM_001330173", "txend": 54886934, "txstart": 54863585}, {"cdsend": 54886796, "cdsstart": 54863786, "chr": "14", "position": [[54863724, 54863795], [54866611, 54866694], [54868241, 54868297], [54875462, 54875507], [54878201, 54878424], [54882616, 54882648], [54884565, 54884669], [54886709, 54886934]], "strand": 1, "transcript": "NM_005192", "txend": 54886934, "txstart": 54863724}], "generif": [{"pubmed": 12745075, "text": "Identification of a KAP domain that interacts with cyclin-dependent kinase 2."}, {"pubmed": 15147518, "text": "In choline-deficient neuroblastoma cells, the promoter of the cyclin-dependent kinase inhibitor 3 gene (CDKN3) was hypomethylated. This was associated with increased expression of CDKN3 gene product kinase-associated phosphatase (KAP)"}, {"pubmed": 15671017, "text": "HTm4 binding to KAP.Cdk2.cyclin A complex enhances the phosphatase activity of KAP, dissociates cyclin A, and facilitates KAP dephosphorylation of Cdk2"}, {"pubmed": 16945326, "text": "Pax3 represses transcription through a novel mechanism involving competition between corepressor KAP1 and the heterochromatin-binding protein HP1gamma"}, {"pubmed": 17056014, "text": "the reduction of KAP1 levels promotes p53-dependent p21 induction and inhibits cell proliferation in actinomycin D-treated cells."}, {"pubmed": 19728335, "text": "IKKalpha, IKKbeta, RANK, Maspin, c-FLIP, Cip2 and cyclinD1 were found to show significant differences between hepatocellular tumor tissue and its corresponding adjacent tissue."}, {"pubmed": 20677014, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 21396835, "text": "cyclin-dependent kinase-associated protein phosphatase expression was associated with poorly differentiated Renal cell carcinoma"}, {"pubmed": 21535270, "text": "analysis of CDKN3 cell cycle networks and the molecular mechanism of non-malignancy associated hepatitis/cirrhosis and HCC transformation"}, {"pubmed": 22390936, "text": "These results suggest that CDKN3 acts as an oncogene in human hepatocellular carcinoma and antagonism of CDKN3 may be of interest for the treatment of HCC."}, {"pubmed": 23292002, "text": "KAP is overexpressed in alcohol-related hepatocellular carcinoma."}, {"pubmed": 23405241, "text": "High CDKN3 expression is associated with cervical cancer."}, {"pubmed": 23555623, "text": "RelB activation is key for promoting multiple myeloma cell survival through the upregulation of anti-apoptotic proteins, in particular, CIAP2."}, {"pubmed": 23775190, "text": "CDKN3 controls mitosis through the CDC2 signaling axis."}, {"pubmed": 24573179, "text": "CDKN3 may play an important role in the development and proliferation of epithelial ovarian cancer"}, {"pubmed": 24704824, "text": "these data identify KAP as a major regulator of cell division via interactions with numerous target proteins. aberrant splicing or miR-26a decreases KAP expression to promote glioblastoma growth and invasion."}, {"pubmed": 25360622, "text": "These results highlight the importance of CDKN3 in Bcr-Abl-mediated leukemogenesis."}, {"pubmed": 25735390, "text": "High CDKN3 expression is associated with colorectal cancer."}, {"pubmed": 26191143, "text": "CDKN3 knockdown reduces cell proliferation, invasion and promotes apoptosis in human ovarian cancer cells."}, {"pubmed": 26372210, "text": "CDKN3 mRNA may be a good survival biomarker and potential therapeutic target in cervical cancer"}, {"pubmed": 26554648, "text": "CDKN3 overexpression in lung adenocarcinoma is not attributed to alternative splicing or mutation but is likely due to increased mitotic activity, arguing against CDKN3 as a tumour suppressor"}, {"pubmed": 26586430, "text": "Knockdown of Cdkn3 stabilizes Mps1 at centrosomes."}, {"pubmed": 27314282, "text": "CDKN3 may act as a tumor suppressor in liver tissues by modulation of the cell survival signal transduction, monitoring carcinogenesis and elimination of abnormal cells."}, {"pubmed": 27983933, "text": "CDKN3 is frequently upregulated in gastric cancer tissues and cell lines and is related to advanced clinical stage, recurrence, and poor clinical outcome in gastric cancer. The functional data strongly suggest that CDKN3 behaves as an oncogene in gastric cancer, and downregulation of CDKN3 could inhibit gastric cancer cell proliferation, migration, and invasion, and induce cell cycle arrest and apoptosis."}, {"pubmed": 28109073, "text": "Results demonstrate that upregulation of p27 is involved in the knockdown of CDKN3-induced decrease in cell proliferation, increase in cell cycle arrest and apoptosis, decrease in invasion, and increase in radiosensitivity."}, {"pubmed": 28504190, "text": "the mechanism of CDKN3 overexpression in human cancer"}, {"pubmed": 30628487, "text": "The aberrant expression of miR-181d-5p and cyclin-dependent kinase inhibitor 3 (CDKN3) in non-small-cell lung cancer (NSCLC) was screened by microarray analysis. There was a high expression of CDKN3, whereas miR-181d-5p was downregulated in NSCLC."}, {"pubmed": 31352814, "text": "NEAT1 may exert a protective effect on HUVECs by regulating the miR-181d-5p/CDKN3A axis"}, {"pubmed": 32329836, "text": "CDKN3 regulates cisplatin resistance to colorectal cancer through TIPE1."}, {"pubmed": 32721244, "text": "CDKN3 expression predicates poor prognosis and regulates adriamycin sensitivity in hepatocellular carcinoma in vitro."}, {"pubmed": 34044866, "text": "Integrated transcriptomics explored the cancer-promoting genes CDKN3 in esophageal squamous cell cancer."}, {"pubmed": 35944363, "text": "Identification of cell cycle-associated and -unassociated regulators for expression of a hepatocellular carcinoma oncogene cyclin-dependent kinase inhibitor 3."}, {"pubmed": 36828188, "text": "GID2 Interacts With CDKN3 and Regulates Pancreatic Cancer Growth and Apoptosis."}, {"pubmed": 37037907, "text": "PSMD12 interacts with CDKN3 and facilitates pancreatic cancer progression."}, {"pubmed": 37113134, "text": "Identifying key genes related to inflammasome in severe COVID-19 patients based on a joint model with random forest and artificial neural network."}, {"pubmed": 37682177, "text": "Cyclin dependent kinase inhibitor 3 (CDKN3) upregulation is associated with unfavorable prognosis in clear cell renal cell carcinoma and shapes tumor immune microenvironment: A bioinformatics analysis."}, {"pubmed": 38030294, "text": "MicroRNA-127-3p Inhibits Cardiomyocyte Inflammation and Apoptosis after Acute Myocardial Infarction via Targeting CDKN3."}, {"pubmed": 38720365, "text": "Human pan-cancer analysis of the predictive biomarker for the CDKN3."}, {"pubmed": 38814199, "text": "N6-Methyladenosine modified circ-NAB1 modulates cell cycle and epithelial-mesenchymal transition via CDKN3 in endometrial cancer."}], "genomic_pos": {"chr": "14", "end": 54420218, "ensemblgene": "ENSG00000100526", "start": 54396849, "strand": 1}, "genomic_pos_hg19": {"chr": "14", "end": 54886936, "start": 54863567, "strand": 1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0000079", "pubmed": 8242750, "qualifier": "involved_in", "term": "regulation of cyclin-dependent protein serine/threonine kinase activity"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0000082", "pubmed": 8127873, "qualifier": "involved_in", "term": "G1/S transition of mitotic cell cycle"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0008285", "pubmed": 8242750, "qualifier": "involved_in", "term": "negative regulation of cell population proliferation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0051726", "pubmed": 8242750, "qualifier": "acts_upstream_of_or_within", "term": "regulation of cell cycle"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "is_active_in", "term": "nucleus"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005737", "qualifier": "is_active_in", "term": "cytoplasm"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0048471", "pubmed": 10669749, "qualifier": "located_in", "term": "perinuclear region of cytoplasm"}], "MF": [{"category": "MF", "evidence": "IDA", "id": "GO:0004722", "pubmed": 12745075, "qualifier": "enables", "term": "protein serine/threonine phosphatase activity"}, {"category": "MF", "evidence": "IBA", "id": "GO:0004725", "qualifier": "enables", "term": "protein tyrosine phosphatase activity"}, {"category": "MF", "evidence": "IDA", "id": "GO:0004725", "pubmed": 8242750, "qualifier": "enables", "term": "protein tyrosine phosphatase activity"}, {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [8242750, 9840943, 11463386, 15530371, 15671017, 24218572, 25852190], "qu