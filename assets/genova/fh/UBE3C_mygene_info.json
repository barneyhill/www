hg19": [{"cdsend": 157060449, "cdsstart": 156931966, "chr": "7", "position": [[156931619, 156932032], [156956503, 156956557], [156961741, 156961816], [156962997, 156963144], [156967612, 156967728], [156971383, 156971541], [156974211, 156974365], [156974801, 156975022], [156976571, 156976723], [156979527, 156979715], [156994414, 156994501], [157000091, 157000249], [157000396, 157000629], [157009560, 157009665], [157013382, 157013470], [157015947, 157016045], [157018100, 157018233], [157023773, 157024021], [157041061, 157041274], [157046647, 157046836], [157046937, 157047004], [157049607, 157049738], [157060278, 157062064]], "strand": 1, "transcript": "NM_014671", "txend": 157062064, "txstart": 156931619}], "generif": [{"pubmed": 12692129, "text": "TIP120B is a specific substrate of KIAA10; both are highly expressed in human skeletal muscle, suggesting that KIAA10 may regulate TIP120B homeostasis specifically in this tissue."}, {"pubmed": 19846067, "text": "Observational study and genome-wide association study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 19913121, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20628086, "text": "Observational study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 20934631, "text": "Observational study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 20934631, "text": "gene polymorphism is associated with the risk of Aspirin-intolerant asthma"}, {"pubmed": 21167755, "text": "negatively regulates type I interferon through ubiquitination of the transcription factors"}, {"pubmed": 21881582, "text": "Our findings provide evidence that variations in UBE3C are potent genetic markers of nasal polyps development in Korean asthmatics."}, {"pubmed": 24158444, "text": "knockdown renders cells more susceptible to the Hsp90 inhibitor 17-AAG, suggesting that UBE3C protects against the harmful accumulation of protein fragments arising from incompletely degraded proteasome substrates."}, {"pubmed": 24425307, "text": "UBE3C is a mutant candidate oncogene involved in tumor development and progression of hepatocellular carcinoma."}, {"pubmed": 25658088, "text": "these observations suggest that UBE3C plays an important role in RCC development and progression, and UBE3C may be a novel target for prevention and treatment of ccRCC."}, {"pubmed": 26067607, "text": "data reveal that high UBE3C expression contributes to glioma progression by ubiquitination and degradation of ANXA7, and thus presents a novel and promising target for glioma therapy."}, {"pubmed": 26894856, "text": "UBE3C promotes melanoma progression, possibly by inducing epithelial-mesenchymal transition in melanoma cells"}, {"pubmed": 27003255, "text": "Findings indicate that miR-30a-5p inhibits ubiquitin Protein ligase E3C (UBE3C) expression by directly targeting its 3' untranslated regions (3'-UTR)."}, {"pubmed": 27862841, "text": "Ectopic overexpression of UBE4A, but not UBE3C, in cells was downregulated in vitro migration and invasion in these cells. Cumulatively, our data reveals a novel post-translational regulatory mechanism of regulating ILEI1 expression, a protein required for metastatic progression in prostate cancer cells"}, {"pubmed": 28396413, "text": "Usp14 and Ube3c cycle together on and off proteasomes, and the presence of ubiquitinated substrates promotes their association. This mechanism enables proteasome activity to adapt to the supply of substrates."}, {"pubmed": 28666208, "text": "miR-542-3p functioned as a tumor suppressor gene in regulating the epithelial-mesenchymal transition and metastasis of hepatocellular carcinoma via targeting UBE3C."}, {"pubmed": 30503554, "text": "Data suggest UBE3C-mediated AHNAK ubiquitination and degradation accompanied by subsequent blockage of p53 inhibition of stemness-related-gene transcription as a new posttranslational regulatory model describing maintenance of non-small-cell lung cancer (NSCLC) cell stemness. UBE3C knockdown increased p53 binding to the promoter regions inhibiting their transcription and preventing NSCLC tumorigenesis."}, {"pubmed": 32039437, "text": "Crystal structure of HECT domain of UBE3C E3 ligase and its ubiquitination activity."}, {"pubmed": 32930707, "text": "UBE3C promotes proliferation and inhibits apoptosis by activating the beta-catenin signaling via degradation of AXIN1 in gastric cancer."}, {"pubmed": 33637724, "text": "VPS34 K29/K48 branched ubiquitination governed by UBE3C and TRABID regulates autophagy, proteostasis and liver metabolism."}, {"pubmed": 35840930, "text": "Circular RNA circPOLR2A promotes clear cell renal cell carcinoma progression by facilitating the UBE3C-induced ubiquitination of PEBP1 and, thereby, activating the ERK signaling pathway."}, {"pubmed": 36380488, "text": "Novel gene-intergenic fusion involving ubiquitin E3 ligase UBE3C causes distal hereditary motor neuropathy."}, {"pubmed": 36401616, "text": "Biallelic variants in HECT E3 paralogs, HECTD4 and UBE3C, encoding ubiquitin ligases cause neurodevelopmental disorders that overlap with Angelman syndrome."}], "genomic_pos": {"chr": "7", "end": 157269370, "ensemblgene": "ENSG00000009335", "start": 157138926, "strand": 1}, "genomic_pos_hg19": {"chr": "7", "end": 157062066, "start": 156931607, "strand": 1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0000209", "qualifier": "involved_in", "term": "protein polyubiquitination"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0000209", "pubmed": 11278995, "qualifier": "involved_in", "term": "protein polyubiquitination"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006511", "qualifier": "involved_in", "term": "ubiquitin-dependent protein catabolic process"}, {"evidence": "IDA", "gocategory": "BP", "id": "