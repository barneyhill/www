[158088326, 158088454], [158089501, 158089628], [158089717, 158089818], [158091357, 158091556], [158093338, 158093446], [158093622, 158093762], [158094312, 158094390], [158094643, 158100262]], "strand": 1, "transcript": "NM_001286349", "txend": 158100262, "txstart": 157993272}, {"cdsend": 158095120, "cdsstart": 157993676, "chr": "1", "position": [[157993644, 157993728], [158076112, 158076262], [158077990, 158078140], [158084421, 158084579], [158086595, 158086746], [158087754, 158087860], [158088005, 158088154], [158088326, 158088454], [158089501, 158089628], [158089717, 158089818], [158091357, 158091556], [158093338, 158093446], [158093622, 158093762], [158094312, 158094390], [158094643, 158100262]], "strand": 1, "transcript": "NM_018240", "txend": 158100262, "txstart": 157993644}], "exons_hg19": [{"cdsend": 158064910, "cdsstart": 157963466, "chr": "1", "position": [[157963062, 157963518], [158054211, 158054369], [158056385, 158056536], [158057544, 158057650], [158057795, 158057944], [158058116, 158058244], [158059291, 158059418], [158059507, 158059608], [158061147, 158061346], [158063128, 158063236], [158063412, 158063552], [158064102, 158064180], [158064433, 158070052]], "strand": 1, "transcript": "NM_001286349", "txend": 158070052, "txstart": 157963062}, {"cdsend": 158064910, "cdsstart": 157963466, "chr": "1", "position": [[157963434, 157963518], [158045902, 158046052], [158047780, 158047930], [158054211, 158054369], [158056385, 158056536], [158057544, 158057650], [158057795, 158057944], [158058116, 158058244], [158059291, 158059418], [158059507, 158059608], [158061147, 158061346], [158063128, 158063236], [158063412, 158063552], [158064102, 158064180], [158064433, 158070052]], "strand": 1, "transcript": "NM_018240", "txend": 158070052, "txstart": 157963434}], "generif": [{"pubmed": 16968734, "text": "A striking finding in this study is the lack of contribution of NEPH1, NPHS1, and NPHS2 genes in 15 Asian families with steroid-resistant nephrotic syndrome."}, {"pubmed": 17968527, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 17968527, "text": "common variants in LRRC7, KIRREL, NPHS2 and ACTN4 do not appeear to contribute to susceptibility to diabetic nephropathy in Finnish patients with type 1 diabetes"}, {"pubmed": 18258597, "text": "Neph1 but not nephrin specifically binds to adaptor protein Grb2 and tyrosine kinase Csk in a phosphorylation-dependent manner."}, {"pubmed": 18562307, "text": "Neph1-Nephrin proteins bind the Par3-Par6-atypical protein kinase C (aPKC) complex to regulate podocyte cell polarity"}, {"pubmed": 18922801, "text": "tyrosine phosphorylation of Neph1 mediated by Fyn results in significantly increased Neph1 and ZO-1 binding, suggesting a critical role for Neph1 tyrosine phosphorylation in reorganizing the Neph1-ZO-1 complex."}, {"pubmed": 20507940, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 21402783, "text": "Localization of Neph1 to the podocyte cell membrane is altered in the presence of mutant Myo1c, and is actin dependent. Knockdown of Myo1c inhibits Neph1 membrane localization. Neph1 is critical for the maintenance of glomerular function."}, {"pubmed": 22262837, "text": "Neph1-CD adopts a global shape in solution, and its interaction with ZO-1 involves multiple sites."}, {"pubmed": 24554715, "text": "maintaining high levels of Neph1 at the membrane using a podocyte cell line overexpressing chimeric Neph1 increased the ability of podocytes to resist PAN-induced injury and PAN-induced albumin leakage"}, {"pubmed": 27044863, "text": "Analysis of interaction surfaces between NEPH1 and MYO1c led to the identification of a critical residue in Neph1 involved in binding to Myo1c. Indeed, a point mutant from this site abolished interaction between Neph1 and Myo1c."}, {"pubmed": 31472902, "text": "Mutations in KIRREL1, a slit diaphragm component, cause steroid-resistant nephrotic syndrome."}, {"pubmed": 32534710, "text": "Overexpression of kin of IRRE-Like protein 1 (KIRREL) as a prognostic biomarker for breast cancer."}, {"pubmed": 35177623, "text": "Cell adhesion molecule KIRREL1 is a feedback regulator of Hippo signaling recruiting SAV1 to cell-cell contact sites."}, {"pubmed": 36044856, "text": "Transmembrane protein KIRREL1 regulates Hippo signaling via a feedback loop and represents a therapeutic target in YAP/TAZ-active cancers."}, {"pubmed": 37909722, "text": "KIRREL promotes the proliferation of gastric cancer cells and angiogenesis through the PI3K/AKT/mTOR pathway."}], "genomic_pos": {"chr": "1", "end": 158100262, "ensemblgene": "ENSG00000183853", "start": 157993273, "strand": 1}, "genomic_pos_hg19": {"chr": "1", "end": 158070052, "start": 157963063, "strand": 1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0001933", "qualifier": "involved_in", "term": "negative regulation of protein phosphorylation"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0003094", "pubmed": 31472902, "qualifier": "involved_in", "term": "glomerular filtration"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0030838", "qualifier": "involved_in", "term": "positive regulation of actin filament polymerization"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0045217", "pubmed": 31472902, "qualifier": "involved_in", "term": "cell-cell junction maintenance"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0097017", "qualifier": "involved_in", "term": "renal protein absorption"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0098609", "qualifier": "involved_in", "term": "cell-cell adhesion"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005886", "qualifier": "is_active_in", "term": "plasma membrane"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005886", "pubmed": 21402783, "qualifier": "located_in", "term": "plasma membrane"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005886", "qualifier": "located_in", "term": "plasma membrane"}, {"e