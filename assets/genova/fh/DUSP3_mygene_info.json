nonpsych": {"exp_lof": 3.7000976965, "exp_mis": 67.5155185477, "exp_syn": 35.9596317349, "lof_z": 1.88911582029952, "mis_z": 1.19795358883062, "mu_lof": 3.93201894299e-07, "mu_mis": 7.78752996994e-06, "mu_syn": 4.3511986394e-06, "n_lof": 0.0, "n_mis": 48.0, "n_syn": 28.0, "p_li": 0.728477061104327, "p_null": 0.0149930861293077, "p_rec": 0.256529852766365, "syn_z": 0.835696965361245}, "nontcga": {"exp_lof": 3.80742202994, "exp_mis": 68.6736434566, "exp_syn": 36.5981496167, "lof_z": 1.92484940392902, "mis_z": 1.24288745853283, "mu_lof": 3.93201894299e-07, "mu_mis": 7.78752996994e-06, "mu_syn": 4.3511986394e-06, "n_lof": 0.0, "n_mis": 48.0, "n_syn": 28.0, "p_li": 0.718789613195954, "p_null": 0.0138355089485037, "p_rec": 0.267374877855543, "syn_z": 0.899907286390953}, "transcript": "ENST00000226004.3"}, "exons": [{"cdsend": 43778924, "cdsstart": 43769608, "chr": "17", "position": [[43766125, 43769814], [43774711, 43774938], [43778799, 43778977]], "strand": -1, "transcript": "NM_004090", "txend": 43778977, "txstart": 43766125}], "exons_hg19": [{"cdsend": 41856292, "cdsstart": 41846976, "chr": "17", "position": [[41843493, 41847182], [41852079, 41852306], [41856167, 41856345]], "strand": -1, "transcript": "NM_004090", "txend": 41856345, "txstart": 41843493}], "generif": [{"pubmed": 12447358, "text": "VHR, a Vaccinia virus VH1-related dual-specific protein phosphatase, is phosphorylated at Y138 by ZAP-70."}, {"pubmed": 16604064, "text": "VHR is required for cell-cycle progression as it modulates MAP kinase activation in a cell-cycle phase-dependent manner."}, {"pubmed": 17278098, "text": "deregulated expression of BRCA1-IRIS is likely to reduce dependence on normal physiological growth stimuli"}, {"pubmed": 17785772, "text": "Small dual-specificity phosphatase VHR selectively dephosphorylates tyrosine-phosphorylated interferon-alpha- and beta-activated transcription factor STAT5, leading to the subsequent inhibition of STAT5 function."}, {"pubmed": 18311544, "text": "Results highlight the importance of a high intracellular Zn(2+) content and the VHR/ZAP-70/ERK1,2-associated pathways in the modulation of LNCaP prostate cancer cell growth."}, {"pubmed": 18505570, "text": "VHR can be considered as a new marker for cancer progression in cervix carcinoma and potential new target for anticancer therapy"}, {"pubmed": 19010898, "text": "VHR has a direct role in the inhibition of JNK-dependent apoptosis in LNCaP cells and may therefore have a role in prostate cancer progression."}, {"pubmed": 21262974, "text": "VHR expression enhances the signaling of ErbB receptors and may be involved in NSCLC pathogenesis."}, {"pubmed": 21921027, "text": "Enteric commensal bacteria induce extracellular signal-regulated kinase pathway signaling via formyl peptide receptor-dependent redox modulation of dual specific phosphatase 3"}, {"pubmed": 23322772, "text": "Proteins containing the class II motifs are efficient VHR substrates in vitro, suggesting that VHR may act on a novel class of yet unidentified Tyr(P) proteins in vivo."}, {"pubmed": 23918168, "text": "we report the first successful site-specific incorporation of sulfotyrisine into VHR through the use of expanding genetic code"}, {"pubmed": 24245651, "text": "DUSP3 interacting partners are nucleolar proteins involved in processes related to DNA repair and senescence."}, {"pubmed": 24798147, "text": "VHR can dimerize inside cells, and that VHR catalytic activity is reduced upon dimerization."}, {"pubmed": 24886454, "text": "DUSP3 is a pro-angiogenic atypical dual-specificity phosphatase."}, {"pubmed": 24901344, "text": "Data suggest levels of gene expression of both DUSP3 (dual specificity phosphatase 3) and PSME3 (proteasome activator subunit 3) are associated with susceptibility to Staphylococcus aureus infection/sepsis in humans and in mouse disease model."}, {"pubmed": 25876765, "text": "Our results demonstrate that DUSP3 plays a key and nonredundant role as a regulator of innate immune responses"}, {"pubmed": 27423135, "text": "In the phosphatase-silenced cells, the normal bipolar spindle structure was restored by chemical inhibition of Erk1/2 and ectopic overexpression of Dusp3. We propose that at M phase Dusp3 keeps Erk1/2 activity in check to facilitate normal mitosis."}, {"pubmed": 27941812, "text": "Nuclear HSP70 leads to enhancement of vaccinia H1-related phosphatase (VHR) activity via protein-protein interaction rather than its molecular chaperone activity, thereby suppressing excessive ERK activation. Downregulation of either VRK3 or HSP70 rendered cells vulnerable to glutamate-induced apoptosis."}, {"pubmed": 28389334, "text": "The loss of DUSP3 activity markedly increases gamma radiation-induced DNA strand breaks, suggesting a potential novel role for DUSP3 in DNA repair."}, {"pubmed": 28625849, "text": "In PTP1B and VHR, two new allosteric clusters were identified in each enzyme."}, {"pubmed": 28759036, "text": "Data indicated that regulating non-receptor tyrosine kinase FAK and cell migration is a newly revealed function of VHR/DUSP3."}, {"pubmed": 30048472, "text": "Study demonstrated that miR-1915-3p might promote the proliferation and metastasis of breast cancer by repression of DUSP3 and serum miR-1915-3p and miR-455-3p could serve as diagnostic and predictive biomarkers for breast cancer."}, {"pubmed": 32348128, "text": "Allosteric Impact of the Variable Insert Loop in Vaccinia H1-Related (VHR) Phosphatase."}, {"pubmed": 32380926, "text": "DUSP3 maintains genomic stability and cell proliferation by modulating NER pathway and cell cycle regulatory proteins."}, {"pubmed": 32475380, "text": "Regulation of signal transducer and activator of transcription 3 activation by dual-specificity phosphatase 3."}, {"pubmed": 35899430, "text": "The role of dual-specificity phosphatase 3 in melanocytic oncogenesis."}, {"pubmed": 38538536, "text": "DUSP3 modulates IRES-dependent translation of mRNAs through dephosphorylation of the HNRNPC protein in cells under genotoxic stimulus."}], "genomic_pos": {"chr": "17", "end": 43778977, "ensemblgene": "ENSG00000108861", "start": 43766125, "strand": -1}, "genomic_pos_hg19": {"chr": "17", "end": 41856356, "start": 41843489, "strand": -1}, "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0016311", "pubmed": 24531476, "qualifier": "involved_in", "term": "dephosphorylation"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0030336", "pubmed": 28759036, "qualifier": "involved_in", "term": "negative regulation of cell migration"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0035335", "pubmed": [7876121, 11863439], "qualifier": "involved_in", "term": "peptidyl-tyrosine dephosphorylation"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0035335", "pubmed": 28759036, "qualifier": "involved_in", "term": "peptidyl-tyrosine dephosphorylation"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0042059", "pubmed": 28759036, "qualifier": "involved_in", "term": "negative regulation of epidermal growth factor receptor signaling pathway"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0043409", "pubmed": 16604064, "qualifier": "involved_in", "term": "negative regulation of MAPK cascade"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0045931", "qualifier": "involved_in", "term": "positive regulation of mitotic cell cycle"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0045931", "pubmed": 16604064, "qualifier": "involved_in", "term": "positive regulation of mitotic cell cycle"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0046329", "qualifier": "involved_in", "term": "negative regulation of JNK cascade"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0046329", "pubmed": 11085983, "qualifier": "involved_in", "term": "negative regulation of JNK cascade"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0046329", "pubmed": 19010898, "qualifier": "involved_in", "term": "negative regulation of JNK cascade"}, {"evidence": "IBA", "gocategory": "BP", "