onpsych": {"exp_lof": 6.56054001512, "exp_mis": 229.998997467, "exp_syn": 133.379569465, "lof_z": 2.5154851941647, "mis_z": 3.35904468247601, "mu_lof": 6.39600833602e-07, "mu_mis": 2.55185217239e-05, "mu_syn": 1.4880961245e-05, "n_lof": 0.0, "n_mis": 129.0, "n_syn": 80.0, "p_li": 0.89100187162799, "p_null": 0.00135407235176606, "p_rec": 0.107644056020244, "syn_z": 2.91000750473654}, "nontcga": {"exp_lof": 6.70369337877, "exp_mis": 233.821343742, "exp_syn": 135.610956312, "lof_z": 2.55410241690283, "mis_z": 3.38262919563648, "mu_lof": 6.39600833602e-07, "mu_mis": 2.55185217239e-05, "mu_syn": 1.4880961245e-05, "n_lof": 0.0, "n_mis": 130.0, "n_syn": 81.0, "p_li": 0.887080126332793, "p_null": 0.00122030619713579, "p_rec": 0.111699567470071, "syn_z": 2.96930601567542}, "transcript": "ENST00000491143.2"}, "exons": [{"cdsend": 57476723, "cdsstart": 57435716, "chr": "18", "position": [[57435373, 57436944], [57476436, 57491298]], "strand": 1, "transcript": "NM_004852", "txend": 57491298, "txstart": 57435373}], "exons_hg19": [{"cdsend": 55143955, "cdsstart": 55102948, "chr": "18", "position": [[55102605, 55104176], [55143668, 55158530]], "strand": 1, "transcript": "NM_004852", "txend": 55158530, "txstart": 55102605}], "generif": [{"pubmed": 18288132, "text": "A CpG island proximal to ONECUT2 is frequently methylated in diffuse large B-cell lymphoma (DLBCL)."}, {"pubmed": 18418398, "text": "OC2 transcription factor is a direct target of T-bet in type-1 T-helper cells."}, {"pubmed": 23472758, "text": "Mutations at the -5/-6 site of the F9 promotor account for the majority of hemophilia B Leyden cases and disrupt the binding of ONECUT1 and ONECUT2."}, {"pubmed": 24402783, "text": "our data showed that transcript factor Onecut2 is involved in the EMT, migration and invasion of CRC cells; miR-429 inhibits the initiation of EMT and regulated expression of EMT-related markers"}, {"pubmed": 26547929, "text": "Data show that ONECUT2, IGF2BP1, and ANXA2 proteins were confirmed to be microRNA-9 (miR-9) targets and aberrantly upregulated in hepatocellular carcinoma (HCC)."}, {"pubmed": 27756894, "text": "Bioinformatics-based prediction, combined with qRT-PCR analysis and Western blotting, identified Onecut2 and SIRT1 as the 2 prominent miR-9 target genes under the conditions tested.  In this study, Onecut2 and SIRT1 were confirmed as miR-9 target genes, which will help future investigations to probe in greater detail the role of miR-9 in Nonalcoholic Fatty Liver Disease."}, {"pubmed": 30478421, "text": "Here we identify the transcription factor ONECUT2 (OC2) as a master regulator of AR networks in metastatic castration-resistant prostate cancer (mCRPC). OC2 acts as a survival factor in mCRPC models, suppresses the AR transcriptional program by direct regulation of AR target genes and the AR licensing factor FOXA1, and activates genes associated with neural differentiation and progression to lethal disease."}, {"pubmed": 30655535, "text": "ONECUT2 activates SMAD3, which regulates hypoxia signaling through modulating HIF1alpha chromatin-binding, leading neuroendocrine prostate cancers to exhibit higher degrees of hypoxia compared to prostate adenocarcinomas."}, {"pubmed": 31049588, "text": "Data show that chromatin regions that contain the ONECUT motif were in- or lowly accessible in fibroblasts and became accessible after the overexpression of ONECUT1, ONECUT2 or ONECUT3."}, {"pubmed": 31118200, "text": "Chemotherapy induced breast cancer cells to secrete multiple EV miRNAs, including miR-9-5p, miR-195-5p, and miR-203a-3p, which simultaneously targeted the transcription factor One Cut Homeobox 2 (ONECUT2), leading to induction of CSC traits and expression of stemness-associated genes."}, {"pubmed": 31882655, "text": "ONECUT2 overexpression promotes RAS-driven lung adenocarcinoma progression."}, {"pubmed": 32129880, "text": "ONECUT2 upregulation is associated with CpG hypomethylation at promoter-proximal DNA in gastric cancer and triggers ACSL5."}, {"pubmed": 33015779, "text": "Up-regulated ONECUT2 and down-regulated SST promote gastric cell migration, invasion, epithelial-mesenchymal transition and tumor growth in gastric cancer."}, {"pubmed": 34365839, "text": "ONECUT2 which is targeted by hsa-miR-15a-5p enhances stemness maintenance of gastric cancer stem cells."}, {"pubmed": 36178080, "text": "LncRNA TM1-3P Regulates Proliferation, Apoptosis and Inflammation of Fibroblasts in Osteoarthritis through miR-144-3p/ONECUT2 Axis."}, {"pubmed": 36508388, "text": "MicroRNA-1298-3p induces tumor-suppressive effects in human cervical cancer cells via post-transcriptional suppression of ONECUT2."}, {"pubmed": 38997271, "text": "Increased ONECUT2 induced by Helicobacter pylori promotes gastric cancer cell stemness via an AKT-related pathway."}], "genomic_pos": {"chr": "18", "end": 57491298, "ensemblgene": "ENSG00000119547", "start": 57435374, "strand": 1}, "genomic_pos_hg19": {"chr": "18", "end": 55158529, "start": 55102917, "strand": 1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0001889", "qualifier": "involved_in", "term": "liver development"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0001952", "qualifier": "involved_in", "term": "regulation of cell-matrix adhesion"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0002064", "qualifier": "involved_in", "term": "epithelial cell development"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006357", "qualifier": "involved_in", "term": "regulation of transcription by RNA polymerase II"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0007179", "qualifier": "involved_in", "term": "transforming growth factor beta receptor signaling pathway"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0009887", "pubmed": 9915796, "qualifier": "involved_in", "term": "animal organ morphogenesis"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0030512", "qualifier": "involved_in", "term": "negative regulation of transforming growth factor beta