, "chr": "20", "position": [[1629151, 1629636], [1630221, 1630306], [1635266, 1635599], [1636187, 1636505], [1649051, 1649408], [1657641, 1657779]], "strand": -1, "transcript": "NM_018556", "txend": 1657779, "txstart": 1629151}, {"cdsend": 1657714, "cdsstart": 1630223, "chr": "20", "position": [[1629151, 1629636], [1630221, 1630306], [1649051, 1649408], [1657641, 1657779]], "strand": -1, "transcript": "NM_080816", "txend": 1657779, "txstart": 1629151}], "exons_hg19": [{"cdsend": 1638360, "cdsstart": 1610869, "chr": "20", "position": [[1609797, 1610282], [1610867, 1610952], [1616833, 1617151], [1629697, 1630054], [1638287, 1638425]], "strand": -1, "transcript": "NM_001039508", "txend": 1638425, "txstart": 1609797}, {"cdsend": 1638360, "cdsstart": 1610869, "chr": "20", "position": [[1609797, 1610282], [1610867, 1610952], [1615912, 1616245], [1616833, 1617151], [1629697, 1630054], [1638287, 1638425]], "strand": -1, "transcript": "NM_018556", "txend": 1638425, "txstart": 1609797}, {"cdsend": 1638360, "cdsstart": 1610869, "chr": "20", "position": [[1609797, 1610282], [1610867, 1610952], [1629697, 1630054], [1638287, 1638425]], "strand": -1, "transcript": "NM_080816", "txend": 1638425, "txstart": 1609797}], "generif": [{"pubmed": 15294972, "text": "a novel member of the signal regulatory protein (SIRP) family- with unique characteristics from both alpha and beta genes- termed SIRPgamma"}, {"pubmed": 18524990, "text": "CD47 is enriched at endothelial junctions, and its interaction with SIRPgamma is required for human T-cell transendothelial migration"}, {"pubmed": 19473566, "text": "tumor necrosis factor receptor superfamily, member 14 (TNFRSF14) and signal regulatory protein, gamma (SIRPG) appear to contribute to gender difference in incidence of systemic lupus erythematosus."}, {"pubmed": 23826770, "text": "SIRPgamma in complex with FabOX117 forms a dimer in the crystal. Binding to the Fab fixes the position of domain 1 relative to domains 2/3 exposing a surface which favours formation of a homotypic dimer."}, {"pubmed": 24162948, "text": "The independent non-obstructive azoospermia risk alleles are driven by variants in the protein-coding sequence of the two genes, SIRPA and SIRPG."}, {"pubmed": 30337675, "text": "The SNP rs2281808 TT variant reduced SIRPgamma expression on T-cells. SIRPgamma(low) CD8 T-cells in CT and TT individuals were in a heightened effector state with lower activation threshold and had greater gene expression of molecules associated with migratory and cytotoxic potential.  SIRPgamma(low) CD8 T-cells were deficient in transcription factors associated with long-term functional memory formation."}, {"pubmed": 32853219, "text": "Altered expression of SIRPgamma on the T-cells of relapsing remitting multiple sclerosis and type 1 diabetes patients could potentiate effector responses from T-cells."}, {"pubmed": 34799406, "text": "Genetic Control of Splicing at SIRPG Modulates Risk of Type 1 Diabetes."}], "genomic_pos": {"chr": "20", "end": 1657779, "ensemblgene": "ENSG00000089012", "start": 1629152, "strand": -1}, "genomic_pos_hg19": {"chr": "20", "end": 1638425, "start": 1609798, "strand": -1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0007155", "qualifier": "involved_in", "term": "cell adhesion"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0007267", "pubmed": 9062191, "qualifier": "involved_in", "term": "cell-cell signaling"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0008284", "pubmed": 15383453, "qualifier": "involved_in", "term": "positive regulation of cell population proliferation"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0008285", "pubmed": 9062191, "qualifier": "involved_in", "term": "negative regulation of cell population proliferation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0022409", "pubmed": 15383453, "qualifier": "involved_in", "term": "positive regulation of cell-cell adhesion"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0035556", "p