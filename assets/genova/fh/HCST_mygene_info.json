n": [[36393430, 36393538], [36394252, 36394318], [36394673, 36394805], [36395021, 36395279]], "strand": 1, "transcript": "NM_014266", "txend": 36395279, "txstart": 36393430}], "generif": [{"pubmed": 12740575, "text": "NKG2D-DAP10 triggers human NK cell-mediated killing via a Syk-independent regulatory pathway."}, {"pubmed": 16339517, "text": "both activated and expanded CD8+ T cells and NK cells use DAP10 for cytolysis"}, {"pubmed": 19075187, "text": "Downstream targets of DAP10 and DAP12 are constitutively activated in large granular lymphocyte leukemia cells, and expression of dominant-negative DAP10 and DAP12 dramatically reduces their lytic capacity."}, {"pubmed": 19329438, "text": "the rapid degradation of NKG2D/DAP10 observed coincident with recruitment of the receptor to the cytotoxic immune synapse may explain the loss of NKG2D receptor expression after chronic exposure to NKG2D ligands"}, {"pubmed": 21263069, "text": "A transgenic mouse model demonstrates that induction of tolerance in Ly49H-positive natural killer (NK) cells by chronic exposure to virus-encoded ligand m157 requires signaling through the Ly49H adaptor protein DAP12, not the DAP10 adaptor protein."}, {"pubmed": 21706044, "text": "HMBOX1 negatively regulates the expression of NKG2D and the activation of the NKG2D/DAP10 signaling pathway in NK cells."}, {"pubmed": 21789015, "text": "TGF-BETA1 down-modulates surface NKG2D expression by controlling the transcriptional and translational levels of DAP10."}, {"pubmed": 21816829, "text": "Other gamma(c) cytokines act similarly to IL-2 in up-regulating DAP10 expression and NKG2D-DAP10 surface expression."}, {"pubmed": 22438812, "text": "TGF-beta1 may reduce the expression of NKG2D/DAP10 and 2B4/SAPin patients with hepatitis B."}, {"pubmed": 25002577, "text": "findings indicate that the functional interaction between RAGE and DAP10 coordinately regulates S100A8/A9-mediated survival and/or apoptotic response of keratinocytes"}, {"pubmed": 26508790, "text": "These results suggest that NKG2D-DAP10 endocytosis represents a means to decrease cell surface receptor abundance, as well as to control signaling outcome in cytotoxic lymphocytes."}, {"pubmed": 31235500, "text": "NKG2D-DAP10 signaling recruits EVL to the cytotoxic synapse to generate F-actin and promote NK cell cytotoxicity."}, {"pubmed": 33153411, "text": "CSF1R and HCST: Novel Candidate Biomarkers Predicting the Response to Immunotherapy in Non-Small Cell Lung Cancer."}], "genomic_pos": {"chr": "19", "end": 35904377, "ensemblgene": "ENSG00000126264", "start": 35902529, "strand": 1}, "genomic_pos_hg19": {"chr": "19", "end": 36395279, "start": 36393382, "strand": 1}, "go": {"BP": [{"evidence": "IGI", "gocategory": "BP", "id": "GO:0006468", "pubmed": 10528161, "qualifier": "involved_in", "term": "protein phosphorylation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0042267", "pubmed": 16582911, "qualifier": "involved_in", "term": "natural killer cell mediated cytotoxicity"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0050776", "qualifier": "involved_in", "term": "regulation of immune response"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0051897", "qualifier": "involved_in", "term": "positive regulation of phosphatidylinositol 3-kinase/protein kinase B signal transduction"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0051897", "pubmed": 10426994, "qualifier": "involved_in", "term": "positive regulation of phosphatidylinositol 3-kinase/protein kinase B signal transduction"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0005886", "pubmed": 15894612, "qualifier": "is_active_in", "term": "plasma membrane"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005886", "qualifier": "located_in", "term": "plasma membrane"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0009986", "pubmed": [10426994, 15294961], "qualifier": "located_in", "term": "cell surface"}], "MF": [{"category": "MF", "evidence": "IBA", "id": "GO:0005102