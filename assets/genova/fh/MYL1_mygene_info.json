 "position": [[211154873, 211155191], [211155755, 211155798], [211158446, 211158524], [211158968, 211159142], [211163143, 211163287], [211167211, 211167239], [211179634, 211179898]], "strand": -1, "transcript": "NM_079420", "txend": 211179898, "txstart": 211154873}, {"cdsend": 211168268, "cdsstart": 211155769, "chr": "2", "position": [[211154873, 211155191], [211155755, 211155798], [211158446, 211158524], [211158968, 211159142], [211163143, 211163287], [211167491, 211167516], [211168265, 211168339]], "strand": -1, "transcript": "NM_079422", "txend": 211168339, "txstart": 211154873}], "generif": [{"pubmed": 20518849, "text": "In vitro degradation of human recombinant MLC1 by MMP-2 increased after ONOO(-) exposure of MLC1 in a concentration-dependent manner"}, {"pubmed": 21731751, "text": "Influenza infection activates a series of signaling pathways that converge to induce myosin light chain (MLC) phosphorylation and remodeling of the actin cytoskeleton."}, {"pubmed": 22131351, "text": "Disturbed myosin binding of mutated hVLC-1 may provide a pathomechanism for the development of hypertrohic cardiomyopathy."}, {"pubmed": 23319595, "text": "MRK is a novel RhoC effector that controls LPA-stimulated cell invasion at least in part by regulating myosin dynamics, ERK and p38"}, {"pubmed": 26011449, "text": "This study provides an additional mechanistic link between Angiotensin II and vasoconstriction via SFK-enhanced MLC phosphorylation in smooth muscle cells."}, {"pubmed": 26473445, "text": "polymorphism of intron MYL1 was associated with endurance performance, specifically in endurance trainability, but not genetic predisposition, in human adults."}, {"pubmed": 28427997, "text": "these results provide new insights into the sequences and modifications of myosin light chain isoforms in the human and swine hearts, which will pave the way for a better understanding of their functional roles in cardiac physiology and pathophysiology."}, {"pubmed": 28618653, "text": "MLCK and phosphorylated MLC are potential prognostic indicators of leiomyosarcoma."}, {"pubmed": 29102634, "text": "These results support the concept of the ATP-induced transient interaction of the essential light chain-1 N-terminus with the motor domain of the myosin head."}, {"pubmed": 30215711, "text": "Our data implicate MYL1 as a crucial protein for adequate skeletal muscle function and that MYL1 deficiency is associated with severe congenital myopathy."}, {"pubmed": 33499774, "text": "MYBPC2 and MYL1 as Significant Gene Markers for Rhabdomyosarcoma."}, {"pubmed": 34596111, "text": "MYL2 as a potential predictive biomarker for rhabdomyosarcoma."}, {"pubmed": 37679666, "text": "Analysis of myosin genes in HNSCC and identify MYL1 as a specific poor prognostic biomarker, promotes tumor metastasis and correlates with tumor immune infiltration in HNSCC."}], "genomic_pos": {"chr": "2", "end": 210315174, "ensemblgene": "ENSG00000168530", "start": 210290150, "strand": -1}, "genomic_pos_hg19": {"chr": "2", "end": 211179914, "start": 211154874, "strand": -1}, "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0006936", "pubmed": 8145163, "qualifier": "involved_in", "term": "muscle contraction"}, {"evidence": "NAS", "gocategory": "BP", "id": "GO:0030049", "pubmed": 3904738, "qualifier": "involved_in", "term": "muscle filament sliding"}], "CC": [{"evidence": "TAS", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "NAS", "gocategory": "CC", "id": "GO:0005859", "pubmed": 3904738, "qualifier": "part_of", "term": "muscle myosin complex"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0016460", "qualifier": "part_of", "term": "myosin II complex"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0030016", "pubmed": 8145163, "qualifier": "located_in", "term": "myofibril"}, {"evidence": "NAS", "gocategory": "CC", "id": "GO:0030017", "pubmed": 3904738, "qualifier": "located_in", "term": "sarcomere"}, {"evidence": "IBA", "g