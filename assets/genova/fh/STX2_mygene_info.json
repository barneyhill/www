: [[131274144, 131276522], [131283069, 131283180], [131285697, 131285835], [131285959, 131286033], [131291526, 131291635], [131293175, 131293249], [131297501, 131297576], [131306233, 131306333], [131311737, 131311812], [131323614, 131323795]], "strand": -1, "transcript": "NM_001980", "txend": 131323795, "txstart": 131274144}, {"cdsend": 131323644, "cdsstart": 131280584, "chr": "12", "position": [[131274144, 131276522], [131280539, 131280665], [131283069, 131283180], [131285697, 131285835], [131285959, 131286033], [131291526, 131291635], [131293175, 131293249], [131297501, 131297576], [131306233, 131306333], [131311737, 131311812], [131323614, 131323795]], "strand": -1, "transcript": "NM_194356", "txend": 131323795, "txstart": 131274144}], "generif": [{"pubmed": 12198139, "text": "Munc18b binds to syntaxins 1A, 2, and 3 and regulates vesicle transport to the apical plasma membrane"}, {"pubmed": 15651999, "text": "These findings suggest that epimorphin contributes to repair of pulmonary fibrosis in nonspecific interstitial pneumonia, perhaps partly by inducing expression of matrix metalloproteinase 2, which is an important proteolytic factor in lung remodeling."}, {"pubmed": 17485553, "text": "expression of SNAP-23 and syntaxin-2 on the extracellular surface of the platelet plasma membrane."}, {"pubmed": 18273050, "text": "Inadequate epimorphin elicits an abnormal differentiation response in keratinocytes, and indicate a causal function of the epimorphin signaling gradient for the establishment of differentiated epidermal structure in the skin."}, {"pubmed": 19129200, "text": "Homology with vesicle fusion mediator syntaxin-1a predicts determinants of epimorphin/syntaxin-2 function in mammary epithelial morphogenesis.("}, {"pubmed": 21156930, "text": "Genetic variability in STXBP5 and STX2 affects both VWF concentration and activity in young individuals with premature arterial thrombosis."}, {"pubmed": 22045676, "text": "EPM, secreted by activated stellate cells within stroma, promotes invasion and metastasis of hepatocellular cancer cells by activating MMP-9 expression through the FAK-ERK pathway."}, {"pubmed": 22792389, "text": "Genetic variation in STX2 is associated with von Willebrand factor:Ag levels in patients diagnosed with type 1 von Willebrand Disease."}, {"pubmed": 23219092, "text": "epimorphin modulates the signaling pathways mediated by EGFR for epidermal tissue organization"}, {"pubmed": 24039787, "text": "Epimorphin can revert ovarian cancer cells away from their mesenchymal phenotype and toward an epithelial phenotype."}, {"pubmed": 24578379, "text": "Identify 3 loci associated with circulating tPA levels, the PLAT region, STXBP5, and STX2. Functional studies implicate a novel role for STXBP5 and STX2 in regulating tPA release."}, {"pubmed": 27362846, "text": "The effects of HIV/HCV co-infection on hepatic fibrosis might be mediated in part by epimorphin (EPM). Strategies to limit the expression of EPM might represent a novel therapeutic approach to prevent the progression of hepatic fibrosis during HIV/HCV co-infection."}, {"pubmed": 28209294, "text": "TGF-beta, bFGF and epimorphin in the extracellular microenvironment cooperatively affect HSF behaviors under the control of a highly sulfated chondroitin sulfate"}, {"pubmed": 29570232, "text": "he patient showed azoospermia, relatively small testes, and a mildly elevated follicle stimulating hormone level, but no additional clinical features. Testicular histology of the patient showed universal maturation arrest and multinucleated spermatocytes, which have also been observed in mice lacking Stx2. PCR-based cDNA screening revealed wildtype STX2 expression in various tissues including the testis"}, {"pubmed": 29767240, "text": "Munc134 mediates human neutrophil elastaseinduced airway mucin5AC hypersecretion by interacting with syntaxin2"}, {"pubmed": 29855462, "text": "STX2 as a crucial modulator of the aggressive colorectal cancer phenotype."}, {"pubmed": 33075373, "text": "STX2 drives colorectal cancer proliferation via upregulation of EXOSC4."}, {"pubmed": 36316316, "text": "Plasma membrane flipping of Syntaxin-2 regulates its inhibitory action on insulin granule exocytosis."}], "genomic_pos": {"chr": "12", "end": 130839254, "ensemblgene": "ENSG00000111450", "start": 130789600, "strand": -1}, "genomic_pos_hg19": {"chr": "12", "end": 131323811, "start": 131274145, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0006886", "qualifier": "involved_in", "term": "intracellular protein transport"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006887", "qualifier": "involved_in", "term": "exocytosis"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0007165", "pubmed": 1581962, "qualifier": "involved_in", "term": "signal transduction"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0007340", "qualifier": "involved_in", "term": "acrosome reaction"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0007398", "pubmed": 8466509, "qualifier": "involved_in", "term": "ectoderm development"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0009887", "pubmed": 1581962, "qualifier": "involved_in", "term": "animal organ morphogenesis"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0031629", "qualifier": "involved_in", "term": "synaptic vesicle fusion to presynaptic active zone membrane"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0034599", "pubmed": 22226963, "qualifier": "involved_in", "term": "cellular response to oxidative stress"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0048278", "qualifier": "involved_in", "term": "vesicle docking"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:1903575", "pubmed": 22226963, "qualifier": "involved_in", "term": "cornified envelope assembly"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0005615", "pubmed": 22226963, "qualifier": "located_in", "term": "extracellular space"}, {"evidence": "IBA", "gocategory": "CC", "id": 