510854, 36511000], [36511502, 36511786], [36512459, 36512602], [36516477, 36516607], [36516704, 36516790], [36517300, 36517431], [36518762, 36518910], [36519404, 36519537], [36520575, 36520688], [36523208, 36523281], [36523894, 36523980], [36524595, 36524732], [36525663, 36525709], [36525998, 36526131], [36529075, 36529166]], "strand": -1, "transcript": "NM_003753", "txend": 36529166, "txstart": 36510854}, {"cdsend": 36529166, "cdsstart": 36529166, "chr": "22", "position": [[36510854, 36511000], [36511502, 36511786], [36512459, 36512602], [36516477, 36516607], [36516704, 36516790], [36517300, 36517493], [36518762, 36518910], [36519404, 36519537], [36520575, 36520688], [36523208, 36523281], [36523894, 36523980], [36524595, 36524732], [36525663, 36525709], [36525998, 36526131], [36529075, 36529166]], "strand": -1, "transcript": "NR_156418", "txend": 36529166, "txstart": 36510854}], "exons_hg19": [{"cdsend": 36922168, "cdsstart": 36907033, "chr": "22", "position": [[36906901, 36907047], [36907549, 36907833], [36908506, 36908649], [36912524, 36912654], [36912751, 36912837], [36913347, 36913478], [36914809, 36914957], [36915451, 36915584], [36916622, 36916735], [36919255, 36919328], [36919941, 36920027], [36920642, 36920779], [36921710, 36921756], [36922045, 36922178], [36925122, 36925213]], "strand": -1, "transcript": "NM_003753", "txend": 36925213, "txstart": 36906901}, {"cdsend": 36925213, "cdsstart": 36925213, "chr": "22", "position": [[36906901, 36907047], [36907549, 36907833], [36908506, 36908649], [36912524, 36912654], [36912751, 36912837], [36913347, 36913540], [36914809, 36914957], [36915451, 36915584], [36916622, 36916735], [36919255, 36919328], [36919941, 36920027], [36920642, 36920779], [36921710, 36921756], [36922045, 36922178], [36925122, 36925213]], "strand": -1, "transcript": "NR_156418", "txend": 36925213, "txstart": 36906901}], "generif": [{"pubmed": 18571626, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 24092755, "text": "eukaryotic initiation factor 4G (eIF4G) protein binds to eIF3c, -d, and -e to promote mRNA recruitment to the ribosome."}, {"pubmed": 25322666, "text": "the critical involvement of EIF3D in the survival and progression of melanoma cells"}, {"pubmed": 25370813, "text": "Findings suggest that eukaryotic translation initiation factor 3 subunit D (eIF3D) might play an important role in colon cancer progression."}, {"pubmed": 25682860, "text": "EIF3D plays an important role in modulating glioma cell growth and migration."}, {"pubmed": 26617750, "text": "Knockdown of eIF3D inhibited breast cancer cell proliferation and invasion."}, {"pubmed": 27035563, "text": "Results provide evidences that EIF3D may function as a potential proto-oncogene that participates in the occurrence and progression of renal cell carcinoma."}, {"pubmed": 27462815, "text": "eIF3d makes specific contacts with the RNA cap, as exemplified by cap analogue competition, and these interactions are essential for assembly of translation initiation complexes on eIF3-specialized mRNAs such as the cell proliferation regulator c-Jun"}, {"pubmed": 27477275, "text": "A transcript-specific eIF3 complex mediates global translational control of energy metabolism."}, {"pubmed": 28594409, "text": "eIF3d promotes gallbladder cancer (GBC) progression mainly via eIF3d-GRK2-AKT axis and it may be used as a prognostic factor. The therapeutic targeting of eIF3d-GRK2 axis may be a potential treatment approach for GBC."}, {"pubmed": 28801778, "text": "results indicate that eIF3D plays a key role in the proliferation of AML cells, and suggest that eIF3D silencing might be a potential therapeutic strategy for leukemia."}, {"pubmed": 31118081, "text": "The current data highlight the importance of eIF3d in HIV infection by inhibiting CD8+ T cell function and promoting viral replication."}, {"pubmed": 31280863, "text": "EIF3D is a novel substrate of CUL3/KCTD10 ubiquitin ligase.  The ubiquitin code of EIF3D is K27-polyubiquitination at the lysine 153 and 275 residues."}, {"pubmed": 31669222, "text": "Mechanistical study demonstrated that EIF3D interacted with GRP78 and enhanced protein stability through blocking the ubiquitin-mediated-proteasome degradation of GRP78."}, {"pubmed": 31885740, "text": "Results demonstrated that the overexpression of eIF3D could independently predict poor prognosis for patients with lung adenocarcinoma (LUAD). In silico analysis demonstrated that the altered expression of eIF3D was at least regulated by both copy number alterations (CNAs) and the hypomethylation of cg14297023 site."}, {"pubmed": 33273988, "text": "Systematic Investigation of mRNA N (6)-Methyladenosine Machinery in Primary Prostate Cancer."}, {"pubmed": 34520790, "text": "EIF3D promotes the progression of preeclampsia by inhibiting of MAPK/ERK1/2 pathway."}, {"pubmed": 34848685, "text": "A DAP5/eIF3d alternate mRNA translation mechanism promotes differentiation and immune suppression by human regulatory T cells."}, {"pubmed": 35104170, "text": "Overexpression of Eukaryotic translation initiation factor 3D induces stem cell-like properties and metastasis in cervix cancer by activating FAK through inhibiting degradation of GRP78."}, {"pubmed": 36264610, "text": "EIF3D promoted cervical carcinoma through Warburg effect by interacting with GRP78."}, {"pubmed": 36997088, "text": "eIF3d: A driver of noncanonical cap-dependent translation of specific mRNAs and a trigger of biological/pathological processes."}, {"pubmed": 37144468, "text": "Translational fidelity screens in mammalian cells reveal eIF3 and eIF4G2 as regulators of start codon selectivity."}, {"pubmed": 37314929, "text": "Breast cancer cell mesenchymal transition and metastasis directed by DAP5/eIF3d-mediated selective mRNA translation."}, {"pubmed": 37683648, "text": "eIF3d controls the persistent integrated stress response."}], "genomic_pos": {"chr": "22", "end": 36529184, "ensemblgene": "ENSG00000100353", "start": 36510855,