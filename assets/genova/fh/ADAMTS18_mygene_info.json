n": [[77316024, 77317968], [77323160, 77323308], [77325162, 77325375], [77326972, 77327155], [77328819, 77329024], [77331185, 77331312], [77334159, 77334301], [77353745, 77353990], [77354975, 77355099], [77356232, 77356363], [77359762, 77359935], [77369652, 77369801], [77375600, 77375696], [77387629, 77387783], [77389836, 77389974], [77393214, 77393320], [77396001, 77396161], [77397698, 77397782], [77398084, 77398278], [77401337, 77401620], [77465191, 77465508], [77468314, 77468402], [77468502, 77468931]], "strand": -1, "transcript": "NM_199355", "txend": 77468931, "txstart": 77316024}], "generif": [{"pubmed": 16507336, "text": "comparison of the effects of C-terminal truncation on the GAG-binding properties and aggrecanase activity of ADAMTS-5 relative to three other ADAMTS family members, ADAMTS-9, ADAMTS-16 and ADAMTS-18"}, {"pubmed": 17546048, "text": "Functional epigenetics show ADAMTS18 to be a novel functional tumor suppressor, being frequently inactivated epigenetically in multiple carcinomas."}, {"pubmed": 18398821, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19249006, "text": "ADAMTS18 and TGFBR3 might underlie BMD determination in the major human ethnic groups."}, {"pubmed": 19249006, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19806480, "text": "ADAMTS18 gene methylation in 3 types of cancers was significantly higher than normal tissues. No significant association was found between methylation status & TNM staging. Epigenetic regulation of ADAMTS18 was associated with carcinogenesis."}, {"pubmed": 19844255, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20164292, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 21047771, "text": "ADAMTS18 mutations promote growth, migration, and metastasis in melanoma"}, {"pubmed": 21862674, "text": "the study identified ADAMTS18 as the only gene carrying a homozygous protein altering mutation."}, {"pubmed": 23356391, "text": "study reveals that mutations in the ADAMTS18 gene can cause a broad phenotypic spectrum of eye disorders and contribute to shed further light on the complexity of retinal diseases"}, {"pubmed": 23818446, "text": "Results suggest that ADAMTS18 plays an essential role in early eye development and that mutations therein cause a distinct eye phenotype that is mainly characterized by microcornea and myopia."}, {"pubmed": 24874986, "text": "Novel homozygous mutations in ADAMTS18 were identified, consisting of c.1067T>A [p.L356*] in the first proband, c.2159G>C [p.C720S] in the 2 affected brothers"}, {"pubmed": 24896365, "text": "Studies suggest that ADAM metallopeptidase with thrombospondin type 1 motif, 18 protein (ADAMTS-18) as a promising diagnostic and therapeutic target."}, {"pubmed": 28362704, "text": "study suggests that ADAMTS18 is downregulated in cervical cancer and ADAMTS18 may serve as a potential prognostic biomarker for cervical cancer."}, {"pubmed": 28503860, "text": "In summary, we demonstrate that ADAMTS18 silencing in breast cancer is significantly correlated with promoter CpG methylation. ADAMTS18 acts as an antagonist of AKT and NF-kappaB signaling, further suppressing EMT and metastasis of breast cancer cells."}, {"pubmed": 28814085, "text": "This study showed that ADAMTS1, 8, and 18 are highly expressed in GC and its nodal metastases, suggesting important roles of these proteases in carcinogenesis and lymphatic metastasis. The findings from the present study indicate that these proteases may be promising candidates for novel and alternative treatments in GC (gastric cancer)"}, {"pubmed": 30417422, "text": "ADAMTS18 promoter methylation is a potential epigenetic biomarker."}, {"pubmed": 32218432, "text": "Adamts18 links luminal hormone receptor signaling to basement membrane remodeling and stem cell activation."}, {"pubmed": 33469885, "text": "Aberrant methylation of the 16q23.1 tumor suppressor gene ADAMTS18 promotes tumorigenesis and progression of clear cell renal cell carcinoma."}, {"pubmed": 34320216, "text": "Characterization of a functional endothelial super-enhancer that regulates ADAMTS18 and angiogenesis."}, {"pubmed": 36632911, "text": "Secreted protease ADAMTS18 in development and disease."}, {"pubmed": 38199772, "text": "[A family with developmental glaucoma and microcornea due to novel ADAMTS18 gene mutations]."}], "genomic_pos": {"chr": "16", "end": 77435034, "ensemblgene": "ENSG00000140873", "start": 77247813, "strand": -1}, "genomic_pos_hg19": {"chr": "16", "end": 77469011, "start": 77281710, "strand": -1}, "go": {"BP": [{"evidence": "IMP", "gocategory": "BP", "id": "GO:0001654", "pubmed": 23818446, "qualifier": "involved_in", "term": "eye development"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006508", "qualifier": "involved_in", "term": "proteolysis"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0006508", "qualifier": "involved_in", "term": "proteolysis"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0030198", "qualifier": "involved_in", "term": "extracellular matrix organization"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0090331", "pubmed": 19218546, "qualifier": "acts_upstream_of_or_within", "term": "negative regulation of platelet aggregation"}], "CC": [{"evidence": "IEA", "gocategory": "CC", "id": "GO:0005576", "qualifier": "located_in", "term": "extracellular region"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0031012", "qualifier": "is_active_in", "term": "extracellular matrix"}], "MF": [{"category": "MF", "evidence": "IBA", "id": "GO:0004222", "qualifier": "enables", "term": "metalloendopeptidase activity"}, {"category": "MF", "evidence": "IEA", "id": "GO:0046872", "qualifier": "en