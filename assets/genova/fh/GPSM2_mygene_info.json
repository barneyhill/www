109419850], [109428128, 109428200], [109439485, 109439707], [109440113, 109440249], [109440580, 109440723], [109441263, 109441387], [109441500, 109441616], [109444411, 109444567], [109445747, 109445856], [109446746, 109446876], [109456959, 109457030], [109461234, 109461411], [109465038, 109465198], [109466621, 109466836], [109472322, 109477166]], "strand": 1, "transcript": "NM_001321038", "txend": 109477166, "txstart": 109419606}, {"cdsend": 109472562, "cdsstart": 109428144, "chr": "1", "position": [[109419606, 109419850], [109427896, 109428200], [109439485, 109439707], [109440113, 109440249], [109440580, 109440723], [109441263, 109441387], [109441500, 109441616], [109444411, 109444567], [109445747, 109445856], [109446746, 109446876], [109456959, 109457030], [109461234, 109461411], [109465038, 109465198], [109466621, 109466836], [109472322, 109472597], [109473934, 109476957]], "strand": 1, "transcript": "NM_001321039", "txend": 109476957, "txstart": 109419606}, {"cdsend": 109472562, "cdsstart": 109428144, "chr": "1", "position": [[109419606, 109419850], [109427896, 109428200], [109439485, 109439707], [109440113, 109440249], [109440580, 109440723], [109441263, 109441387], [109441500, 109441616], [109444411, 109444567], [109445747, 109445856], [109446746, 109446876], [109456959, 109457030], [109461234, 109461411], [109465038, 109465198], [109466621, 109466836], [109472322, 109477166]], "strand": 1, "transcript": "NM_013296", "txend": 109477166, "txstart": 109419606}], "generif": [{"pubmed": 11832491, "text": "LGN is expressed in neuronal, astroglial, and microglial cultures"}, {"pubmed": 15632202, "text": "binding between Lgl2 and LGN play a role in mitotic spindle organization through regulating formation of the LGN.NuMA complex; Lgl2 forms a Lgl2.Par-6.aPKC.LGN complex, which responds to mitotic signaling to establish normal cell division"}, {"pubmed": 20056645, "text": "Observational study of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20479129, "text": "Ric-8A and Gi alpha recruit LGN, NuMA, and dynein to the cell cortex to help orient the mitotic spindle."}, {"pubmed": 20508983, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20589935, "text": "Upregulation of GPSM2 is associated with breast cancer."}, {"pubmed": 20602914, "text": "Identification of GPSM2 as essential to the development of normal hearing suggests dysregulation of cell polarity as a mechanism underlying hearing loss."}, {"pubmed": 20933426, "text": "During mitosis, Pins is mislocalized to the apical surface in the absence of Par3 or by inhibition of atypical protein kinase C."}, {"pubmed": 21348867, "text": "A novel truncating mutation of GPSM2 is associated with autosomal recessive non-syndromic hearing loss."}, {"pubmed": 22074847, "text": "mInsc-LGN interaction is vital for stabilization of LGN and for intracellular localization of mInsc."}, {"pubmed": 22578326, "text": "GPSM2 is required for orienting the mitotic spindle during cell division in multiple tissues, suggesting that the sensorineural hearing loss and characteristic brain malformations of CMS are due to defects in asymmetric cell divisions during development"}, {"pubmed": 22690686, "text": "Overexpression of LGN decreases the activity of cellular sGC, whereas knockdown of LGN mRNA and protein correlated with increased sGC activity"}, {"pubmed": 22977735, "text": "Studies indicate that the Inscuteable (Insc)and NuMA are mutually exclusive interactors of LGN."}, {"pubmed": 22987632, "text": "Results show compound heterozygous mutations in the GPSM2 gene, in affected members of a family with Chudley-McCullough syndrome, co-segregate in the family as an autosomal recessive trait."}, {"pubmed": 23389635, "text": "Data indicate that dynein- and astral microtubule-mediated transport of Galphai/LGN/nuclear mitotic apparatus (NuMA) complex from cell cortex to spindle poles."}, {"pubmed": 23494849, "text": "one homozygous frameshift GPSM2 variants c.1473delG was identified in three Chudley-McCullough syndrome Dutch patients."}, {"pubmed": 23907121, "text": "LGN is required for mitotic spindle rotation but not orientation maintenance."}, {"pubmed": 24165937, "text": "Hepatocyte Par1b defines lumen position in concert with the position of the astral microtubule anchoring complex LGN-NuMA to yield the distinct epithelial division phenotypes."}, {"pubmed": 25664792, "text": "A crystal structure of Frmpd4-bound LGN in an oxidized form is also reported, although oxidation does not appear to strongly affect the interaction with Frmpd4."}, {"pubmed": 26398908, "text": "results fit a model whereby LGN influences interphase microtubule dynamics in endothelial cells to regulate migration, cell adhesion, and sprout extension, and reveal a novel non-mitotic role for LGN in sprouting angiogenesis"}, {"pubmed": 26662512, "text": "Kinocilium is essential for proper localization of Lgn, as well as Gai and aPKC, suggesting that cilium function plays a role in positioning of apical proteins critical for hearing."}, {"pubmed": 26751642, "text": "This study determined the crystallographic structure of human Afadin in complex with LGN."}, {"pubmed": 26987813, "text": "Data support the notion that the Galpha, but not Gbetagamma, arm of the Gi/o signalling is involved in TRPC4 activation and unveil new roles for RGS and LGN in fine-tuning TRPC4 activities."}, {"pubmed": 27064331, "text": "A novel mutation (c.1093C > T; p.Arg365*) is described in a family with dizygotic twins with variable phenotype of Chudley-McCullough syndrome."}, {"pubmed": 27180139, "text": "This mutation is predicted to abolish all four GoLoco domains in GPSM2 and this explains the bioinformatic prediction for this mutation to be functionally damaging. Full clinical and molecular accounts of the novel mutation are provided in this paper."}, {"pubmed": 28045117, "text": "The results show how E-cadherin instructs the assembly of the LGN/NuMA complex at cell-cell contacts, and define a mechanism that couples cell division orientation to intercellular adhesion."}, {"pubmed": 28347229, "text": "high expression of G-protein signaling modulator 2 was involved in the pathological processes of hepatocellular carcinoma through activation of the phosphatidylinositol 3-kinase/protein kinase B signaling pathway, which may provide an attractive potential diagnostic biomarker and therapeutic target for treatment of hepatocellular carcinoma."}, {"pubmed": 28712573, "text": "Endothelial flow mechanotransduction through the junctional complex is mediated by a specific pool of VE-cadherin that is phosphorylated on cytoplasmic tyrosine Y658 and bound to LGN."}, {"pubmed": 29523789, "text": "In mammary stem cells, the asymmetric domain of Insc bound to LGN:Galphai(GDP) suffices to drive asymmetric fate, and reverts aberrant symmetric divisions induced by p53 loss."}, {"pubmed": 31101817, "text": "Study reports the crystal structure of NuMA:LGN hetero-hexamers, and unveil their role in promoting the assembly of active cortical dynein/dynactin motors that are required in orchestrating oriented divisions in polarized cells."}, {"pubmed": 31732560, "text": "LGN has a role in cell cortex recruitment of NuMA"}, {"pubmed": 32020211, "text": "Downregulation of GPSM2 is associated with primary resistance to paclitaxel in breast cancer."}, {"pubmed": 32058048, "text": "Loss of G-protein-signaling modulator 2 accelerates proliferation of lung adenocarcinoma via EGFR signaling pathway."}, {"pubmed": 32812493, "text": "GPSM2 Serves as an Independent Prognostic Biomarker for Liver Cancer Survival."}, {"pubmed": 35940821, "text": "Truncated isoforms of GPSM2 containing the GoLoco motif region promote CD4(+) T-cell migration in SLE."}, {"pubmed": 38674408, "text": "G-Protein Signaling Modulator 2 as a Potential Biomarker in Colorectal Cancer: Integrative Analysis Using Genetic Profiling and Pan-Cancer Studies."}], "genomic_pos": {"chr": "1", "end": 108934545, "ensemblgene": "ENSG00000121957", "start": 108875350, "strand": 1}, "genomic_pos_hg19": {"chr": "1", "end": 109477167, "start": 109417972, "strand": 1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0000132", "qualifier": "involved_in", "term": "establishment of mitotic spindle orientation"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0000132", "pubmed": [22327364, 23870127], "qualifier": "involved_in", "term": "establishment of mitotic spindle orientation"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0007052", "pubmed": 21816348, "qualifier": "involved_in", "term": "mitotic spindle organization"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0007186", "pubmed": 8973305, "qualifier": "involved_in", "term": "G protein-coupled receptor signaling pathway"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0051301", "qualifier": "involved_in", "term": "cell division"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0051661", "pubmed": 21816348, "qualifier": "involved_in", "term": "maintenance of centrosome location"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0060236", "pubmed": 23027904, "qualifier": "involved_in", "term": "regulation of mitotic spindle organization"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:1904778", "pubmed": [22327364, 23027904, 23870127], "qualifier": "involved_in", "term": "positive regulation of protein localization to cell cortex"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:1905832", "pubmed": 11781568, "qualifier": "involved_in", "term": "positive regulation of spindle assembly"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0005737", "pubmed": 11781568, "qualifier": "located_in", "term": "cytoplasm"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005813", "qualifier": "located_in", "term": "centrosome"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "IBA"