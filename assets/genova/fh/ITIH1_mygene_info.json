position": [[52811614, 52811748], [52812012, 52812033], [52812355, 52812522], [52812957, 52813062], [52813447, 52813610], [52814284, 52814398], [52815955, 52816081], [52816166, 52816283], [52816972, 52817141], [52817229, 52817355], [52818311, 52818493], [52819059, 52819245], [52820310, 52820450], [52820960, 52821115], [52821203, 52821218], [52821607, 52821628], [52822001, 52822082], [52822247, 52822361], [52823668, 52823870], [52824764, 52824937], [52825532, 52825644], [52825797, 52826078]], "strand": 1, "transcript": "NM_002215", "txend": 52826078, "txstart": 52811614}], "generif": [{"pubmed": 11807786, "text": "Transfection into H460M Human large cell lung carcinoma line decreased tumor metastasis in nude mice"}, {"pubmed": 17675295, "text": "PTX3 directly interacts with inter-alpha-trypsin inhibitor and has a role in hyaluronan organization and cumulus oophorus expansion"}, {"pubmed": 18226209, "text": "The first systematic analysis on the differential expression of ITIH genes in human cancer, showing frequent downregulation that may be associated with initiation and/or progression of these malignancies."}, {"pubmed": 19367581, "text": "Observational study of gene-disease association and gene-gene interaction. (HuGE Navigator)"}, {"pubmed": 19416921, "text": "Observational study, meta-analysis, and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 21939789, "text": "Human inter-alpha-inhibitor is a substrate for factor XIIIa and tissue transglutaminase."}, {"pubmed": 24005673, "text": "Data indicate that TSG-6-mediated cross-linking of hyaluronan (HA) films is impaired in the presence of inter-alpha-inhibitor (IalphaI) and that this effect suppresses the TSG-6-mediated enhancement of HA binding to CD44-positive cells."}, {"pubmed": 24389398, "text": "The results of this study revealed that the shared genetic risk factors of both schizophrenia and major depressive disorder exist in ITIH family genes in the Han Chinese population."}, {"pubmed": 26728454, "text": "Data suggest that Mg2+ or Mn2+ (but not Ca2+) induce a conformational change in inter-alpha-inhibitor (ITIH1 and ITIH2) and a bikunin/chondroitin sulfate-dependent increase in thermodynamic stability; bikunin binds adjacent to the two heavy chains."}, {"pubmed": 27679489, "text": "Furthermore, thrombin treatment of the inflammatory matrix leads to dissolution of HC-HA cable structures and abolishes leukocyte adhesion. These data establish a novel mechanism whereby thrombin cleavage of HC1 regulates the adhesive properties of an inflammatory HA matrix."}, {"pubmed": 31484722, "text": "Both IalphaI and the HC-hyaluronan complex are substrates for the extracellular matrix proteases ADAMTS-5 and matrix metalloprotease (MMP) -3, -7, and -13."}, {"pubmed": 33744857, "text": "Pan-cancer analysis identifies ITIH1 as a novel prognostic indicator for hepatocellular carcinoma."}, {"pubmed": 35080113, "text": "Lysine demethylase 5C epigenetically reduces transcription of ITIH1 that results in augmented progression of liver hepatocellular carcinoma."}], "genomic_pos": {"chr": "3", "end": 52792068, "ensemblgene": "ENSG00000055957", "start": 52777595, "strand": 1}, "genomic_pos_hg19": {"chr": "3", "end": 52826078, "start": 52811603, "strand": 1}, "go": {"BP": {"evidence": "IEA", "gocategory": "BP", "id": "GO:0030212", "qualifier": "involved_in", "term": "hyaluronan metabolic process"}, "CC": [{"evidence": "NAS", "gocategory": "CC", "id": "GO:0005576", "pubmed": 14718574, "qualifier": "located_in", "term": "extracellular region"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0062023", "pubmed": [25037231, 28675934], "qualifier": "located_in", "term": "collagen-containing extracellular matrix"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0070062", "pubmed": 23533145, "qualifier": "located_in", "term": "extracellular exosome"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0072562", "pubmed": 22516433, "qualifier": "located_in", 