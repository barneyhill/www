, "generif": [{"pubmed": 17175032, "text": "Observational study and meta-analysis of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19081134, "text": "Results suggest that Hemokinin-1 is regulated by a unique transcription regulation system that likely governs its differential expression pattern and suggests a role for Hemokinin-1 distinct from Substance P."}, {"pubmed": 20580442, "text": "A possible association is found between a tachykinin-4 single nucleotide polymorphism and susceptibility to asthma."}, {"pubmed": 20580442, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20929541, "text": "Endogenous expression of hemokinin-1 is demonstrated in bronchi and is involved in contraction of airway explants."}, {"pubmed": 21168392, "text": "hHK-1 and its C-terminal fragments are human NK1 receptor agonists with different functional selectivity properties and that such functional selectivity leads to differential activation of downstream signaling and receptor trafficking."}, {"pubmed": 21342363, "text": "Data provide evidence for the first time that hemokinin-1, like substance P, may be involved in the pathophysiology of inflammatory bowel disease."}, {"pubmed": 21368235, "text": "HK-1 and substance P are novel T helper (Th)17 cell-inducing factors that may act locally on memory T cells to amplify inflammatory responses."}, {"pubmed": 22554585, "text": "HKs emerge as pivotal endogenous regulators of angiogenesis through neurokinin-1 receptor"}, {"pubmed": 27146034, "text": "expressed in mural granulosa and cumulus cells"}, {"pubmed": 27456549, "text": "Expression substance P/neurokinin A/hemokinin-1 and their preferred neurokinin 1/neurokinin 2 receptors are dysregulated in uterine leiomyomata."}, {"pubmed": 27458061, "text": "Kinase activation led to increased MMP-2 and MT1-MMP expression and melanoma cell migration induced by hHK-1. Thus, hHK-1 and the NK1 receptor are critical to melanoma cell migration and each may be a promising chemotherapeutic target"}, {"pubmed": 32600668, "text": "Hemokinin-1 and substance P stimulate production of inflammatory cytokines and chemokines in human colonic mucosa via both NK1 and NK2 tachykinin receptors."}, {"pubmed": 34090787, "text": "Serum level of hemokinin-1 is significantly lower in patients with chronic spontaneous urticaria than in healthy subjects."}], "genomic_pos": {"chr": "17", "end": 49848069, "ensemblgene": "ENSG00000176358", "start": 49838300, "strand": -1}, "genomic_pos_hg19": {"chr": "17", "end": 47925379, "start": 47915671, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0006954", "qualifier": "involved_in", "term": "inflammatory response"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007204", "qualifier": "involved_in", "term": "positive regulation of cytosolic calcium ion concentration"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007217", "qualifier": "involved_in", "term": "tachykinin receptor signaling pathway"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0008217", "qualifier": "involved_in", "term": "regulation of blood pressure"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:1902093", "pubmed": 17437961, "qualifier": "involved_in", "term": "positive regulation of flagellated sperm motility"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005615", "qualifier": "is_active_in", "term": "extracellular space"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005615", "pubmed": 17437961, "qualifier": "located_in", "term": "extracellular space"}], "MF": {"category": "MF", "evidence": "IBA", "id": "GO:0031835", "qualifier": "enables", "term": "substance P receptor binding"}}, "homologene": {"genes": [[9598, 104004507], [9606, 255061]], "id": 89219}, "interpro": {"desc": "Tachykinin/Neurokinin-like, conserved site", "id": "IPR013055", "short_desc": "Tachy_Neuro_lke_CS"}, "ipi": ["IPI00384256", "IPI00384257", "IPI00384258", "IPI00791814", "IPI00816266",