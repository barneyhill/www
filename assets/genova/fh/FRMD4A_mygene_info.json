: [{"pubmed": 16385451, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 22006218, "text": "A single nucleotide polymorphism in the FERM domain containing 4A protein is associated with nicotine dependence."}, {"pubmed": 22430674, "text": "data suggest that FRMD4A could be a relevant candidate gene for AD risk."}, {"pubmed": 22564525, "text": "findings suggest FRMD4A as a novel candidate therapeutic target in HNSCC based on the key role in metastatic growth we have identified"}, {"pubmed": 25388005, "text": "novel microcephaly, intellectual disability and dysmorphism syndrome is associated with a mutation in FRMD4A."}, {"pubmed": 27044754, "text": "FRMD4A RNAi or inhibition of cytohesins strongly upregulated secretion of endogenous tau. These results suggest that FRMD4A, a genetic risk factor for late-onset Alzheimer's disease, regulates tau secretion by activating cytohesin-Arf6 signaling."}, {"pubmed": 27666346, "text": "Findings suggest that FRMD4A expression correlates with the development of tongue squamous cell carcinoma."}, {"pubmed": 35312514, "text": "Polymorphisms in the FRMD4A Gene Are Associated With Chronic Obstructive Pulmonary Disease Susceptibility in a Latin American Population.\", trans \"Polimorfismos en el gen FRMD4A se asocian a riesgo de enfermedad pulmonar obstructiva cronica en poblacion latinoamericana."}], "genomic_pos": {"chr": "10", "end": 14462142, "ensemblgene": "ENSG00000151474", "start": 13643706, "strand": -1}, "genomic_pos_hg19": {"chr": "10", "end": 14504141, "start": 13685706, "strand": -1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0050709", "qualifier": "involved_in", "term": "negative regulation of protein secretion"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0050714", "pubmed": 27044754, "qualifier": "involved_in", "term": "positive 