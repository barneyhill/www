, "cds_start": 34972879, "n_exons": 8, "nonpsych": {"exp_lof": 10.8927910665, "exp_mis": 87.3145857514, "exp_syn": 41.0539675321, "lof_z": 1.45592512600871, "mis_z": 1.04256652555847, "mu_lof": 1.13415131041e-06, "mu_mis": 2.57925247121e-05, "mu_syn": 1.35772022446e-05, "n_lof": 6.0, "n_mis": 68.0, "n_syn": 42.0, "p_li": 0.00187313125294067, "p_null": 0.110654676067479, "p_rec": 0.88747219267958, "syn_z": -0.0929588724580772}, "nontcga": {"exp_lof": 11.1922796434, "exp_mis": 88.8724546058, "exp_syn": 41.8500975158, "lof_z": 1.82588121348607, "mis_z": 0.733127628373965, "mu_lof": 1.13415131041e-06, "mu_mis": 2.57925247121e-05, "mu_syn": 1.35772022446e-05, "n_lof": 5.0, "n_mis": 75.0, "n_syn": 43.0, "p_li": 0.0105641502545816, "p_null": 0.04360296588694, "p_rec": 0.945832883858478, "syn_z": -0.112547043247063}, "transcript": "ENST00000590071.2"}, "exons": [{"cdsend": 34500269, "cdsstart": 34481974, "chr": "19", "position": [[34481757, 34482641], [34490375, 34490477], [34493036, 34493104], [34493262, 34493325], [34493491, 34493622], [34494585, 34494637], [34495702, 34495771], [34500128, 34512304]], "strand": 1, "transcript": "NM_001080436", "txend": 34512304, "txstart": 34481757}], "exons_hg19": [{"cdsend": 34991174, "cdsstart": 34972879, "chr": "19", "position": [[34972662, 34973546], [34981280, 34981382], [34983941, 34984009], [34984167, 34984230], [34984396, 34984527], [34985490, 34985542], [34986607, 34986676], [34991033, 35003209]], "strand": 1, "transcript": "NM_001080436", "txend": 35003209, "txstart": 34972662}], "generif": [{"pubmed": 14736876, "text": "WTIP monitors slit diaphragm protein assembly as part of a multiple protein complex, linking this specialized adhesion junction to the actin cytoskeleton, and shuttles into the nucleus after podocyte injury"}, {"pubmed": 18331720, "text": "WTIP is a member of the Ajuba family of LIM-domain containing proteins. Ajuba LIM proteins co-repress E-cadherin expression through interaction with the SNAG domain of Snail/Slug and promote epithelial to mesenchymal differentiation in a Xenopus model."}, {"pubmed": 19785987, "text": "Full-length Ror2 recruits Wtip to the cell membrane, a mutant involved in human disease fails to do so. Both genes and proteins show overlapping expression in the mouse embryo."}, {"pubmed": 20086015, "text": "Stress signaling pathways initiate WTIP nuclear translocation, and the concomitant loss of WTIP from cell contacts changes podocyte morphology and dynamic actin assembly, suggesting a mechanism that transmits changes in podocyte morphology to the nucleus."}, {"pubmed": 21686224, "text": "Report role of WTIP in regulating podocyte phenotype in chronic kidney disease."}, {"pubmed": 21900451, "text": "Wtip regulates the stable formation of cell adhesions to both extracellular matrix and neighboring cells and suggest that Wtip is necessary for normal glomerular filtration barrier function"}, {"pubmed": 25065743, "text": "WTIP interacts with ASXL2 and blocks ASXL2-mediated activation of retinoic acid signaling."}, {"pubmed": 27033705, "text": "Nck1 and Nck2 Interact with WTIP. Nck1/2 integrates nephrin with the Hippo kinase cascade through association with the adaptor protein WTIP."}, {"pubmed": 27535760, "text": "WTIP interacts with BRCA2 and might be responsible for BRCA2 centrosome localization in cervical cancer cell."}, {"pubmed": 29440237, "text": "We show that all three mammalian Ajuba family proteins - AJUBA, LIMD1 and WTIP - exhibit tension-dependent localization to adherens junctions, and that both LATS family proteins, LATS1 and LATS2, exhibit an overlapping tension-dependent junctional localization"}, {"pubmed": 30179602, "text": "The findings suggest that the UBA2-WTIP fusion is an oncogenic fusion gene, as well as a promising therapeutic target for the treatment of acute myeloid leukemia."}, {"pubmed": 30690883, "text": "WTIP impairs AKT phosphorylation and activation, leading to enhanced expression and transcriptional activity of FOXO1, which further increases p21Cip1 and p27Kip1, and decreases cyclin D1, which consequently results in cell cycle arrest."}, {"pubmed": 33108271, "text": "The S-phase-induced lncRNA SUNO1 promotes cell proliferation by controlling YAP1/Hippo signaling pathway."}, {"pubmed": 34930905, "text": "WTIP upregulates FOXO3a and induces apoptosis through PUMA in acute myeloid leukemia."}, {"pubmed": 35671065, "text": "Wnt Signaling Interactor WTIP (Wilms Tumor Interacting Protein) Underlies Novel Mechanism for Cardiac Hypertrophy."}], "genomic_pos": {"chr": "19", "end": 34512304, "ensemblgene": "ENSG00000142279", "start": 34481758, "strand": 1}, "genomic_pos_hg19": {"chr": "19", "end": 34997258, "start": 34971874, "strand": 1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0001666", "qualifier": "involved_in", "term": "response to hypoxia"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0001666", "pubmed": 22286099, "qualifier": "involved_in", "term": "response to hypoxia"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006355", "qualifier": "involved_in", "term": "regulation of DNA-templated transcription"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007010", "qualifier": "involved_in", "term": "cytoskeleton organization"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0007010", "pubmed": 21834987, "qualifier": "involved_in", "term": "cytoskeleton organization"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0022604", "pubmed": 21834987, "qualifier": "involved_in", "term": "regulation of cell morphogenesis"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0030030", "qualifier": "involved_in", "term": "cell projection organization"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0035195", "pubmed": 20616046, "qualifier": "acts_upstream_of_or_within", "term": "miRNA-mediated post-transcriptional gene silencing"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0035278", "pubmed": 20616046, "qualifier": "involved_in", "term": "miRN