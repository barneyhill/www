, "exons": [{"cdsend": 1172071, "cdsstart": 1167471, "chr": "4", "position": [[1166932, 1167656], [1170401, 1170576], [1170998, 1171190], [1171262, 1171486], [1171851, 1172074], [1172543, 1172679], [1173079, 1173211]], "strand": -1, "transcript": "NM_001128325", "txend": 1173211, "txstart": 1166932}, {"cdsend": 1172071, "cdsstart": 1167471, "chr": "4", "position": [[1166932, 1167656], [1170401, 1170576], [1170998, 1171190], [1171262, 1171486], [1171851, 1172074], [1172543, 1172684], [1179447, 1179541], [1208575, 1208962]], "strand": -1, "transcript": "NM_001199021", "txend": 1208962, "txstart": 1166932}, {"cdsend": 1172071, "cdsstart": 1167471, "chr": "4", "position": [[1166932, 1167656], [1170401, 1170576], [1170998, 1171190], [1171262, 1171486], [1171851, 1172074], [1172543, 1172583]], "strand": -1, "transcript": "NM_012445", "txend": 1172583, "txstart": 1166932}], "exons_hg19": [{"cdsend": 1165859, "cdsstart": 1161259, "chr": "4", "position": [[1160720, 1161444], [1164189, 1164364], [1164786, 1164978], [1165050, 1165274], [1165639, 1165862], [1166331, 1166467], [1166867, 1166999]], "strand": -1, "transcript": "NM_001128325", "txend": 1166999, "txstart": 1160720}, {"cdsend": 1165859, "cdsstart": 1161259, "chr": "4", "position": [[1160720, 1161444], [1164189, 1164364], [1164786, 1164978], [1165050, 1165274], [1165639, 1165862], [1166331, 1166472], [1173235, 1173329], [1202363, 1202750]], "strand": -1, "transcript": "NM_001199021", "txend": 1202750, "txstart": 1160720}, {"cdsend": 1165859, "cdsstart": 1161259, "chr": "4", "position": [[1160720, 1161444], [1164189, 1164364], [1164786, 1164978], [1165050, 1165274], [1165639, 1165862], [1166331, 1166371]], "strand": -1, "transcript": "NM_012445", "txend": 1166371, "txstart": 1160720}], "generif": [{"pubmed": 19153605, "text": "The structure of the F-spondin domain of mindin was determined at 1.8-A resolution."}, {"pubmed": 19903741, "text": "Spondin 2, which is regulated by T(3), has an important role in cell invasion, cell migration, and hepatoma tumor progression."}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 21098016, "text": "High Mindin is associated with diabetic nephropathy in type 2 diabetes."}, {"pubmed": 21632881, "text": "mindin serves as a novel mediator that protects against cardiac hypertrophy and the transition to heart failure by blocking AKT/GSK3beta and TGF-beta1-Smad signalling."}, {"pubmed": 22615945, "text": "Using tissue microarray by immunohistostaining, we found SPON2 to be over-expressed in prostate cancer (PCa). SPON2 staining was more intense in Gleason score sum 7-8 and in PCa patients with metastasis."}, {"pubmed": 25751394, "text": "Mindin protects against vascular hyperplasia by suppression of abnormal VSMC proliferation, migration and phenotypic switching in an AKT-dependent manner."}, {"pubmed": 25945835, "text": "Data suggest that spondin-2 could be an independent diagnostic and prognostic biomarker of colon cancer."}, {"pubmed": 26686083, "text": "SPON2 is a transcriptional target of the metastasis gene MACC1. SPON2 induces cell motility in vitro and CRC metastasis in mice. In patients, SPON2 serves as prognostic indicator for CRC metastasis and survival, and might represent a promising target for therapeutic approaches."}, {"pubmed": 28060752, "text": "Spondin-2 overexpression contributes to tumor aggressiveness and prognosis in gastric cancer."}, {"pubmed": 28991232, "text": "Egr-1 regulates mindin expression by directly binding to the mindin promoter; mindin suppresses colon cancer progression by blocking angiogenesis."}, {"pubmed": 29440144, "text": "Matricellular protein SPON2 acts as an HCC suppressor and utilizes distinct signaling events to perform dual functions in HCC microenvironment"}, {"pubmed": 30318967, "text": "High SPON2 expression is associated with hepatocellular carcinoma."}, {"pubmed": 30527359, "text": "spondin-2 might be a novel therapeutic target and prognostic biomarker for squamous cell carcinoma patients"}, {"pubmed": 31168562, "text": "Study identified the extracelular matrix protein mindin in the secretome of prostate adenocarcinoma cells and show that mindin overexpression in human and mouse TRAMP-C1-induced prostate tumors correlates with upregulated levels of bone-related genes in the tumorigenic prostate tissues. Mindin silencing decreased osteomimicry in adenocarcinoma cells and in the prostate tumor mice model."}, {"pubmed": 31691494, "text": "Spondin 2 promotes the proliferation, migration and invasion of gastric cancer cells."}, {"pubmed": 32793186, "text": "PIK3AP1 and SPON2 Genes Are Differentially Methylated in Patients With Periodic Fever, Aphthous Stomatitis, Pharyngitis, and Adenitis (PFAPA) Syndrome."}, {"pubmed": 32952742, "text": "Overexpression of Spondin-2 Is Associated with Recurrence-Free Survival in Patients with Localized Clear Cell Renal Cell Carcinoma."}, {"pubmed": 33998076, "text": "The clinical significance of spondin 2 eccentric expression in peripheral blood mononuclear cells in bronchial asthma."}, {"pubmed": 34183385, "text": "Cancer-associated Fibroblast-derived Spondin-2 Promotes Motility of Gastric Cancer Cells."}, {"pubmed": 34583750, "text": "Tumor cell-derived SPON2 promotes M2-polarized tumor-associated macrophage infiltration and cancer progression by activating PYK2 in CRC."}, {"pubmed": 36427776, "text": "SPON2 promotes the bone metastasis of lung adenocarcinoma via activation of the NF-kappaB signaling pathway."}, {"pubmed": 36484410, "text": "Prostate cancer cellderived spondin 2 boosts osteogenic factor levels in osteoblasts via the PI3K/AKT/mTOR pathway."}, {"pubmed": 37130106, "text": "In situ assessment of Mindin as a biomarker of podocyte lesions in diabetic nephropathy."}, {"pubmed": 37713832, "text": "Comprehensive analysis of the oncogenic and immunological role of SPON2 in human tumors."}], "genomic_pos": {"chr": "4", "end": 1208962