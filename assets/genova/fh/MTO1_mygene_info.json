74189567], [74189658, 74189849], [74190397, 74190528], [74190719, 74190839], [74191762, 74191967], [74192171, 74192343], [74201956, 74202075], [74207458, 74207619], [74210296, 74218959]], "strand": 1, "transcript": "NM_001123226", "txend": 74218959, "txstart": 74171459}, {"cdsend": 74210458, "cdsstart": 74171577, "chr": "6", "position": [[74171459, 74171794], [74175931, 74176131], [74176211, 74176329], [74183087, 74183377], [74189454, 74189567], [74189658, 74189849], [74190397, 74190528], [74191762, 74191967], [74192171, 74192343], [74201956, 74202075], [74207458, 74207619], [74210296, 74218959]], "strand": 1, "transcript": "NM_012123", "txend": 74218959, "txstart": 74171459}, {"cdsend": 74210458, "cdsstart": 74171577, "chr": "6", "position": [[74171459, 74171794], [74175931, 74176131], [74176211, 74176329], [74183087, 74183377], [74189454, 74189567], [74189658, 74189849], [74190015, 74190090], [74190397, 74190528], [74191762, 74191967], [74192171, 74192343], [74201956, 74202075], [74207458, 74207619], [74210296, 74218959]], "strand": 1, "transcript": "NM_133645", "txend": 74218959, "txstart": 74171459}], "generif": [{"pubmed": 12011058, "text": "identification of full-length cDNA and elucidation of genomic organization of the human MTO1 homolog"}, {"pubmed": 15542390, "text": "Phenotype of non-syndromic deafness associated with the mitochondrial A1555G mutation is modulated by mitochondrial RNA modifying enzymes MTO1 and GTPBP3."}, {"pubmed": 18391568, "text": "proposed linkage in chromosome 8 and the association with TRMU and MTO1 genes were studied in A1555G deafness"}, {"pubmed": 19209188, "text": "Meta-analysis of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19338775, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20877624, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 22608499, "text": "MTO1 mutations have roles in hypertrophic cardiomyopathy and lactic acidosis"}, {"pubmed": 23929671, "text": "MTO1 mutations are commonly associated with a presentation of hypertrophic cardiomyopathy, lactic acidosis, and MRC deficiency."}, {"pubmed": 24160266, "text": "Nuclear-encoded mitochondrial MTO1 and MRPL41 are regulated in an opposite epigenetic mode based on estrogen receptor status in breast cancer."}, {"pubmed": 25149473, "text": "Results show that post-transcriptional expression of GTPBP3, MTO1 and TRMU genes is down-regulated, leading to mt-tRNA hypomodification and contributing to mitochondrial dysfunction in MELAS cybrids."}, {"pubmed": 25552653, "text": "MTO1 mediates tRNA modification and controls mitochondrial translation rate in a highly tissue-specific manner associated with tissue-specific oxidative phosphorylation defects."}, {"pubmed": 26061759, "text": "Optic neuropathy, cardiomyopathy, and cognitive disability was found in patients with a homozygous mutation in the nuclear MTO1 and a mitochondrial MT-TF variant."}, {"pubmed": 27256614, "text": "Whole-exome sequencing (WES) indicated a double homozygous mutation in the mitochondrial tRNA translation optimization 1 protein (MTO1) gene (p.R504C and p.V557M) in two of the patients and the homozygous mutation p.R504C in the other."}, {"pubmed": 28740091, "text": "Low MTO1 expression is associated with Mitochondrial diseases in neoplasms."}, {"pubmed": 29331171, "text": "MTO1 deficiency is lethal in some but not all cases, and a genotype-phenotype relation is suggested. Aside from lactic acidosis and cardiomyopathy, developmental delay and other phenotypic features affecting multiple organ systems are often present in these patients, suggesting a broader spectrum than hitherto reported."}, {"pubmed": 29348686, "text": "Defects in the mitochondrial tRNA modification enzymes MTO1 and GTPBP3 promote different metabolic reprogramming through a HIF-PPARgamma-UCP2-AMPK axis."}, {"pubmed": 29440775, "text": "Two novel variants (c.[253G > A];[938G > A]) in the MTO1 gene were identified in a child with cardiomyopathy with early-onset brain disease."}, {"pubmed": 31148365, "text": "CircMTO1 inhibits liver fibrosis via regulation of miR-17-5p and Smad7."}, {"pubmed": 31842146, "text": "Novel Mitochondrial Translation Optimizer-1 Mutations as a Cause of Hereditary Optic Neuropathy."}, {"pubmed": 32731816, "text": "Circular RNA MTO1 Inhibits the Proliferation and Invasion of Ovarian Cancer Cells Through the miR-182-5p/KLF15 Axis."}, {"pubmed": 34238206, "text": "CircMTO1 inhibits ox-LDL-stimulated vascular smooth muscle cell proliferation and migration via regulating the miR-182-5p/RASA1 axis."}, {"pubmed": 34930906, "text": "CircMTO1 suppresses hepatocellular carcinoma progression via the miR-541-5p/ZIC1 axis by regulating Wnt/beta-catenin signaling pathway and epithelial-to-mesenchymal transition."}, {"pubmed": 34990597, "text": "Clinical and genetic analysis of combined oxidative phosphorylation defificiency-10 caused by MTO1 mutation."}], "genomic_pos": {"chr": "6", "end": 73509236, "ensemblgene": "ENSG00000135297", "start": 73461578, "strand": 1}, "genomic_pos_hg19": {"chr": "6", "end": 74218959, "start": 74171301, "strand": 1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0030488", "qualifier": "involved_in", "term": "tRNA methylation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0070899", "qualifier": "involved_in", "term": "mitochondrial tRNA wobble uridine modification"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005739", "qualifier": "is_active_in", "term": "mitochondrion"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005829", "qualifier": "is_active_in", "term": "cytosol"}], "MF": [{"category": "MF", "evidence": "HDA", "id": "GO:0003723", "pubmed": 22681889, "qualifier": "enables", "term": "RNA binding"}, {"category": "MF", "evidence": "IBA", "id": "GO:0050660", "qualifier": "enables", "term": "flavin adenine dinucleotide binding"}]}, "homologene": {"genes": [[3702, 815830], [4530, 43252