 [{"pubmed": 12820722, "text": "The stable transfection of MDA-MB-231 with an expression vector encoding ST6GalNAc I induces the expression of STn antigen at the cell surface."}, {"pubmed": 16319059, "text": "ST6GalNAc-I sialyltransferase localizes throughout the Golgi and has a role in synthesis of the tumor-associated sialyl-Tn O-glycan in human breast cancer"}, {"pubmed": 19287074, "text": "Expression of ppGalNAc-T6 is significantly higher in breast cancer compared to 'normal'/benign breast tissue samples. ST6GalNAc-I expression in breast cancer is associated with better prognosis."}, {"pubmed": 20378551, "text": "Golgi N-glycosyltransferases beta-1,2-N-acetylglucosaminyltransferase I, beta-1,2-N-acetylglucosaminyltransferase II, 1,4-galactosyltransferase I, and alpha-2,6-sialyltransferase I form both homo- and heterodimeric enzyme complexes in live cells"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 22228572, "text": "It was confirmed that MUC1 carries sialyl Tn also in human advanced gastric cancer tissues"}, {"pubmed": 24840470, "text": "Using qRT-PCR, sialyl-Tn expression was found to be associated with an increase in alpha2,6-sialyltransferase gene (ST6GALNAC1) and a decrease in core 1 synthase gene (C1GALT1) in LS174T cells."}, {"pubmed": 25532910, "text": "ST6GalNAc I regulates the gene expression of IGF-1 through STAT5b activation and plays role in gastric cancer invasion and metastasis."}, {"pubmed": 25860962, "text": "upregulated Siat7A expression, which was paralleled by the increased Klf4 in the ischemic myocardium, contributed to cardiomyocyte apoptosis following myocardial infarction"}, {"pubmed": 28035351, "text": "Our results indicated that ST6GALNAC1 was downregulated in sporadic esophageal squamous cell carcinoma via hyper-methylation and loss of heterozygosity , and it may be a candidate responsible gene for esophageal squamous cell carcinoma"}, {"pubmed": 30854566, "text": "Sialyltransferase7A promotes angiotensin II-induced cardiomyocyte hypertrophy via HIF-1alpha-TAK1 signalling pathway."}, {"pubmed": 31831633, "text": "Cross-talk between Colon Cells and Macrophages Increases ST6GALNAC1 and MUC1-sTn Expression in Ulcerative Colitis and Colitis-Associated Colon Cancer."}, {"pubmed": 33792183, "text": "ST6GalNAc-I promotes lung cancer metastasis by altering MUC5AC sialylation."}, {"pubmed": 35303419, "text": "Mucus sialylation determines intestinal host-commensal homeostasis."}], "genomic_pos": {"chr": "17", "end": 76643786, "ensemblgene": "ENSG00000070526", "start": 76624761, "strand": -1}, "genomic_pos_hg19": {"chr": "17", "end": 74639920, "start": 74620843, "strand": -1}, "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0006486", "pubmed": 16319059, "qualifier": "involved_in", "term": "protein glycosylation"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0006486", "qualifier": "involved_in", "term": "protein glycosylation"}, {"evidence": "NAS", "gocategory": "BP", "id": "GO:0006486", "pubmed": 10536037, "qualifier": "involved_in", "term": "protein glycosylation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0009312", "qualifier": "involved_in", "term": "oligosaccharide biosynthetic process"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0048874", "qualifier": "involved_in", "term": "host-mediated regulation of intestinal microbiota composition"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0000139", "pubmed": [16319059, 35303419], "qualifier": "is_active_in", "term": "Golgi membrane"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0000139", "qualifier": "located_in", "term": "Golgi membrane"}], "MF": [{"category": "MF", "evidence": "IBA", "id": "GO:0001665", "qualifier": "enables", "term": "alpha-N-acetylgalactosaminide alpha-2,6-sialyltransferase activity"}, {"category": "MF", "evidence": "IDA", "id": "GO:0001665", "pubmed": [16319059, 35303419], "qualifier":