ul 2008].", "symbol": "MRPL52", "taxid": 9606, "type_of_gene": "protein-coding", "umls": {"cui": "C1424470"}, "unigene": "Hs.355935", "uniprot": {"Swiss-Prot": "Q86TS9", "TrEMBL": ["G3V4H9", "G5E9P5", "G3V3U6", "G3V4J1"]}}