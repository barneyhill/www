281, 65074404], [65077867, 65077964], [65082535, 65082709], [65083638, 65087004]], "strand": 1, "transcript": "NM_015147", "txend": 65087004, "txstart": 65056415}, {"cdsend": 65087004, "cdsstart": 65087004, "chr": "2", "position": [[65056415, 65056528], [65069398, 65069801], [65071453, 65072980], [65074281, 65074404], [65082535, 65082709], [65083638, 65087004]], "strand": 1, "transcript": "NR_134966", "txend": 65087004, "txstart": 65056415}], "exons_hg19": [{"cdsend": 65309839, "cdsstart": 65296578, "chr": "2", "position": [[65283549, 65283658], [65296532, 65296935], [65298587, 65300114], [65301415, 65301538], [65305001, 65305098], [65309669, 65309843], [65310772, 65314138]], "strand": 1, "transcript": "NM_001319100", "txend": 65314138, "txstart": 65283549}, {"cdsend": 65309839, "cdsstart": 65296578, "chr": "2", "position": [[65283549, 65283662], [65296532, 65296935], [65298587, 65299703], [65301415, 65301538], [65305001, 65305098], [65309669, 65309843], [65310772, 65314138]], "strand": 1, "transcript": "NM_001319101", "txend": 65314138, "txstart": 65283549}, {"cdsend": 65309839, "cdsstart": 65296578, "chr": "2", "position": [[65283549, 65283662], [65296532, 65296935], [65298587, 65300114], [65301415, 65301538], [65305001, 65305098], [65309669, 65309843], [65310772, 65314138]], "strand": 1, "transcript": "NM_015147", "txend": 65314138, "txstart": 65283549}, {"cdsend": 65314138, "cdsstart": 65314138, "chr": "2", "position": [[65283549, 65283662], [65296532, 65296935], [65298587, 65300114], [65301415, 65301538], [65309669, 65309843], [65310772, 65314138]], "strand": 1, "transcript": "NR_134966", "txend": 65314138, "txstart": 65283549}], "generif": [{"pubmed": 17570516, "text": "our study is the first to identify and validate Endofin, DCBLD2, and KIAA0582 as part of a complex EGF phosphotyrosine signaling network"}, {"pubmed": 18042621, "text": "data suggest that Cep68 cooperates with rootletin and C-Nap1 in centrosome cohesion."}, {"pubmed": 21072201, "text": "Findings imply that CEP68 could be a susceptible gene for aspirin intolerance in asthmatics."}, {"pubmed": 21072201, "text": "Observational study and genome-wide association study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 24554434, "text": "Centlein complexes with C-Nap1 and Cep68 at the proximal ends of centrioles during interphase."}, {"pubmed": 24618698, "text": "CEP68 gene variants may play an important role in MNSAID-UA susceptibility and, despite the different regulatory mechanisms involved depending on the specific affected organ, in the development of hypersensitivity reactions to NSAIDs."}, {"pubmed": 25503564, "text": "Cep68 degradation allows Cep215 removal from peripheral pericentriolar material (PCM) preventing centriole separation following disengagement, PCNT cleavage mediates Cep215 removal from core of the PCM to inhibit centriole disengagement and duplication"}, {"pubmed": 25704143, "text": "C-terminal 300-400 amino acids of Cep68 are necessary to localize Cep68 to interphase centrosomes while C-terminal 400-500 amino acids might regulate Cep68 dissociation from centrosomes at mitotic onset."}, {"pubmed": 28089774, "text": "It has been proposed that the archetypal linker protein Rootletin maintains centrosome cohesion in part through inhibition of VHL-mediated Cep68 degradation."}, {"pubmed": 28578000, "text": "The Cep68 protein level needs to be fine-tuned in order to ensure that its direct interactors, such as the microcephaly protein Cep215 and PCNT, function properly."}, {"pubmed": 29463719, "text": "CEP68 is important in forming rootletin filaments that branch off centrioles and to modulate the thickness of rootletin fibers."}, {"pubmed": 30093714, "text": "Two intronic variants (rs2241160 and rs2241161) were significantly associated with an increased risk of single-NSAID-induced urticaria/angioedema or anaphylaxis (SNIUAA), suggesting CEP68 to be a key player in both types of Non-steroidal anti-inflammatory drugs hypersensitivity."}], "genomic_pos": {"chr": "2", "end": 65087004, "ensemblgene": "ENSG00000011523", "start": 65056354, "strand": 1}, "genomic_pos_hg19": {"chr": "2", "end": 65314138, "start": 65283500, "strand": 1}, "go": {"BP": [{"evidence": "IMP", "gocategory": "BP", "id": "GO:0007098", "pubmed": 18042621, "qualifier": "involved_in", "term": "centrosome cycle"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0010457", "pubmed": 24554434, "qualifier": "involved_in", "term": "centriole-centriole cohesion"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0033365", "pubmed": 24554434, "qualifier": "involved_in", "term": "protein localization to organelle"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005813", "pubmed": [18042621, 21399614, 31974111], "qualifier": "located_in", "term": "centrosome"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0030054", "qualifier": "located_in", "term": "cell junction"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0034451", "qualifier": "located_in", "term": "centriolar satellite"}], "MF": [{"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [25416956, 25503564, 35709258], "qualifier": "enables", "term": "protein binding"}, {"category": "MF", "evidence": "IPI", "id": "GO:0019901", "pubmed": 24554434, "qualifier": "enables", "term": "protein kinase binding"}, {"category": "MF", "evidence": "IPI", "id": "GO:0019904", "pubmed": 24554434, "qualifier": "enables", "term": "protein domain specific binding"}]}, "homologene": {"genes": [[9598, 459279], [9606, 23177], [9615, 481393], [9913, 540684], [10090, 216543], [10116, 289822]], "id": 65235}, "ipi": ["IPI00552753", "IPI00554739", "IPI00556642", "IPI01013575"], "map_location": "2