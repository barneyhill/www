, "position": [[20246164, 20250363], [20252781, 20253585], [20254836, 20255315], [20303739, 20303963]], "strand": -1, "transcript": "NM_021020", "txend": 20303963, "txstart": 20246164}], "exons_hg19": [{"cdsend": 20112692, "cdsstart": 20107232, "chr": "8", "position": [[20103675, 20107874], [20110292, 20111096], [20112347, 20112826], [20130518, 20131046]], "strand": -1, "transcript": "NM_001362884", "txend": 20131046, "txstart": 20103675}, {"cdsend": 20112692, "cdsstart": 20107232, "chr": "8", "position": [[20103675, 20107874], [20110292, 20111096], [20112347, 20112826], [20161250, 20161474]], "strand": -1, "transcript": "NM_021020", "txend": 20161474, "txstart": 20103675}], "generif": [{"pubmed": 12377406, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 12377406, "text": "variation in the germline sequence is associated with prostate cancer risk"}, {"pubmed": 12851677, "text": "Down-regulation of FEZ1/LZTS1 gene with frequent loss of heterozygosity is associated with oral squamous cell carcinomas"}, {"pubmed": 18559591, "text": "Forced expression of LZTS1 in metastasizing uveal melanoma cells inhibited their motility and invasion, whereas depletion of LZTS1 increased their motility."}, {"pubmed": 18686028, "text": "Down-regulation of tumor suppressor gene FEZ1/LZTS1 in breast carcinoma involves promoter methylation and associates with metastasis."}, {"pubmed": 19339270, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19885841, "text": "The reduction or loss of FEZ1 protein could be an aid to the clinical management of patients affected by ovarian carcinoma."}, {"pubmed": 20083228, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 21419475, "text": "Lower levels of leucine zipper putative tumor suppressor 1 correlated with high histologic grade, lymph node metastasis, and poor prognosis."}, {"pubmed": 23695671, "text": "Expression of miR-135b, LZTS1, LATS2 and nuclear TAZ predicts poor outcomes of non-small-cell lung cancer."}, {"pubmed": 24448468, "text": "Lzts1 was significantly downregulated in breast cancer samples and its deregulation was associated with a higher incidence of tumor recurrence, and to a worse overall survival."}, {"pubmed": 24802407, "text": "miR-214 functions as an onco-miRNA in osteosarcoma, and its oncogenic effects are mediated chiefly through downregulation of LZTS1"}, {"pubmed": 25667121, "text": "Suggest that LZTS1 plays a potential tumor suppressor role in colorectal cancer progression and represents a valuable clinical prognostic marker of this disease."}, {"pubmed": 25813822, "text": "LZTS1 promoter was frequently methylated in IMPC samples."}, {"pubmed": 25938461, "text": "miR-135b expression inversely correlated with LZTS1 staining intensity and the Cutaneous Squamous Cell Carcinoma grade."}, {"pubmed": 26504261, "text": "missense variant in the LZTS1 gene was identified in two Ehlers-Danlos syndrome patients in an extended family."}, {"pubmed": 26653561, "text": "Compared with the normal hepatocyte cells, LZTS1 expression in hepatocellular carcinoma cells was significantly lower"}, {"pubmed": 39023696, "text": "Unveiling the oncogenic role of LZTS1 in colorectal cancer."}], "genomic_pos": {"chr": "8", "end": 20303963, "ensemblgene": "ENSG00000061337", "start": 20246165, "strand": -1}, "genomic_pos_hg19": {"chr": "8", "end": 20161474, "start": 20103676, "strand": -1}, "go": {"BP": [{"evidence": "IMP", "gocategory": "BP", "id": "GO:0016242", "qualifier": "involved_in", "term": "negative regulation of macroautophagy"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0048167", "qualifier": "involved_in", "term": "regulation of synaptic plasticity"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0048814", "qualifier": "involved_in", "term": "regulation of dendrite morphogenesis"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0150052", 