cdsstart": 89183746, "chr": "4", "position": [[89178771, 89183878], [89186152, 89186287], [89189341, 89189486], [89189892, 89190058], [89198294, 89198395], [89199295, 89199794], [89205557, 89205713]], "strand": -1, "transcript": "NM_152542", "txend": 89205713, "txstart": 89178771}], "generif": [{"pubmed": 16685424, "text": "A novel member of human protein phosphatase 2C gene family named PP2Ckappa was isolated from a human fetal brain cDNA library."}, {"pubmed": 17336929, "text": "The cloned phosphatase has a mitochondrial leader sequence and its amino acid sequence places it in the PP2C family like two known mitochondrial phosphatases."}, {"pubmed": 18987618, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20877624, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 22291014, "text": "The 2.4 A PP2Cm structure reveals a central beta-sandwich with two bound metal ions in the active-site cleft."}, {"pubmed": 23446828, "text": "Overweight people with the C allele of the branched-chain amino acid/aromatic amino acid ratio-associated variant rs1440581 may benefit less in weight loss and improved insulin sensitivity than those without it on a energy-restricted high-fat diet."}, {"pubmed": 26592513, "text": "miR-204 and miR-211 directly repress PPM1K expression via 3'UTR."}, {"pubmed": 28768654, "text": "PPM1K rs1440581 may affect changes in glucose metabolism during weight loss, and this effect is dependent on dietary fat and carbohydrate intakes."}, {"pubmed": 29855804, "text": "Branched chain amino acids related genetic variant rs1440581 was associated with an increased risk of incident T2 Diabetes in a Chinese population. This variant might modify effect of weight gain on development in Insulin resistance."}, {"pubmed": 30451284, "text": "three Mg(2+) were located in the active site of the PPM1K N94K instead of two Mg(2+) in the PPM1K wild type"}, {"pubmed": 33964402, "text": "The role of PP5 and PP2C in cardiac health and disease."}, {"pubmed": 34382495, "text": "Effects of PPM1K rs1440581 and rs7678928 on serum branched-chain amino acid levels and risk of cardiovascular disease."}, {"pubmed": 36863088, "text": "PPM1K-regulated impaired catabolism of branched-chain amino acids orchestrates polycystic ovary syndrome."}], "genomic_pos": {"chr": "4", "end": 88284769, "ensemblgene": "ENSG00000163644", "start": 88257620, "strand": -1}, "genomic_pos_hg19": {"chr": "4", "end": 89205921, "start": 89178772, "strand": -1}, "go": {"BP": [{"evidence": "IMP", "gocategory": "BP", "id": "GO:0009083", "pubmed": 29779826, "qualifier": "involved_in", "term": "branched-chain amino acid catabolic process"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0035970", "qualifier": "involved_in", "term": "peptidyl-threonine dephosphorylation"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:1902108", "qualifier": "involved_in", "term": "regulation of mitochondrial membrane permeability involved in apoptotic process"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005739", "qualifier": "is_active_in", "term": "mitochondrion"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005739", "qualifier": "located_in", "term": "mitochondrion"}, {"evidence": "ISS", "gocategory": "CC", "id": "GO:0005759", "qualifier": "located_in", "term": "mitochondrial matrix"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005759", "qualifier": "located_in", "term": "mitochondrial matrix"}], "MF": [{"category": "MF", "evidence": "IBA", "id": "GO:0004722", "qualifier": "enables", "term": "protein serine/threonine phosphatase activity"}, {"category": "MF", "evidence": "IDA", "id": "GO:0004722", "pubmed": [17374715, 22291014, 29779826], "qualifier": "enables", "term": "protein serine/threonine phosphatase activity"}, {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [22291014, 25416956, 32296183], "qualifier": "enables", "term"