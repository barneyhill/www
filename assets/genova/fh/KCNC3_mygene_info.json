89586e-05, "n_lof": 1.0, "n_mis": 113.0, "n_syn": 113.0, "p_li": 0.815446636106206, "p_null": 0.00113957062535677, "p_rec": 0.183413793268438, "syn_z": 3.51097810046251}, "nontcga": {"exp_lof": 9.51520034012, "exp_mis": 331.423299944, "exp_syn": 193.013385229, "lof_z": 2.72312226774737, "mis_z": 6.11430100110695, "mu_lof": 1.0083292319e-06, "mu_mis": 4.08078765375e-05, "mu_syn": 2.43656989586e-05, "n_lof": 1.0, "n_mis": 108.0, "n_syn": 114.0, "p_li": 0.812956322325518, "p_null": 0.000970134989908976, "p_rec": 0.186073542684573, "syn_z": 3.60105123190001}, "transcript": "ENST00000477616.1"}, "exons": [{"cdsend": 50328854, "cdsstart": 50320245, "chr": "19", "position": [[50311939, 50316091], [50320222, 50320349], [50320592, 50320784], [50322974, 50324082], [50328212, 50329071]], "strand": -1, "transcript": "NM_001372305", "txend": 50329071, "txstart": 50311939}, {"cdsend": 50329082, "cdsstart": 50320245, "chr": "19", "position": [[50311939, 50316091], [50320222, 50320349], [50320592, 50320784], [50322974, 50324082], [50328212, 50329377]], "strand": -1, "transcript": "NM_004977", "txend": 50329377, "txstart": 50311939}, {"cdsend": 50333536, "cdsstart": 50333536, "chr": "19", "position": [[50311939, 50314990], [50316032, 50316091], [50320592, 50320784], [50333468, 50333536]], "strand": -1, "transcript": "NR_110912", "txend": 50333536, "txstart": 50311939}], "exons_hg19": [{"cdsend": 50832111, "cdsstart": 50823502, "chr": "19", "position": [[50815196, 50819348], [50823479, 50823606], [50823849, 50824041], [50826231, 50827339], [50831469, 50832328]], "strand": -1, "transcript": "NM_001372305", "txend": 50832328, "txstart": 50815196}, {"cdsend": 50832339, "cdsstart": 50823502, "chr": "19", "position": [[50815196, 50819348], [50823479, 50823606], [50823849, 50824041], [50826231, 50827339], [50831469, 50832634]], "strand": -1, "transcript": "NM_004977", "txend": 50832634, "txstart": 50815196}, {"cdsend": 50836793, "cdsstart": 50836793, "chr": "19", "position": [[50815196, 50818247], [50819289, 50819348], [50823849, 50824041], [50836725, 50836793]], "strand": -1, "transcript": "NR_110912", "txend": 50836793, "txstart": 50815196}], "generif": [{"pubmed": 15148151, "text": "Observational study of genotype prevalence. (HuGE Navigator)"}, {"pubmed": 16501573, "text": "results establish a role for KCNC3 in phenotypes ranging from developmental disorders to adult-onset neurodegeneration and suggest voltage-gated K+ channels as candidates for additional neurodegenerative diseases"}, {"pubmed": 18592334, "text": "Mutations in the voltage-gated potassium channel KCNC3 are causative for spinocerebellar ataxia 13."}, {"pubmed": 19953606, "text": "The p.Arg420His mutation, which results in a nonfunctional channel subunit, was recurrent and associated with late-onset progressive ataxia."}, {"pubmed": 21479265, "text": "Mutations in KCNC3 are a rare cause of spinocerebellar ataxia with a frequency of less than 1%."}, {"pubmed": 21543613, "text": "The spinocerebellar ataxia type 13 mutation of the KV3.3 gene specifically suppresses the excitability of Kv3.3-expressing, fast-spiking neurons in zebrafish"}, {"pubmed": 21827913, "text": "The KCNC3 mutation casued Spinocerebellar ataxia 13."}, {"pubmed": 22289912, "text": "Kv3.3 gating contributes significantly to an early age of onset in spinocerebellar ataxia type 13"}, {"pubmed": 23293936, "text": "no disease-related KCNC3 mutation was identified, suggesting that spinocerebellar ataxia type 13 is a rare form of SCA in mainland China"}, {"pubmed": 23734863, "text": "Data suggest that mutant forms of Kv3.3 (as seem in subjects with spinocerebellar ataxia-13) are unstable, are degraded through proteasomes at faster rates, and can be stabilized by a chemical chaperone."}, {"pubmed": 23912307, "text": "This study presented the results of a detailed neurological clinical and diagnostic testing on 21 mutation-positive members of a four-generation Filipino family to further define this disease, aiding diagnosis and prognosis."}, {"pubmed": 24116147, "text": "Data indicate that an autosomal dominant mutation in the gene encoding Kv3.3 has been identified in a large Filipino kindred manifesting as spinocerebellar ataxia type 13 (SCA13)."}, {"pubmed": 25152487, "text": "These results are specific to the KCNC3(R420H) allele and provide new insight into the molecular basis of disease manifestation in SCA13."}, {"pubmed": 25756792, "text": "Functional and in silico analysis identified at least one novel pathogenic mutation in KCNC3 that cause Spinocerebellar ataxia type 13 (SCA13) and two additionally potential ones."}, {"pubmed": 25981959, "text": "investigated using either targeted next generation sequencing or trio-based exome sequencing and were found to have mutations in three different genes, KCNC3, ITPR1 and SPTBN2"}, {"pubmed": 26442672, "text": "This review covers the localization and physiological function of Kv3.3 in the central nervous system and how the normal function of the channel is altered by the disease-causing mutations"}, {"pubmed": 26849432, "text": "The Kv channels, or at least Kv3.3, appear to be associated with cell differentiation"}, {"pubmed": 26997484, "text": "Kv3.3 regulates Arp2/3-dependent cortical actin nucleation mediated by Hax-1; resulting cortical actin structures interact with the channel's gating machinery to slow its inactivation rate during sustained membrane depolarizations; a mutation that leads to late-onset spinocerebellar ataxia type 13."}, {"pubmed": 28467418, "text": "results therefore confirm the KCNC3R423H allele as causative for SCA13, through a dominant negative effect on KCNC3WT and links with EGFR that account for dominant inheritance, congenital onset, and disease pathology"}, {"pubmed": 29949095, "text": "Study expanded the genotype-phenotype-pathophysiology repertoire of SCA13 by addition of a causative KCNC3 mutation, p.Pro583_Pro585del, its associated phenotype of profound spasticity, and the decreased inactivation rate of the mutant channel."}], "genomic_pos": {"chr": "19", "end": 50333515, "ensemblgene": "ENSG00000131398", "start": 50311937, "strand": -1}, "genomic_pos_hg19": {"chr": "19", "end": 50836772, "start": 50815194, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0001508", "qualifier": "involved_in", "term": "action potential"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0006813", "pubmed": [16501573, 21479265], "qualifier": "involved_in", "term": "potassium ion transport"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0030866", "pubmed": 26997484, "qualifier": "involved_in", "term": "cortical actin cytoskeleton organization"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0051260", "qualifier": "involved_in", "term": "protein homooligomerization"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0051262", "pubmed": 23734863, "qualifier": "involved_in", "term": "protein tetramerization"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0071805", "qualifier": "involved_in", "term": "potassium ion transmembrane transport"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0071805", "pubmed": [19953606, 21479265, 23734863, 25756792], "qualifier": "involved_in", "term": "potassium ion transmembrane transport"}], "CC": [{"evidence": "IEA", "gocategory": "CC", "id": "GO:0005856", "qualifier": "located_in", "term": "cytoskeleton"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005886", "pubmed": [23734863, 25152487, 25756792], "qualifier": "located_in", "term": "plasma membrane"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005886", "qualifier": "located_in", "term": "plasma membrane"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005938", "pubmed": 26997484, "qualifier": "located_in", "term": "cell cortex"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0008076", "qualifier": "part_of", "term": "voltage-gated potassium channel complex"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0008076", "pubm