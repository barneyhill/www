2566660, 22566713], [22566821, 22566868], [22567060, 22567175], [22569147, 22569273], [22570909, 22571221], [22571717, 22571821], [22572339, 22572446], [22574666, 22575788]], "strand": 1, "transcript": "NM_005775", "txend": 22575788, "txstart": 22551926}], "exons_hg19": [{"cdsend": 22432241, "cdsstart": 22423933, "chr": "8", "position": [[22423241, 22423385], [22423857, 22423997], [22424173, 22424226], [22424334, 22424381], [22424573, 22424688], [22426660, 22426786], [22428422, 22428734], [22429230, 22429334], [22429852, 22429959], [22432179, 22433301]], "strand": 1, "transcript": "NM_001018003", "txend": 22433301, "txstart": 22423241}, {"cdsend": 22432241, "cdsstart": 22412019, "chr": "8", "position": [[22409439, 22409535], [22411964, 22412121], [22412375, 22412493], [22414227, 22414421], [22415641, 22415705], [22418847, 22418886], [22419377, 22419444], [22421499, 22421590], [22421795, 22421882], [22421980, 22422034], [22422780, 22422867], [22423338, 22423385], [22423857, 22423997], [22424173, 22424226], [22424334, 22424381], [22424573, 22424688], [22426660, 22426786], [22428422, 22428734], [22429230, 22429334], [22429852, 22429959], [22432179, 22433301]], "strand": 1, "transcript": "NM_005775", "txend": 22433301, "txstart": 22409439}], "generif": [{"pubmed": 12510380, "text": "regulates cytoskeletal organization and signal transduction--review"}, {"pubmed": 15184391, "text": "vinexin is a novel substrate of ERK2 and may play roles in ERK-dependent cell regulation"}, {"pubmed": 15734736, "text": "phosphorylation of the AF-1 domain controls RARgamma-mediated transcription through triggering the dissociation of vinexin beta"}, {"pubmed": 16923119, "text": "These results suggest that vinexin beta plays a role in maintaining the phosphorylation of EGFR on the plasma membrane through the regulation of c-Cbl."}, {"pubmed": 17486060, "text": "Vinexin is enriched at the leading edge of migrating cells, lamellipodia and and focal adhesions in well-spread cells."}, {"pubmed": 19294487, "text": "Rhotekin forms a complex with vinexin and may play a role at focal adhesions."}, {"pubmed": 20361963, "text": "Vinexin knockdown using siRNA delayed migration of both HaCaT human keratinocytes and A431 epidermoid carcinoma cells in scratch assay but did not affect cell proliferation."}, {"pubmed": 25824575, "text": "Our findings revealed that Vinexin-beta acts as a novel modulator of ischaemic injury"}, {"pubmed": 25972535, "text": "Taken together, the findings suggest that vinexin beta modulates NS5A phosphorylation via its interaction with NS5A, thereby regulating hepatitis C virus replication, implicating vinexin beta in the viral life cycle."}, {"pubmed": 27311882, "text": "The chromosome 8p tumor suppressor genes SORBS3 and SH2D4A are physically and functionally linked and provide a molecular mechanism of inhibiting STAT3-mediated IL-6 signaling in hepatocellular carcinoma cells."}, {"pubmed": 27437034, "text": "Study identified possible epigenetic influence on differential gene expression in SORBS3 under obese conditions."}, {"pubmed": 28209562, "text": "Vinexin beta deficiency attenuates atherogenesis primarily by suppressing vascular inflammation and inactivating Akt-nuclear factor kappaB signaling. Our data suggest that vinexin beta could be a therapeutic target for the treatment of atherosclerosis."}, {"pubmed": 34848853, "text": "Vinexin contributes to autophagic decline in brain ageing across species."}, {"pubmed": 35822241, "text": "Increased SORBS3 expression in brain ageing contributes to autophagic decline via YAP1-WWTR1/TAZ signaling."}], "genomic_pos": {"chr": "8", "end": 22575788, "ensemblgene": "ENSG00000120896", "start": 22544986, "strand": 1}, "genomic_pos_hg19": {"chr": "8", "end": 22433301, "start": 22402499, "strand": 1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0000122", "qualifier": "involved_in", "term": "negative regulation of transcription by RNA polymerase II"}, {"evidence": "IEA", "gocategor