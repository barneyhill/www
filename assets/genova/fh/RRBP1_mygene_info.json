[{"pubmed": 15184079, "text": "Results identify the ribosome receptor, p180, as a binding partner of the kinesin heavy chain isoform KIF5B."}, {"pubmed": 17634287, "text": "These data suggest that p180 mediates interactions between the endoplasmic reticulum and microtubules mainly through the novel microtubule-binding and -bundling domain MTB-1."}, {"pubmed": 18083570, "text": "over-expression of p180R failed to increase secretory pathway proteins calnexin, SEC61beta, and calreticulin, or ribosome biogenesis"}, {"pubmed": 19037105, "text": "results obtained demonstrate that p180 is both necessary & sufficient to induce a secretory phenotype in mammalian cells; findings support a central role for p180 in the terminal differentiation of secretory cells & tissues"}, {"pubmed": 19425502, "text": "the discovery overexpression of GPD1 and RRBP1 proteins and lack of expression for HNRNPH1 and SERPINB6 proteins which are new candidate biomarkers of colon cancer."}, {"pubmed": 19932094, "text": "A novel function of p180-abundant endoplasmic reticulum on the trans-Golgi network expansion, both of which are highly developed in various professional secretory cells."}, {"pubmed": 20647306, "text": "p180 plays crucial roles in enhancing collagen biosynthesis at the entry site of the secretory compartments by a novel mechanism that mainly involves facilitating ribosome association on the ER."}, {"pubmed": 21111237, "text": "p180 is concentrated in ER sheets by interacting with polysomes and may play a role in ER morphology."}, {"pubmed": 22679391, "text": "p180 promotes the ribosome-independent localization of a subset of mRNA to the endoplasmic reticulum."}, {"pubmed": 23318453, "text": "RRBP1 could alleviate ER stress and help cancer cell survive"}, {"pubmed": 24019514, "text": "Data indicate that p180 is required for the efficient targeting of placental alkaline phosphatase (ALPP) mRNA to the endoplasmic reticulum (ER)."}, {"pubmed": 26196185, "text": "High RRBP1 expression facilitates colorectal cancer progression and predicts an unfavourable post-operative prognosis."}, {"pubmed": 30684972, "text": "RRBP1 expression was an independent prognostic factor for overall survival (OS) and disease-free survival (DFS) in patients with endometrial carcinoma (EC) (both P < 0.05)."}, {"pubmed": 31285390, "text": "Expression of RRBP1 in epithelial ovarian cancer and its clinical significance."}, {"pubmed": 32289433, "text": "we found that E2F1 could bind to the RRBP1 promoter and promote the transcription of RRBP1, and the E2F1/RRBP1 axis played an important role in the process of high glucose promoting the proliferation, migration and invasion of HepG2 cells (hepatocellular carcinoma)"}, {"pubmed": 32536673, "text": "Correlation between reticulum ribosome-binding protein 1 (RRBP1) overexpression and prognosis in cervical squamous cell carcinoma."}, {"pubmed": 33762722, "text": "RRBP1 rewires cisplatin resistance in oral squamous cell carcinoma by regulating Hippo pathway."}, {"pubmed": 34445467, "text": "Hypomethylated RRBP1 Potentiates Tumor Malignancy and Chemoresistance in Upper Tract Urothelial Carcinoma."}, {"pubmed": 36803854, "text": "ER ribosomal-binding protein 1 regulates blood pressure and potassium homeostasis by modulating intracellular renin trafficking."}], "genomic_pos": {"chr": "20", "end": 17682295, "ensemblgene": "ENSG00000125844", "start": 17613678, "strand": -1}, "genomic_pos_hg19": {"chr": "20", "end": 17662940, "start": 17594323, "strand": -1}, "go": {"BP": [{"evidence": "HDA", "gocategory": "BP", "id": "GO:0001649", "pubmed": 16210410, "qualifier": "involved_in", "term": "osteoblast differentiation"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0006412", "pubmed": 9628588, "qualifier": "involved_in", "term": "translation"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0015031", "qualifier": "involved_in", "term": "protein transport"}], "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0005783", "qualifier":