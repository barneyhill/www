"chr": "21", "position": [[40546694, 40547590], [40549360, 40549497], [40550374, 40550573], [40551849, 40551912], [40552210, 40552362], [40553697, 40553804], [40555177, 40555377]], "strand": -1, "transcript": "NM_003720", "txend": 40555377, "txstart": 40546694}, {"cdsend": 40555311, "cdsstart": 40547515, "chr": "21", "position": [[40547371, 40547590], [40549360, 40549497], [40550374, 40550573], [40552210, 40552362], [40553697, 40553804], [40555177, 40555440]], "strand": -1, "transcript": "NM_203433", "txend": 40555440, "txstart": 40547371}, {"cdsend": 40555440, "cdsstart": 40555440, "chr": "21", "position": [[40547371, 40547590], [40549360, 40549497], [40550374, 40550573], [40551849, 40551912], [40552210, 40552362], [40555177, 40555440]], "strand": -1, "transcript": "NR_049728", "txend": 40555440, "txstart": 40547371}], "generif": [{"pubmed": 15590417, "text": "The DSCR2 protein is associated with the endoplasmic reticulum; it is not an integral membrane protein and it is maintained on the cytoplasmic side of the ER by indirect interaction with the ER membrane or with another protein."}, {"pubmed": 15670775, "text": "Further, the results of intracellular localization studies and membrane fractionation assays indicate that DSCR2 is targeted to a cytoplasmic compartment as a soluble form."}, {"pubmed": 16251969, "text": "two chaperones, designated proteasome assembling chaperone-1 (PAC1) and PAC2, form a heterodimer and are involved in the maturation of mammalian 20S proteasomes"}, {"pubmed": 18758464, "text": "Genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 18793612, "text": "DSCR2 was found to interact with PPARbeta and to inhibit it."}, {"pubmed": 21079743, "text": "Data show that an additive gene-gene interaction involving TLR4, PSMG1, TNFRSF6B and IRGM was identified with CD."}, {"pubmed": 21509594, "text": "PSMG1 gene expression is increased in classic variant of papillary thyroid carcinoma."}, {"pubmed": 22593026, "text": "PSMG1 mutation is associated with Crohn's disease."}, {"pubmed": 27100087, "text": "RNA-seq evidence of biallelic expression of PSMG1 and 10 neighboring genes in at least one primary human tissue tested indicates that the expression of PSMG1 is uncoupled from the control of the maternally inherited 5mCpG imprints at the WRB differentially methylated region (DMR) in disomic controls or trisomy (Down syndrome) individuals."}, {"pubmed": 32338277, "text": "miR-484 is associated with disease recurrence and promotes migration in prostate cancer."}], "genomic_pos": {"chr": "21", "end": 39183488, "ensemblgene": "ENSG00000183527", "start": 39174769, "strand": -1}, "genomic_pos_hg19": {"chr": "21", "end": 40555777, "start": 40546695, "strand": -1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0021930", "qualifier": "involved_in", "term": "cerebellar granule cell precursor proliferation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0051131", "pubmed": 16251969, "qualifier": "involved_in", "term": "chaperone-mediated protein complex assembly"}, {"evidence": "IGI", "gocategory": "BP", "id": "GO:0051131", "pubmed": 17189198, "qualifier": "involved_in", "term": "chaperone-mediated protein complex assembly"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0080129", "qualifier": "involved_in", "term": "proteasome core complex assembly"}], "CC": [{"evidence": "HDA", "gocategory": "CC", "id": "GO:0005634", "pubmed": 21630459, "qualifier": "located_in", "term": "nucleus"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005737", "pubmed": 15670775, "qualifier": "located_in", "term": "cytoplasm"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005783", "qualifier": "is_active_in", "term": "endoplasmic reticulum"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005783", "pubmed": 15590417, "qualifier": "located_in", "term": "endoplasmic reticulum"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005794", "qualifier": "located_in", "term": "Golgi apparatus"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0101031", "pubmed": 16251969, "qualifier": "part_of", "term": "protein folding chaperone complex"}], "MF": [{"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [16251969, 32296183, 35914814], "qualifier": "enables", "term": "protein binding"}, {"category": "MF", "evidence": "IDA", "id": "GO:0060090", "pubmed": 16251969, "qualifier": "enables", "term": "molecular adaptor activity"}, {"category": "MF", "evidence": "IGI", "id": "GO:0060090", "pubmed": 17189198, "qualifier": "enables", "term": "molecular adaptor activity"}, {"category": "MF", "evidence": "IBA", "id": "GO:0070628", "qualifier": "enables", "term": "proteasome binding"}]}, "homologene": {"genes": [[7955, 436987], [8364, 100124915], [9031, 418520], [9544, 715758], [9598, 473997], [9606, 8624], [9615, 608101], [9913, 614817], [10090, 56088], [10116, 288236]], "id": 2759}, "interpro": {"desc": "Proteasome assembly chaperone 1", "id": "IPR016565", "short_desc": "Proteasome_assmbl_chp_1"}, "ipi": ["IPI00030770", "IPI00401072", "IPI00893118"], "map_location": "21q22.2", "name": "proteasome assembly chaperone 1", "other_names": ["Down syndrome critical region gene 2", "Down syndrome critical region protein 2", "chromosome 21 leucine-rich protein", "leucine rich protein C21-LRP", "proteasome (prosome, macropain) assembly chaperone 1", "proteasome assembling chaperone 1", "proteasome assembly chaperone 1"], "pantherdb": {"HGNC": "3043", "_license": "http://pantherdb.org/tou.jsp", "ortholog": [{"MGI": "1860263", "ortholog_type": "LDO", "panther_family": "PTHR15069", "taxid": 10090, "uniprot_kb": "Q9JK23"}, {"RGD": "1305454", "ortholog_type": "LDO", "panther_family": "PTHR15069", "taxid": 10116, "uniprot_kb": "D4AAH6"}