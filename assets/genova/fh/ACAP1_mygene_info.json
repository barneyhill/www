chr": "17", "position": [[7336528, 7336787], [7337311, 7337369], [7341947, 7342067], [7342274, 7342328], [7342415, 7342474], [7343378, 7343562], [7343705, 7343750], [7343860, 7343956], [7344048, 7344123], [7344538, 7344648], [7346243, 7346295], [7346390, 7346491], [7346807, 7346931], [7347030, 7347242], [7347921, 7347991], [7348126, 7348221], [7348305, 7348475], [7348994, 7349167], [7349944, 7350054], [7350126, 7350237], [7350949, 7350999], [7351294, 7351477]], "strand": 1, "transcript": "NM_014716", "txend": 7351477, "txstart": 7336528}], "exons_hg19": [{"cdsend": 7254714, "cdsstart": 7240053, "chr": "17", "position": [[7239847, 7240106], [7240630, 7240688], [7245266, 7245386], [7245593, 7245647], [7245734, 7245793], [7246697, 7246881], [7247024, 7247069], [7247179, 7247275], [7247367, 7247442], [7247857, 7247967], [7249562, 7249614], [7249709, 7249810], [7250126, 7250250], [7250349, 7250561], [7251240, 7251310], [7251445, 7251540], [7251624, 7251794], [7252313, 7252486], [7253263, 7253373], [7253445, 7253556], [7254268, 7254318], [7254613, 7254796]], "strand": 1, "transcript": "NM_014716", "txend": 7254796, "txstart": 7239847}], "generif": [{"pubmed": 11050434, "text": "Centaurin beta1/KIAA0050, is a member of the centaurin family of ADP-ribosylation factor directed GTPase-activating proteins. Binding of phosphoinositides to a pleckstrin homology domain may regulate subcellular localisation and activity."}, {"pubmed": 11062263, "text": "Centaurin beta1/KIAA0050, or ACAP1, is a phospholipid dependent, ARF6 preferring GTPase-activating protein. It can regulate cytoskeletal and membrane trafficking events in the cell periphery and may be regulated via a phosphoinositide-binding PH domain."}, {"pubmed": 15525538, "text": "ACAP1 promotes endocytic recycling by recognizing recycling sorting signals."}, {"pubmed": 17005562, "text": "CENTB1 selectively down-regulates NF-kappaB activation via NODs pathways, creating a \"feedback\" loop and suggesting a novel role of CENTB1 in innate immune responses to bacteria and inflammatory responses"}, {"pubmed": 17010122, "text": "Results suggest that two ARF6 downstream molecules, PIP5K and ACAP1, function together in endosomal tubulation and that phosphoinositide levels may regulate endosomal dynamics."}, {"pubmed": 17664335, "text": "Results suggest that ACAP1, a GTPase-activating protein (GAP) for ADP-ribosylation factor (ARF) 6, is part of a novel clathrin coat complex that is regulated by ARF6 for endocytic recycling in two key physiological settings."}, {"pubmed": 18003747, "text": "ACAP1 has robust, constitutive Arf6 GAP activity in vivo, with little activity toward Arf1."}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20800603, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 22188517, "text": "the oncogenic potential of USP6 is linked to its ability to integrate cell migration and cytokinesis by regulating Arf6/ACAP1."}, {"pubmed": 22645133, "text": "phosphorylation of ACAP1 relieves a localized mechanism of autoinhibition in regulating cargo binding"}, {"pubmed": 23317930, "text": "CENTB1 was up-regulated in patients with active UC as compared to UC remission and healthy control groups suggesting that it is involved in the inflammatory process."}, {"pubmed": 25225293, "text": "ACAP1 and ARAP2 each colocalize with Arf6 but they did not colocalize with each other and have opposing effects on focal adhesions."}, {"pubmed": 25502766, "text": "The serum lever of Alzheimer's disease were decrease and the expression of ACAP1 strongly correlated with the Mini-Mental State Examination scores of the AD patients"}, {"pubmed": 31291238, "text": "As ACAP1 is an unconventional BAR protein, our findings broaden the understanding of the mechanistic spectrum by which proteins assemble into higher-ordered structures to achieve membrane deformation"}, {"pubmed": 32456571, "text": "Expression of ACAP1 Is Associated with Tumor Immune Infiltration and Clinical Outcome of Ovarian Cancer."}], "genomic_pos": [{"chr": "17", "end": 7351477, "ensemblgene": "ENSG00000072818", "start": 7336529, "strand": 1}, {"chr": "HG2087_PATCH", "end": 109842, "ensemblgene": "ENSG00000288169", "start": 94894, "strand": 1}], "genomic_pos_hg19": {"chr": "17", "end": 7254797, "start": 7239848, "strand": 1}, "go": {"BP": {"evidence": "IEA", "gocategory": "BP", "id": "GO:0015031", "qualifier": "involved_in", "term": "protein transport"}, "CC": [{"evidence": "HDA", "gocategory": "CC", "id": "GO:0016020", "pubmed": 19946888, "qualifier": "located_in", "term": "membrane"}, {"evidence": "IEA", "gocategory": "CC", "id": "GO:0055038", "qualifier": "located_in", "term": "recycling endosome membrane"}], "MF": [{"category": "MF", "evidence": "IEA", "id": "GO:0005096", "qualifier": "enables", "term": "GTPase activator activity"}, {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [17474147, 25814554, 32296183, 32814053], "qualifier": "enables", "term": "protein binding"}, {"category": "MF", "evidence": "IEA", "id": "GO:0046872", "qualifier": "enables", "term": "metal ion binding"}]}, "homologene": {"genes": [[7955, 561771], [9544, 721824], [9598, 455275], [9606, 9744], [9615, 479481], [9913, 508582], [10090, 216859], [10116, 287443]], "id": 22835}, "interpro": [{"desc": "BAR domain", "id": "IPR004148", "short_desc": "BAR_dom"}, {"desc": "AH/BAR domain superfamily", "id": "IPR027267", "short_desc": "AH/BAR_dom_sf"}, {"desc": "ArfGAP ACAP1/2/3-like", "id": "IPR045258", "short_desc": "ACAP1/2/3-like"}, {"desc": "Arf GTPase activating protein", "id": "IPR001164", "short_desc": "ArfGAP_dom"}, {"desc": "Pleckstrin homology domain", "id": "IPR001849", "short_desc": "PH_domain"}, {"desc": "Ankyrin repeat", "id": "IPR002110", "short_desc": "Ankyrin_rpt"}, {"desc": "PH-like domain superfamily", "id": "IPR011993", "short_desc": "PH-like_dom_sf"}, {"desc": "Ankyrin r