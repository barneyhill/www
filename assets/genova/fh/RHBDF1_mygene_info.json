start": 108338, "n_exons": 17, "nonpsych": {"exp_lof": 26.4165039252, "exp_mis": 345.2107763, "exp_syn": 156.363927119, "lof_z": 2.94577928810993, "mis_z": 1.66168071366699, "mu_lof": 2.31987668581e-06, "mu_mis": 3.88667375151e-05, "mu_syn": 1.7427596075e-05, "n_lof": 11.0, "n_mis": 284.0, "n_syn": 153.0, "p_li": 0.000183749376955571, "p_null": 0.00140229815300122, "p_rec": 0.998413952470043, "syn_z": 0.169370711718373}, "nontcga": {"exp_lof": 27.1410038006, "exp_mis": 351.184621359, "exp_syn": 159.228173432, "lof_z": 2.86696874882969, "mis_z": 2.05197846257567, "mu_lof": 2.31987668581e-06, "mu_mis": 3.88667375151e-05, "mu_syn": 1.7427596075e-05, "n_lof": 12.0, "n_mis": 274.0, "n_syn": 158.0, "p_li": 4.21034864388015e-05, "p_null": 0.00192433198062787, "p_rec": 0.998033564532933, "syn_z": 0.0616284722641242}, "transcript": "ENST00000262316.6"}, "exons": [{"cdsend": 65015, "cdsstart": 58339, "chr": "16", "position": [[58058, 58759], [58973, 59127], [59248, 59349], [59418, 59494], [59731, 59826], [60215, 60279], [60438, 60539], [61119, 61281], [61384, 61459], [61584, 61696], [61797, 62052], [62537, 62695], [62774, 62897], [62972, 63182], [63586, 63800], [64698, 64829], [64898, 65039], [72512, 72631]], "strand": -1, "transcript": "NM_022450", "txend": 72631, "txstart": 58058}], "exons_hg19": [{"cdsend": 115013, "cdsstart": 108338, "chr": "16", "position": [[108057, 108758], [108971, 109125], [109246, 109347], [109416, 109492], [109729, 109824], [110213, 110277], [110436, 110537], [111117, 111279], [111382, 111457], [111582, 111694], [111795, 112050], [112535, 112693], [112772, 112895], [112970, 113180], [113584, 113798], [114696, 114827], [114896, 115037], [122510, 122629]], "strand": -1, "transcript": "NM_022450", "txend": 122629, "txstart": 108057}], "generif": [{"pubmed": 15965977, "text": "RHBDF1 is a seven-transmembrane protein with a long N-terminal cytoplasmic extension that comprises half of the polypeptide sequence, and is found in the endoplasmic reticulum and Golgi, but not on the cell surface"}, {"pubmed": 18524845, "text": "RHBDF1 has a pivotal role in sustaining growth signals in epithelial cancer cells and thus may serve as a therapeutic target for treating epithelial cancers."}, {"pubmed": 18832597, "text": "RHBDF1 is critically involved in a G protein-coupled receptors ligand-stimulated process leading to the activation of latent epidermal growth factor receptor ligands"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20970119, "text": "Observational study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 24648344, "text": "RHBDF1 is a critical component of a molecular switch that regulates HIF1alpha stability in cancer cells in hypoxia."}, {"pubmed": 26109405, "text": "The expression of iRhom1 was increased by endoplasmic reticulum (ER) stressors, such as thapsigargin and tunicamycin, leading to the enhancement of proteasome activity, especially in ER-containing microsomes."}, {"pubmed": 26535007, "text": "results explain how loss of the amino terminus in iRhom1 and iRhom2 impairs TNF signaling, despite enhancing ADAM17 activity"}, {"pubmed": 29654741, "text": "that the regulatory effects of RHBDF1 on epithelial-to-mesenchymal transition and on cell proliferation are partially attributable to the Wnt/beta-catenin signalling pathway"}, {"pubmed": 29727209, "text": "findings indicate that perturbations of apicobasal polarity by high levels of RHBDF1 is a significant attribute in the development of breast neoplasia."}, {"pubmed": 30279141, "text": "RHBDF1 is an indispensable component of the protein trafficking machinery involved in GPCR-mediated EGFR transactivation, and is an attractive therapeutic target for cancer."}, {"pubmed": 31661139, "text": "Results revealed that cervical cancer (CC) tissues had higher levels of iRhom1 than adjacent normal tissues. Its increased expression was significantly associated with tumor stage, size, and parametrium invasion as well as poor outcomes. Knockdown of iRhom1 in HeLa cells inhibited cell growth, disrupted the cell cycle, and promoted apoptosis."}, {"pubmed": 34276048, "text": "RHBDF1 promotes AP-1-activated endothelial-mesenchymal transition in tumor fibrotic stroma formation."}, {"pubmed": 35585977, "text": "Attenuation of Excess TNF-alpha Release in Crohn's Disease by Silencing of iRHOMs 1/2 and the Restoration of TGF-beta Mediated Immunosuppression Through Modulation of TACE Trafficking."}, {"pubmed": 35595096, "text": "Alternative splicing of the human rhomboid family-1 gene RHBDF1 inhibits epidermal growth factor receptor activation."}, {"pubmed": 37979663, "text": "RHBDF1 deficiency suppresses melanoma glycolysis and enhances efficacy of immunotherapy by facilitating glucose-6-phosphate isomerase degradation via TRIM32."}], "genomic_pos": {"chr": "16", "end": 76355, "ensemblgene": "ENSG00000007384", "start": 58059, "strand": -1}, "genomic_pos_hg19": {"chr": "16", "end": 126354, "start": 108058, "strand": -1}, "go": {"BP": [{"evidence": "IMP", "gocategory": "BP", "id": "GO:0008283", "pubmed": 18832597, "qualifier": "involved_in", "term": "cell population proliferation"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0015031", "qualifier": "involved_in", "term": "protein transport"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0016477", "pubmed": 18832597, "qualifier": "involved_in", "term": "cell migration"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0042058", "qualifier": "involved_in", "term": "regulation of epidermal growth factor receptor signaling pathway"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0042058", "pubmed": 18832597, "qualifier": "involved_in", "term": "regulation of epidermal growth factor receptor signaling pathway"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0050708", "qualifier": "involved_in", "term": "re