send": 118134868, "cdsstart": 118127964, "chr": "11", "position": [[118124130, 118125948], [118127952, 118128028], [118130768, 118130916], [118133152, 118133363], [118133645, 118133812], [118134810, 118135012]], "strand": -1, "transcript": "NM_005797", "txend": 118135012, "txstart": 118124130}, {"cdsend": 118134868, "cdsstart": 118127964, "chr": "11", "position": [[118127625, 118128028], [118130768, 118130916], [118133152, 118133363], [118133645, 118133812], [118134810, 118135251]], "strand": -1, "transcript": "NM_144765", "txend": 118135251, "txstart": 118127625}], "generif": [{"pubmed": 21440040, "text": "EVA is expressed in human choroid plexus epithelial cells and CD4 T lymphocytes and regulates CD4+ T lymphocyte adhesion to human choroid plexus epithelial cells in vitro. These data suggest a novel mechanism to regulate CNS immune surveillance."}, {"pubmed": 26677976, "text": "Findings highlight Eva1 as a novel regulator of GIC function and also provide new mechanistic insight into the role of noncanonical NF-kappaB activation in GIC."}, {"pubmed": 26930581, "text": "CLCA2 links junctional adhesion molecule EVA1, to cytosolic signaling proteins that modulate proliferation and differentiation."}, {"pubmed": 29982980, "text": "We show that Mpzl2 is expressed in mouse inner ear, and the protein localizes in the auditory inner and outer hair cells, with an asymmetric subcellular localization. We thus present MPZL2 as a novel gene associated with sensorineural hearing loss."}, {"pubmed": 31997489, "text": "Epithelial V-like antigen 1 (EVA1) expression is increased in hepatocellular carcinoma (HCC) and is associated with a poor prognosis and recurrence in HCC patients. Overexpression of EVA1 promotes cell growth, invasion and migration in vitro. EVA1 is able to upregulate the expression of genes in the ERBB3-PI3K pathway. An increased level of AKT phosphorylation is detected in HCC cells after EVA1 overexpression."}, {"pubmed": 33234333, "text": "A homozygous MPZL2 deletion is associated with non syndromic hearing loss in a moroccan family."}, {"pubmed": 35249471, "text": "Upregulation of FHL1, SPNS3, and MPZL2 predicts poor prognosis in pediatric acute myeloid leukemia patients with FLT3-ITD mutation."}, {"pubmed": 37390746, "text": "MPZL2 variant analysis with whole exome sequencing in a cohort of Chinese hearing loss patients."}, {"pubmed": 38197511, "text": "Recurrent missense variant identified in two unrelated families with MPZL2-related hearing loss, expanding the variant spectrum associated with DFNB111."}, {"pubmed": 38254107, "text": "MPZL2-a common autosomal recessive deafness gene related to moderate sensorineural hearing loss in the Chinese population."}], "genomic_pos": {"chr": "11", "end": 118264536, "ensemblgene": "ENSG00000149573", "start": 118253416, "strand": -1}, "genomic_pos_hg19": {"chr": "11", "end": 118135251, "start": 118124118, "strand": -1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0007156", "pubmed": 9585423, "qualifier": "involved_in", "term": "homophilic cell adhesion via plasma membrane adhesion molecules"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0009653", "pubmed": 9585423, "qualifier": "involved_in", "term": "anatomical structure morphogenesis"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0098609", "qualifier": "involved_in", "term": "cell-cell adhesion"}], "CC": [{"evidence": "TAS", "gocategory": "CC", "id": "GO:0005856", "pubmed": 9585423, "qualifier": "located_in", "term": "cytoskeleton"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005886", "qualifier": "is_active_in", "term": "plasma membrane"}], "MF": {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": 21982860, "qualifier": "enables", "term": "protein binding"}}, "homologene": {"genes": [[8364, 549209], [9031, 419779], [9544, 699330], [9598, 451583], [9606, 10205], [9615, 489384], [9913, 540423], [10090, 14012], [10116, 300679]], "id": 7309}, "interpro": [{"desc": "Myelin P0 p