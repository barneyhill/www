"}, {"category": "MF", "evidence": "IDA", "id": "GO:0005096", "pubmed": 15304343, "qualifier": "enables", "term": "GTPase activator activity"}]}, "homologene": {"genes": [[7955, 557288], [8364, 100490424], [9031, 420172], [9544, 705243], [9598, 464222], [9606, 80243], [9615, 486984], [9913, 520704], [10090, 109294], [10116, 312912]], "id": 23523}, "interpro": [{"desc": "Dbl homology (DH) domain", "id": "IPR000219", "short_desc": "DH-domain"}, {"desc": "DEP domain", "id": "IPR000591", "short_desc": "DEP_dom"}, {"desc": "Guanine-nucleotide dissociation stimulator, CDC24, conserved site", "id": "IPR001331", "short_desc": "GDS_CDC24_CS"}, {"desc": "PDZ domain", "id": "IPR001478", "short_desc": "PDZ"}, {"desc": "Pleckstrin homology domain", "id": "IPR001849", "short_desc": "PH_domain"}, {"desc": "PH-like domain superfamily", "id": "IPR011993", "short_desc": "PH-like_dom_sf"}, {"desc": "Dbl homology (DH) domain superfamily", "id": "IPR035899", "short_desc": "DBL_dom_sf"}, {"desc": "PDZ superfamily", "id": "IPR036034", "short_desc": "PDZ_sf"}, {"desc": "Winged helix-like DNA-binding domain superfamily", "id": "IPR036388", "short_desc": "WH-like_DNA-bd_sf"}, {"desc": "Winged helix DNA-binding domain superfamily", "id": "IPR036390", "short_desc": "WH_DNA-bd_sf"}, {"desc": "PREX2, DEP domain 1", "id": "IPR037367", "short_desc": "Rex2_DEP_1"}], "ipi": ["IPI00418875", "IPI00418876", "IPI00845229"], "map_location": "8q13.2", "name": "phosphatidylinositol-3,4,5-trisphosphate dependent Rac exchange factor 2", "other_names": ["DEP domain-containing protein 2", "PtdIns(3,4,5)-dependent Rac exchanger 2", "phosphatidylinositol 3,4,5-trisphosphate-dependent Rac exchanger 2 protein", "protein phosphatase 1, regulatory subunit 129"], "pantherdb": {"HGNC": "22950", "_license": "http://pantherdb.org/tou.jsp", "ortholog": [{"MGI": "1923385", "ortholog_type": "LDO", "panther_family": "PTHR22829", "taxid": 10090, "uniprot_kb": "Q3LAC4"}, {"RGD": "1307865", "ortholog_type": "LDO", "panther_fam