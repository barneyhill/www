}, {"cdsend": 92765284, "cdsstart": 92760529, "chr": "7", "position": [[92759368, 92765304], [92774044, 92774146], [92774697, 92774845], [92777506, 92777698]], "strand": -1, "transcript": "NM_001303500", "txend": 92777698, "txstart": 92759368}, {"cdsend": 92765284, "cdsstart": 92760529, "chr": "7", "position": [[92759368, 92765304], [92774044, 92774146], [92774697, 92774893], [92775221, 92775353], [92776195, 92776459], [92777506, 92777698]], "strand": -1, "transcript": "NM_001350082", "txend": 92777698, "txstart": 92759368}, {"cdsend": 92765284, "cdsstart": 92760529, "chr": "7", "position": [[92759368, 92765304], [92774044, 92774146], [92774697, 92775353], [92777506, 92777698]], "strand": -1, "transcript": "NM_001350083", "txend": 92777698, "txstart": 92759368}, {"cdsend": 92765284, "cdsstart": 92760529, "chr": "7", "position": [[92759368, 92765304], [92774044, 92774146], [92774697, 92774845], [92775221, 92775353], [92776400, 92776459], [92777506, 92777698]], "strand": -1, "transcript": "NM_001350084", "txend": 92777698, "txstart": 92759368}, {"cdsend": 92765284, "cdsstart": 92760529, "chr": "7", "position": [[92759368, 92765304], [92774044, 92774146], [92774697, 92774845], [92776195, 92776459], [92777506, 92777698]], "strand": -1, "transcript": "NM_001350085", "txend": 92777698, "txstart": 92759368}, {"cdsend": 92765284, "cdsstart": 92760529, "chr": "7", "position": [[92759368, 92765304], [92774044, 92774146], [92774697, 92775353], [92776195, 92776459], [92777506, 92777698]], "strand": -1, "transcript": "NM_152703", "txend": 92777698, "txstart": 92759368}], "generif": [{"pubmed": 25076857, "text": "The findings highlight a novel tumor-suppressive role of SAMD9L inactivation by somatic mutation and decreased expression in human HBV-related HCC"}, {"pubmed": 27259050, "text": "Missense Mutations in SAMD9L gene is Associated with Ataxia-Pancytopenia Syndrome."}, {"pubmed": 28202457, "text": "Gain-of-function SAMD9L mutations cause a syndrome of cytopenia, immunodeficiency, MDS, and neurological symptoms in two unrelated Caucasian families."}, {"pubmed": 29217778, "text": "Constitutional mutations (p.H880Q, p.R986H, p.R986C and p.V1512M) in the SAMD9L gene on 7q21 define a new subtype of familial myelodysplastic syndrome and provide an explanation for the phenomenon of transient monosomy 7."}, {"pubmed": 29447249, "text": "In humans, both SAMD9 and SAMD9L are poxvirus restriction factors, although the latter requires interferon induction in many cell types."}, {"pubmed": 29535429, "text": "REVIEW: expert-based recommendations regarding diagnosis, follow-up, and treatment of mutation carriers."}, {"pubmed": 30322869, "text": "SAMD9 and SAMD9L germline loss-of-function variants exist in 3% of adult myelodysplastic syndromes and are located more in the N terminus relative to pediatric germline loss-of-function variants, which exist more in the C terminus."}, {"pubmed": 30923096, "text": "Germline SAMD9L mutation is associated with ataxia-pancytopenia syndrome and pediatric acute lymphoblastic leukemia."}, {"pubmed": 30971808, "text": "Identification of SAMD9L as a susceptibility locus for intravenous immunoglobulin resistance in Kawasaki disease by genome-wide association analysis."}, {"pubmed": 32770553, "text": "Prevalence of germline GATA2 and SAMD9/9L variants in paediatric haematological disorders with monosomy 7."}, {"pubmed": 33038986, "text": "Germline predisposition in myeloid neoplasms: Unique genetic and clinical features of GATA2 deficiency and SAMD9/SAMD9L syndromes."}, {"pubmed": 33373325, "text": "Multiorgan failure with abnormal receptor metabolism in mice mimicking Samd9/9L syndromes."}, {"pubmed": 33731850, "text": "Pediatric MDS and bone marrow failure-associated germline mutations in SAMD9 and SAMD9L impair multiple pathways in primary hematopoietic cells."}, {"pubmed": 34417303, "text": "SAMD9L autoinflammatory or ataxia pancytopenia disease mutations activate cell-autonomous translational repression."}, {"pubmed": 34621053, "text": "Clinical evolution, genetic landscape and trajectories of clonal hematopoiesis in SAMD9/SAMD9L syndromes."}, {"pubmed": 35388602, "text": "Up-regulated SAMD9L modulated by TLR2 and HIF-1alpha as a promising biomarker in tuberculosis."}, {"pubmed": 36553623, "text": "Clonal Elimination of the Pathogenic Allele as Diagnostic Pitfall in SAMD9L-Associated Neuropathy."}, {"pubmed": 36894052, "text": "Viral host range factors antagonize pathogenic SAMD9 and SAMD9L variants."}, {"pubmed": 37158533, "text": "Functional Study of SAMD9L in Familial Gastric Cancer."}, {"pubmed": 37357006, "text": "[Pancytopenia in children caused by SAMD9/9L mutation: 5 cases report and literature review]."}, {"pubmed": 37612499, "text": "PARP14 correlates with GBM proliferation and poor prognosis by elevating expression of SAMD/SAMD9L."}], "genomic_pos": {"chr": "7", "end": 93148385, "ensemblgene": "ENSG00000177409", "start": 93130056, "strand": -1}, "genomic_pos_hg19": {"chr": "7", "end": 92777682, "start": 92759368, "strand": -1}, "go": {"CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005737", "qualifier": "is_active_in", "term": "cytoplasm"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005739", "pubmed": 35310830, "qualifier": "located_in", "term": "mitochondrion"}, {"evidence": "IEA", "gocategory": "CC", "id": "GO:0005769", "qualifier": "located_in", "term": "early endosome"}], "MF": {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": 24029230, "qualifier": "enables", "term": "protein binding"}}, "homologene": {"genes": [[8364, 100490592], [9031, 420559], [9544, 701572], [9598, 472444], [9606, 219285], [9615, 482305], [10090, 209086]], "id": 7707}, "interpro": [{"desc": "Sterile alpha motif domain", "id": "IPR001660", "short_desc": "SAM"}, {"desc": "Sterile alpha motif/pointed domain superfamily", "id": "IPR013761", "short_desc": "SAM/pointed_sf"}], "ipi": ["IPI00719690", "IPI00855755"], "map_location": "7q21.2", "name": "sterile alp