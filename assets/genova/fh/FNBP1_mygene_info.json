 [[129895262, 129895996], [129899964, 129900101], [129900425, 129900547], [129902868, 129903001], [129908889, 129908999], [129915965, 129915980], [129923843, 129923939], [129924959, 129925157], [129927194, 129927341], [129929566, 129929695], [129957359, 129957464], [129958490, 129958553], [129978464, 129978612], [129979317, 129979374], [129994842, 129994958], [130042951, 130043189]], "strand": -1, "transcript": "NM_001363755", "txend": 130043189, "txstart": 129895262}, {"cdsend": 130042975, "cdsstart": 129890538, "chr": "9", "position": [[129887186, 129890546], [129895837, 129895996], [129899964, 129900101], [129900425, 129900547], [129902868, 129903001], [129908889, 129908999], [129915965, 129915980], [129923843, 129924026], [129924959, 129925157], [129927194, 129927341], [129929566, 129929695], [129957359, 129957464], [129958490, 129958553], [129978464, 129978612], [129979317, 129979374], [129994842, 129994958], [130042951, 130043189]], "strand": -1, "transcript": "NM_015033", "txend": 130043189, "txstart": 129887186}], "exons_hg19": [{"cdsend": 132805254, "cdsstart": 132658096, "chr": "9", "position": [[132657539, 132658275], [132662243, 132662380], [132662704, 132662826], [132665147, 132665280], [132671168, 132671278], [132678244, 132678259], [132686122, 132686218], [132687238, 132687436], [132689473, 132689620], [132691845, 132691974], [132719638, 132719743], [132720769, 132720832], [132740743, 132740891], [132741596, 132741653], [132757121, 132757237], [132805230, 132805468]], "strand": -1, "transcript": "NM_001363755", "txend": 132805468, "txstart": 132657539}, {"cdsend": 132805254, "cdsstart": 132652817, "chr": "9", "position": [[132649465, 132652825], [132658116, 132658275], [132662243, 132662380], [132662704, 132662826], [132665147, 132665280], [132671168, 132671278], [132678244, 132678259], [132686122, 132686305], [132687238, 132687436], [132689473, 132689620], [132691845, 132691974], [132719638, 132719743], [132720769, 132720832], [132740743, 132740891], [132741596, 132741653], [132757121, 132757237], [132805230, 132805468]], "strand": -1, "transcript": "NM_015033", "txend": 132805468, "txstart": 132649465}], "generif": [{"pubmed": 12244061, "text": "Rat rapostlin protein has 93% aa identity to human FBP17."}, {"pubmed": 12736724, "text": "FNBP1 family proteins (FNBP1 and TRIP10) consist of FCH, FBH and SH3 domains."}, {"pubmed": 14596906, "text": "co-immunoprecipitation of endogenous proteins in 293T cells confirms physiological relevance of TNKS as interaction partner of FBP17"}, {"pubmed": 15252009, "text": "FBP17 interacts with dynamin and regulates endocytosis by forming vesicotubular structures"}, {"pubmed": 16418535, "text": "Data indicate that PCH protein family members such as FBP17 couple membrane deformation to actin cytoskeleton reorganization in various cellular processes."}, {"pubmed": 17512409, "text": "FBP17 is recruited to clathrin-coated pits in the late stage of endocytosis, indicating its physiological role."}, {"pubmed": 18923421, "text": "N-WASP-WIP complex-mediated actin polymerization is activated by phosphatidylserine-containing membranes depending on membrane curvature in the presence of Toca-1 or FBP17 and in the absence of Cdc42 and phosphatidylinositol (4,5)-bisphosphate."}, {"pubmed": 19155218, "text": "formin-binding protein 17 (FBP17) recruits WASP, WASP-interacting protein (WIP), and dynamin-2 to the plasma membrane and that this recruitment is necessary for the formation of podosomes and phagocytic cups."}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20729836, "text": "Results suggest a functional link between FBP17-dependent membrane tubulation and clathrin-dependent budding. Clathrin spatially directs membrane invaginations that lead to the generation of endocytic vesicles larger than those enclosed by the coat."}, {"pubmed": 21768103, "text": "F-BAR protein Rapostlin, whose activity is regulated by Rnd2, plays a key role in spine formation through the regulation of membrane dynamics."}, {"pubmed": 23525018, "text": "Knockdown of PSTPIP2 inmacrophages enhances the assembly of FBP17, leading to'hyperactivation' of actin nucleation at podosomes and ECM degradation."}, {"pubmed": 25938814, "text": "FBP17 is the local activator of actin polymerization that is inhibited by PM tension in the feedback loop that regulates cell migration."}, {"pubmed": 29651632, "text": "Results suggest that FBP17 is highly expressed in breast cancer cells and facilitates the invasion of breast cancer cells."}, {"pubmed": 30061681, "text": "FBP17 and CIP4 prime the membrane of resting cells for fast endophilin-mediated endocytosis by recruiting the 5'-lipid phosphatase SHIP2 and lamellipodin to mediate the local production of phosphatidylinositol-3,4-bisphosphate and endophilin pre-enrichment."}, {"pubmed": 31862885, "text": "Study shows that plasma membrane (PM) mechanoadaptation is controlled by a tension-sensing pathway composed of c-Abl tyrosine kinase and membrane curvature regulator FBP17. FBP17 is recruited to caveolae to induce the formation of caveolar rosettes. FBP17 deficient cells have reduced rosette density, lack PM tension buffering capacity under osmotic shock, and cannot adapt to mechanical strain."}, {"pubmed": 32665637, "text": "High formin binding protein 17 (FBP17) expression indicates poor differentiation and invasiveness of ductal carcinomas."}, {"pubmed": 34202606, "text": "Sp1-Induced FNBP1 Drives Rigorous 3D Cell Motility in EMT-Type Gastric Cancer Cells."}, {"pubmed": 34998385, "text": "Membrane tension sensing molecule-FNBP1 is a prognostic biomarker related to immune infiltration in BRCA, LUAD and STAD."}, {"pubmed": 35134126, "text": "Wild-type p53 suppresses formin-binding protein-17 (FBP17) to reduce invasion."}, {"pubmed": 37566043, "text": "FNBP1 Facilitates Cervical Cancer Cell Survival by the Constitutive Activation of FAK/PI3K/AKT/mTOR Signaling."}, {"pubmed": 38945423, "text": "Formin Binding Protein 1 (FNBP1) regulates non-canonical Wnt signaling and vertebrate gastrulation."}], "genomic_pos": {"chr": "9", "end": 130043189, "ensemblgene": "ENSG00000187239", "start": 129887187, "strand": -1}, "genomic_pos_hg19": {"chr": "9", "end": 132805473, "start": 132649466, "strand": -1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0006897", "qualifier": "involved_in", "term": "endocytosis"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0007165", "qualifier": "involved_in", "term": "signal transduction"}], "CC": [{"evidence": "IEA", "gocategory": "CC", "id": "GO:0005764", "qualifier": "located_in", "term": "lysosome"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "IEA", "gocategory": "CC", "id": "GO:0005856", "qualifier": "located_in", "term": "cytoskeleton"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005886", "qualifier": "located_in", "term": "plasma membrane"}, {"evidence": "IEA", "gocategory": "CC", "id": "GO:0005905", "qualifier": "located_in", "term": "clathrin-coated pit"}, {"evidence": "IEA", "gocategory": "CC", "id": "GO:0005938", "qualifier": "located_in", "term": "cell cortex"}, {"evidence": "IEA", "gocategory": "CC", "id": "GO:0031410", "qualifier": "located_in", "term": "cytoplasmic vesicle"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0043231", "qualifier": "located_in", "term": "intracellular membrane-bounded organelle"}], "MF": [{"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [11438682, 14596906, 15252009, 16318909, 20562859, 26751515], "qualifier": "enables", "term": "protein binding"}, {"category": "MF", "evidence": "IEA", "id": "GO:0008289", "qualifier": "enables", "term": "lipid binding"}, {"category": "MF", "evidence": "IPI", "id": "GO:0042802", "pubmed": [15252009, 17512409], "qualifier": "enables", "term": "identical protein binding"}]}, "homologene": {