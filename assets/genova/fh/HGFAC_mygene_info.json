art": 3443728, "chr": "4", "position": [[3443694, 3443845], [3444458, 3444639], [3444776, 3444873], [3445067, 3445147], [3445765, 3445888], [3446037, 3446169], [3446349, 3446460], [3446545, 3446720], [3446991, 3447077], [3447768, 3448021], [3449218, 3449358], [3449621, 3449762], [3449854, 3450003], [3450963, 3451213]], "strand": 1, "transcript": "NM_001528", "txend": 3451213, "txstart": 3443694}], "generif": [{"pubmed": 11013244, "text": "HAI-1 (serine peptidase inhibitor, Kunitz type 1) specifically traps active form of HGF activator on cellular surface and regulates HGF activation in pericellular microenvironment."}, {"pubmed": 15172968, "text": "the HGF-activator has a role in multiple myeloma cells' catalysis of hepatocyte growth factor (HGF) activation"}, {"pubmed": 16189274, "text": "HGF/MET signaling and aberrant HGF-activator expression is associated with diffuse large B-cell lymphoma"}, {"pubmed": 18221492, "text": "KLK4 and KLK5 activate pro-HGFA."}, {"pubmed": 18422749, "text": "Induction of hepatocyte growth factor activator gene expression under hypoxia activates the hepatocyte growth factor/c-Met system via hypoxia inducible factor-1 in pancreatic cancer."}, {"pubmed": 18691255, "text": "the activated form of HGFA is present at high levels in serum and bone marrow of myeloma patients, thus providing a necessary prerequisite for the activation of HGF."}, {"pubmed": 19456860, "text": "Results suggest that hepatocyte growth factor activator is a major serum activator of pro-macrophage-stimulating protein."}, {"pubmed": 20424473, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 25998175, "text": "Based upon ethnicity-stratified single-variant association analysis and trans-ethnic meta-analysis of 6201 participants of the Multi-Ethnic Study of Atherosclerosis, discovered 5 common and low-frequency variants: HGF missense polymorphism rs5745687 (p.E299K) and 4 variants (rs16844364, rs4690098, rs114303452, rs3748034) within or in proximity to HGFAC.[Meta-analysis]"}, {"pubmed": 30635948, "text": "Hypermethylation around the promoter region contributed to the decreased expression of HGFAC. HGFAC may be a useful and predictive biomarker for the prognosis of liver cancer patients."}, {"pubmed": 36182093, "text": "Role of hypoxia on microRNA-dependant regulation of HGFA - HGF - c-Met signalling pathway in human progenitor and mature endothelial cells."}, {"pubmed": 36413406, "text": "HGFAC is a ChREBP-regulated hepatokine that enhances glucose and lipid homeostasis."}, {"pubmed": 38077422, "text": "miR-4270 suppresses hepatocellular carcinoma progression by inhibiting DNMT3A-mediated methylation of HGFAC promoter."}], "genomic_pos": {"chr": "4", "end": 3449486, "ensemblgene": "ENSG00000109758", "start": 3441968, "strand": 1}, "genomic_pos_hg19": {"chr": "4", "end": 3451211, "start": 3443614, "strand": 1}, "go": {"BP": [{"evidence": "TAS", "gocategory": "BP", "id": "GO:0006508", "pubmed": 8226803, "qualifier": "involved_in", "term": "proteolysis"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007596", "qualifier": "involved_in", "term": "blood coagulation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0031638", "qualifier": "involved_in", "term": "zymogen activation"}], "CC": [{"evidence": "NAS", "gocategory": "CC", "id": "GO:0005576", "pubmed": 14718574, "qualifier": "located_in", "term": "extracellular region"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005615", "qualifier": "is_active_in", "term": "extracellular space"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005791", "qualifier": "is_active_in", "term": "rough endoplasmic reticulum"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}], "MF": [{"category": "MF", "evidence": "IBA", "id": "GO:0004252", "qualifier": "enables", "term": "serine-type endopeptidase activity"}, {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": 32296183,