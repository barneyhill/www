: "2", "position": [[74152527, 74156645], [74159090, 74159254], [74165217, 74165351], [74167013, 74167107], [74172585, 74172752], [74178660, 74178879]], "strand": -1, "transcript": "NM_018221", "txend": 74178879, "txstart": 74152527}], "exons_hg19": [{"cdsend": 74405801, "cdsstart": 74383694, "chr": "2", "position": [[74379654, 74383772], [74386217, 74386381], [74392344, 74392478], [74394140, 74394234], [74399712, 74399876], [74405787, 74406006]], "strand": -1, "transcript": "NM_001317110", "txend": 74406006, "txstart": 74379654}, {"cdsend": 74405381, "cdsstart": 74383694, "chr": "2", "position": [[74379724, 74383772], [74386217, 74386381], [74392344, 74392478], [74394140, 74394234], [74399712, 74399879], [74405241, 74405441]], "strand": -1, "transcript": "NM_001317111", "txend": 74405441, "txstart": 74379724}, {"cdsend": 74405801, "cdsstart": 74383764, "chr": "2", "position": [[74379654, 74383772], [74392344, 74392478], [74394140, 74394234], [74399712, 74399879], [74405787, 74406006]], "strand": -1, "transcript": "NM_001317112", "txend": 74406006, "txstart": 74379654}, {"cdsend": 74405801, "cdsstart": 74383694, "chr": "2", "position": [[74379654, 74383772], [74386217, 74386381], [74392344, 74392478], [74394140, 74394234], [74399712, 74399879], [74405787, 74406006]], "strand": -1, "transcript": "NM_018221", "txend": 74406006, "txstart": 74379654}], "generif": [{"pubmed": 15766530, "text": "Mats1 can rescue the lethality associated with loss of Mats function in Drosophila; As Mats1 is mutated in human tumors, Mats-mediated growth inhibition and tumor suppression is likely conserved in humans"}, {"pubmed": 17611689, "text": "MATS1 mRNA expression is suppressed in tumor tissue and its low expression is associated with tumor growth, invasion and metastasis of colorectal cancer"}, {"pubmed": 19739119, "text": "hMOB1A and hMOB1B are 2 LATS-binding proteins that may function as tumor suppressors in human cancer cells."}, {"pubmed": 19955215, "text": "Results suggest that Mob1 and the other mammalian orthologues of the mitotic exit network regulate mitotic progression by facilitating the timely mobilization of the chromosomal passenger complex to the spindle midzone."}, {"pubmed": 22454515, "text": "Mob1A and Mob1B are needed for cell abscission and centriole re-joining after telophase and cytokinesis."}, {"pubmed": 23652010, "text": "The RING ligase praja2 ubiquitylates and degrades Mob, a core component of NDR/LATS kinase and a positive regulator of the tumour-suppressor Hippo cascade."}, {"pubmed": 25031347, "text": "No relationship has been found between the MOBKL1B-NS5A interaction and hepatitis C virus replication."}, {"pubmed": 28373297, "text": "Through a comprehensive set of biochemical, biophysical, mutational and structural studies, we quantitatively assess how phosphorylation of MOB1A regulates its interaction with both MST kinases and LATS/NDR family kinases in vitro."}, {"pubmed": 28373298, "text": "In this study, we investigated the mechanisms behind the recruitment of MST1 and MST2 kinases to MOB1, which facilitate signal transmission in the Hippo pathway by bringing the MST1 and MST2 kinases in close vicinity to their substrates, the LATS family kinases."}, {"pubmed": 28675297, "text": "validated the functional importance of an identified interaction network by characterizing a distinct novel interaction between PTPN5 and Mob1a."}, {"pubmed": 28947795, "text": "MOB1 binds differently to the NDR2 and LATS1 kinases.MOB1 interaction with Hippo and MST protein is not essential for development and tissue growth control."}, {"pubmed": 32513747, "text": "The Legionella kinase LegK7 exploits the Hippo pathway scaffold protein MOB1A for allostery and substrate phosphorylation."}, {"pubmed": 32780197, "text": "RNA-binding protein Musashi2 regulates Hippo signaling via SAV1 and MOB1 in pancreatic cancer."}, {"pubmed": 34983617, "text": "Circular RNA circCCDC85A inhibits breast cancer progression via acting as a miR-550a-5p sponge to enhance MOB1A expression."}, {"pubmed": 35349706, "text": "Oxidative stress-CBP axis modulates MOB1 acetylation and activates the Hippo signaling pathway."}, {"pubmed": 36123327, "text": "CAF-derived exosomal WEE2-AS1 facilitates colorectal cancer progression via promoting degradation of MOB1A to inhibit the Hippo pathway."}], "genomic_pos": {"chr": "2", "end": 74178898, "ensemblgene": "ENSG00000114978", "start": 74152528, "strand": -1}, "genomic_pos_hg19": {"chr": "2", "end": 74406025, "start": 74379655, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0001934", "qualifier": "involved_in", "term": "positive regulation of protein phosphorylation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0035329", "qualifier": "involved_in", "term": "hippo signaling"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0035329", "pubmed": [18328708, 19739119], "qualifier": "involved_in", "term": "hippo signaling"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "is_active_in", "term": "nucleus"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005737", "qualifier": "is_active_in", "term": "cytoplasm"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005737", "pubmed": 18328708, "qualifier": "is_active_in", "term": "cytoplasm"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0070062", "pubmed": 20458337, "qualifier": "located_in", "term": "extracellular exosome"}], "MF": [{"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [15197186, 16189514, 18362890, 18985028, 19739119, 21516123, 23386615, 23972470, 24255178, 24366813, 25416956, 25852190, 28514442, 30212651, 32296183], "qualifier": "enables", "term": "protein binding"}, {"category": "MF", "evidence": "IBA", "id": "GO:0030295", "qualifier": "enables", "term": "protein kinase activator activity"}, {"category": "MF", "evidence"