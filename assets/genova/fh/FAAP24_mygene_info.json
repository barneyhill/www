: [[32972241, 32972346], [32973183, 32973302], [32973425, 32973562], [32974059, 32974212], [32976430, 32978224]], "strand": 1, "transcript": "NM_152266", "txend": 32978224, "txstart": 32972241}], "exons_hg19": [{"cdsend": 33467588, "cdsstart": 33465007, "chr": "19", "position": [[33463147, 33463252], [33464965, 33465118], [33467336, 33469130]], "strand": 1, "transcript": "NM_001300978", "txend": 33469130, "txstart": 33463147}, {"cdsend": 33467588, "cdsstart": 33464102, "chr": "19", "position": [[33463147, 33463252], [33464089, 33464208], [33464331, 33464468], [33464965, 33465118], [33467336, 33469130]], "strand": 1, "transcript": "NM_152266", "txend": 33469130, "txstart": 33463147}], "generif": [{"pubmed": 17289582, "text": "FAAP24 targets FANCM to structures that mimic intermediates formed during the replication/repair of damaged DNA."}, {"pubmed": 18174376, "text": "FANCM is an anchor required for recruitment of the FA core complex to chromatin, and the FANCM/FAAP24 interaction is essential for this chromatin-loading activity"}, {"pubmed": 18206976, "text": "FAAP24 is dispensable for DNA binding and branch migration activity of FANCM."}, {"pubmed": 18995830, "text": "DNA damage recognition and remodeling activities of FANCM and FAAP24 cooperate to promote efficient activation of DNA damage checkpoints in Fanconi anemia."}, {"pubmed": 20670894, "text": "FANCM/FAAP24 plays a role in ICL-induced checkpoint activation through regulating RPA recruiment at ICL-stalled replication forks."}, {"pubmed": 23333308, "text": "FANCM and FAAP24 play multiple, while not fully epistatic, roles in maintaining genomic integrity."}, {"pubmed": 23932590, "text": "Crystal structure of the FANCM-FAAP24 complex."}, {"pubmed": 23999858, "text": "These results demonstrate dual roles of FAAP24 in DNA damage response against crosslinking lesions, one through the formation of FANCM/FAAP24 heterodimer and the other via its ssDNA-binding activity required in optimized checkpoint activation."}, {"pubmed": 24003026, "text": "Results show that the first HhH motif of FAAP24 is a potential binding site for DNA, which plays a critical role in targeting FANCM-FAAP24 to chromatin."}, {"pubmed": 27473539, "text": "This is the first report of an FAAP24 loss of function mutation found in human patients with EBV-associated lymphoproliferation."}], "genomic_pos": {"chr": "19", "end": 32978229, "ensemblgene": "ENSG00000131944", "start": 32972209, "strand": 1}, "genomic_pos_hg19": {"chr": "19", "end": 33469128, "start": 33463115, "strand": 1}, "go": {"BP": {"evidence": "IDA", "gocategory": "BP", "id": "GO:0036297", "pubmed": 20347429, "qualifier": "involved_in", "term": "interstrand cross-link repair"}, "CC": [{"evidence": "IDA", "gocategory": "CC", "id": "GO:0000785", "pubmed": 20347429, "qualifier": "part_of", "term": "chromatin"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0043231", "qualifier": "located_in", "term": "intracellular membrane-bounded organelle"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0043240", "qualifier": "part_of", "term": "Fanconi anaemia nuclear complex"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0043240", "pubmed": [20347428, 20347429], "qualifier": "part_of", "term": "Fanconi anaemia nuclear complex"}, {"evidence": "IPI", "gocategory": "CC", "id": "GO:0071821", "pubmed": 20347429, "qualifier": "part_of", "term": "FANCM-MHF complex"}], "MF": [{"category": "MF", "evidence": "IEA", "id": "GO:0003677", "qualifier": "enables", "term": "DNA binding"}, {"category": "MF", "evidence": "IBA", "id": "GO:0003682", "qualifier": "enables", "term": "chromatin binding"}, {"category": "MF", "evidence": "IDA", "id