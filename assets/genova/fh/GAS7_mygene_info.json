ons_hg19": [{"cdsend": 9923205, "cdsstart": 9820544, "chr": "17", "position": [[9813923, 9820658], [9821317, 9821416], [9822942, 9823022], [9828792, 9828916], [9829957, 9830086], [9837482, 9837561], [9843442, 9843517], [9846437, 9846553], [9850210, 9850300], [9862518, 9862572], [9872993, 9873079], [9885120, 9885201], [9923093, 9923214], [9929466, 9929602]], "strand": -1, "transcript": "NM_001130831", "txend": 9929602, "txstart": 9813923}, {"cdsend": 9862623, "cdsstart": 9820544, "chr": "17", "position": [[9813923, 9820658], [9821317, 9821416], [9822942, 9823022], [9828792, 9828916], [9829957, 9830086], [9837482, 9837561], [9843442, 9843517], [9846437, 9846553], [9850210, 9850300], [9862518, 9862812]], "strand": -1, "transcript": "NM_003644", "txend": 9862812, "txstart": 9813923}, {"cdsend": 9939753, "cdsstart": 9820544, "chr": "17", "position": [[9813923, 9820658], [9821317, 9821416], [9822942, 9823022], [9828792, 9828916], [9829957, 9830086], [9837482, 9837561], [9843442, 9843517], [9846437, 9846553], [9850210, 9850300], [9862518, 9862572], [9872993, 9873079], [9885120, 9885201], [9923093, 9923214], [9939750, 9939974]], "strand": -1, "transcript": "NM_201432", "txend": 9939974, "txstart": 9813923}, {"cdsend": 10101707, "cdsstart": 9820544, "chr": "17", "position": [[9813923, 9820658], [9821317, 9821416], [9822942, 9823022], [9828792, 9828916], [9829957, 9830086], [9837482, 9837561], [9843442, 9843517], [9846437, 9846553], [9850210, 9850300], [9862518, 9862572], [9872993, 9873079], [9885120, 9885201], [9923093, 9923214], [10101524, 10101923]], "strand": -1, "transcript": "NM_201433", "txend": 10101923, "txstart": 9813923}], "generif": [{"pubmed": 15948147, "text": "This work reports the identification of two human Gas7 cDNA: hGas7-a with 2,427 nucleotides, which encodes 330 amino acids, and hGas7-b with 2,610 nucleotides, which encodes 412 amino acids according to predicted open-reading-frames."}, {"pubmed": 19801671, "text": "hGas7b has a role in microtubular maintenance and is possibly implicated in Alzheimer disease"}, {"pubmed": 19851296, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19861958, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19913121, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20198315, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20628086, "text": "Observational study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 20890993, "text": "Knockdown of Gas7 by shRNA preferentially suppressed the Gas7b protein isoform in human preneuronal cells and to reduce apoptosis induced either by serum starvation or by the antineoplastic agents in human neuroblastoma SH-SY5Y cells."}, {"pubmed": 21452305, "text": "regulates the differentiation of mesenchymal stem cells into osteoblasts by enhancing Runx2-dependent gene expression"}, {"pubmed": 22496485, "text": "The results suggest that regulation to maintain an appropriate concentration of hGas7b is required for healthy neurotransmission."}, {"pubmed": 22570627, "text": "Intraocular pressure (IOP)was significantly associated with rs11656696, located in GAS7 at 17p13.1 and with rs7555523, located in TMCO1 at 1q24.1.These data suggest that we have identified two clinically relevant genes involved in IOP regulation."}, {"pubmed": 22662195, "text": "These findings indicate that Gas7 is involved in motor neuron function associated with muscle strength maintenance."}, {"pubmed": 24151073, "text": "We conclude that lower neuronal Gas7b levels may impact Alzheimer disease progression."}, {"pubmed": 26506240, "text": "Low GAS7C increases cancer cell motility by promoting N-WASP/FAK/F-actin cytoskeleton dynamics. It also enhances beta-catenin stability via hnRNP U/beta-TrCP complex formation."}, {"pubmed": 27189492, "text": "These results demonstrate GAS7 as a susceptibility gene for schizophrenia."}, {"pubmed": 29022762, "text": "Polymorphism rs11656696 is not associated with primary open angle glaucoma (POAG) nor any of its endophenotypic traits such as intraocular pressure and cup/disk ratio and is thus not a risk factor for POAG in this Saudi cohort"}, {"pubmed": 29261660, "text": "Among 8 SNPs in 3 loci that showed at least nominal association (P < 5.00E-02) in the primary cohort, a representative SNP for each loci (rs2157719 for CDKN2B-AS1, rs33912345 for SIX6, and rs9913911 for GAS7) were selected"}, {"pubmed": 29269300, "text": "Collectively, our findings provide a novel basis for using miR-181a/GAS7-based therapeutic strategies to reverse gefitinib resistance in NSCLC."}, {"pubmed": 29706651, "text": "A novel regulatory mechanism of p53 in early onset breast cancer progression through GAS7-CYFIP1-mediated signaling pathways."}, {"pubmed": 31269845, "text": "Single nucleotide polymorphism in GAS7 gene is associated as a protective factor for primary open-Angle Glaucoma."}, {"pubmed": 31296381, "text": "Reported here is the solution NMR structure of Gas7 SH3 domain which displays a typical SH3 beta-barrel fold comprising five beta-strands and one 310-helix. Structural and sequence comparison showed that Gas7-SH3 shares high similarity with Abl SH3 domain, which binds to a high-affinity proline-rich peptide P41 in a canonical SH3-ligand binding mode through two hydrophobic pockets and a specificity site in the RT-loop."}, {"pubmed": 32216967, "text": "Growth arrest specific protein 7 inhibits tau fibrillogenesis."}, {"pubmed": 33602789, "text": "GAS7 Deficiency Promotes Metastasis in MYCN-Driven Neuroblastoma."}, {"pubmed": 35400990, "text": "Association of ABCA1 (rs2472493) and GAS7 (rs9913911) gene variants with primary open-angle glaucoma in a Brazilian population."}, {"pubmed": 36908025, "text": "Circ_0078607 increases platinum drug sensitivity via miR-196b-5p/GAS7 axis in ovarian cancer."}, {"pubmed": 37769891, "text": "Gas7 attenuates hepatocellular carcinoma progression and chemoresistance through the PI3K/Akt signaling pathway."}, {"pubmed": 38240248, "text": "Single-nucleotide polymorphisms of TLR4 and GAS7 linked to primary open-angle glaucoma among patients of Shenyang, China."}, {"pubmed": 38429909, "text": "Adipose stem cell-derived exosomes promote wound healing by regulating the let-7i-5p/GAS7 axis."}], "genomic_pos": {"chr": "17", "end": 10198606, "ensemblgene": "ENSG00000007237", "start": 9910606, "strand": -1}, "genomic_pos_hg19": {"chr": "17", "end": 10101868, "start": 9813926, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0030041", "qualifier": "involved_in", "term": "actin filament polymerization"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0048812", "qualifier": "involved_in", "term": "neuron projection morphogenesis"}], "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0005737", "qualifier": "is_active_in", "term": "cytoplasm"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005884", "qualifier": "is_active_in", "term": "actin filament"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005886", "qualifier": "is_active_in", "term": "plasma membrane"}], "MF": [{"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [25416956, 31515488], "qualifier": "enables", "term": "protein binding"}, {"category": "MF", "evidence": "IBA", "id": "GO:0051015", "qualifier": "enables", "term": "actin filament binding"}]}, "homologene": {"genes": [[7955, 793701], [8364, 100145655], [9031, 417305], [9598, 455077], [9606, 8522], [9615, 608189], [9913, 614517], [10090, 14457], [10116, 85246]], "id": 69015}, "interpro": [{"desc": "WW domain", "id": "IPR001202", "short_desc": "WW_dom"}, {"desc": "WW domain superfamily", "id": "IPR036020", "short_desc": "WW_dom_sf