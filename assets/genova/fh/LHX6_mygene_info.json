124975884, 124976059], [124976252, 124976348], [124976561, 124976662], [124979346, 124979567], [124988654, 124988776], [124989126, 124989309], [124989687, 124989820]], "strand": -1, "transcript": "NM_001348190", "txend": 124989820, "txstart": 124964860}, {"cdsend": 124991019, "cdsstart": 124967038, "chr": "9", "position": [[124964860, 124967059], [124971892, 124971996], [124975884, 124976059], [124976252, 124976348], [124976561, 124976662], [124979346, 124979567], [124988654, 124988776], [124989126, 124989309], [124989687, 124989759], [124990935, 124991236]], "strand": -1, "transcript": "NM_014368", "txend": 124991236, "txstart": 124964860}, {"cdsend": 124991019, "cdsstart": 124966979, "chr": "9", "position": [[124964860, 124967059], [124975884, 124976059], [124976252, 124976348], [124976561, 124976662], [124979346, 124979567], [124988654, 124988776], [124989126, 124989309], [124989687, 124989759], [124990935, 124991236]], "strand": -1, "transcript": "NM_199160", "txend": 124991236, "txstart": 124964860}], "generif": [{"pubmed": 16732332, "text": "differentially methylated hypermethylation of LHX6 is a good cancer marker in HNSCC as well as in other kinds of neoplasias and confirm the importance of searching for markers of epigenetic dysregulation in cancer"}, {"pubmed": 20201926, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20428825, "text": "Hypermethylation of LHX6 is associated with cervical cancer."}, {"pubmed": 21240459, "text": "hLHX6.1 could be a new tumor suppressor gene in the cervix."}, {"pubmed": 22435649, "text": "found positive association of LHX6 with Tourette syndrome in some European populations, not others. LHX6 demonstrates a high degree of population differentiation and is correlated with the North to South axis of European genetic variation."}, {"pubmed": 23229549, "text": "LHX6 is a transcriptional repressor that interacts with pituitary homeobox 2 (PITX2) to regulate odontogenesis"}, {"pubmed": 24157876, "text": "LHX6 is a putative tumour suppressor gene with epigenetic silencing in lung cancer."}, {"pubmed": 26129710, "text": "LHX6 inhibits breast cancer cell proliferation and invasion via repression of the Wnt/beta-catenin signaling pathway."}, {"pubmed": 28396596, "text": "that down-regulation of miR-214 may reverse acquired resistance to erlotinib in NSCLC through mediating its direct target gene LHX6 expression."}, {"pubmed": 29226322, "text": "The orthotopic implantation model showed that miR-1290 overexpression promoted tumor growth while LHX6 overexpression inhibited it. MiR-1290 could promote glioma cell propagation and metastasis by inhibiting LHX6."}, {"pubmed": 29533904, "text": "The downregulation of LHX6 expression might be important in promoting cancer progression. [review]"}, {"pubmed": 30251953, "text": "expression of the single transcription factor LHX6 under GABAergic interneurons differentiation condition is sufficient to robustly induce human parvalbumin and somatostatin subtypes."}, {"pubmed": 31151060, "text": "LHX6 gene expression is regulated by DNA methylation and can inhibit the proliferation, invasion and migration through Wnt/beta-catenin and P53 signaling pathways during the Microcystin-LR-induced hepatocarcinogenesis."}, {"pubmed": 31907793, "text": "LHX6 is essential for the migration of human pluripotent stem cell-derived GABAergic interneurons."}, {"pubmed": 32248980, "text": "Methylation-mediated silencing of the LIM homeobox 6 (LHX6) gene promotes cell proliferation in human pancreatic cancer."}, {"pubmed": 34773443, "text": "Tumor suppressor LHX6 upregulation contributes to the inhibitory effect of miR-346 knockdown on colorectal cancer cell growth."}, {"pubmed": 35384355, "text": "The exon 12-containing LHX6 isoforms promote cervical cancer cell proliferation by regulating the MAPK signaling pathway."}, {"pubmed": 36154949, "text": "LHX6 promoter hypermethylation in oncological pediatric patients conceived by IVF."}], "genomic_pos": {"chr": "9", "end": 122229626, "ensemblgene": "ENSG00000106852", "start": 122202577, "strand": -1}, "genomic_pos_hg19": {"chr": "9", "end": 124991905, "start": 124964856, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0006357", "qualifier": "involved_in", "term": "regulation of transcription by RNA polymerase II"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0021799", "qualifier": "involved_in", "term": "cerebral cortex radially oriented cell migration"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0021800", "qualifier": "involved_in", "term": "cerebral cortex tangential migration"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0021853", "qualifier": "involved_in", "term": "cerebral cortex GABAergic interneuron migration"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0021884", "qualifier": "involved_in", "term": "forebrain neuron development"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0030182", "qualifier": "involved_in", "term": "neuron differentiation"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0048469", "qualifier": "involved_in", "term": "cell maturation"}], "CC": [{"evidence": "ISA", "gocategory": "CC", "id": "GO:0000785", "qualifier": "part_of", "term": "chromatin"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "is_active_in", "term": "nucleus"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "located_in", "term": "nucleus"}], "MF": [{"category": "MF", "evidence": "IBA", "id": "GO:0000977", "qualifier": "enables", "term": "RNA polymerase II transcription regulatory region sequence-specific DNA binding"}, {"category": "MF", "evidence": "IBA", "id": "GO:0000981", "qualifier": "enables", "term": "DNA-binding transcription factor activity, RNA polymerase II-specific"}, {"