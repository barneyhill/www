iated"}, {"pubmed": 27406698, "text": "Whole exome sequencing in an Italian pedigree suggests involvement of TECPR2 in a complex form of progressive motor neuron disease."}, {"pubmed": 30681437, "text": "The first was the Tectonin beta-propeller repeat containing 2 gene (TECPR2), with allele G on rs10149146 present in 15 controls and no AD cases. The Fisher exact test revealed statistically significant association to healthy brain aging with SNP rs10149146 on TECPR2 (P-value=4.055e-4)."}, {"pubmed": 33213269, "text": "Lysosomal targeting of autophagosomes by the TECPR domain of TECPR2."}, {"pubmed": 33847017, "text": "Clinical, neuroimaging, and molecular spectrum of TECPR2-associated hereditary sensory and autonomic neuropathy with intellectual disability."}, {"pubmed": 34933910, "text": "Multimodal bioinformatic analyses of the neurodegenerative disease-associated TECPR2 gene reveal its diverse roles."}], "genomic_pos": {"chr": "14", "end": 102502477, "ensemblgene": "ENSG00000196663", "start": 102362941, "strand": 1}, "genomic_pos_hg19": {"chr": "14", "end": 102968818, "start": 102829300, "strand": 1}, "go": {"BP": [{"evidence": "IEA", "gocategory": "BP", "id": "GO:0006914", "qualifier": "involved_in", "term": "autophagy"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0032527", "qualifier": "involved_in", "term": "protein exit from endoplasmic reticulum"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0032527", "pubmed": 26431026, "qualifier": "involved_in", "term": "protein exit from endoplasmic reticulum"}], "CC": {"evidence": "IEA", "gocategory": "CC", "id": "GO:0005737", "qualifier": "located_in", "term": "cytoplasm"}, "MF": {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": 20562859, "qualifier": "enables", "term": "protein binding"}}, "homologene": {"genes": [[7955, 569212], [8364, 100495806], [9031, 423468], [9544, 708959], [9598, 453177], [9606, 9895], [9615, 490864], [9913, 530121], [10090, 104859], [10116, 314456]], "id": 8897},