hg19": [{"cdsend": 15615318, "cdsstart": 15397939, "chr": "8", "position": [[15397771, 15398077], [15480588, 15480758], [15508205, 15508323], [15517015, 15517156], [15519664, 15519805], [15531255, 15531345], [15588174, 15588238], [15601046, 15601121], [15605883, 15605974], [15615299, 15615364], [15615538, 15618176]], "strand": 1, "transcript": "NM_001356429", "txend": 15618176, "txstart": 15397771}, {"cdsend": 15615318, "cdsstart": 15397939, "chr": "8", "position": [[15397771, 15398077], [15480588, 15480758], [15508205, 15508323], [15517015, 15517156], [15519664, 15519805], [15531255, 15531345], [15588174, 15588238], [15601046, 15601121], [15605883, 15605974], [15615299, 15615364], [15621711, 15624147]], "strand": 1, "transcript": "NM_006765", "txend": 15624147, "txstart": 15397771}, {"cdsend": 15621727, "cdsstart": 15397939, "chr": "8", "position": [[15397595, 15398077], [15480588, 15480758], [15508205, 15508323], [15517015, 15517156], [15519664, 15519805], [15531255, 15531345], [15588174, 15588238], [15601046, 15601121], [15605883, 15605974], [15621711, 15624158]], "strand": 1, "transcript": "NM_178234", "txend": 15624158, "txstart": 15397595}], "generif": [{"pubmed": 16270321, "text": "Down regulated in ovarian cancer or absent in ovarian cancer and impact survival."}, {"pubmed": 17641416, "text": "N33, STK11 (19p13) and TP53 might play a role in the development of metastasis in larynx and pharynx squamous cell carcinomas."}, {"pubmed": 18452889, "text": "TUSC3 is only the fifth gene implicated in NS-ARMR and the first for which mutations have been reported in more than one family."}, {"pubmed": 18455129, "text": "study shows that mutations in two OTase-subunit genes, N33/TUSC3 and IAP result in autosomal-recessive nonsyndromic mental retardation"}, {"pubmed": 19717468, "text": "Knockdown of either MagT1 or TUSC3 protein significantly lowers the total and free intracellular Mg(2+) concentrations in cell lines."}, {"pubmed": 20505342, "text": "These findings suggest that inactivation through methylation of the putative tumor suppressor genes N-33 may not be associated with colorectal carcinogenesis in UC."}, {"pubmed": 21513506, "text": "analysis of a novel deletion mutation in the TUSC3 gene in a consanguineous Pakistani family that may have a role in autosomal recessive nonsyndromic intellectual"}, {"pubmed": 21557188, "text": "Genotyping and linkage analysis excluded linkage of the GRIK2 gene and TUSC3 gene with mental retardation."}, {"pubmed": 21739581, "text": "A novel nonsense mutation in TUSC3 is responsible for non-syndromic autosomal recessive mental retardation"}, {"pubmed": 23404293, "text": "TUSC3 is a tumor suppressor gene in ovarian cancer."}, {"pubmed": 23825019, "text": "Homozygous deletion in TUSC3 causes syndromic intellectual disability."}, {"pubmed": 24435307, "text": "TUSC3 loss alters the ER stress response and accelerates prostate cancer growth in vivo"}, {"pubmed": 24685145, "text": "TUSC3 increases glycosylation efficiency for a subset of human glycoproteins by slowing glycoprotein folding."}, {"pubmed": 25086101, "text": "IGFII and N33 methylation status may be related to gastric carcinogenesis."}, {"pubmed": 25735931, "text": "Loss of TUSC3 alters the molecular response to endoplasmic reticulum stress."}, {"pubmed": 25966277, "text": "The TUSC3 gene is associated with mental retardation in the Qinba mountain area in China; the sixth exon of the TUSC3 gene may contribute to the risk of developing the disease."}, {"pubmed": 26427135, "text": "Report frequencies of short tandem repeat markers linked to TUSC3 (MRT7) or NSUN2 (MRT5) genes used for homozygosity mapping of recessive intellectual disability."}, {"pubmed": 26871953, "text": "decreased immunological TUSC3 staining is a factor prognostic of poor survival in pancreatic cancer patients."}, {"pubmed": 27148795, "text": "This paper supports the previous clinical descriptions of the condition caused by TUSC3 mutations and describes the seventh family with mutations in this gene, thus contributing to the genetic spectrum of mutations. This is the first report of a family from the Arabian peninsula with this form of Intellectual disability ."}, {"pubmed": 27177902, "text": "TUSC3 regulates proliferation and invasion of glioblastoma cells by inhibiting the activity of the Akt signaling pathway."}, {"pubmed": 27994502, "text": "Decreased Tumor Suppressor Candidate 3 Predicts Poor Prognosis of Patients with Esophageal Squamous Cell Carcinoma"}, {"pubmed": 28288641, "text": "SOX2 regulates the proliferation, migration and invasiveness of breast cancer cells through miR-181a-5p and miR-30e-5p which modulate TUSC3 protein levels"}, {"pubmed": 28303930, "text": "miR-UL112-3p exerts its oncogene function by directly targeting TUSC3 in Glioblastoma."}, {"pubmed": 28487226, "text": "study demonstrated an oncogenic role of TUSC3 in Non-small cell lung cancer and showed that dis-regulation of TUSC3 may affect tumour cell invasion and migration through possible involvement in the Hedgehog (Hh) signalling pathway."}, {"pubmed": 28901390, "text": "our data indicate that miR-132 induces temozolomide resistance and promotes the formation of cancer stem cell phenotypes by targeting TUSC3 in glioblastoma."}, {"pubmed": 28929175, "text": "TUSC3 can function both as an oncogene and as a tumor suppressor. (Review)"}, {"pubmed": 30098333, "text": "TUSC3 accelerates cancer growth and induces the epithelial-mesenchymal transition in non-small-cell lung cancer cells by upregulating claudin-1."}, {"pubmed": 30115537, "text": "TUSC3 may act as an oncogene in the progression of colorectal cancer."}, {"pubmed": 31582837, "text": "UHRF1-KAT7-mediated regulation of TUSC3 expression via histone methylation/acetylation is critical for the proliferation of colon cancer cells."}, {"pubmed": 32090352, "text": "Tumour suppressor candidate 3 inhibits biological function and increases endoplasmic reticulum stress of melanoma cells WM451 by regulating AKT/GSK3-beta/beta-catenin pathway."}, {"pubmed": 32338281, "text": "TUSC3 induces drug resistance and cellular stemness via Hedgehog signaling pathway in colorectal cancer."}, {"pubmed": 32653021, "text": "Concentrations of persistent organic pollutants in maternal plasma and epigenome-wide placental DNA methylation."}, {"pubmed": 35137520, "text": "TUSC3 inhibits cell proliferation and invasion in cervical squamous cell carcinoma via suppression of the AKT signalling pathway."}, {"pubmed": 36274132, "text": "Downregulation of TUSC3 promotes EMT and hepatocellular carcinoma progression through LIPC/AKT axis."}], "genomic_pos": {"chr": "8", "end": 15766649, "ensemblgene": "ENSG00000104723", "start": 15417215, "strand": 1}, "genomic_pos_hg19": {"chr": "8", "end": 15624158, "start": 15274724, "strand": 1}, "go": {"BP": [{"evidence": "NAS", "gocategory": "BP", "id": "GO:0006487", "pubmed": 18455129, "qualifier": "involved_in", "term": "protein N-linked glycosylation"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0015693", "pubmed": 19717468, "qualifier": "involved_in", "term": "magnesium ion transport"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0018279", "qualifier": "involved_in", "term": "protein N-linked glycosylation via asparagine"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0018279", "pubmed": 26864433, "qualifier": "involved_in", "term": "protein N-linked glycosylation via asparagine"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0050890", "pubmed": 18455129, "qualifier": "involved_in", "term": "cognition"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0055085", "qualifier": "involved_in", "term": "transmembrane transport"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:1903830", "qualifier": "involved_in", "term": "magnesium ion transmembrane transport"}], "CC": [{"evidence": "ISS", "gocategory": "CC", "id": "GO:0005739", "qualifier": "located_in", "term": "mitochondrion"}, {"evidence": "TAS", "gocategory": "CC", "id": "