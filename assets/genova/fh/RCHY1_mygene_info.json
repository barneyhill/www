{"AllianceGenome": "17479", "HGNC": "17479", "MIM": "607680", "_id": "25898", "_version": 1, "accession": {"genomic": ["AC096759.3", "CH471057.1", "CP068274.2", "NC_000004.12", "NC_060928.1", "NG_029152.1"], "protein": ["AAH15464.1", "AAH47393.1", "AAK96896.1", "AAL09356.1", "AAL76101.1", "AAX78233.1", "ACT35531.1", "ACT35532.1", "ACT35533.1", "ADD21555.1", "AEO00252.1", "AEO00253.1", "BAD92309.1", "BAG52375.1", "CAB43290.1", "EAX05724.1", "EAX05725.1", "EAX05726.1", "EAX05727.1", "EAX05728.1", "NP_001009922.1", "NP_001265465.1", "NP_001265466.1", "NP_001265467.1", "NP_001265468.1", "NP_001374065.1", "NP_001374066.1", "NP_056251.2", "Q96PM5.1", "XP_024309752.1", "XP_047305992.1", "XP_054205595.1", "XP_054205596.1"], "rna": ["AB209072.1", "AF247041.1", "AF255666.1", "AF305424.1", "AK091501.1", "AK308754.1", "AK308848.1", "AK309555.1", "AL050144.1", "AW236209.1", "AY888047.1", "BC015464.1", "BC031057.1", "BC047393.1", "BF102905.1", "BG703058.1", "BI913695.1", "BP314571.1", "BQ024871.1", "BQ959587.1", "CB241669.1", "CF597260.1", "DA197338.1", "DB044826.1", "DN919417.1", "GQ250944.1", "GQ250945.1", "GQ250946.1", "GU937000.1", "JF691538.1", "JF691539.1", "NM_001008925.2", "NM_001009922.3", "NM_001278536.2", "NM_001278537.2", "NM_001278538.2", "NM_001278539.2", "NM_001387136.1", "NM_001387137.1", "NM_015436.4", "NR_037913.1", "NR_037914.1", "NR_103723.1", "NR_103724.1", "NR_103725.1", "XM_024453984.2", "XM_047450036.1", "XM_054349620.1", "XM_054349621.1"], "translation": [{"protein": "NP_001374065.1", "rna": "NM_001387136.1"}, {"protein": "AAH15464.1", "rna": "BC015464.1"}, {"protein": "ADD21555.1", "rna": "GU937000.1"}, {"protein": "AAX78233.1", "rna": "AY888047.1"}, {"protein": "AAL09356.1", "rna": "AF305424.1"}, {"protein": "ACT35533.1", "rna": "GQ250946.1"}, {"protein": "NP_001374066.1", "rna": "NM_001387137.1"}, {"protein": "AAH47393.1", "rna": "BC047393.1"}, {"protein": "NP_001265468.1", "rna": "NM_001278539.2"}, {"protein": "AEO00253.1", "rna": "JF691539.1"}, {"pro