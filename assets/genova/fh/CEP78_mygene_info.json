, {"cdsend": 80885767, "cdsstart": 80851266, "chr": "9", "position": [[80850990, 80851519], [80854938, 80855111], [80855207, 80855280], [80856611, 80856715], [80858377, 80858552], [80861584, 80861698], [80863206, 80863271], [80863677, 80863789], [80866820, 80866959], [80868147, 80868193], [80869751, 80869880], [80877822, 80877900], [80879065, 80879232], [80880287, 80880459], [80880774, 80880822], [80881357, 80881619], [80885756, 80894606]], "strand": 1, "transcript": "NM_001349839", "txend": 80894606, "txstart": 80850990}, {"cdsend": 80885767, "cdsstart": 80851266, "chr": "9", "position": [[80850990, 80851519], [80854938, 80855111], [80855207, 80855280], [80856611, 80856715], [80858377, 80858552], [80861584, 80861698], [80863206, 80863271], [80863677, 80863789], [80866820, 80866959], [80868147, 80868193], [80869751, 80869880], [80877822, 80877900], [80879065, 80879232], [80880287, 80880459], [80881357, 80881619], [80885756, 80894606]], "strand": 1, "transcript": "NM_001349840", "txend": 80894606, "txstart": 80850990}, {"cdsend": 80881678, "cdsstart": 80851266, "chr": "9", "position": [[80850990, 80851519], [80854938, 80855111], [80855207, 80855280], [80856611, 80856715], [80858377, 80858552], [80861584, 80861698], [80863206, 80863271], [80863677, 80863789], [80866820, 80866959], [80868147, 80868193], [80869751, 80869880], [80877822, 80877900], [80879065, 80879232], [80880287, 80880459], [80881357, 80881991]], "strand": 1, "transcript": "NM_032171", "txend": 80881991, "txstart": 80850990}], "generif": [{"pubmed": 27246242, "text": "the interaction between Cep78 and the N-terminal catalytic domain of Plk4 is a new and important element in the centrosome overduplication process."}, {"pubmed": 27357513, "text": "CEP78 functions as a tumor suppressor in colorectal cancer and low CEP78 expression leads to shorter survival in colorectal cancer patients."}, {"pubmed": 27588451, "text": "data strongly suggest that mutations in CEP78 cause a previously undescribed clinical entity of a ciliary nature characterized by blindness and deafness but clearly distinct from Usher syndrome, a condition for which visual impairment is due to retinitis pigmentosa"}, {"pubmed": 27588452, "text": "truncating mutations in CEP78 result in a phenotype involving both the visual and auditory systems but different from typical Usher syndrome"}, {"pubmed": 27627988, "text": "Our results provide evidence that CEP78 is a novel disease-causing gene for Usher syndrome, demonstrating an additional link between ciliopathy and Usher protein network in photoreceptor cells and inner ear hair cells."}, {"pubmed": 28242748, "text": "we identify Cep78 as a new player that regulates centrosome homeostasis by inhibiting the final step of the enzymatic reaction catalyzed by EDD-DYRK2-DDB1(Vpr)(BP)."}, {"pubmed": 30884127, "text": "Low CEP78 expression is associated with differentiated thyroid carcinoma."}, {"pubmed": 31999394, "text": "Functional characterization of the first missense variant in CEP78, a founder allele associated with cone-rod dystrophy, hearing loss, and reduced male fertility."}, {"pubmed": 33119552, "text": "Centrosome Protein 78 Is Overexpressed in Muscle-Invasive Bladder Cancer and Is Associated with Tumor Molecular Subtypes and Mutation Signatures."}, {"pubmed": 34223797, "text": "Expanding the clinical phenotype in patients with disease causing variants associated with atypical Usher syndrome."}, {"pubmed": 34259627, "text": "CEP78 functions downstream of CEP350 to control biogenesis of primary cilia by negatively regulating CP110 levels."}, {"pubmed": 35240912, "text": "A novel frameshift variant in CEP78 associated with nonsyndromic retinitis pigmentosa, and a review of CEP78-related phenotypes."}, {"pubmed": 36206347, "text": "Loss-of-function mutations in CEP78 cause male infertility in humans and mice."}], "genomic_pos": {"chr": "9", "end": 78279690, "ensemblgene": "ENSG00000148019", "start": 78236062, "strand": 1}, "genomic_pos_hg19": {"chr": "