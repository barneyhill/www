99328428, 99328500], [99347143, 99347357], [99353301, 99353546], [99365249, 99365595], [99374352, 99374936], [99375580, 99375698], [99382637, 99382755], [99395681, 99395822]], "strand": -1, "transcript": "NM_001278716", "txend": 99395822, "txstart": 99316410}, {"cdsend": 99374864, "cdsstart": 99322153, "chr": "6", "position": [[99321600, 99322317], [99323290, 99323603], [99328428, 99328500], [99347143, 99347357], [99353301, 99353546], [99365249, 99365595], [99374352, 99374936], [99375580, 99375698], [99395681, 99395882]], "strand": -1, "transcript": "NM_012160", "txend": 99395882, "txstart": 99321600}, {"cdsend": 99395822, "cdsstart": 99395822, "chr": "6", "position": [[99316410, 99322317], [99323290, 99323603], [99328428, 99328500], [99347143, 99347357], [99353301, 99353546], [99374352, 99374936], [99375580, 99375698], [99395681, 99395822]], "strand": -1, "transcript": "NR_103836", "txend": 99395822, "txstart": 99316410}, {"cdsend": 99395822, "cdsstart": 99395822, "chr": "6", "position": [[99344607, 99347357], [99353301, 99353546], [99374352, 99374936], [99375580, 99375698], [99395681, 99395822]], "strand": -1, "transcript": "NR_103837", "txend": 99395822, "txstart": 99344607}], "generif": [{"pubmed": 21757720, "text": "SKP1-Cul1-F-box and leucine-rich repeat protein 4 (SCF-FbxL4) ubiquitin ligase regulates lysine demethylase 4A (KDM4A)/Jumonji domain-containing 2A (JMJD2A) protein"}, {"pubmed": 23993193, "text": "Mutations in FBXL4 are disease causing and establish FBXL4 as a mitochondrial protein with a possible role in maintaining mtDNA integrity and stability."}, {"pubmed": 23993194, "text": "These data strongly support a role for FBXL4 in controlling bioenergetic homeostasis and mtDNA maintenance."}, {"pubmed": 25868664, "text": "A clinical pattern of early-onset encephalopathy, persistent lactic acidosis, profound muscular hypotonia and typical facial dysmorphism should prompt initiation of molecular genetic analysis of FBXL4."}, {"pubmed": 27182039, "text": "On clinical indication of mitochondrial encephalomyopathy, sequencing of FBXL4 should be performed, even when the activity levels of the MRC enzymes are normal"}, {"pubmed": 27743463, "text": "Overall, FBXL4 defects account for at least 0.7% (6 out of 808) of subjects suspected to have a mitochondrial disorder, and as high as 14.3% (4 out of 28) in young children with congenital lactic acidosis and clinical features of mitochondrial disease. Including FBLX4 in the mitochondrial diseases panel should be particularly important for patients with congenital lactic acidosis"}, {"pubmed": 28698647, "text": "Study found a common genomic copy number loss at 6q16.1-16.2, containing the FBXL4 gene in prostate cancer bone metastases. Loss of FBXL4 was also detected in primary tumors and it was highly associated with prognostic factors including high Gleason score, PSA, as well as poor patient survival, suggesting that FBXL4 loss contributes to prostate cancer progression."}, {"pubmed": 28940506, "text": "Biallelic pathogenic variants in FBXL4 are associated with an encephalopathic mtDNA maintenance defect syndrome that is a multisystem disease."}, {"pubmed": 31442532, "text": "FBXL4 protein promotes mitochondrial fusion. The C584R FBXL4 variant cannot promote mitochondrial fusion."}, {"pubmed": 32525278, "text": "FBXL4 deficiency increases mitochondrial removal by autophagy."}, {"pubmed": 32559514, "text": "Novel homozygous mutation in the FBXL4 gene is associated with mitochondria DNA depletion syndrome-13."}, {"pubmed": 37102372, "text": "FBXL4 ubiquitin ligase deficiency promotes mitophagy by elevating NIX levels."}, {"pubmed": 37161784, "text": "FBXL4 suppresses mitophagy by restricting the accumulation of NIX and BNIP3 mitophagy receptors."}, {"pubmed": 37876279, "text": "Excessive BNIP3- and BNIP3L-dependent mitophagy underlies the pathogenesis of FBXL4-mutated mitochondrial DNA depletion syndrome."}], "genomic_pos": {"chr": "6", "end": 98948006, "ensemblgene": "ENSG000