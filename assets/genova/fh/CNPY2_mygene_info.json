dsstart": 56704335, "chr": "12", "position": [[56703627, 56704379], [56704741, 56704838], [56704994, 56705198], [56708634, 56708750], [56708913, 56709026], [56709604, 56709772]], "strand": -1, "transcript": "NM_014255", "txend": 56709772, "txstart": 56703627}], "generif": [{"pubmed": 12826659, "text": "MSAP is a novel MIR-interacting protein that enhances neurite outgrowth and increases myosin regulatory light chain in fetal and adult brain."}, {"pubmed": 19240061, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 22378787, "text": "Our results are consistent with an important role of FGF21 and Cnpy2/Msap in the regulation of LDLRs in cultured cells."}, {"pubmed": 25589425, "text": "CNPY2 is a HIF-1alpha-regulated, secreted angiogenic growth factor that promotes smooth muscle cell migration, proliferation, and tissue revascularization."}, {"pubmed": 26835537, "text": "CNPY2 may play a critical role in colorectal cancer development by enhancing cell proliferation, migration, and angiogenesis and by inhibiting apoptosis through negative regulation of the p53 pathway"}, {"pubmed": 28235487, "text": "These findings suggested that CNPY2 promoted cancer cell growth in renal cell carcinoma cells through regulating TP53 gene expression."}, {"pubmed": 29135454, "text": "CNPY2 isoform2 represents as a novel and valuable prognostic indicator for colorectal cancer patients."}, {"pubmed": 29569784, "text": "Overexpression of CNPY2 can activate the AKT/GSK3beta pathway, which leads to the inactivation of GSK-3beta. The inactivation of GSK-3beta increases the level of Snail, and then decreases the expression of E-cadherin to promote EMT."}, {"pubmed": 29864955, "text": "CNPY2 can serve as a therapeutic target to promote the effect of chemotherapy in non-small cell lung cancer."}, {"pubmed": 30070972, "text": "This study showed that serum CNPY2 isoform 2 may be a valuable biomarker for the early detection of colorectal cancer and presents an improvement in the diagnostic efficiency by combination of CEA and CA19-9."}, {"pubmed": 30415246, "text": "CNPY2 knockout resulted in the significant suppression of MHCC97H cell proliferation, tumor growth, and hemorrhage."}, {"pubmed": 31047986, "text": "Knockout of Cnpy2 resulted in up-regulation of p16(INK4a)."}, {"pubmed": 31894275, "text": "miR30a3p suppresses the proliferation and migration of lung adenocarcinoma cells by downregulating CNPY2."}, {"pubmed": 33482578, "text": "Sleeping Beauty insertional mutagenesis screen identifies the pro-metastatic roles of CNPY2 and ACTN2 in hepatocellular carcinoma tumor progression."}, {"pubmed": 33721832, "text": "Hypoxia-induced CNPY2 upregulation promotes glycolysis in cervical cancer through activation of AKT pathway."}], "genomic_pos": {"chr": "12", "end": 56316119, "ensemblgene": "ENSG00000257727", "start": 56309842, "strand": -1}, "genomic_pos_hg19": {"chr": "12", "end": 56710120, "start": 56703626, "strand": -1}, "go": {"BP": [{"evidence": "ISS", "gocategory": "BP", "id": "GO:0010629", "pubmed": 22378787, "qualifier": "acts_upstream_of", "term": "negative regulation of gene expression"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0010988", "pubmed": 22378787, "qualifier": "acts_upstream_of", "term": "regulation of low-density lipoprotein particle clearance"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:1905599", "pubmed": 22378787, "qualifier": "acts_upstream_of", "term": "positive regulation of low-density lipoprotein receptor activity"}], "CC": {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005783", "qualifier": "is_active_in", "term": "endoplasmic reticulum"}, "MF": {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [12826659, 32296183], "qualifier": "enables", "term": "protein binding"}}, "homologene": {"genes": [[7955, 664684], [8364, 549005], [9031, 100858933], [9598, 451994], [9606, 10330], [9615, 474403], [9913, 506534], [10090, 56530], [10116, 685814]], "id": 40934}, "in