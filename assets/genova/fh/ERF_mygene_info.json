, "nontcga": {"exp_lof": 12.8002951657, "exp_mis": 238.981267078, "exp_syn": 115.312440024, "lof_z": 2.97787787392668, "mis_z": 2.93210751067789, "mu_lof": 1.09558049674e-06, "mu_mis": 2.28736986146e-05, "mu_syn": 1.09792311323e-05, "n_lof": 2.0, "n_mis": 148.0, "n_syn": 130.0, "p_li": 0.741186125903004, "p_null": 0.000498402080388547, "p_rec": 0.258315472016607, "syn_z": -0.866034446918787}, "transcript": "ENST00000222329.4"}, "exons": [{"cdsend": 42250362, "cdsstart": 42248464, "chr": "19", "position": [[42247560, 42249738], [42249826, 42249942], [42250330, 42250565], [42254536, 42254635]], "strand": -1, "transcript": "NM_001301035", "txend": 42254635, "txstart": 42247560}, {"cdsend": 42250362, "cdsstart": 42248464, "chr": "19", "position": [[42247568, 42249738], [42249826, 42249942], [42250330, 42250565], [42253841, 42253899]], "strand": -1, "transcript": "NM_001308402", "txend": 42253899, "txstart": 42247568}, {"cdsend": 42250362, "cdsstart": 42248464, "chr": "19", "position": [[42247568, 42249738], [42249826, 42249942], [42250330, 42250565], [42255103, 42255128]], "strand": -1, "transcript": "NM_001312656", "txend": 42255128, "txstart": 42247568}, {"cdsend": 42254999, "cdsstart": 42248464, "chr": "19", "position": [[42247568, 42249738], [42249826, 42249942], [42250330, 42250565], [42254977, 42255128]], "strand": -1, "transcript": "NM_006494", "txend": 42255128, "txstart": 42247568}], "exons_hg19": [{"cdsend": 42754514, "cdsstart": 42752616, "chr": "19", "position": [[42751712, 42753890], [42753978, 42754094], [42754482, 42754717], [42758688, 42758787]], "strand": -1, "transcript": "NM_001301035", "txend": 42758787, "txstart": 42751712}, {"cdsend": 42754514, "cdsstart": 42752616, "chr": "19", "position": [[42751720, 42753890], [42753978, 42754094], [42754482, 42754717], [42757993, 42758051]], "strand": -1, "transcript": "NM_001308402", "txend": 42758051, "txstart": 42751720}, {"cdsend": 42754514, "cdsstart": 42752616, "chr": "19", "position": [[42751720, 42753890], [42753978, 42754094], [42754482, 42754717], [42759255, 42759280]], "strand": -1, "transcript": "NM_001312656", "txend": 42759280, "txstart": 42751720}, {"cdsend": 42759151, "cdsstart": 42752616, "chr": "19", "position": [[42751720, 42753890], [42753978, 42754094], [42754482, 42754717], [42759129, 42759280]], "strand": -1, "transcript": "NM_006494", "txend": 42759280, "txstart": 42751720}], "generif": [{"pubmed": 12533699, "text": "Ets-2 Repressor Factor (ERF) physically interacts with the cytomegalovirus major immediate-early promoter (MIEP) and represses MIEP activity in undifferentiated non-permissive T2 embryonal carcinoma cells"}, {"pubmed": 14611815, "text": "a complex interplay between Ets/Id family members and c-Myc that may be an important determinant of the diversity of telomerase activity in leukemia and other cancers"}, {"pubmed": 14678752, "text": "Chromosome 21 transcription factor Ets2, overexpressed in Down syndrome, is expressed in neurons. Overexpression causes increased apoptosis of neurons from Ets2 transgenic mice and also involves activation of caspase-3."}, {"pubmed": 17525531, "text": "Differential repression of c-myc and cdc2 gene expression by ERF and PE-1/METS."}, {"pubmed": 17699159, "text": "Erf provides a direct link between the RAS/ERK signaling and the transcriptional regulation of c-Myc and suggests that RAS/ERK attenuation actively regulates cell fate"}, {"pubmed": 22198386, "text": "Data demonstrate that induction of EGR1 involves ERK-mediated down-regulation of microRNA-191 and phosphorylation of the ETS2 repressor factor (ERF) repressor, which subsequently leaves the nucleus."}, {"pubmed": 23354439, "text": "This work identifies ERF as a novel regulator of osteogenic stimulation by RAS-ERK signaling, potentially by competing with activating ETS factors in multifactor transcriptional complexes"}, {"pubmed": 26097063, "text": "ERF-related craniosynostosis should be suspected in patients presenting with multiple suture or sagittal synostosis"}, {"pubmed": 27738187, "text": "We report the molecular aetiology of Chitayat syndrome and discuss potential mechanisms for this distinctive phenotype associated with the p.Tyr89Cys substitution in ERF."}, {"pubmed": 28515055, "text": "Systematic genomic sequencing of prostate cancer in African-American men revealed new insights into prostate cancer, including the identification of ERF as a prostate cancer gene; somatic copy-number alteration differences; and uncommon PIK3CA and PTEN alterations. This study highlights the importance of inclusion of underrepresented minorities in cancer sequencing studies."}, {"pubmed": 28614298, "text": "data provide evidence that the oncogenicity of ERG is mediated, in part, by competition with ERF and they raise the larger question of whether other gain-of-function oncogenic transcription factors might also inactivate endogenous tumour suppressors"}, {"pubmed": 28750683, "text": "Identification of novel prostate cancer drivers, ERF, CREB3L1, and POU2F2, using RegNetDriver, a framework for integration of genetic and epigenetic alterations with tissue-specific regulatory network."}, {"pubmed": 30758909, "text": "Mutation in the ERF gene is associated with craniosynostosis syndrome."}, {"pubmed": 32370745, "text": "Here we describe a boy and his mother with different craniosynostosis patterns, but both with verified intracranial hypertension and heterozygosity for a truncating variant of ERF c.1201_1202delAA (p.Lys401Glufs*10)."}, {"pubmed": 32592542, "text": "Variable pulmonary manifestations in Chitayat syndrome: Six additional affected individuals."}, {"pubmed": 32986930, "text": "Identification and analysis of micro-exons in AP2/ERF and MADS gene families."}, {"pubmed": 33993607, "text": "Dissection of contiguous gene effects for deletions around ERF on chromosome 19."}, {"pubmed": 34117072, "text": "Deletion of ERF and CIC causes abnormal skull morphology and global developmental delay."}, {"pubmed": 35761471, "text": "Cognitive, Behavioural, Speech, Language and Developmental Outcomes Associated with Pathogenic Variants in the ERF Gene."}, {"pubmed": 36383412, "text": "The CIC-ERF co-deletion underlies fusion-independent activation of ETS family member, ETV1, to drive prostate cancer progression."}, {"pubmed": 38218889, "text": "Activation of gamma-globin expression by LncRNA-mediated ERF promoter hypermethylation in beta-thalassemia."}], "genomic_pos": {"chr": "19", "end": 42255128, "ensemblgene": "ENSG00000105722", "start": 42247569, "strand": -1}, "genomic_pos_hg19": {"chr": "19", "end": 42759309, "start": 42751724, "strand": -1}, "go": {"BP": [{"evidence": "IDA", "gocategory": "BP", "id": "GO:0000122", "pubmed": 7588608, "qualifier": "involved_in", "term": "negative regulation of transcription by RNA polymerase II"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006357", "qualifier": "involved_in", "term": "regulation of transcription by RNA polymerase II"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0030154", "qualifier": "involved_in", "term": "cell differentiation"}], "CC": [{"evidence": "ISA", "gocategory": "CC", "id": "GO:0000785", "qualifier": "part_of", "term": "chromatin"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier": "is_active_in", "term": "nucleus"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005829", "qualifier": "located_in", "term": "cytosol"}], "MF": [{"category": "MF", "evidence": "IBA", "id": "GO:0000981", "qualifier": "enables", "term": "DNA-binding transcription factor activity, RNA polymerase II-specific"}, {"category": "MF", "evidence": "ISA", "id": "GO:0000981", "qualifier": "ena