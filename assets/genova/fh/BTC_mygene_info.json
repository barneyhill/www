on": [[75669969, 75671885], [75673249, 75673359], [75675782, 75675929], [75681068, 75681186], [75695267, 75695366], [75719471, 75719733]], "strand": -1, "transcript": "NM_001729", "txend": 75719733, "txstart": 75669969}], "generif": [{"pubmed": 12074582, "text": "The solution structure of the EGF-like domain of betacellulin has been determined using two-dimensional nuclear magnetic resonance spectroscopy"}, {"pubmed": 12111392, "text": "The main structural properties of the model and the templates are compared and the hBTC conformation is closely similar to that of hTGFalpha."}, {"pubmed": 12768307, "text": "Betacellulin and heregulin/NDF-alpha are involved in epidermal morphogenesis and/or in maintenance of the differentiated phenotype."}, {"pubmed": 15192046, "text": "although BTC and EGF share overlapping signaling properties, the ability of BTC to enhance Erk activation occurs independent of Ras."}, {"pubmed": 15248827, "text": "BTC may play a pivotal role as a local growth factor in promoting the differentiated villous trophoblastic function via ErbB-1 in early placentas and in contributing to placental growth through EVT cell function via ErbB-4 in term placentas."}, {"pubmed": 15274392, "text": "Betacellulin is expressed in malignant fibrous histiocytoma and is a regulator of tumor growth."}, {"pubmed": 15507448, "text": "shedding of precursor is mediated by ADAM10"}, {"pubmed": 15793259, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 15793259, "text": "To determine if mutations in the betacellulin gene play a role in the development of type 2 diabetes, we screened subjects with type 2 diabetes for the presence of mutations."}, {"pubmed": 15936459, "text": "Genetic variations in the protein-coding region of the human BTC gene are unlikely to be a major contributor to development of type 2 diabetes."}, {"pubmed": 15936459, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 16306376, "text": "-226A/G polymorphism of the BTC gene may contribute to the development of diabetes."}, {"pubmed": 16306376, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 16683131, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 16683131, "text": "intron 4 T-4 allele in the betacellulin gene is associated with lower risk of type 1 diabetes mellitus and may interact with human leukocyte antigen"}, {"pubmed": 16869959, "text": "Failure to confirm a role for nonsynonymous coding variants of betacellulin in the propensity to type 2 diabetes or to impaired insulin secretion in African American subjects."}, {"pubmed": 16869959, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 17479438, "text": "Variants in the betacellulin gene do not play a major role in the development of type 2 diabetes in an Amish Caucasian populations"}, {"pubmed": 17525275, "text": "In vivo, EGFR signaling is hyperactive in tumor cells of skin SCC but not of BCC, and in nearby asymptomatic epidermis of both tumor types. Hyperactivation is due to upregulation of EGFR ligands AREG, HBEGF and TGFA, and downregulation of BTC"}, {"pubmed": 17895248, "text": "the ADAM10 prodomain inhibits betacellulin shedding, demonstrating that it could be of potential use as a therapeutic agent to treat cancer."}, {"pubmed": 17962208, "text": "1st report of BTC expression in breast cancer. Its expression was lower in lobular breast cancers than in ductal carcinomas."}, {"pubmed": 18508082, "text": "These results indicate the possibility of designing BTC mutants, which have an activity of inducing differentiation only, without facilitating growth promotion."}, {"pubmed": 20165885, "text": "These are the first data that demonstrate an influence of betacellulin upon mesenchymal stem cells and the first to implicate HIF-alpha in betacellulin-mediated proliferation."}, {"pubmed": 20530572, "text": "Data suggest a novel receptor-independent role for betacellulin intracellular-domain fragment signaling due to its ability to inhibit cell growth in vitro."}, {"pubmed": 22183345, "text": "BTC has properties of increasing retinal vascular leakage that could contribute to the development of diabetic retinopathy."}, {"pubmed": 24092824, "text": "Data suggest that BTC (betacellulin), AREG (amphiregulin), and EREG (epiregulin) induced prostaglandin E2 production by induction of COX-2 (prostaglandin-endoperoxide synthase 2) through MAP kinase signaling in granulosa cells."}, {"pubmed": 24629040, "text": "CXCL8 production from lung cancer cells could be initiated by an autocrine mechanism or external sources of BTC."}, {"pubmed": 27129169, "text": "Our results suggest that betacellulin induces ovarian cancer migration and Slug-dependent E-cadherin down-regulation"}, {"pubmed": 30249613, "text": "Despite both ligands binding to the EGFR, BTC biases the EGFR to dimerize with ErbB3 to regulate the biologic response."}, {"pubmed": 31654720, "text": "Betacellulin enhances ovarian cancer cell migration by up-regulating Connexin43 via MEK-ERK signaling."}, {"pubmed": 37231448, "text": "Betacellulin regulates gap junction intercellular communication by inducing the phosphorylation of connexin 43 in human granulosa-lutein cells."}], "genomic_pos": {"chr": "4", "end": 74794523, "ensemblgene": "ENSG00000174808", "start": 74744759, "strand": -1}, "genomic_pos_hg19": [{"chr": "4", "end": 75719896, "start": 75669969, "strand": -1}, {"chr": "HG706_PATCH", "end": 75660403, "start": 75610476, "strand": -1}], "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0007173", "qualifier": "involved_in", "term": "epidermal growth factor receptor signaling pathway"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0008283", "qualifier": "involved_in", "term": "cell population proliferation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0008284", "qualifier": "involved_in", "term": "posi