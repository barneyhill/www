[7676402, 7676472], [7676572, 7678049], [7680068, 7680208], [7680303, 7680605], [7681408, 7681508], [7682211, 7682324], [7682428, 7682547], [7682637, 7683190]], "strand": 1, "transcript": "NM_001080429", "txend": 7683190, "txstart": 7660748}, {"cdsend": 7682943, "cdsstart": 7660888, "chr": "19", "position": [[7660748, 7661036], [7670111, 7670365], [7671156, 7671279], [7671361, 7671457], [7673011, 7673150], [7675361, 7675501], [7675585, 7675679], [7675762, 7675817], [7675980, 7676054], [7676402, 7676472], [7676572, 7678049], [7680068, 7680208], [7680303, 7680605], [7681408, 7681508], [7682211, 7682324], [7682428, 7682547], [7682637, 7683190]], "strand": 1, "transcript": "NM_020902", "txend": 7683190, "txstart": 7660748}], "generif": [{"pubmed": 19041755, "text": "KIAA1543 is a non-centrosomal minus end binding protein. It is termed Nezha(a character in Chinese mythic novel (Journey to the West))."}, {"pubmed": 19508979, "text": "The CKK domain binds microtubules and represents a domain that evolved with the metazoa."}, {"pubmed": 26715742, "text": "These findings demonstrate that apically localized CAMSAP3 determines the proper orientation of microtubules, and in turn that of organelles, in mature mammalian epithelial cells."}, {"pubmed": 27349180, "text": "data suggest that CDH23-C is a CAMSAP3/Marshalin-binding protein that can modify MT networks indirectly through its interaction with CAMSAP3/Marshalin."}, {"pubmed": 27693509, "text": "ACF7, a member of the spectraplakin family of cytoskeletal crosslinking proteins, interacts with Nezha (also called CAMSAP3) at the minus ends of noncentrosomal microtubules and anchors them to actin filaments."}, {"pubmed": 27802168, "text": "in mammalian intestinal epithelial cells, the spectraplakin ACF7 (also known as MACF1) specifically binds to CAMSAP3 and is required for the apical localization of CAMSAP3-decorated microtubule minus ends."}, {"pubmed": 28089391, "text": "CAMSAP3-dependent Golgi vesicle clustering and graded microtubule dynamics"}, {"pubmed": 30282632, "text": "Findings suggest that CAMSAP3 functions to protect lung carcinoma cells against EMT by suppressing Akt activity via microtubule regulation and that CAMSAP3 loss promotes EMT in these cells."}, {"pubmed": 33566684, "text": "Rab14/MACF2 complex regulates endosomal targeting during cytokinesis."}, {"pubmed": 37019300, "text": "CAMSAP3 negatively regulates lung cancer cell invasion and angiogenesis through nucleolin/HIF-1alpha mRNA complex stabilization."}, {"pubmed": 38190868, "text": "CAMSAP3, a microtubule orientation regulator, plays a vital role in manifesting differentiation-dependent characteristics in keratinocytes."}], "genomic_pos": {"chr": "19", "end": 7618304, "ensemblgene": "ENSG00000076826", "start": 7595863, "strand": 1}, "genomic_pos_hg19": {"chr": "19", "end": 7683190, "start": 7660788, "strand": 1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0000076", "qualifier": "involved_in", "term": "DNA replication checkpoint signaling"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0000226", "pubmed": [24486153, 27693509], "qualifier": "involved_in", "term": "microtubule cytoskeleton organization"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0000226", "pubmed": 23169647, "qualifier": "involved_in", "term": "microtubule cytoskeleton organization"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0001701", "qualifier": "involved_in", "term": "in utero embryonic development"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0003341", "qualifier": "involved_in", "term": "cilium movement"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0009792", "qualifier": "involved_in", "term": "embryo development ending in birth or egg hatching"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0030334", "pubmed": 27693509, "qualifier": "involved_in", "term": "regulation of cell migration"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0030951", "pubmed": [26715742, 27802168], "qua