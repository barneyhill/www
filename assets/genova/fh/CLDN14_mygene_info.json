nscript": "ENST00000399137.1"}, "exons": [{"cdsend": 36461695, "cdsstart": 36460975, "chr": "21", "position": [[36460620, 36461776], [36510362, 36510500], [36576410, 36576569]], "strand": -1, "transcript": "NM_001146077", "txend": 36576569, "txstart": 36460620}, {"cdsend": 36461695, "cdsstart": 36460975, "chr": "21", "position": [[36460620, 36461776], [36510362, 36510500], [36542564, 36542784]], "strand": -1, "transcript": "NM_001146078", "txend": 36542784, "txstart": 36460620}, {"cdsend": 36461695, "cdsstart": 36460975, "chr": "21", "position": [[36460620, 36461776], [36479494, 36480122]], "strand": -1, "transcript": "NM_001146079", "txend": 36480122, "txstart": 36460620}, {"cdsend": 36461695, "cdsstart": 36460975, "chr": "21", "position": [[36460620, 36461776], [36466366, 36466467]], "strand": -1, "transcript": "NM_012130", "txend": 36466467, "txstart": 36460620}, {"cdsend": 36461695, "cdsstart": 36460975, "chr": "21", "position": [[36460620, 36461776], [36477373, 36477563], [36479494, 36480122]], "strand": -1, "transcript": "NM_144492", "txend": 36480122, "txstart": 36460620}], "exons_hg19": [{"cdsend": 37833993, "cdsstart": 37833273, "chr": "21", "position": [[37832919, 37834074], [37882660, 37882798], [37948708, 37948867]], "strand": -1, "transcript": "NM_001146077", "txend": 37948867, "txstart": 37832919}, {"cdsend": 37833993, "cdsstart": 37833273, "chr": "21", "position": [[37832919, 37834074], [37882660, 37882798], [37914862, 37915082]], "strand": -1, "transcript": "NM_001146078", "txend": 37915082, "txstart": 37832919}, {"cdsend": 37833993, "cdsstart": 37833273, "chr": "21", "position": [[37832919, 37834074], [37851792, 37852420]], "strand": -1, "transcript": "NM_001146079", "txend": 37852420, "txstart": 37832919}, {"cdsend": 37833993, "cdsstart": 37833273, "chr": "21", "position": [[37832919, 37834074], [37838664, 37838765]], "strand": -1, "transcript": "NM_012130", "txend": 37838765, "txstart": 37832919}, {"cdsend": 37833993, "cdsstart": 37833273, "chr": "21", "position": [[37832919, 37834074], [37849671, 37849861], [37851792, 37852420]], "strand": -1, "transcript": "NM_144492", "txend": 37852420, "txstart": 37832919}], "generif": [{"pubmed": 15769849, "text": "The palmitoylation of claudin-14 is required for efficient localization into tight junctions but not stability or strand assembly."}, {"pubmed": 15880785, "text": "The ability of CLDN14 to be recruited to these junctions is crucial for the hearing process."}, {"pubmed": 19254696, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19561606, "text": "Common, synonymous variants in the CLDN14 gene that associate with kidney stones, were discovered."}, {"pubmed": 19561606, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20494980, "text": "The CLDN14 promoter is activated by Trichostatin A (TSA) treatment according to promoter reporter assays in HEK 293 cells."}, {"pubmed": 20811388, "text": "Individuals with mutations of CLDN14 may have different degrees of hearing loss and the loss is greater at higher frequencies."}, {"pubmed": 22246673, "text": "The hearing loss due to novel CLDN14 mutations is prelingual, severe-to-profound with greater loss in the high frequencies."}, {"pubmed": 23235333, "text": "OPRM1 genetic polymorphisms are associated with the plasma concentration of cotinine in a Taiwanese MMT cohort. Carriers with the major allele of SNP rs1799971 had a higher plasma cotinine concentration."}, {"pubmed": 23322640, "text": "Human Cldn-8 and -14 were shown to convey Clostridium perfringens enterotoxin-mediated cytotoxicity at pathophysiologically relevant concentrations of this toxin, although ~2-to-10-fold less efficiently than Cldn-4."}, {"pubmed": 23590985, "text": "CLDN14 mutations can contribute to the aetiology of childhood/congenital deafness in Moroccan patients."}, {"pubmed": 23991001, "text": "Data suggest a possible role for Claudin14 in urinary calcium excretion."}, {"pubmed": 24325792, "text": "Claudin 14 expression was up-regulated in gastric cancer."}, {"pubmed": 26107257, "text": "Rs1801725 (Ala986-Ser), rs1042636 (Arg990Gly) of CaSR gene and rs219778, rs219780 (Thr229Thr) of CLDN14 gene were significantly associated with kidney stone disease in patients from the Eastern part of India."}, {"pubmed": 26842849, "text": "The rs170183 was correlated with a decline in claudin 14 expression in both lymphoblastoid cell lines and T cells."}, {"pubmed": 27207647, "text": "CLDN14 is a novel direct target of EZH2-mediated H3K27ME3 and plays role in EZH2-H3K27ME3-mediated hepatocellular carcinoma aggressiveness."}, {"pubmed": 27629923, "text": "All hearing impaired individuals, including the proband, are homozygous for a pathogenic variant of CLDN14, but this only explains the deafness."}, {"pubmed": 27838790, "text": "Extensive clinical recruitment and targeted screening suggest that CLDN14 p.(Ala163Val) represents a major founder variant for prelingual sensorineural hearing loss in the Newfoundland population."}, {"pubmed": 28229505, "text": "Our data suggest that children with the INSM1 binding site within the CLDN14 risk haplotype have a higher likelihood of hypercalciuria and kidney stones. Enhanced CLDN14 expression may play a role in the pathophysiology of their hypercalciuria."}, {"pubmed": 29434063, "text": "This study suggested considerable genetic heterogeneity in the causation of hearing loss in Dhadkai. Recessive mutations were observed in at least three genes causing hearing loss: OTOF (p.R708X), SLC26A4 (p.Y556X) and CLDN14 (p.V85D). Mutation p.R708X appeared to be the major cause of hearing impairment in Dhadkai."}, {"pubmed": 29447821, "text": "CLDN14 might not be a major causative gene for NSHL in Chinese populations, which would contribute to fully understanding the genetic cause of NSHL in the East Asian populations"}, {"pubmed": 30232134, "text": "No significant associations were found among claudin-16 and claudin-19 single-nucleotide polymorphisms and calcium excretion and between claudin-14, claudin-16, and claudin-19 single-nucleotide polymorphisms and stones"}, {"pubmed": 31527509, "text": "This report indicated that claudin-14 is essential for maintaining the inner ear environment and suggested the possible phenotypic expansion of DFNB29. This is the first report of a patient with a tight junction variant receiving a cochlear implantation."}, {"pubmed": 35546405, "text": "The correlation between promoter hypermethylation of VDR, CLDN, and CasR genes and recurrent stone formation."}, {"pubmed": 38942107, "text": "Downregulation of chemoresistance by claudin-14 silencing in human colorectal cancer cells."}], "genomic_pos": {"chr": "21", "end": 36576569, "ensemblgene": "ENSG00000159261", "start": 36460621, "strand": -1}, "genomic_pos_hg19": {"chr": "21", "end": 37948867, "start": 37832919, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0007155", "qualifier": "involved_in", "term": "cell adhesion"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0016338", "qualifier": "involved_in", "term": "calcium-independent cell-cell adhesion via plasma membrane cell-adhesion molecules"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0065003", "pubmed": 7644498, "qualifier": "involved_in", "term": "protein-containing complex assembly"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0070830", "qualifier": "involved_in", "term": "bicellular tight junction assembly"}], "CC": [{"evidence": "HDA", "gocategory": "CC", "id": "GO:0005783", "pubmed": 16780588, "qualifier": "located_in", "term": "endoplasmic reticulum"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0005886", "pubmed": 16780588, "qualifier": "located_in", "term": "plasma membrane"}, {"evidence": "IBA", "gocategory": "CC", "