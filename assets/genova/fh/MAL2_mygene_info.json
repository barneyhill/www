: [{"pubmed": 12370246, "text": "Data show that MAL2 is essential for transcytosis in HepG2 cells."}, {"pubmed": 14576188, "text": "MAL2 protein mRNA species were detected in the thyroid and immunohistochemical analysis of thyroid follicles indicated that MAL2 distributed to the apical membrane"}, {"pubmed": 16620967, "text": "The results indicate that MAL2 protein genes are not ubiquitously expressed in leukemic bone marrow in children, and that RNA sample parameters may influence measures of gene expression more than commonly appreciated."}, {"pubmed": 17145811, "text": "The MAL2 gene was shown to effectively separate these two tumor(chromophobe renal cell carcinoma (RCC) and benign oncocytoma) groups by quantitative reverse transcription-PCR using fresh tissue samples, with similar trends seen on formalin-fixed tissues."}, {"pubmed": 19175940, "text": "a role for MAL2 in regulating tumor-associated protein mucin expression and/or localisation"}, {"pubmed": 19683524, "text": "after differentiation, murine oligodendrocyte precursor and human oligodendroglioma derived cell lines up-regulate the expression of MAL2 and accumulate it in an intracellular compartment, exhibiting a peri-centrosomal localization."}, {"pubmed": 20846453, "text": "MAL2 overexpression was highest in serous carcinomas relative to other histological subtypes of ovarian cancer."}, {"pubmed": 21573057, "text": "Results prove for the first time the interaction of PLP and MAL2 in oligodendrocytic cells, supporting the transcytotic model of PLP transport previously suggested."}, {"pubmed": 25084525, "text": "MAL2 and STK16 function to sort secretory soluble cargo into the constitutive secretory pathway in hepatocytes."}, {"pubmed": 28562687, "text": "data suggested that MAL2 and TPD52 might be potential biomarkers for clinical prognosis and might be a promising therapeutic target for CRC"}, {"pubmed": 30021343, "text": "Up-regulation of MAL2 was hound in papillary thyroid gland tissues and significantly correlated with poor overall survival."}, {"pubmed": 30195491, "text": "MAL2 has a role in promoting proliferation, migration, and invasion through regulating epithelial-mesenchymal transition in breast cancer cell lines"}, {"pubmed": 31397491, "text": "RP11-284F21.9 knockdown could reduce MAL2 expression, while miR-383-5p inhibitors abolished this repressive effect"}, {"pubmed": 32990678, "text": "MAL2 drives immune evasion in breast cancer by suppressing tumor antigen presentation."}, {"pubmed": 33780861, "text": "MAL2 interacts with IQGAP1 to promote pancreatic cancer progression by increasing ERK1/2 phosphorylation."}, {"pubmed": 35033901, "text": "The novel circ_0084904/miR-802/MAL2 axis promotes the development of cervical cancer."}, {"pubmed": 36938678, "text": "LINC00460 Facilitates Cell Proliferation and Inhibits Ferroptosis in Breast Cancer Through the miR-320a/MAL2 Axis."}], "genomic_pos": {"chr": "8", "end": 119245673, "ensemblgene": "ENSG00000147676", "start": 119165034, "strand": 1}, "genomic_pos_hg19": {"chr": "8", "end": 120257913, "start": 120177273, "strand": 1}, "go": {"BP": {"evidence": "IBA", "gocategory": "BP", "id": "GO:0042552", "qualifier": "involved_in", "term": "myelination"}, "CC": [{"evidence": "IBA", "gocategory": "CC", "id": "GO:0016020", "qualifier": "is_active_in", "term": "membrane"}, {"evidence": "IEA", "gocategory": "CC", "id": "GO:0016324", "qualifier": "located_in", "term": "apical plasma membrane"}, {"evidence": "IEA", "gocategory": "CC", "id": "GO:0030672", "qualifier": "located_in", "term": "synaptic vesicle membrane"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0045121", "pubmed": 23264465, "qualifier": "located_in", "term": "membrane raft"}, {"evidence": "IEA", "gocategory": "CC", "id": "GO:0048471", "qualifier": "located_in", "term": "perinuclear region of cytoplasm"}, {"evidence": "HDA", "gocategory": "CC", "id": "GO:0070062", "pubmed": 19056867, "qualifier": "located_in", "term": "extracellular exosome"}, {"e