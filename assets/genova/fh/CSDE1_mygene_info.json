115263338], [115266503, 115266623], [115267841, 115267954], [115268831, 115269007], [115269603, 115269711], [115272878, 115273043], [115273128, 115273269], [115275224, 115275437], [115276352, 115276478], [115276609, 115276738], [115277062, 115277144], [115279378, 115279476], [115280091, 115280184], [115280583, 115280693], [115282312, 115282511], [115300545, 115300605]], "strand": -1, "transcript": "NM_001242892", "txend": 115300605, "txstart": 115259536}, {"cdsend": 115282511, "cdsstart": 115260789, "chr": "1", "position": [[115259536, 115260837], [115261233, 115261366], [115262199, 115262363], [115263159, 115263338], [115266503, 115266623], [115267841, 115267954], [115268831, 115269007], [115269603, 115269711], [115272878, 115273043], [115273128, 115273269], [115275224, 115275437], [115276352, 115276478], [115276609, 115276738], [115277062, 115277144], [115279378, 115279476], [115280583, 115280693], [115282312, 115282511], [115300545, 115300605]], "strand": -1, "transcript": "NM_001242893", "txend": 115300605, "txstart": 115259536}, {"cdsend": 115282511, "cdsstart": 115260789, "chr": "1", "position": [[115259536, 115260837], [115261233, 115261366], [115262199, 115262363], [115263159, 115263338], [115266503, 115266623], [115267841, 115267954], [115268831, 115269007], [115269603, 115269711], [115272878, 115273043], [115273128, 115273269], [115275224, 115275437], [115276352, 115276478], [115276609, 115276738], [115277062, 115277144], [115279378, 115279476], [115280583, 115280693], [115282312, 115282511], [115292441, 115292828], [115300545, 115300605]], "strand": -1, "transcript": "NM_007158", "txend": 115300605, "txstart": 115259536}], "generif": [{"pubmed": 12667457, "text": "unr and nPTB act as RNA chaperones by changing the structure of the IRES into one that permits translation initiation"}, {"pubmed": 15928332, "text": "UNR translation is regulated by an internal ribosome entry site that is downregulated by polypyrimidine tract binding protein."}, {"pubmed": 17159903, "text": "regulation of IRES-mediated translation by hnRNP C1/C2 and Unr might be important in mitosis"}, {"pubmed": 17947529, "text": "The data suggest that Unr acts as an RNA chaperone on rhinovirus RNA 5' UTR to maintain a complex tertiary internal ribosome entry site structure required for translational competency."}, {"pubmed": 19913121, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20628086, "text": "Observational study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 21048031, "text": "Observational study of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 24189344, "text": "Study suggests TRIM33 and NRAS-CSDE1 as candidate genes for autism, and may provide a novel insight into the etiology of autism"}, {"pubmed": 25576733, "text": "Hepsin suppressed CDK11p58 internal ribosome entry site activity in prostate cancer cells by modulating UNR expression and eIF-2alpha phosphorylation."}, {"pubmed": 26614667, "text": "Studies indicate that Unr (upstream of N-ras) is post-translationally modified by phosphorylation and lysine acetylation."}, {"pubmed": 27908735, "text": "Results identify UNR as an oncogenic modulator of melanoma progression, unraveling the underlying molecular mechanisms. UNR seems to regulate translation elongation of Vimentin and RAC1 mRNAs."}, {"pubmed": 28763470, "text": "UNR/CDSE1 expression is correlated with outcome in patients with resectable pancreatic ductal adenocarcinoma"}, {"pubmed": 29129916, "text": "CSDE1 post-transcriptionally modulates core components of multiple regulatory nodes of human embryonic stem cell identity, neuroectoderm commitment and neurogenesis and acts as a negative regulator of neural differentiation and neurogenesis."}, {"pubmed": 29422612, "text": "Csde1 is involved in feed-back control in protein homeostasis and that it dampens stochastic changes in mRNA expression"}, {"pubmed": 30138317, "text": "Strap associates with Csde1 and affects expression of select Csde1-bound transcripts"}, {"pubmed": 30247708, "text": "CSDE1 binds directly to BC200 RNA.CSDE1 and BC200 expression levels are mutually dependent."}, {"pubmed": 31579823, "text": "Our study defines a new autism-related syndrome and highlights the functional role of CSDE1 in synapse development and synaptic transmission."}, {"pubmed": 31987048, "text": "The role of CSDE1 in translational reprogramming and human diseases."}, {"pubmed": 32170829, "text": "Cold Shock Domain Containing E1 (CSDE1) Protein is Overexpressed and Can be Targeted to Inhibit Invasiveness in Pancreatic Cancer Cells."}, {"pubmed": 33662035, "text": "The CDT of Helicobacter hepaticus induces pro-survival autophagy and nucleoplasmic reticulum formation concentrating the RNA binding proteins UNR/CSDE1 and P62/SQSTM1."}, {"pubmed": 33772027, "text": "Oncolytic virotherapy induced CSDE1 neo-antigenesis restricts VSV replication but can be targeted by immunotherapy."}, {"pubmed": 33833398, "text": "CSDE1 attenuates microRNA-mediated silencing of PMEPA1 in melanoma."}, {"pubmed": 35021076, "text": "Coordinated post-transcriptional control of oncogene-induced senescence by UNR/CSDE1."}, {"pubmed": 35490208, "text": "Reduced miR-371b-5p expression drives tumor progression via CSDE1/RAC1 regulation in triple-negative breast cancer."}, {"pubmed": 36103516, "text": "Cold shock domain-containing protein E1 is a posttranscriptional regulator of the LDL receptor."}, {"pubmed": 37493604, "text": "CSDE1 promotes miR-451 biogenesis."}], "genomic_pos": {"chr": "1", "end": 114758676, "ensemblgene": "ENSG00000009307", "start": 114716913, "strand": -1}, "genomic_pos_hg19": {"chr": "1", "end": 115301297, "start": 115259534, "strand": -1}, "go": {"BP": [{"evidence": "ISS", "gocatego