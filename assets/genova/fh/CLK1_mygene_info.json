200855003, 200855086], [200856681, 200856811], [200856890, 200856985], [200857717, 200857884], [200857972, 200858089], [200859679, 200859746], [200860124, 200860215], [200861237, 200861466], [200861701, 200861862], [200864563, 200864658]], "strand": -1, "transcript": "NM_004071", "txend": 200864658, "txstart": 200853008}, {"cdsend": 200864658, "cdsstart": 200864658, "chr": "2", "position": [[200853008, 200853449], [200853902, 200853993], [200854615, 200854695], [200855003, 200855086], [200856681, 200856811], [200856890, 200856985], [200857717, 200857884], [200857972, 200858089], [200859679, 200859746], [200861237, 200861466], [200861701, 200861862], [200864563, 200864658]], "strand": -1, "transcript": "NR_027855", "txend": 200864658, "txstart": 200853008}, {"cdsend": 200864658, "cdsstart": 200864658, "chr": "2", "position": [[200853008, 200853449], [200853902, 200853993], [200854615, 200854695], [200855003, 200855086], [200856681, 200856811], [200856890, 200856985], [200857717, 200857884], [200857972, 200858089], [200859679, 200861466], [200861701, 200861862], [200864563, 200864658]], "strand": -1, "transcript": "NR_027856", "txend": 200864658, "txstart": 200853008}], "exons_hg19": [{"cdsend": 201728949, "cdsstart": 201718028, "chr": "2", "position": [[201717731, 201718172], [201718625, 201718716], [201719338, 201719418], [201719726, 201719809], [201721404, 201721534], [201721613, 201721708], [201722440, 201722607], [201722695, 201722812], [201724402, 201724469], [201724847, 201724938], [201725960, 201726189], [201726424, 201726585], [201728823, 201729284]], "strand": -1, "transcript": "NM_001162407", "txend": 201729284, "txstart": 201717731}, {"cdsend": 201726585, "cdsstart": 201718028, "chr": "2", "position": [[201717731, 201718172], [201718625, 201718716], [201719338, 201719418], [201719726, 201719809], [201721404, 201721534], [201721613, 201721708], [201722440, 201722607], [201722695, 201722812], [201724402, 201724469], [201724847, 201724938], [201725960, 201726189], [201726424, 201726585], [201729286, 201729381]], "strand": -1, "transcript": "NM_004071", "txend": 201729381, "txstart": 201717731}, {"cdsend": 201729381, "cdsstart": 201729381, "chr": "2", "position": [[201717731, 201718172], [201718625, 201718716], [201719338, 201719418], [201719726, 201719809], [201721404, 201721534], [201721613, 201721708], [201722440, 201722607], [201722695, 201722812], [201724402, 201724469], [201725960, 201726189], [201726424, 201726585], [201729286, 201729381]], "strand": -1, "transcript": "NR_027855", "txend": 201729381, "txstart": 201717731}, {"cdsend": 201729381, "cdsstart": 201729381, "chr": "2", "position": [[201717731, 201718172], [201718625, 201718716], [201719338, 201719418], [201719726, 201719809], [201721404, 201721534], [201721613, 201721708], [201722440, 201722607], [201722695, 201722812], [201724402, 201726189], [201726424, 201726585], [201729286, 201729381]], "strand": -1, "transcript": "NR_027856", "txend": 201729381, "txstart": 201717731}], "generif": [{"pubmed": 12773558, "text": "Clk/Sty is found in the nucleus of several different cell types and is a factor directly involved in splicing control"}, {"pubmed": 16223727, "text": "ASF/SF2 is phosphorylated by SRPK1 and Clk/Sty"}, {"pubmed": 18814951, "text": "five genes (TNFSF10/TRAIL, IL1RN, IFI27, GZMB, and CCR5) were upregulated and three genes (CLK1, TNFAIP3 and BTG1) were downregulated in at least three out of four subpopulations during acute GVHD."}, {"pubmed": 19168442, "text": "Cdc2-like kinases and DNA topoisomerase I regulate alternative splicing of tissue factor in human endothelial cells."}, {"pubmed": 21682887, "text": "CLK1 Increases While CLK2 Decreases HIV-1 Gene Expression."}, {"pubmed": 23707382, "text": "The data establish a new view of SRSF1 protein regulation in which SRPK1 and CLK1 partition activities based on Ser-Pro versus Arg-Ser placement rather than on N- and C-terminal preferences along the RS domain."}, {"pubmed": 24869919, "text": "findings suggest that CLK1-dependent hyperphosphorylation is the result of a general mechanism in which the N-terminus acts as a bridge connecting the kinase domain and the RS domain of the SR protein."}, {"pubmed": 25529026, "text": "Data suggest that proline phosphorylation by CLK1/CDC-like kinase 1 (but not by SRPK1/serine/arginine-rich splicing factor kinase 1) regulates conformation and alternative splicing function of SFRS1 (serine/arginine-rich splicing factor 1)."}, {"pubmed": 25961505, "text": "Nuclear CLK-1 mediates a retrograde signalling pathway that is conserved from Caenorhabditis elegans to humans and is responsive to mitochondrial reactive oxygen species, thus acting as a barometer of oxidative metabolism."}, {"pubmed": 26443864, "text": "removal of the N-terminus or dilution of CLK1 induces monomer formation and reverses this specificity. CLK1 self-association also occurs in the nucleus"}, {"pubmed": 27015110, "text": "These results thus reveal a large program of CLK1-regulated periodic alternative splicing intimately associated with cell cycle control."}, {"pubmed": 27126587, "text": "Clk1, Clk2 and Clk4 prevent chromatin breakage by regulating the Aurora B-dependent abscission checkpoint."}, {"pubmed": 27397683, "text": "SRPK1 interacts with an RS-like domain in the N terminus of CLK1 to facilitate the release of phosphorylated SR proteins, which then promotes efficient splice-site recognition and subsequent spliceosome assembly."}, {"pubmed": 28232751, "text": "Global CLK-dependent exon recognition and conjoined gene formation revealed with a novel small molecule inhibitor has been described."}, {"pubmed": 28581641, "text": "that mitochondrial Clk1 regulated chemoresistance in glioma cells through AMPK/mTOR/HIF-1alpha mediated glycolysis pathway"}, {"pubmed": 29335301, "text": "We now show that the ability of SRPK1 to mobilize SRSF1 from speckles to the nucleoplasm is dependent on active CLK1. Diffusion from speckles is promoted by the formation of an SRPK1-CLK1 complex that facilitates dissociation of SRSF1 from CLK1 and enhances the phosphorylation of several serine-proline dipeptides in this SR protein"}, {"pubmed": 29802995, "text": "Data suggest that CLK1 activity in both transformed and normal cells is carefully regulated via CLK1 alternative splicing through both exon 4 skipping and intron 4 retention. CLK1 autoregulates by favoring the expression of truncated isoforms, CLK1T1 and CLKT2 in environmental stress."}, {"pubmed": 31064840, "text": "found that CLK1 amplifies its presence in the nucleus by forming a stable complex with the Ser-Arg protein substrate"}, {"pubmed": 31782550, "text": "LncRNA-dependent nuclear stress bodies promote intron retention through SR protein phosphorylation."}, {"pubmed": 32333232, "text": "Phosphoproteomic analysis identifies CLK1 as a novel therapeutic target in gastric cancer."}, {"pubmed": 32359191, "text": "A conserved sequence motif bridges two protein kinases for enhanced phosphorylation and nuclear function of a splicing factor."}, {"pubmed": 33811140, "text": "CLK1 reorganizes the splicing factor U1-70K for early spliceosomal protein assembly."}, {"pubmed": 37607329, "text": "Functions of SRPK, CLK and DYRK kinases in stem cells, development, and human developmental disorders."}], "genomic_pos": {"chr": "2", "end": 200864691, "ensemblgene": "ENSG00000013441", "start": 200853009, "strand": -1}, "genomic_pos_hg19": {"chr": "2", "end": 201729422, "start": 201717732, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0018108", "qualifier": "involved_in", "term": "peptidyl-tyrosine phosphorylation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0043484", "qualifier": "involved_in", "term": "regulation of RNA splicing"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0043484", "pubmed": 19168442, "qualifier": "involved_in", "term": "regulation of RNA splicing"}], "CC": {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005634", "qualifier"