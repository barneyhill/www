ition": [[52372828, 52373375], [52374809, 52374897], [52375429, 52375599], [52381641, 52381761], [52383419, 52383569], [52383829, 52383937], [52384549, 52384721], [52385160, 52385262], [52385851, 52385949], [52388456, 52388637], [52389216, 52389321], [52393442, 52393803], [52396045, 52396364], [52397684, 52397863], [52401361, 52401489], [52402128, 52402228], [52404245, 52404471]], "strand": -1, "transcript": "NM_001190819", "txend": 52404471, "txstart": 52372828}, {"cdsend": 52402223, "cdsstart": 52373180, "chr": "1", "position": [[52372828, 52373375], [52374809, 52374897], [52375429, 52375599], [52381641, 52381761], [52383419, 52383569], [52383829, 52383937], [52384549, 52384721], [52385160, 52385262], [52385851, 52385949], [52388441, 52388637], [52389216, 52389321], [52393442, 52393803], [52396045, 52396364], [52397684, 52397863], [52401361, 52401489], [52402128, 52402228], [52404245, 52404423]], "strand": -1, "transcript": "NM_004153", "txend": 52404423, "txstart": 52372828}], "exons_hg19": [{"cdsend": 52867895, "cdsstart": 52838852, "chr": "1", "position": [[52838500, 52839047], [52840481, 52840569], [52841101, 52841271], [52847313, 52847433], [52849091, 52849241], [52849501, 52849609], [52850221, 52850393], [52850832, 52850934], [52851523, 52851621], [52854113, 52854309], [52854888, 52854993], [52859114, 52859475], [52861717, 52862036], [52863356, 52863535], [52867033, 52867161], [52867800, 52867897], [52869917, 52870143]], "strand": -1, "transcript": "NM_001190818", "txend": 52870143, "txstart": 52838500}, {"cdsend": 52867895, "cdsstart": 52838852, "chr": "1", "position": [[52838500, 52839047], [52840481, 52840569], [52841101, 52841271], [52847313, 52847433], [52849091, 52849241], [52849501, 52849609], [52850221, 52850393], [52850832, 52850934], [52851523, 52851621], [52854128, 52854309], [52854888, 52854993], [52859114, 52859475], [52861717, 52862036], [52863356, 52863535], [52867033, 52867161], [52867800, 52867900], [52869917, 52870143]], "strand": -1, "transcript": "NM_001190819", "txend": 52870143, "txstart": 52838500}, {"cdsend": 52867895, "cdsstart": 52838852, "chr": "1", "position": [[52838500, 52839047], [52840481, 52840569], [52841101, 52841271], [52847313, 52847433], [52849091, 52849241], [52849501, 52849609], [52850221, 52850393], [52850832, 52850934], [52851523, 52851621], [52854113, 52854309], [52854888, 52854993], [52859114, 52859475], [52861717, 52862036], [52863356, 52863535], [52867033, 52867161], [52867800, 52867900], [52869917, 52870095]], "strand": -1, "transcript": "NM_004153", "txend": 52870095, "txstart": 52838500}], "generif": [{"pubmed": 11716535, "text": "a component of the origin recognition complex (ORC) that functions in DNA replication"}, {"pubmed": 11779870, "text": "ORC2 physically interacts with ORC1 on non-chromatin nuclear structures"}, {"pubmed": 11931757, "text": "Data suggest that hOrc1p is targeted for destruction by an SCF-Skp2 complex during S phase."}, {"pubmed": 12909626, "text": "ORC1 regulates the status of the ORC complex in human nuclei by tethering ORCs 2-5 to nuclear structures"}, {"pubmed": 12909627, "text": "the ORC1 cycle in human cells is highly linked with cell cycle progression, allowing the initiation of replication to be coordinated with the cell cycle and preventing origins from refiring"}, {"pubmed": 12912926, "text": "The precise nucleotide of binding for ORC1 was identified near the start sites for leading-strand DNA synthesis."}, {"pubmed": 15454574, "text": "HP1 has a role in the recruitment but not in the stable association of Orc1p with heterochromatin"}, {"pubmed": 17066079, "text": "BAH domain in human Orc1 facilitates its ability to activate replication origins in vivo by promoting association of ORC with chromatin."}, {"pubmed": 18761675, "text": "Interacts with TRF2 to achieve stable binding in pre-replication complex assembly at telomeres. These data suggest that ORC might be involved in telomere homeostasis in human cells."}, {"pubmed": 19197067, "text": "data suggest that Orc1 is a regulator of centriole and centrosome reduplication as well as the initiation of DNA replication"}, {"pubmed": 20800603, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 21085491, "text": "Data show that Orc1 specifically binds hypo-phosphorylated Rb and that this interaction is competitive with the binding of Rb to E2F1."}, {"pubmed": 21115485, "text": "in human cancer cells, RBX1 silencing causes the accumulation of DNA replication licensing proteins CDT1 and ORC1, leading to DNA double-strand breaks, DDR, G(2) arrest, and, eventually, aneuploidy"}, {"pubmed": 21358633, "text": "Mutations in ORC1, encoding a subunit of the origin recognition complex, cause microcephalic primordial dwarfism resembling Meier-Gorlin syndrome. These mutations disrupt ORC1 functions including pre-replicative complex formation and origin activation."}, {"pubmed": 22045277, "text": "As cells moved through the cell cycle, the local-ization of ORC1 shifted, suggesting changes in thelocalization of ORC-bound origin sequences."}, {"pubmed": 22333897, "text": "A lethal phenotype was seen in four individuals with compound heterozygous ORC1 mutations"}, {"pubmed": 22398447, "text": "results identify the BAH domain as a novel methyl-lysine-binding module, thereby establishing the first direct link between histone methylation and the metazoan DNA replication machinery, and defining a pivotal aetiological role for the canonical H4K20me2 mark, via ORC1, in primordial dwarfism"}, {"pubmed": 22589552, "text": "Data show that purified Epstein-Barr nuclear antigen 1 EBNA1 recruits purified Human Orc1 and Cdc6 onto replication origin oriP."}, {"pubmed": 22855792, "text": "Orc1 harbors a PACT centrosome-targeting domain and a separate domain that differentially inhibits the protein kinase activities of Cyclin E-CDK2 and Cyclin A-CDK2"}, {"pubmed": 23187890, "text": "ORC1 associated with transcription start sites of coding or noncoding RNAs. Transcription levels at the ORC1 sites directly correlated with replication timing."}, {"pubmed": 24003239, "text": "ORC1 harbors a G-rich RNA/ssDNA-binding domain, which may be involved in the preferential binding to G-quadruplex-formable RNA/DNA by ORC.  Structure modeling predicts the structural similarity between the G-rich RNA/ssDNA-binding domain of ORC1 and part of mammalian DNA methyltransferase 1."}, {"pubmed": 25784553, "text": "Orc1 acts as a nucleating center for origin recognition complex assembly and then pre-replication complex assembly by binding to mitotic chromosomes, followed by gradual removal from chromatin during the G1 phase."}, {"pubmed": 27458800, "text": "The opposing effects of ORC1 (represor) and CDC6 (gene activator) in controlling the level of Cyclin E ensures genome stability and a mechanism for linking directly DNA replication and cell division commitment."}, {"pubmed": 27906128, "text": "The authors have discovered that human cell lines in culture proliferate with intact chromosomal origins of replication after disruption of both alleles of ORC2 or of the ATPase subunit, ORC1."}, {"pubmed": 31309634, "text": "Authors report the crystal structure of Cyclin A-CDK2 complex bound to a peptide derived from ORC1 at 2.54 a resolution; structure revealed that the ORC1 peptide interacts with a hydrophobic groove, termed cyclin binding groove, of Cyclin A via a KXL motif."}, {"pubmed": 31866342, "text": "Origin recognition complex subunit 1 regulates cell growth and metastasis in glioma by altering activation of ERK and JNK signaling pathway."}, {"pubmed": 33761311, "text": "Multiple, short protein binding motifs in ORC1 and CDC6 control the initiation of DNA replication."}, {"pubmed": 36700467, "text": "Origin recognition complex subunit 1 (ORC1) augments malignant behaviors of lung adenocarcinoma cells via targeting Wnt signaling."}, {"pubmed": 37772772, "text": "Direct regulation of FNIP1 and FNIP2 by MEF2 sustains MTORC1 activation and tumor progression in pancreatic cancer."}, {"pubmed": 37833711, "text": "Origin recognition complex subunit 1(ORC1) is a potential biomarker and therapeutic target in cancer."}], "genomic_pos": {"chr": "1", "end": 52404423, "ensemblgene": "ENSG00000085840", "start": 52372829, "strand": -1}, "genomic_pos_hg19": {"chr": "1", "end": 52870131, "start": 52838501, "strand": -1}, "go": {"BP": [{"evidence": "IBA", "gocategory": "BP", "id": "GO:0006270", "qualifier": "involved_in", "term": "DNA replication initiation"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0006270", "pubmed": 16549788, "qualifier": "involved_in", "term": "DNA replication initiation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0033314", "qualifier": "involved_in", "term": "mitotic DNA replication checkpoint signaling"}], "CC": [{"evidence": "HDA", "gocategory": "CC", "id": "GO:0000781", "pubmed": 19135898, "qualifier": "located_in", "term": "chromosome, telomeric region"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0000781", "pubmed": 24270157, "qualifier": "colocalizes_with", "term": "chromosome, telomeric region"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0000808", "pubmed": 17716973, "qualifier": "part_of", "term": "origin recognition complex"}, {"evidence": "NAS", "gocategory": "CC", "id": "GO:0005634", "pubmed": 32808929, "qualifier": "located_in", "term": "nucleus"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "TAS", "gocategory": "CC", "id": "GO:0005654", "qualifier": "located_in", "term": "nucleoplasm"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0005664", "qualifier": "part_of", "term": "nuclear origin of replication recognition complex"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005664", "pubmed": [20932478, 21029866], "qualifier": "part_of", "term": "nuclear origin of replication recognition complex"}, {"evidence": "IPI", "gocategory": "CC", "id": "GO:0005664"