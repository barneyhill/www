 101729213], [101731567, 101731666], [101733574, 101733674], [101734926, 101735044], [101737036, 101737071], [101737231, 101737298], [101737802, 101737948], [101739361, 101739472]], "strand": -1, "transcript": "NM_001177948", "txend": 101739472, "txstart": 101728647}, {"cdsend": 101737935, "cdsstart": 101728926, "chr": "12", "position": [[101728647, 101728980], [101729108, 101729213], [101731567, 101731666], [101733574, 101733674], [101734926, 101735044], [101737036, 101737071], [101737231, 101737298], [101737802, 101737952], [101739350, 101739462]], "strand": -1, "transcript": "NM_001177949", "txend": 101739462, "txstart": 101728647}, {"cdsend": 101737935, "cdsstart": 101728926, "chr": "12", "position": [[101728647, 101728980], [101729108, 101729213], [101731567, 101731666], [101733574, 101733674], [101734926, 101735044], [101737036, 101737071], [101737231, 101737298], [101737802, 101737948], [101739350, 101739472]], "strand": -1, "transcript": "NM_153694", "txend": 101739472, "txstart": 101728647}], "exons_hg19": [{"cdsend": 102131713, "cdsstart": 102122704, "chr": "12", "position": [[102122425, 102122758], [102122886, 102122991], [102125345, 102125444], [102127352, 102127452], [102128704, 102128822], [102130814, 102130849], [102131009, 102131076], [102131580, 102131726], [102133139, 102133250]], "strand": -1, "transcript": "NM_001177948", "txend": 102133250, "txstart": 102122425}, {"cdsend": 102131713, "cdsstart": 102122704, "chr": "12", "position": [[102122425, 102122758], [102122886, 102122991], [102125345, 102125444], [102127352, 102127452], [102128704, 102128822], [102130814, 102130849], [102131009, 102131076], [102131580, 102131730], [102133128, 102133240]], "strand": -1, "transcript": "NM_001177949", "txend": 102133240, "txstart": 102122425}, {"cdsend": 102131713, "cdsstart": 102122704, "chr": "12", "position": [[102122425, 102122758], [102122886, 102122991], [102125345, 102125444], [102127352, 102127452], [102128704, 102128822], [102130814, 102130849], [102131009, 102131076], [102131580, 102131726], [102133128, 102133250]], "strand": -1, "transcript": "NM_153694", "txend": 102133250, "txstart": 102122425}], "generif": [{"pubmed": 14643120, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 14643120, "text": "We suggest that SYCP3 has an essential meiotic function in human spermatogenesis that is compromised by the mutant protein via dominant negative interference."}, {"pubmed": 16213863, "text": "In contrast to previously reported high frequency of SYCP3 mutations in patients with azoospermia, only polymorphisms are found in the present study"}, {"pubmed": 16824523, "text": "SYCP3 is expressed in human testis and is restricted to germ cells. Lack of SYCP3 expression in human testis may have a negative effect on spermatogenesis and male fertility."}, {"pubmed": 17434513, "text": "No functional SYCP3 exonic SNP was found in infertile Spanish patients with meiosis arrest, suggesting that SYCP3 mutations very likely are not relevant in Spain in genetic susceptibility to meiosis arrest."}, {"pubmed": 18256545, "text": "These observations suggest that SCP3 is expressed in adult human and monkey ovaries."}, {"pubmed": 19110213, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 19110213, "text": "mutations in SYPR3 are likely to generate an aberrant synaptonemal complex in a dominant-negative manner and contribute to abnormal chromosomal behavior that might lead to recurrent miscarriage."}, {"pubmed": 20395201, "text": "X-linked lymphocyte-regulated protein pM1 (XMR), XLR, and SCP3 render tumor cells resistant to antitumor immunity"}, {"pubmed": 21126912, "text": "mutation analysis of SYCP3, DNMT3L and MSH4 in patients with maturation arrest of spermatogenesis and couples with recurrent miscarriages; heterozygous change, detected in a conserved functional domain of the SYCP3 gene, was absent in >200 controls"}, {"pubmed": 21357605, "text": "The 657T>C mutation of SYCP3 may not be associated with recurrent miscarriage caused by aneuploidy."}, {"pubmed": 22116401, "text": "Expression of SYCP3 inhibits the homologous recombination (HR) pathway mediated by RAD51."}, {"pubmed": 22197129, "text": "mutations in SYCP3 do not contribute significantly to risk for recurrent miscarriage through maternal meiotic nondisjunction."}, {"pubmed": 22670862, "text": "SYCP3 mutations are not associated with the genetic susceptibility for meiotic arrest in infertile male patients with nonobstructive azoospermia in the Turkish population"}, {"pubmed": 23069255, "text": "Positive synaptonemal complex protein 3 expression is a portent of poor outcome and may be a potential biomarker in the early stages of the non-small cell lung cancer for survival."}, {"pubmed": 23100464, "text": "screening for genetic variants in AURKB and SYCP3 among these patients with reproductive problems using Sanger sequencing. Only one apparently non-pathogenic deletion was found in SYCP3."}, {"pubmed": 24905095, "text": "SCP3 plays an important role in the progression of cervical cancer"}, {"pubmed": 24950965, "text": "SYCP3 binding and assembly on meiotic chromosomes leads to their organisation into compact structures compatible with recombination and crossover formation"}, {"pubmed": 25059562, "text": "The T657C polymorphism of the SYCP3 gene is possibly associated with recurrent pregnancy loss of unknown cause in human."}, {"pubmed": 28287952, "text": "In the model, the reported compaction of chromosomal DNA caused by SYCP3 would result from its ability to bridge distant sites on a DNA molecule with the DNA-binding domains located at each end of its strut-like structure."}, {"pubmed": 28623914, "text": "SCP3 is associated with lymphangiogenesis and provides insight into the SCP3-VEGF-C/VEGF-D axis based cancer therapy strategy."}, {"pubmed": 28745000, "text": "SYCP3 binds to RAD51 and attenuates RAD51 activity during meiosis."}], "genomic_pos": {"chr": "12", "end": 101739472, "ensemblgene": "ENSG00000139351", "start": 101728648, "strand": -1}, "genomic_pos_hg19": {"chr": "12", "end": 102133250, "start": 102122426, "strand": -1}, "go": {"BP": [{"evidence": "NAS", "gocategory": "BP", "id": "GO:0007141", "pubmed": 15218256, "qualifier": "involved_in", "term": "male meiosis I"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007286", "qualifier": "involved_in", "term": "spermatid development"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0035092", "pubmed": 14643120, "qualifier": "involved_in", "term": "sperm DNA condensation"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0051301", "qualifier": "involved_in", "term": "cell division"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0051321", "qualifier": "involved_in", "term": "meiotic cell cycle"}], "CC": [{"evidence": "ISS", "gocategory": "CC", "id": "GO:0000775", "qualifier": "located_in", "term": "chromosome, centromeric region"}, {"evidence": "IBA", "gocategory": "CC", "id": "GO:0000795", "qualifier": "is_active_in", "term": "synaptonemal complex"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0000795", "pubmed": 9774970, "qualifier": "located_in", "term": "synaptonemal complex"}, {"evidence": "ISS", "gocategory": "CC", "id": "GO:0000800", "qualifier": "located_in", "term": "lateral element"}, {"evidence": "IDA", "gocategory": "CC", "id": "GO:0005634", "pubmed": 22899867, "qualifier": "located_in", "term": "nucleus"}], "MF": [{"category": "MF", "evidence": "IEA", "id": "GO:0003677", "qualifier": "enables", "term": "DNA binding"}, {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": 32296183, "qualifier": "enables", "term": "protein binding"}]}, "homologene": {"genes": [[7955, 678602], [8364, 779673], [9031, 418097], [9544, 697075], [9598, 452174], [9606, 50511], [9615, 100684357], [9913, 615896], [10090, 20962], [10116, 25561]], "id": 7964}, "interpro": {"desc": "XLR/SYCP3/FAM9 domain", "id": "IPR006888", "short_desc": "XLR/SYC