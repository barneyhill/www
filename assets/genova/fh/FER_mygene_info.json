08373122, 108373179], [108380380, 108380496], [108382804, 108382899], [108436096, 108436220], [108516447, 108516602], [108521900, 108522023], [108523133, 108532541]], "strand": 1, "transcript": "NM_001308028", "txend": 108532541, "txstart": 108084648}, {"cdsend": 108523276, "cdsstart": 108260361, "chr": "5", "position": [[108260306, 108260490], [108281830, 108281923], [108290429, 108290633], [108294925, 108295048], [108373122, 108373179], [108380380, 108380496], [108382804, 108382899], [108436096, 108436220], [108516447, 108516602], [108521900, 108522023], [108523133, 108532541]], "strand": 1, "transcript": "NM_001308031", "txend": 108532541, "txstart": 108260306}, {"cdsend": 108177542, "cdsstart": 108133883, "chr": "5", "position": [[108084648, 108084694], [108103793, 108103939], [108118363, 108118456], [108133824, 108134090], [108168470, 108168644], [108171408, 108171508], [108177531, 108177569], [108177962, 108178702]], "strand": 1, "transcript": "NM_001308038", "txend": 108178702, "txstart": 108084648}, {"cdsend": 108523276, "cdsstart": 108133883, "chr": "5", "position": [[108083597, 108083701], [108103793, 108103939], [108133824, 108134090], [108168470, 108168644], [108171408, 108171508], [108203467, 108203651], [108207065, 108207203], [108207793, 108207913], [108219096, 108219219], [108233359, 108233549], [108281830, 108281923], [108290429, 108290633], [108294925, 108295048], [108373122, 108373179], [108380380, 108380496], [108382804, 108382899], [108436096, 108436220], [108516447, 108516602], [108521900, 108522023], [108523133, 108532541]], "strand": 1, "transcript": "NM_005246", "txend": 108532541, "txstart": 108083597}, {"cdsend": 108532541, "cdsstart": 108532541, "chr": "5", "position": [[108084648, 108084694], [108133824, 108134090], [108168470, 108168644], [108171408, 108171508], [108203467, 108203651], [108207065, 108207203], [108207793, 108207913], [108219096, 108219219], [108233359, 108233549], [108233663, 108233833], [108281830, 108281923], [108290429, 108290633], [108294925, 108295048], [108373122, 108373179], [108380380, 108380496], [108382804, 108382899], [108436096, 108436220], [108516447, 108516602], [108521900, 108522023], [108523133, 108532541]], "strand": 1, "transcript": "NR_146155", "txend": 108532541, "txstart": 108084648}], "generif": [{"pubmed": 11994747, "text": "Closing in on the biological functions of Fps/Fes and Fer. A review."}, {"pubmed": 12871378, "text": "Fps/Fes and Fer are expressed in human and mouse platelets, and are activated following stimulation with collagen and collagen-related peptide (CRP), suggesting a role in GPVI receptor signaling"}, {"pubmed": 16732323, "text": "Fer is a regulator of cell-cycle progression in malignant cells and a potential target for cancer intervention."}, {"pubmed": 18985748, "text": "FerT coexist in the acroplaxome with phosphorylated cortactin, a regulator of F-actin dynamics[Fer testis ]"}, {"pubmed": 19147545, "text": "Fer tyrosine kinase level correlates with the development of prostate cancer and aggressiveness of prostate cancer cell lines"}, {"pubmed": 19339212, "text": "Results suggest that Fer may allow a bypass of focal adhesion kinase-related cell anchorage dependency for intracellular signal transduction in hepatocytes."}, {"pubmed": 19738202, "text": "Overexpression of Fer enhanced lamellipodia formation and cell migration in a manner dependent on PLD activity and the PA-FX interaction."}, {"pubmed": 19835603, "text": "FER plays a role in the invasion and metastasis of hepatocellular carcinoma cells."}, {"pubmed": 19913121, "text": "Observational study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20628086, "text": "Observational study of gene-disease association, gene-environment interaction, and pharmacogenomic / toxicogenomic. (HuGE Navigator)"}, {"pubmed": 21122136, "text": "tyrosine phosphorylation of RhoGDIalpha by Fer as a mechanism to regulate binding of RhoGDIalpha to Rac"}, {"pubmed": 21518868, "text": "Overexpression of FER from a cDNA confers quinacrine resistance to several different types of cancer cell lines."}, {"pubmed": 21700879, "text": "The A allele of SNP rs10447248 in the FER locus was nominally associated with lower MMW (P = 0.016) but was not associated with LMW (P > 0.05) adiponectin levels"}, {"pubmed": 22223638, "text": "Transcription of the ferT gene in CC cells was found to be driven by an intronic promoter residing in intron 10 of the fer gene and to be regulated by the Brother of the Regulator of Imprinted Sites (BORIS) transcription factor."}, {"pubmed": 22238358, "text": "Fer, a non-receptor-type tyrosine kinase, plays a critical role in synthesis of the laminin-binding glycans on alpha-DG."}, {"pubmed": 23445469, "text": "Fer expression correlates with renal cell carcinoma cell proliferation both in vitro and in vivo, and with tumor progression and survival"}, {"pubmed": 23699534, "text": "data show that Fer kinase is elevated in non-small cell lung cancer tumors and is important for cellular invasion and metastasis"}, {"pubmed": 23873028, "text": "FER kinase promotes breast cancer metastasis by regulating alpha6- and beta1-integrin-dependent cell adhesion and anoikis resistance."}, {"pubmed": 23906537, "text": "Fer contributes to aberrant androgen receptor signaling via pSTAT3 cross-talk during castrate-resistant prostate cancer progression."}, {"pubmed": 23931849, "text": "Detection of PJA2-FER fusion mRNA is correlated with poor postoperative survival periods in non-small cell lung cancer."}, {"pubmed": 25533491, "text": "FER encodes a cytosolic non-receptor tyrosine kinase that influences neutrophil chemotaxis and endothelial permeability."}, {"pubmed": 25867068, "text": "Fer serves as a crucial mediator and amplifier of Src-induced tumor progression."}, {"pubmed": 28245430, "text": "Many human tumor types and cancer cell lines express the MAN2A1-FER fusion, which increases proliferation and invasiveness of cancer cell lines and has liver oncogenic activity in mice."}, {"pubmed": 28851893, "text": "The FER rs4957796 TT genotype remained a significant covariate for the 90-day mortality risk in the multivariate analysis (hazard ratio, 4.62; 95% CI, 1.58-13.50; p = 0.0050). In conclusion, FER rs4957796 might act as a prognostic variable for survival in patients with severe ARDS due to pneumonia."}, {"pubmed": 29038547, "text": "Data show that FER tyrosine kinase (Fer/FerT) activity was disrupted by E260, which selectively evokes metabolic stress in cancer cells by imposing mitochondrial dysfunction and deformation."}, {"pubmed": 29099290, "text": "FER mediated HGF-independent regulation of HGFR/MET activates RAC1-PAK1 pathway to potentiate metastasis in ovarian cancer."}, {"pubmed": 29208465, "text": "We propose a model for the regulation of Fer based on an intramolecular interaction and the curvature-dependent membrane binding mediated by its intrinsically disordered region."}, {"pubmed": 29540831, "text": "FER enhances IGF-1R expression, phosphorylation, and signaling to promote cooperative growth and adhesion signaling that may facilitate cancer progression."}, {"pubmed": 29907877, "text": "FER overexpression improves survival through STAT activation enhancing innate immunity and accelerating bacterial clearance in the lung."}, {"pubmed": 29920310, "text": "Targeting of both FER and MET may be an effective strategy for therapeutic intervention in ovarian cancer."}, {"pubmed": 31746983, "text": "This study supports the critical role of FER and FES tyrosine kinase fusions in the pathogenesis of follicular T-cell lymphoma and provides additional evidence that these can drive follicular T-cell lymphoma in the absence of RHOA mutations."}, {"pubmed": 33411917, "text": "Phosphorylation of PKCdelta by FER tips the balance from EGFR degradation to recycling."}, {"pubmed": 33430475, "text": "Fer and FerT Govern Mitochondrial Susceptibility to Metformin and Hypoxic Stress in Colon and Lung Carcinoma Cells."}, {"pubmed": 33806191, "text": "Loss of Fer Jeopardizes Metabolic Plasticity and Mitochondrial Homeostasis in Lung and Breast Carcinoma Cells."}, {"pubmed": 35550247, "text": "FER-mediated phosphorylation and PIK3R2 recruitment on IRS4 promotes AKT activation and tumorigenesis in ovarian cancer cells."}], "genomic_pos": {"chr": "5", "end": 109196841, "ensemblgene": "ENSG00000151422", "start": 108747841, "strand": 1}, "genomic_pos_hg19": {"chr": "5", "end": 108532542, "start": 108083523, "strand": 1}, "go": {"BP": [{"evidence": "IMP", "gocategory": "BP", "id": "GO:0000226", "pubmed": 12972546, "qualifier": "involved_in", "term": "microtubule cytoskeleton organization"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0001932", "qualifier": "involved_in", "term": "regulation of protein phosphorylation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006468", "qualifier": "involved_in", "term": "protein phosphorylation"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0006468", "pubmed": 2725517, "qualifier": "involved_in", "term": "protein phosphorylation"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0006935", "qualifier": "involved_in", "term": "chemotaxis"}, {"evidence": "IBA", "gocategory": "BP", "id": "GO:0007155", "qualifier": "involved_in", "term": "cell adhesion"}, {"evidence": "IDA", "gocategory": "BP", "id": "GO:0007260", "pubmed": 19147545, "qualifier": "involved_in", "term": "tyrosine phosphorylation of STAT protein"}, {"evidence": "IEA", "gocategory": "BP", "id": "GO:0007281", "qualifier": "involved_in", "term": "germ cell development"}, {"evidence": "IMP", "gocategory": "BP", "id": "GO:0008284", "pubmed": 25540073, "qualifier": "involved_in", "term": "positive regulation of cell population proliferation"}, {"evidence": "TAS", "gocategory": "BP", "id": "GO:0008284", "pubmed": 20111072, "qualifier": "involved_in", "term": "positive regulation of cell population proliferation"}, {"evidence": "ISS", "gocategory": "BP", "id": "GO:0008286", "qualifier": "i