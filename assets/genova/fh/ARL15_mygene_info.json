: 53886560, "chr": "5", "position": [[53883941, 53886713], [54113201, 54113410], [54154579, 54154639], [54171783, 54171928], [54310431, 54310573]], "strand": -1, "transcript": "NM_019087", "txend": 54310573, "txstart": 53883941}], "exons_hg19": [{"cdsend": 53606309, "cdsstart": 53182390, "chr": "5", "position": [[53179771, 53182543], [53409031, 53409240], [53450409, 53450469], [53467613, 53467758], [53606261, 53606403]], "strand": -1, "transcript": "NM_019087", "txend": 53606403, "txstart": 53179771}], "generif": [{"pubmed": 20011104, "text": "Observational study, meta-analysis, and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 20011104, "text": "findings identify a novel protein, ARL15, which influences circulating adiponectin levels and may impact upon CHD risk"}, {"pubmed": 20379614, "text": "Clinical trial of gene-disease association and gene-environment interaction. (HuGE Navigator)"}, {"pubmed": 20950786, "text": "Observational study and genome-wide association study of gene-disease association. (HuGE Navigator)"}, {"pubmed": 23918589, "text": "In this first GWAS of rheumatoid arthritis among North Indians, ARL15 emerged as a novel genetic risk factor in addition to the classic HLA locus."}, {"pubmed": 24688318, "text": "The SNPs in ADP-ribosylation factor-like protein 15 (ARL15) and the T-cadherin (CDH13) genes did not exhibit significant association with individual metabolic traits in the T2DM and NDM groups."}, {"pubmed": 26722494, "text": "These findings indicated that there may be a racial/ethnic- and/or sex-specific association of the ARL15 rs6450176 single nucleotide polymorphism and serum lipid levels."}, {"pubmed": 29242557, "text": "ARL15 plays a role in adipocyte differentiation and adiponectin secretion"}, {"pubmed": 29382430, "text": "The ARL15 and HLA-DMA gene polymorphisms are associated with RA risk in Northwestern Han Chinese population."}, {"pubmed": 29465355, "text": "ARL15 expression in rheumatoid arthritis synovial fibroblasts (RASF) play likely role in inflammation and metabolic syndromes through a TNF independent pathway."}, {"pubmed": 33356902, "text": "Computational insight into the three-dimensional structure of ADP ribosylation factor like protein 15, a novel susceptibility gene for rheumatoid arthritis."}, {"pubmed": 34089346, "text": "Magnesium homeostasis is regulated by ARL15 through N-glycosylations of CNNMs."}, {"pubmed": 34187973, "text": "Correlation between an intronic SNP genotype and ARL15 level in rheumatoid arthritis."}, {"pubmed": 36302536, "text": "Adenosine diphosphate-ribosylation factor-like 15 can regulate glycolysis and lipogenesis related genes in colon cancer."}, {"pubmed": 37718516, "text": "ARL15 and its Multiple Disease Association: Emerging Functions and Potential Therapeutic Application."}], "genomic_pos": {"chr": "5", "end": 54310582, "ensemblgene": "ENSG00000185305", "start": 53883942, "strand": -1}, "genomic_pos_hg19": {"chr": "5", "end": 53606412, "start": 53179775, "strand": -1}, "go": {"BP": {"evidence": "ND", "gocategory": "BP", "id": "GO:0008150", "qualifier": "involved_in", "term": "biological_process"}, "CC": {"evidence": "HDA", "gocategory": "CC", "id": "GO:0070062", "pubmed": [19056867, 23533145], "qualifier": "located_in", "term": "extracellular exosome"}, "MF": [{"category": "MF", "evidence": "ND", "id": "GO:0003674", "qualifier": "enables", "term": "molecular_function"}, {"category": "MF", "evidence": "IEA", "id": "GO:0003924", "qualifier": "enables", "term": "GTPase activity"}, {"category": "MF", "evidence": "IPI", "id": "GO:0005515", "pubmed": [16169070, 32296183], "qualifier": "enables", "term": "protein binding"}, {"category": "MF", "evidence": "IEA", "id": "GO:0005525", "qualifier": "enables", "term": "GTP binding"}]}, "homologene": {"genes": [[7955, 560030], [8364, 493266], [9031, 769515], [9544, 707840], [9598, 461876], [9606, 54622], [9615, 489204], [9913, 534329], [10090, 218639], [10116, 689079]], "i